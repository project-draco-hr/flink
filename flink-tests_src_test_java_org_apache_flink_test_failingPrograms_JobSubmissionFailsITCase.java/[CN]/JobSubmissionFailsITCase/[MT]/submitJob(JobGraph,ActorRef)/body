{
  if (detached) {
    JobClient.submitJobDetached(jobGraph,jobClient,TestingUtils.TESTING_DURATION());
    return null;
  }
 else {
    SerializedJobExecutionResult result=JobClient.submitJobAndWait(jobGraph,false,jobClient,TestingUtils.TESTING_DURATION());
    return result.toJobExecutionResult(getClass().getClassLoader());
  }
}
