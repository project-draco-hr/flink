{
  try {
    try {
      submitJob(null);
      fail("Expected JobSubmissionException.");
    }
 catch (    NullPointerException e) {
    }
catch (    Throwable t) {
      t.printStackTrace();
      fail("Caught wrong exception of type " + t.getClass() + ".");
    }
    cluser.submitJobAndWait(workingJobGraph,false);
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail(e.getMessage());
  }
}
