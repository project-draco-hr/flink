{
  if (other == null || !(other instanceof GroupedStreamDiscretizer)) {
    return false;
  }
 else {
    try {
      @SuppressWarnings("unchecked") GroupedStreamDiscretizer<IN> otherDiscretizer=(GroupedStreamDiscretizer<IN>)other;
      return triggerPolicy.equals(otherDiscretizer.triggerPolicy) && evictionPolicy.equals(otherDiscretizer.evictionPolicy) && keySelector.equals(otherDiscretizer.keySelector);
    }
 catch (    ClassCastException e) {
      return false;
    }
  }
}
