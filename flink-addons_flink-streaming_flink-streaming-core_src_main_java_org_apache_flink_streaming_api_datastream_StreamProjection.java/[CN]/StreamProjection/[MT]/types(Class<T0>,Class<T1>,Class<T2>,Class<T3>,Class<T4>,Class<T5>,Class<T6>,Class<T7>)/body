{
  Class<?>[] types={type0,type1,type2,type3,type4,type5,type6,type7};
  if (types.length != this.fieldIndexes.length) {
    throw new IllegalArgumentException("Numbers of projected fields and types do not match.");
  }
  TypeWrapper<Tuple8<T0,T1,T2,T3,T4,T5,T6,T7>> outTypeWrapper=new ProjectTypeWrapper<IN,Tuple8<T0,T1,T2,T3,T4,T5,T6,T7>>(inTypeWrapper,fieldIndexes,types);
  return dataStream.addFunction("projection",null,inTypeWrapper,outTypeWrapper,new ProjectInvokable<IN,Tuple8<T0,T1,T2,T3,T4,T5,T6,T7>>(fieldIndexes,outTypeWrapper));
}
