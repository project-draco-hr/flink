{
  Class<?>[] types={type0};
  if (types.length != this.fieldIndexes.length) {
    throw new IllegalArgumentException("Numbers of projected fields and types do not match.");
  }
  TypeWrapper<Tuple1<T0>> outTypeWrapper=new ProjectTypeWrapper<IN,Tuple1<T0>>(inTypeWrapper,fieldIndexes,types);
  return dataStream.addFunction("projection",null,inTypeWrapper,outTypeWrapper,new ProjectInvokable<IN,Tuple1<T0>>(fieldIndexes,outTypeWrapper));
}
