{
  Class<?>[] types={type0,type1,type2,type3,type4};
  if (types.length != this.fieldIndexes.length) {
    throw new IllegalArgumentException("Numbers of projected fields and types do not match.");
  }
  TypeWrapper<Tuple5<T0,T1,T2,T3,T4>> outTypeWrapper=new ProjectTypeWrapper<IN,Tuple5<T0,T1,T2,T3,T4>>(inTypeWrapper,fieldIndexes,types);
  return dataStream.addFunction("projection",null,inTypeWrapper,outTypeWrapper,new ProjectInvokable<IN,Tuple5<T0,T1,T2,T3,T4>>(fieldIndexes,outTypeWrapper));
}
