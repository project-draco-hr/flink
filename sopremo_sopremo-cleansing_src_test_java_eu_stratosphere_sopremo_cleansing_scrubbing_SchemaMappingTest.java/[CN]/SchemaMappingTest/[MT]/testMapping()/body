{
  final SchemaMapping schemaMapping=new SchemaMapping();
  final SopremoTestPlan sopremoTestPlan=new SopremoTestPlan(schemaMapping);
  for (  ValidationRule rule : this.validationRules)   schemaMapping.addRule(rule);
  sopremoTestPlan.getInput(0).add(createPactJsonObject("stringInsteadOfInteger","12","outsideMonthRange",14,"shouldBeNonNull",null));
  if (this.expectedObject == ERROR)   sopremoTestPlan.getExpectedOutput(0).setEmpty();
 else   sopremoTestPlan.getExpectedOutput(0).add(this.expectedObject);
  sopremoTestPlan.run();
}
