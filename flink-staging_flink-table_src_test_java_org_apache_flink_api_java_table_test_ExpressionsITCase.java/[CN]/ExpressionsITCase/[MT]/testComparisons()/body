{
  ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  TableEnvironment tableEnv=new TableEnvironment();
  DataSource<Tuple3<Integer,Integer,Integer>> input=env.fromElements(new Tuple3<Integer,Integer,Integer>(5,5,4));
  Table table=tableEnv.toTable(input,"a, b, c");
  Table result=table.select("a > c, a >= b, a < c, a.isNull, a.isNotNull");
  DataSet<Row> ds=tableEnv.toSet(result,Row.class);
  ds.writeAsText(resultPath,FileSystem.WriteMode.OVERWRITE);
  env.execute();
  expected="true,true,false,false,true";
}
