{
  uid=new UID();
  uid.read(in);
  numOfFields=in.readByte();
  numOfTuples=in.readInt();
  tupleBatch=new ArrayList<T>(numOfTuples);
  byte[] typesInByte=new byte[numOfFields];
  in.readFully(typesInByte,0,numOfFields);
  TupleTypeInfo<T> typeInfo=new TupleTypeInfo<T>(tupleTypesFromByteArray(typesInByte));
  TupleSerializer<T> tupleSerializer=typeInfo.createSerializer();
  DeserializationDelegate<T> dd=new DeserializationDelegate<T>(tupleSerializer);
  for (int k=0; k < numOfTuples; ++k) {
    dd.setInstance(tupleSerializer.createInstance());
    dd.read(in);
    tupleBatch.add(dd.getInstance());
  }
}
