{
switch (triggerResult) {
case FIRE:
{
      Context context=windows.get(window);
      if (context == null) {
        LOG.debug("Window {} already gone.",window);
        return;
      }
      emitWindow(context);
      break;
    }
case FIRE_AND_PURGE:
{
    Context context=windows.remove(window);
    if (context == null) {
      LOG.debug("Window {} already gone.",window);
      return;
    }
    emitWindow(context);
    break;
  }
case CONTINUE:
}
}
