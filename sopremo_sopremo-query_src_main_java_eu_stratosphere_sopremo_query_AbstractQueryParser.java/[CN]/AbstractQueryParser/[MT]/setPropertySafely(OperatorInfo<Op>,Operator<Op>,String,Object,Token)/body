{
  OperatorPropertyInfo property=info.getOperatorProperty(propertyName);
  if (property == null)   throw new QueryParserException("Unknown property",reference);
  try {
    property.setValue(op,value);
  }
 catch (  Exception e) {
    throw new QueryParserException(String.format("Cannot set value of property %s to %s",propertyName,value),reference);
  }
}
