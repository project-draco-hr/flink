{
  if (args.length > 0) {
    if (args.length == 2) {
      fileInput=true;
      fileOutput=true;
      inputPath=args[0];
      outputPath=args[1];
    }
 else {
      System.err.println("Usage: TopSpeedWindowingExample <input path> <output path>");
      return false;
    }
  }
  return true;
}
