{
  super.open(split);
  @SuppressWarnings("unchecked") FieldParser<Object>[] parsers=new FieldParser[fieldTypes.length];
  for (int i=0; i < fieldTypes.length; i++) {
    if (fieldTypes[i] != null) {
      Class<? extends FieldParser<?>> parserType=FieldParser.getParserForType(fieldTypes[i]);
      if (parserType == null) {
        throw new RuntimeException("No parser available for type '" + fieldTypes[i].getName() + "'.");
      }
      @SuppressWarnings("unchecked") FieldParser<Object> p=(FieldParser<Object>)InstantiationUtil.instantiate(parserType,FieldParser.class);
      parsers[i]=p;
    }
  }
  this.fieldParsers=parsers;
  if (this.skipFirstLineAsHeader && this.splitStart == 0) {
    readLine();
  }
}
