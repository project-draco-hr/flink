{
  try {
    JsonNode result=TypeCoercer.INSTANCE.coerce(value,targetType);
    if (expectedResult instanceof BigIntegerNode) {
      Assert.assertTrue(String.format("%s->%s=%s",this.value.getClass(),this.targetType,result.getClass()),targetType.isInstance(result));
      Assert.assertEquals(String.format("%s->%s=%s",this.value,this.targetType,result),((BigIntegerNode)expectedResult).getBigIntegerValue(),result.getBigIntegerValue());
    }
 else {
      Assert.assertTrue(String.format("%s->%s=%s",this.value.getClass(),this.targetType,result.getClass()),targetType.isInstance(result));
      Assert.assertEquals(String.format("%s->%s=%s",this.value,this.targetType,result),expectedResult,result);
    }
  }
 catch (  CoercionException e) {
    Assert.assertTrue(String.format("%s->%s=Exception",this.value,this.targetType),expectedResult == CONVERSION_ERROR);
  }
}
