{
  try {
    final JsonNode result=TypeCoercer.INSTANCE.coerce(this.value,this.targetType);
    if (this.expectedResult instanceof BigIntegerNode) {
      Assert.assertTrue(String.format("%s->%s=%s",this.value.getClass(),this.targetType,result.getClass()),this.targetType.isInstance(result));
      Assert.assertEquals(String.format("%s->%s=%s",this.value,this.targetType,result),((BigIntegerNode)this.expectedResult).getBigIntegerValue(),((BigIntegerNode)result).getBigIntegerValue());
    }
 else {
      Assert.assertTrue(String.format("%s->%s=%s",this.value.getClass(),this.targetType,result.getClass()),this.targetType.isInstance(result));
      Assert.assertEquals(String.format("%s->%s=%s",this.value,this.targetType,result),this.expectedResult,result);
    }
  }
 catch (  final CoercionException e) {
    Assert.assertTrue(String.format("%s->%s=Exception",this.value,this.targetType),this.expectedResult == CONVERSION_ERROR);
  }
}
