{
  if (target == null || !clazz.isInstance(target)) {
    try {
      target=clazz.newInstance();
    }
 catch (    InstantiationException e) {
      throw new IllegalStateException("Expected type " + clazz.toString() + " has no public parameterless constructor.");
    }
catch (    IllegalAccessException e) {
      throw new IllegalStateException("Expected type " + clazz.toString() + " has no public parameterless constructor.");
    }
  }
 else {
    target.clear();
  }
  return target;
}
