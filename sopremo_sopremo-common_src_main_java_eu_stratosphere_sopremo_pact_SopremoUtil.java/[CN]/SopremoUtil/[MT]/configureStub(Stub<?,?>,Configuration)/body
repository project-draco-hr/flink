{
  for (  final Field stubField : stub.getClass().getDeclaredFields())   if ((stubField.getModifiers() & (Modifier.TRANSIENT | Modifier.FINAL | Modifier.STATIC)) == 0)   if (parameters.getString(stubField.getName(),null) != null)   try {
    stubField.setAccessible(true);
    stubField.set(stub,SopremoUtil.deserialize(parameters,stubField.getName(),Serializable.class));
  }
 catch (  final Exception e) {
    LOG.error(String.format("Could not set field %s of class %s: %s",stubField.getName(),stub.getClass(),e));
  }
}
