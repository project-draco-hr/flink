{
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple1<Integer>> source1=env.addSource(new MySource1(),1);
  DataStream<Tuple1<Integer>> source2=env.addSource(new MySource2(),1);
  DataStream<Tuple1<Integer>> source3=env.addSource(new MySource3(),1).connectWith(source1,source2).partitionBy(0).map(new MyJoinMap(),1).addSink(new JoinSink());
  env.execute();
  fillMultipleJoinSet();
  assertEquals(multipleJoinSetExpected,joinSetResult);
}
