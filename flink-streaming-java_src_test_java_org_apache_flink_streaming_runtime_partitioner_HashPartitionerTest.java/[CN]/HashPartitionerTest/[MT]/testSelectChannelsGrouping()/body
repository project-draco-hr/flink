{
  sd1.setInstance(streamRecord1);
  sd2.setInstance(streamRecord2);
  assertArrayEquals(hashPartitioner.selectChannels(sd1,1),hashPartitioner.selectChannels(sd2,1));
  assertArrayEquals(hashPartitioner.selectChannels(sd1,2),hashPartitioner.selectChannels(sd2,2));
  assertArrayEquals(hashPartitioner.selectChannels(sd1,1024),hashPartitioner.selectChannels(sd2,1024));
}
