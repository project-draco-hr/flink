{
  validateStateId(stateId);
  try (Statement smt=con.createStatement()){
    smt.executeUpdate("DELETE state.* FROM kvstate_" + stateId + " AS state"+ " JOIN"+ " ("+ " 	SELECT MAX(timestamp) AS maxts, k FROM kvstate_"+ stateId+ " 	WHERE timestamp BETWEEN "+ lowerId+ " AND "+ upperId+ " 	GROUP BY k"+ " ) m"+ " ON state.k = m.k"+ " AND state.timestamp >= "+ lowerId);
  }
 }
