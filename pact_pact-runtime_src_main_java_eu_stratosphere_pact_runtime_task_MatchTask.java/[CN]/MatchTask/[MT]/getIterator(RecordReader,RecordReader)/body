{
  final MemoryManager memoryManager=getEnvironment().getMemoryManager();
  final IOManager ioManager=getEnvironment().getIOManager();
switch (config.getLocalStrategy()) {
case SORTMERGE:
    return new SortMergeMatchIterator(memoryManager,ioManager,reader1,reader2,matchStub.getFirstInKeyType(),matchStub.getFirstInValueType(),matchStub.getSecondInValueType(),(long)(this.availableMemory * (1.0 - MEMORY_SHARE_RATIO)),this.maxFileHandles,this);
default :
  throw new RuntimeException("Unknown local strategy for MatchTask");
}
}
