{
  if (this.isRunning) {
    try {
      outputHandler.broadcastBarrier(id);
      confirmBarrier(id);
      if (LOG.isDebugEnabled()) {
        LOG.debug("Superstep " + id + " processed: "+ StreamVertex.this);
      }
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
}
