{
  try {
    outputHandler.broadcastBarrier(id);
    System.out.println("Superstep " + id + " processed: "+ StreamVertex.this);
    if (LOG.isDebugEnabled()) {
      LOG.debug("Superstep " + id + " processed: "+ StreamVertex.this);
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  InterruptedException e) {
    e.printStackTrace();
  }
}
