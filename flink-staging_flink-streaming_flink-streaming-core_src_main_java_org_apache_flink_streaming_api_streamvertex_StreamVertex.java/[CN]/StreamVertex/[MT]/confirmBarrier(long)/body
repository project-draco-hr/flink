{
  if (configuration.getStateMonitoring() && states != null) {
    getEnvironment().getJobManager().tell(new StateBarrierAck(getEnvironment().getJobID(),getEnvironment().getJobVertexId(),context.getIndexOfThisSubtask(),barrierID,states),ActorRef.noSender());
  }
 else {
    getEnvironment().getJobManager().tell(new BarrierAck(getEnvironment().getJobID(),getEnvironment().getJobVertexId(),context.getIndexOfThisSubtask(),barrierID),ActorRef.noSender());
  }
}
