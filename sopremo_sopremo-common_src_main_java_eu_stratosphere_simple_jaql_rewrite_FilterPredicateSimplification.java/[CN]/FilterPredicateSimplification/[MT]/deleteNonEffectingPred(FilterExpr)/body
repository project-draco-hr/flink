{
  boolean modify_flag=false;
  for (int i=0; i < fe.conjunctivePred_count(); i++) {
    Expr pred=fe.conjunctivePred(i);
    if ((pred.getProperty(ExprProperty.HAS_SIDE_EFFECTS,true).never()) && (!(pred instanceof ConstExpr))) {
      pred.detach();
      modify_flag=true;
    }
  }
  return modify_flag;
}
