{
  long val=0;
  boolean neg=false;
  if (bytes[startPos] == '-') {
    neg=true;
    startPos++;
  }
  for (int i=startPos; i < length; i++) {
    if (bytes[i] == delim) {
      field.setValue(val * (neg ? -1 : 1));
      return i + 1;
    }
    if (bytes[i] < 48 || bytes[i] > 57) {
      return -1;
    }
    val*=10;
    val+=bytes[i] - 48;
  }
  field.setValue(val * (neg ? -1 : 1));
  return length;
}
