def __init__(self, env, info, copy_set=False):
    self._env = env
    self._info = info
    if (not copy_set):
        self._info[_Fields.ID] = env._counter
        self._info[_Fields.BCVARS] = []
        self._info[_Fields.CHILDREN] = []
        self._info[_Fields.SINKS] = []
        self._info[_Fields.NAME] = None
        env._counter += 1
