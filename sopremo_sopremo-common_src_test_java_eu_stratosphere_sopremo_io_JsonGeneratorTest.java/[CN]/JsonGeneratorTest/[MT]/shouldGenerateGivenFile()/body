{
  try {
    final JsonParser parser=new JsonParser(new URL(SopremoTest.getResourcePath("SopremoTestPlan/test.json")));
    final File file=File.createTempFile("test","json");
    final JsonGenerator gen=new JsonGenerator(file);
    gen.writeStartArray();
    while (!parser.checkEnd())     gen.writeTree(parser.readValueAsTree());
    gen.writeEndArray();
    gen.close();
  }
 catch (  final IOException e) {
    e.printStackTrace();
  }
}
