{
  CoStreamFlatMap<String,Integer,String> invokable=new CoStreamFlatMap<String,Integer,String>(new MyCoFlatMap());
  List<String> expectedList=Arrays.asList("a","b","c","1","d","e","f","2","g","h","e","3","4","5");
  List<String> actualList=MockCoContext.createAndExecute(invokable,Arrays.asList("abc","def","ghe"),Arrays.asList(1,2,3,4,5));
  assertEquals(expectedList,actualList);
}
