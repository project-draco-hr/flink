    profilerClass=(Class<? extends TaskManagerProfiler>)Class.forName(profilerClassName);
  }
 catch (  ClassNotFoundException e) {
    LOG.error("Cannot find class " + profilerClassName + ": "+ StringUtils.stringifyException(e));
