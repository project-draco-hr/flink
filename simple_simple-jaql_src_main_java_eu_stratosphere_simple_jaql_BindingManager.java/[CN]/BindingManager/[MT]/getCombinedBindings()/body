{
  LinkedHashMap<String,Object> combinedBindings=new LinkedHashMap<String,Object>();
  Iterator<Map<String,Binding>> iterator=this.bindings.descendingIterator();
  while (iterator.hasNext())   for (  Map.Entry<String,Binding> binding : iterator.next().entrySet())   if (!combinedBindings.containsKey(binding.getKey()))   combinedBindings.put(binding.getKey(),binding.getValue());
  return combinedBindings;
}
