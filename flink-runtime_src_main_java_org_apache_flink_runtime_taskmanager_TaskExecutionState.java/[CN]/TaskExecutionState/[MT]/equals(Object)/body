{
  if (obj instanceof TaskExecutionState) {
    TaskExecutionState other=(TaskExecutionState)obj;
    return other.jobID.equals(this.jobID) && other.executionId.equals(this.executionId) && other.executionState == this.executionState && (other.description == null ? this.description == null : (this.description != null && other.description.equals(this.description)));
  }
 else {
    return false;
  }
}
