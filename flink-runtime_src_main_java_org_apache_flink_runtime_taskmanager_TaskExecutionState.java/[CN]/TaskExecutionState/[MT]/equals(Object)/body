{
  if (obj instanceof TaskExecutionState) {
    TaskExecutionState other=(TaskExecutionState)obj;
    return other.jobID.equals(this.jobID) && other.executionId.equals(this.executionId) && other.executionState == this.executionState && (other.throwable == null) == (this.throwable == null);
  }
 else {
    return false;
  }
}
