{
  if (obj instanceof TaskExecutionState) {
    TaskExecutionState other=(TaskExecutionState)obj;
    return other.jobID.equals(this.jobID) && other.executionId.equals(this.executionId) && other.executionState == this.executionState && (other.serializedError == null ? this.serializedError == null : (this.serializedError != null && Arrays.equals(this.serializedError,other.serializedError)));
  }
 else {
    return false;
  }
}
