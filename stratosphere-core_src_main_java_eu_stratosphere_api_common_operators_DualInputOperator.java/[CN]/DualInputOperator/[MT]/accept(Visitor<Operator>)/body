{
  boolean descend=visitor.preVisit(this);
  if (descend) {
    for (    Operator c : this.input1) {
      c.accept(visitor);
    }
    for (    Operator c : this.input2) {
      c.accept(visitor);
    }
    for (    Operator c : this.broadcastInputs.values()) {
      c.accept(visitor);
    }
    visitor.postVisit(this);
  }
}
