{
  final List<Method> functions=new ArrayList<Method>();
  for (  final Method method : methods) {
    final boolean compatibleSignature=isCompatibleSignature(method);
    if (SopremoUtil.LOG.isDebugEnabled())     SopremoUtil.LOG.debug(String.format("Method %s is %s compatible",method,compatibleSignature ? "" : " not"));
    if (compatibleSignature)     functions.add(method);
  }
  return functions;
}
