{
  final ExecutionEnvironment env=ExecutionEnvironment.createRemoteEnvironment("<remote>",6124,"/path/to/your/file.jar");
  DataSet<String> text=env.fromElements("To be, or not to be,--that is the question:--","Whether 'tis nobler in the mind to suffer","The slings and arrows of outrageous fortune","Or to take arms against a sea of troubles,");
  DataSet<Tuple2<String,Integer>> counts=text.flatMap(new LineSplitter()).groupBy(0).aggregate(Aggregations.SUM,1);
  RemoteCollectorImpl.collectLocal(counts,new RemoteCollectorConsumer<Tuple2<String,Integer>>(){
    @Override public void collect(    Tuple2<String,Integer> element){
      System.out.println("word/occurrences:" + element);
    }
  }
);
  Set<Tuple2<String,Integer>> collection=new HashSet<Tuple2<String,Integer>>();
  RemoteCollectorImpl.collectLocal(counts,collection);
  env.execute("WordCount Example with RemoteCollectorOutputFormat");
  System.out.println(collection);
  RemoteCollectorImpl.shutdownAll();
}
