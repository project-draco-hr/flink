{
  this.context.increaseInputCounter();
  if (SopremoUtil.LOG.isTraceEnabled()) {
    final ArrayList<JsonNode> cached=new ArrayList<JsonNode>();
    while (values.hasNext())     cached.add(values.next());
    SopremoUtil.LOG.trace(String.format("%s %s/%s",this.getContext().operatorTrace(),key,cached));
    values=cached.iterator();
  }
  try {
    this.reduce(key,JsonUtil.wrapWithNode(this.needsResettableIterator(key,values),values),new JsonCollector(out));
  }
 catch (  final RuntimeException e) {
    SopremoUtil.LOG.error(String.format("Error occurred @ %s with k/v %s/%s: %s",this.getContext().operatorTrace(),key,values,e));
    throw e;
  }
}
