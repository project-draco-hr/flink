{
  int batchSize=taskConfiguration.getInteger("batchSize",1);
  long batchTimeout=taskConfiguration.getLong("batchTimeout",1000);
  collector=new StreamCollector2<Tuple>(batchsizes_s,batchsizes_f,numOfOutputs_f,keyPosition,batchTimeout,id,outSerializationDelegate,outputs_f,outputs_s);
  return collector;
}
