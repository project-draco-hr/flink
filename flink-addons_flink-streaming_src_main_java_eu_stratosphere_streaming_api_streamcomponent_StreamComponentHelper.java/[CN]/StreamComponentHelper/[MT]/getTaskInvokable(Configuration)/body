{
  Class<? extends UserTaskInvokable> userFunctionClass=taskConfiguration.getClass("userfunction",DefaultTaskInvokable.class,UserTaskInvokable.class);
  UserTaskInvokable userFunction=null;
  byte[] userFunctionSerialized=taskConfiguration.getBytes("serializedudf",null);
  try {
    ObjectInputStream ois=new ObjectInputStream(new ByteArrayInputStream(userFunctionSerialized));
    userFunction=(UserTaskInvokable)ois.readObject();
  }
 catch (  Exception e) {
    if (log.isErrorEnabled()) {
      log.error("Cannot instanciate user function: " + userFunctionClass.getSimpleName());
    }
  }
  return userFunction;
}
