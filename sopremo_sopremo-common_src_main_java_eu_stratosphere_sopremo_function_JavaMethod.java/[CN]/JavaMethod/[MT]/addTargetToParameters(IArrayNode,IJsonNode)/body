{
  final int numParams=params.size();
  for (int cacheSize=this.paramCache.size(); cacheSize <= numParams; cacheSize++)   this.paramCache.add(new Object[cacheSize + 1]);
  final Object[] expandedParams=this.paramCache.get(numParams);
  expandedParams[0]=target;
  for (int index=0; index < numParams; index++)   expandedParams[index + 1]=params.get(index);
  return expandedParams;
}
