{
  for (int i=0; i < 100000; i++) {
    if (i % 50000 == 0) {
      time=System.currentTimeMillis();
      System.out.println("Source:\t\t" + i + "\t\tTime: "+ (time - prevTime));
      prevTime=time;
    }
    if (i % 2 == 0) {
      record.setString(0,"Gyula Marci");
    }
 else {
      record.setString(0,"Gabor Frank");
    }
    emit(record);
  }
}
