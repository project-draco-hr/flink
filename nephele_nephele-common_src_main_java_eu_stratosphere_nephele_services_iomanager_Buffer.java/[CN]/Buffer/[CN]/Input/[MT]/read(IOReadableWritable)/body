{
  final int pos=this.position;
  try {
    if (pos >= this.limit) {
      return false;
    }
    object.read(inView);
    final int newPos=this.inView.getPosition();
    if (newPos > limit) {
      return false;
    }
    this.repeatPosition=pos;
    this.position=newPos;
    return true;
  }
 catch (  IOException e) {
    this.inView.setPosition(pos);
    return false;
  }
}
