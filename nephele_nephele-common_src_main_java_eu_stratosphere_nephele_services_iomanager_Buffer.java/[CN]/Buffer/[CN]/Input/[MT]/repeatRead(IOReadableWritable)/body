{
  if (!isBound()) {
    throw new UnboundMemoryBackedException();
  }
  try {
    if (repeatPosition >= limit) {
      return false;
    }
    memory.inputView.setPosition(repeatPosition);
    memory.inputView.skip(4);
    object.read(memory.inputView);
    position=memory.inputView.getPosition();
    return position <= limit;
  }
 catch (  IOException e) {
    return false;
  }
}
