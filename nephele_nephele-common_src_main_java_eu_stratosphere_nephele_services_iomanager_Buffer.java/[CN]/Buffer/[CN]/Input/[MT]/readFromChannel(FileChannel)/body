{
  final long bytesRemaining=channel.size() - channel.position();
  if (bytesRemaining < 1) {
    this.position=0;
    this.limit=0;
    return;
  }
  final int toRead=(int)Math.min(this.memory.size(),bytesRemaining);
  int bytesRead=channel.read(memory.wrap(0,toRead));
  this.position=0;
  this.limit=bytesRead;
  this.inView.reset();
}
