{
  if (discretizerKey == null) {
    return new StreamDiscretizer<OUT>(getTrigger(),getEviction());
  }
 else   if (getTrigger() instanceof CentralActiveTrigger) {
    return new GroupedActiveDiscretizer<OUT>(discretizerKey,(CentralActiveTrigger<OUT>)getTrigger(),(CloneableEvictionPolicy<OUT>)getEviction());
  }
 else {
    return new GroupedStreamDiscretizer<OUT>(discretizerKey,(CloneableTriggerPolicy<OUT>)getTrigger(),(CloneableEvictionPolicy<OUT>)getEviction());
  }
}
