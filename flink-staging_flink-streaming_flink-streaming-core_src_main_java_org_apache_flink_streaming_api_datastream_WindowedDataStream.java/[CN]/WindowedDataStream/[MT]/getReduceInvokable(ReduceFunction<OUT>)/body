{
  StreamInvokable<OUT,OUT> invokable;
  if (isGrouped) {
    invokable=new GroupedWindowInvokable<OUT,OUT>(clean(reducer),keySelector,getDistributedTriggers(),getDistributedEvicters(),getCentralTriggers(),getCentralEvicters());
  }
 else {
    invokable=new WindowReduceInvokable<OUT>(clean(reducer),getTriggers(),getEvicters());
  }
  return invokable;
}
