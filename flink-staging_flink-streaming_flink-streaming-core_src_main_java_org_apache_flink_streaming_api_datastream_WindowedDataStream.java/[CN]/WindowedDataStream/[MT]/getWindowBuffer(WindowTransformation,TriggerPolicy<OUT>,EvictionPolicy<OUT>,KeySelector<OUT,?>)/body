{
  if (transformation == WindowTransformation.REDUCEWINDOW && eviction instanceof TumblingEvictionPolicy) {
    if (groupByKey == null) {
      return new TumblingPreReducer<OUT>(clean((ReduceFunction<OUT>)transformation.UDF),getType().createSerializer(getExecutionConfig()));
    }
 else {
      return new TumblingGroupedPreReducer<OUT>(clean((ReduceFunction<OUT>)transformation.UDF),groupByKey,getType().createSerializer(getExecutionConfig()));
    }
  }
  return new BasicWindowBuffer<OUT>();
}
