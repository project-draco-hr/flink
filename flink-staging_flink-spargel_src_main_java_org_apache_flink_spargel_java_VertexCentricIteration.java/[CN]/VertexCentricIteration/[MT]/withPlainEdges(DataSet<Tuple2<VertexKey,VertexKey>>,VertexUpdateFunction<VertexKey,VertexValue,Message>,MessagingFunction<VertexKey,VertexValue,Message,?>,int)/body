{
  @SuppressWarnings("unchecked") MessagingFunction<VertexKey,VertexValue,Message,Object> tmf=(MessagingFunction<VertexKey,VertexValue,Message,Object>)messagingFunction;
  return new VertexCentricIteration<VertexKey,VertexValue,Message,Object>(vertexUpdateFunction,tmf,edgesWithoutValue,maximumNumberOfIterations);
}
