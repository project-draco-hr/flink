{
  DataStream<F> iterationSink=new DataStreamSink<F>(input.environment,"Iteration Sink",null,null);
  input.streamGraph.addIterationTail(iterationSink.getId(),feedbackStream.getId(),input.iterationID,input.iterationWaitTime);
  input.connectGraph(feedbackStream,iterationSink.getId(),0);
  return feedbackStream;
}
