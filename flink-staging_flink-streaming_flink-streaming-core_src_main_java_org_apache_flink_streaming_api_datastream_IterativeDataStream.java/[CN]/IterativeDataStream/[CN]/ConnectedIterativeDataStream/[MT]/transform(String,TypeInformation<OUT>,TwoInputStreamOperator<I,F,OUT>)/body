{
  @SuppressWarnings({"unchecked","rawtypes"}) SingleOutputStreamOperator<OUT,?> returnStream=new SingleOutputStreamOperator(input.environment,outTypeInfo,operator);
  input.streamGraph.addCoOperator(returnStream.getId(),operator,input.getType(),feedbackType,outTypeInfo,functionName);
  input.connectGraph(input,returnStream.getId(),1);
  input.addIterationSource(returnStream,feedbackType);
  return returnStream;
}
