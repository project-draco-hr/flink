{
  DataStream<IN> iterationSink=new DataStreamSink<IN>(environment,"Iteration Sink",null,null);
  streamGraph.addIterationTail(iterationSink.getId(),iterationTail.getId(),iterationID,iterationWaitTime);
  if (keepPartitioning) {
    connectGraph(iterationTail,iterationSink.getId(),0);
  }
 else {
    connectGraph(iterationTail.forward(),iterationSink.getId(),0);
  }
  return iterationTail;
}
