{
  if (lastRet < 0)   throw new IllegalStateException();
  Object x=array[lastRet];
  lastRet=-1;
  lock.lock();
  try {
    for (Iterator<T> it=queue.iterator(); it.hasNext(); ) {
      if (it.next() == x) {
        it.remove();
        return;
      }
    }
  }
  finally {
    lock.unlock();
  }
}
