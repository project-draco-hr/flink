{
  long nanos=unit.toNanos(timeout);
  lock.lockInterruptibly();
  try {
    while (true) {
      T t=queue.poll();
      if (t != null)       return t;
      if (nanos <= 0)       return null;
      try {
        nanos=notEmpty.awaitNanos(nanos);
      }
 catch (      InterruptedException ie) {
        notEmpty.signal();
        throw ie;
      }
    }
  }
  finally {
    lock.unlock();
  }
}
