{
  if (c == null)   throw new NullPointerException();
  if (c == this)   throw new IllegalArgumentException();
  if (maxElements <= 0)   return 0;
  lock.lock();
  try {
    int n=0;
    T t;
    while (n < maxElements && (t=queue.poll()) != null) {
      c.add(t);
      ++n;
    }
    return n;
  }
  finally {
    lock.unlock();
  }
}
