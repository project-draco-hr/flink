{
  assertArrayEquals(fieldsPartitioner.selectChannels(streamRecord1,1),fieldsPartitioner.selectChannels(streamRecord2,1));
  assertArrayEquals(fieldsPartitioner.selectChannels(streamRecord1,2),fieldsPartitioner.selectChannels(streamRecord2,2));
  assertArrayEquals(fieldsPartitioner.selectChannels(streamRecord1,1024),fieldsPartitioner.selectChannels(streamRecord2,1024));
}
