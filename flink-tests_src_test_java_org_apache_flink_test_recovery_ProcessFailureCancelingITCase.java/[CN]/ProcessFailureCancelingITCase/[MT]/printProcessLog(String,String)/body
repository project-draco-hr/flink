{
  if (log == null || log.length() == 0) {
    return;
  }
  System.out.println("-----------------------------------------");
  System.out.println(" BEGIN SPAWNED PROCESS LOG FOR " + processName);
  System.out.println("-----------------------------------------");
  System.out.println(log);
  System.out.println("-----------------------------------------");
  System.out.println("		END SPAWNED PROCESS LOG");
  System.out.println("-----------------------------------------");
}
