{
  try {
    typeSerializer.serialize(record,outputView);
    for (    AbstractRecordWriter<T> writer : writers) {
      writer.emit(record);
    }
  }
 catch (  IOException e) {
    throw new RuntimeException("Unable to serialize the record",e);
  }
catch (  InterruptedException e) {
    throw new RuntimeException("Unable to serialize the record",e);
  }
}
