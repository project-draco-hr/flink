{
switch (newExecutionState) {
case RUNNING:
    this.taskManagerProfiler.registerMainThreadForCPUProfiling(ee,ee.getExecutingThread(),this.executionVertexID);
  break;
case FINISHING:
case FINISHED:
case CANCELED:
case FAILED:
this.taskManagerProfiler.unregisterMainThreadFromCPUProfiling(ee,ee.getExecutingThread());
break;
default :
LOG.error("Unexpected state transition to " + " for vertex " + this.executionVertexID);
break;
}
}
