{
  final ArrayNode pair=JsonUtil.asArray(value1,value2);
  if (this.similarityCondition.evaluate(pair,this.getContext()) == BooleanNode.TRUE)   out.collect(NullNode.getInstance(),JsonUtil.asArray(this.resultProjection1.evaluate(value1,this.getContext()),this.resultProjection2.evaluate(value2,this.getContext())));
}
