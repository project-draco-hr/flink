{
  if (mergedWindows != null && mergedWindows.size() > 0) {
    try {
      WindowOperator.this.getStateBackend().mergePartitionedStates(window,mergedWindows,windowSerializer,stateDescriptor);
    }
 catch (    Exception e) {
      throw new RuntimeException("Error while merging state.",e);
    }
  }
}
