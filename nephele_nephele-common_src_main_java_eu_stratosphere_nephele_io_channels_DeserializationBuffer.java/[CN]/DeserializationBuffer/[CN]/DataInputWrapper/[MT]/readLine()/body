{
  if (this.source.hasRemaining()) {
    StringBuilder bld=new StringBuilder();
    char curr;
    while (this.source.hasRemaining() && (curr=(char)readUnsignedByte()) != '\n') {
      bld.append(curr);
    }
    int len=bld.length();
    if (len > 0 && bld.charAt(len - 1) == '\r') {
      bld.setLength(len - 1);
    }
    String s=bld.toString();
    bld.setLength(0);
    return s;
  }
 else {
    return null;
  }
}
