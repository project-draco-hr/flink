{
  record.setId(channelID);
  emittedRecords.addRecord(record);
  for (  RecordWriter<StreamRecord> output : outputs) {
    try {
      output.emit(record);
    }
 catch (    Exception e) {
      System.out.println("Emit error: " + e.getMessage());
      emittedRecords.failRecord(record.getId());
    }
  }
}
