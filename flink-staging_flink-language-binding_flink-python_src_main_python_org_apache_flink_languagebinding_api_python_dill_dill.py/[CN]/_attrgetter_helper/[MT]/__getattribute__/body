def __getattribute__(self, attr):
    attrs = object.__getattribute__(self, 'attrs')
    index = object.__getattribute__(self, 'index')
    if (index is None):
        index = len(attrs)
        attrs.append(attr)
    else:
        attrs[index] = '.'.join([attrs[index], attr])
    return type(self)(attrs, index)
