def load(file):
    'unpickle an object from a file'
    pik = Unpickler(file)
    pik._main_module = _main_module
    obj = pik.load()
    if (type(obj).__module__ == _main_module.__name__):
        try:
            (obj.__class__ == getattr(pik._main_module, type(obj).__name__))
        except AttributeError:
            pass
    return obj
