@register(BuiltinMethodType)
def save_builtin_method(pickler, obj):
    if (obj.__self__ is not None):
        log.info(('B1: %s' % obj))
        pickler.save_reduce(_get_attr, (obj.__self__, obj.__name__), obj=obj)
    else:
        log.info(('B2: %s' % obj))
        StockPickler.save_global(pickler, obj)
    return
