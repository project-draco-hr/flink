@register(ClassType)
def save_classobj(pickler, obj):
    if (obj.__module__ == '__main__'):
        log.info(('C1: %s' % obj))
        pickler.save_reduce(ClassType, (obj.__name__, obj.__bases__, obj.__dict__), obj=obj)
    else:
        log.info(('C2: %s' % obj))
        StockPickler.save_global(pickler, obj)
    return
