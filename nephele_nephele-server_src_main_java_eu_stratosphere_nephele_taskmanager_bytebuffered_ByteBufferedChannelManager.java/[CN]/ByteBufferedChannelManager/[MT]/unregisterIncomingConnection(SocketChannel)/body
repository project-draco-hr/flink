{
  final InetSocketAddress remoteInetSocketAddress=(InetSocketAddress)socketChannel.socket().getRemoteSocketAddress();
synchronized (this.incomingConnections) {
    final IncomingConnection incomingConnection=this.incomingConnections.remove(remoteInetSocketAddress.getAddress());
    if (incomingConnection == null) {
      LOG.error("Cannot unregister incoming connection from host " + remoteInetSocketAddress.getAddress());
      return;
    }
    this.sequenceToNumbersToExpectNext.put(remoteInetSocketAddress.getAddress(),incomingConnection.getSequenceNumberToExpectNext());
  }
}
