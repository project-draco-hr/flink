{
  boolean hasInput=true;
  while (hasInput) {
    hasInput=false;
    for (    RecordReader<Record> input : inputs) {
      if (input.hasNext()) {
        hasInput=true;
        Record record=input.next();
        Long id=StreamRecordProvider.popUUID(record);
        userFunction.invoke(record);
      }
    }
  }
}
