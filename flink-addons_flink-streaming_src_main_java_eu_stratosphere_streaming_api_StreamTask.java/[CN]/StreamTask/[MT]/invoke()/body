{
  boolean hasInput=true;
  while (hasInput) {
    hasInput=false;
    for (    RecordReader<Record> input : inputs) {
      if (input.hasNext()) {
        hasInput=true;
        for (        RecordWriter<Record> output : outputs) {
          userFunction.invoke(input.next(),output);
        }
      }
    }
  }
}
