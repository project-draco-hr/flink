{
  boolean hasInput=true;
  while (hasInput) {
    hasInput=false;
    for (    RecordReader<Record> input : inputs) {
      if (input.hasNext()) {
        hasInput=true;
        Record rec=input.next();
        rec.removeField(rec.getNumFields() - 1);
        userFunction.invoke(rec);
      }
    }
  }
}
