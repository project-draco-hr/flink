{
  inputs=new LinkedList<RecordReader<Record>>();
  outputs=new LinkedList<RecordWriter<Record>>();
  partitioners=new LinkedList<ChannelSelector<Record>>();
  userFunction=null;
  numberOfInputs=0;
  numberOfOutputs=0;
  numTasks++;
