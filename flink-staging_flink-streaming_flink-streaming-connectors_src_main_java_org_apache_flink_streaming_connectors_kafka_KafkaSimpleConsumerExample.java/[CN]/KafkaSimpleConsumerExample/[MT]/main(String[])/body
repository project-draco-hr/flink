{
  if (!parseParameters(args)) {
    return;
  }
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment().setParallelism(4);
  DataStream<String> kafkaStream=env.addSource(new PersistentKafkaSource<String>(host + ":" + port,topic,new JavaDefaultStringSchema()));
  kafkaStream.print();
  env.execute();
}
