{
  super.addSignature(member);
  if (this.declaringClass == null)   this.declaringClass=member.getDeclaringClass();
 else   if (member.getDeclaringClass() != this.declaringClass)   this.declaringClass=member.getDeclaringClass().isAssignableFrom(this.declaringClass) ? this.declaringClass : member.getDeclaringClass();
}
