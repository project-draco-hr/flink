{
  if (windowSize < 1) {
    throw new IllegalArgumentException("Window size must be positive");
  }
  if (slideInterval < 1) {
    throw new IllegalArgumentException("Slide interval must be positive");
  }
  return addCoFunction("coWindowReduce",clean(coWindowFunction),outTypeInfo,new CoWindowInvokable<IN1,IN2,OUT>(clean(coWindowFunction),windowSize,slideInterval,timestamp1,timestamp2));
}
