{
  FunctionTypeWrapper<IN1> in1TypeWrapper=new FunctionTypeWrapper<IN1>(coReducer,CoReduceFunction.class,0);
  FunctionTypeWrapper<IN2> in2TypeWrapper=new FunctionTypeWrapper<IN2>(coReducer,CoReduceFunction.class,1);
  FunctionTypeWrapper<OUT> outTypeWrapper=new FunctionTypeWrapper<OUT>(coReducer,CoReduceFunction.class,2);
  if (this.isGrouped) {
    return addCoFunction("coReduce",coReducer,in1TypeWrapper,in2TypeWrapper,outTypeWrapper,new CoGroupedReduceInvokable<IN1,IN2,OUT>(coReducer,keyPosition1,keyPosition2));
  }
 else {
    return addCoFunction("coReduce",coReducer,in1TypeWrapper,in2TypeWrapper,outTypeWrapper,new CoReduceInvokable<IN1,IN2,OUT>(coReducer));
  }
}
