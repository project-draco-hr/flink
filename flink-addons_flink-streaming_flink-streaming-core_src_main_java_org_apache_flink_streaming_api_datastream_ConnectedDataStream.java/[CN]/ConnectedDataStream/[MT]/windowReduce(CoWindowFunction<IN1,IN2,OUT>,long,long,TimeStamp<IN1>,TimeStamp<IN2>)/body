{
  if (windowSize < 1) {
    throw new IllegalArgumentException("Window size must be positive");
  }
  if (slideInterval < 1) {
    throw new IllegalArgumentException("Slide interval must be positive");
  }
  TypeInformation<OUT> outTypeInfo=TypeExtractor.createTypeInfo(CoWindowFunction.class,coWindowFunction.getClass(),2,null,null);
  return addCoFunction("coWindowReduce",coWindowFunction,outTypeInfo,new CoWindowInvokable<IN1,IN2,OUT>(coWindowFunction,windowSize,slideInterval,timestamp1,timestamp2));
}
