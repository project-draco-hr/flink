{
  if (keyPosition1 < 0 || keyPosition2 < 0) {
    throw new IllegalArgumentException("The position of the field must be non-negative");
  }
  return new ConnectedDataStream<IN1,IN2>(this.environment,this.jobGraphBuilder,getFirst().setConnectionType(new FieldsPartitioner<IN1>(keyPosition1)),getSecond().setConnectionType(new FieldsPartitioner<IN2>(keyPosition2)));
}
