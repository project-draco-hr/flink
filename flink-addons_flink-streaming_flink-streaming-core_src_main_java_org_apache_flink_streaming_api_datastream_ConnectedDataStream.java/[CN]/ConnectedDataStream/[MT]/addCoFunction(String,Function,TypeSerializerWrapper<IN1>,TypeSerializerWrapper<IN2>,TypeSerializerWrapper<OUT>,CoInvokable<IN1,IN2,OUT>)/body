{
  @SuppressWarnings({"unchecked","rawtypes"}) SingleOutputStreamOperator<OUT,?> returnStream=new SingleOutputStreamOperator(environment,functionName,outTypeWrapper);
  try {
    input1.jobGraphBuilder.addCoTask(returnStream.getId(),functionInvokable,in1TypeWrapper,in2TypeWrapper,outTypeWrapper,functionName,SerializationUtils.serialize((Serializable)function),environment.getDegreeOfParallelism());
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize user defined function");
  }
  input1.connectGraph(input1,returnStream.getId(),1);
  input1.connectGraph(input2,returnStream.getId(),2);
  return returnStream;
}
