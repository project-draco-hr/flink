{
  if (keyPosition1 < 0 || keyPosition2 < 0) {
    throw new IllegalArgumentException("The position of the field must be non-negative");
  }
  return new GroupedConnectedDataStream<IN1,IN2>(this.environment,this.jobGraphBuilder,getFirst().partitionBy(keyPosition1),getSecond().partitionBy(keyPosition2),keyPosition1,keyPosition2);
}
