{
  dataStream1=dataStream1.groupBy(fieldIn1);
  dataStream2=dataStream2.groupBy(fieldIn2);
  JoinWindowFunction<IN1,IN2> joinWindowFunction=new JoinWindowFunction<IN1,IN2>(dataStream1.getOutputType(),dataStream2.getOutputType(),fieldIn1,fieldIn2);
  return addGeneralWindowJoin(joinWindowFunction,windowSize,slideInterval,timestamp1,timestamp2);
}
