{
  final IJsonNode target=new ArrayNode();
  final IJsonNode result=new ArrayAccess().evaluate(createArrayNode(createObjectNode("fieldName",1),createObjectNode("fieldName",2),createObjectNode("fieldName",3),createObjectNode("fieldName",4),createObjectNode("fieldName",5)),target,this.context);
  Assert.assertEquals(createArrayNode(createObjectNode("fieldName",1),createObjectNode("fieldName",2),createObjectNode("fieldName",3),createObjectNode("fieldName",4),createObjectNode("fieldName",5)),result);
  Assert.assertSame(target,result);
}
