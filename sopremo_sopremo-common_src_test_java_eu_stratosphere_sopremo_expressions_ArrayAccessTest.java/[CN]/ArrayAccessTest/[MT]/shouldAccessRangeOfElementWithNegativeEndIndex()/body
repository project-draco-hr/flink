{
  final JsonNode result=new ArrayAccess(1,-1).evaluate(createArrayNode(createObjectNode("fieldName",1),createObjectNode("fieldName",2),createObjectNode("fieldName",3),createObjectNode("fieldName",4),createObjectNode("fieldName",5)),this.context);
  Assert.assertEquals(createArrayNode(createObjectNode("fieldName",2),createObjectNode("fieldName",3),createObjectNode("fieldName",4),createObjectNode("fieldName",5)),result);
}
