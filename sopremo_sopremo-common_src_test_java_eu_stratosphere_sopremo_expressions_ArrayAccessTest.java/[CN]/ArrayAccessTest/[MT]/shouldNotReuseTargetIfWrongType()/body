{
  IJsonNode target=new ObjectNode();
  final IJsonNode result=new ArrayAccess().evaluate(createArrayNode(createObjectNode("fieldName",1),createObjectNode("fieldName",2)),target,this.context);
  Assert.assertEquals(createArrayNode(createObjectNode("fieldName",1),createObjectNode("fieldName",2)),result);
  Assert.assertNotSame(target,result);
}
