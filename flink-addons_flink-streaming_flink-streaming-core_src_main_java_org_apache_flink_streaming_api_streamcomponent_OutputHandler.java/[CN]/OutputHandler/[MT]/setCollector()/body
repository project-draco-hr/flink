{
  if (streamComponent.configuration.getDirectedEmit()) {
    OutputSelector<OUT> outputSelector=streamComponent.configuration.getOutputSelector();
    collector=new DirectedStreamCollector<OUT>(streamComponent.getInstanceID(),outSerializationDelegate,outputSelector);
  }
 else {
    collector=new StreamCollector<OUT>(streamComponent.getInstanceID(),outSerializationDelegate);
  }
  return collector;
}
