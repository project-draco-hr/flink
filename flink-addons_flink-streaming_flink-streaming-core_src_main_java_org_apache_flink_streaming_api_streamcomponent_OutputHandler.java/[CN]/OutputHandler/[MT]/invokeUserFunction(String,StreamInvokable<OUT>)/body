{
  if (LOG.isDebugEnabled()) {
    LOG.debug("{} {} invoked with instance id {}",componentTypeName,streamComponent.getName(),streamComponent.getInstanceID());
  }
  initializeOutputSerializers();
  try {
    streamComponent.invokeUserFunction(userInvokable);
  }
 catch (  Exception e) {
    flushOutputs();
    throw new RuntimeException(e);
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("{} {} invoke finished instance id {}",componentTypeName,streamComponent.getName(),streamComponent.getInstanceID());
  }
  flushOutputs();
}
