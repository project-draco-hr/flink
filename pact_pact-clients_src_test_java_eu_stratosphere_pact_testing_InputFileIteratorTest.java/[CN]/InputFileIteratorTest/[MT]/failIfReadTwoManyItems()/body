{
  PactRecord[] pairs={new PactRecord(new PactInteger(1),new PactString("test1")),new PactRecord(new PactInteger(2),new PactString("test2"))};
  InputFileIterator inputFileIterator=createFileIterator(pairs);
  while (inputFileIterator.hasNext())   Assert.assertNotNull(inputFileIterator.next());
  try {
    inputFileIterator.next();
    Assert.fail("should have thrown Exception");
  }
 catch (  NoSuchElementException e) {
  }
}
