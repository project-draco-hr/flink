{
  String testPlanFile=TestPlan.getTestPlanFile("fileIteratorTest");
  SequentialInputFormat inputFormat=FormatUtil.createInputFormat(SequentialInputFormat.class,testPlanFile,null);
  InputFileIterator inputFileIterator=new InputFileIterator(inputFormat);
  AssertUtil.assertIteratorEquals("input file iterator is not empty",new ArrayList<PactRecord>().iterator(),inputFileIterator,new PactRecordEqualer(PactInteger.class,PactString.class));
}
