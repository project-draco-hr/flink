{
  String testPlanFile=TestPlan.getTestPlanFile("fileIteratorTest");
  SequentialInputFormat<Key,Value> inputFormat=(SequentialInputFormat<Key,Value>)FormatUtil.createInputFormat(SequentialInputFormat.class,testPlanFile,null);
  InputFileIterator<Key,Value> inputFileIterator=new InputFileIterator<Key,Value>(true,inputFormat);
  AssertUtil.assertIteratorEquals("input file iterator is not empty",Arrays.asList().iterator(),inputFileIterator);
}
