{
  KeyValuePair<?,?>[] pairs={new KeyValuePair<Key,Value>(new PactInteger(1),new PactString("test1")),new KeyValuePair<Key,Value>(new PactInteger(2),new PactString("test2"))};
  InputFileIterator<Key,Value> inputFileIterator=createFileIterator(true,pairs);
  AssertUtil.assertIteratorEquals("input file iterator is does not return the right sequence of pairs",Arrays.asList(pairs).iterator(),inputFileIterator);
}
