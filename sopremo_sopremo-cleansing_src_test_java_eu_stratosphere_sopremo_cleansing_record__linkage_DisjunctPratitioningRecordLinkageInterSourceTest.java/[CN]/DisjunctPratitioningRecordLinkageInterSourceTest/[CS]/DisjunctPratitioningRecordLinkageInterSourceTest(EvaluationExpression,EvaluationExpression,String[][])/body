{
  super(resultProjection1,resultProjection2);
  this.leftBlockingKeys=new EvaluationExpression[blockingKeys[0].length];
  for (int index=0; index < this.leftBlockingKeys.length; index++)   this.leftBlockingKeys[index]=new ObjectAccess(blockingKeys[0][index]);
  this.rightBlockingKeys=new EvaluationExpression[blockingKeys[1].length];
  for (int index=0; index < this.rightBlockingKeys.length; index++)   this.rightBlockingKeys[index]=new ObjectAccess(blockingKeys[1][index]);
