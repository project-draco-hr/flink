{
  final EvaluationExpression[] projections={null,getAggregativeProjection()};
  final double[] thresholds={0.0,0.5,1.0};
  final ArrayList<Object[]> parameters=new ArrayList<Object[]>();
  for (  final EvaluationExpression projection : projections)   for (  final double threshold : thresholds)   for (  final String[][] combinedBlockingKeys : CombinedBlockingKeys)   parameters.add(new Object[]{threshold,projection,combinedBlockingKeys});
  return parameters;
}
