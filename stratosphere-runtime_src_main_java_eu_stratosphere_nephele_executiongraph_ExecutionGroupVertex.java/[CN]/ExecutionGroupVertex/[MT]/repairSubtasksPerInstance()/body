{
  final Iterator<ExecutionVertex> it=this.groupMembers.iterator();
  int count=0;
  while (it.hasNext()) {
    final ExecutionVertex v=it.next();
    v.setAllocatedResource(this.groupMembers.get((count++ / this.numberOfSubtasksPerInstance) * this.numberOfSubtasksPerInstance).getAllocatedResource());
  }
}
