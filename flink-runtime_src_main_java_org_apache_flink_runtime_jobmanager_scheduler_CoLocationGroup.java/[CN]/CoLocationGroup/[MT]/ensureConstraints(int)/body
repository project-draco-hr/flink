{
  if (constraints == null) {
    constraints=new ArrayList<CoLocationConstraint>(num);
  }
 else {
    constraints.ensureCapacity(num);
  }
  if (num > constraints.size()) {
    constraints.ensureCapacity(num);
    for (int i=constraints.size(); i < num; i++) {
      constraints.add(new CoLocationConstraint());
    }
  }
}
