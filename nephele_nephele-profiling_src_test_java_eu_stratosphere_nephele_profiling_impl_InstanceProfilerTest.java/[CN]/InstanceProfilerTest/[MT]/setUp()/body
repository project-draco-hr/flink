{
  initMocks(this);
  when(this.infoMock.getAddress()).thenReturn(this.addressMock);
  when(this.addressMock.getHostAddress()).thenReturn("192.168.1.1");
  createBufferMock(bufferMock,InstanceProfiler.ETC_NETWORK_INTERFACES);
