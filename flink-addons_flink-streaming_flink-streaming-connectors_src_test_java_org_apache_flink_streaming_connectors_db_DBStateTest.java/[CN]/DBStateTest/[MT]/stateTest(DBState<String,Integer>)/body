{
  state.put("k1",1);
  assertEquals(new Integer(1),state.get("k1"));
  state.put("k2",2);
  state.put("k3",3);
  assertEquals(new Integer(2),state.get("k2"));
  assertEquals(new Integer(3),state.get("k3"));
  state.remove("k2");
  try {
    state.get("k2");
    fail();
  }
 catch (  Exception e) {
  }
}
