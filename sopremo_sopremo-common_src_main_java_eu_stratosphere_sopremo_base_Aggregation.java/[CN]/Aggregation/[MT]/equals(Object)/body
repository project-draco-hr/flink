{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (this.getClass() != obj.getClass())   return false;
  Aggregation other=(Aggregation)obj;
  if (!this.groupings.equals(other.groupings))   return false;
  for (int index=0; index < this.groupings.size(); index++)   if (!this.groupings.get(index).equals(other.groupings.get(index)))   return false;
  return true;
}
