{
  GenericDataSource<InfiniteIntegerInputFormat> source=new GenericDataSource<InfiniteIntegerInputFormat>(new InfiniteIntegerInputFormat(),"Source");
  MapContract mapper=MapContract.builder(IdentityMapper.class).input(source).name("Identity Mapper").build();
  GenericDataSink sink=new GenericDataSink(new DiscardingOutputFormat(),mapper,"Sink");
  Plan p=new Plan(sink);
  p.setDefaultParallelism(4);
  runAndCancelJob(p,5 * 1000,10 * 1000);
}
