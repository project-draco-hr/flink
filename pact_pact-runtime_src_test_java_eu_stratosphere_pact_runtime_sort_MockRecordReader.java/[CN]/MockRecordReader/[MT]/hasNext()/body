{
  if (next == null) {
    Record r=null;
    while (r == null) {
      try {
        r=queue.take();
      }
 catch (      InterruptedException iex) {
        throw new RuntimeException("Reader was interrupted.");
      }
    }
    if (r == SENTINEL) {
      try {
        queue.put(r);
      }
 catch (      InterruptedException e) {
        throw new RuntimeException("Reader was interrupted.");
      }
      return false;
    }
 else {
      next=(T)r;
      return true;
    }
  }
 else {
    return true;
  }
}
