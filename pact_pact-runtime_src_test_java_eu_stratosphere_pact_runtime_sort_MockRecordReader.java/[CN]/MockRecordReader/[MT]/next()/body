{
  if (next != null) {
    T elem=next;
    next=null;
    return elem;
  }
  Record r=null;
  while (r == null) {
    try {
      r=queue.take();
    }
 catch (    InterruptedException iex) {
      throw new RuntimeException("Reader was interrupted.");
    }
  }
  if (r == SENTINEL) {
    try {
      queue.put(r);
    }
 catch (    InterruptedException iex) {
      throw new RuntimeException("Reader was interrupted.");
    }
    throw new NoSuchElementException();
  }
 else {
    return (T)r;
  }
}
