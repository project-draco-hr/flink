{
  if (next != null) {
    T elem=next;
    next=null;
    return elem;
  }
  Record r=null;
  while (r == null) {
    try {
      r=queue.take();
    }
 catch (    InterruptedException iex) {
    }
  }
  if (r == SENTINEL) {
    throw new NoSuchElementException();
  }
 else {
    return (T)r;
  }
}
