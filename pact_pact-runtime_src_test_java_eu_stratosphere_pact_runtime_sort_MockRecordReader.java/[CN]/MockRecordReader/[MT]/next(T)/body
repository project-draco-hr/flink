{
  T r=null;
  while (r == null) {
    try {
      r=queue.take();
    }
 catch (    InterruptedException iex) {
      throw new RuntimeException("Reader was interrupted.");
    }
  }
  if (r == SENTINEL) {
    try {
      queue.put((T)r);
    }
 catch (    InterruptedException e) {
      throw new RuntimeException("Reader was interrupted.");
    }
    return null;
  }
 else {
    return r;
  }
}
