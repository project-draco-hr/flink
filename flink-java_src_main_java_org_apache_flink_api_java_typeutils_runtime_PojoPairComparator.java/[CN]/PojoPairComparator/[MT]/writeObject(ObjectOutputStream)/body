{
  out.defaultWriteObject();
  out.writeInt(keyFields1.length);
  for (  Field field : keyFields1) {
    out.writeObject(field.getDeclaringClass());
    out.writeUTF(field.getName());
  }
  out.writeInt(keyFields2.length);
  for (  Field field : keyFields2) {
    out.writeObject(field.getDeclaringClass());
    out.writeUTF(field.getName());
  }
}
