{
  if (this.isSelectingAll())   return node;
  if (this.isSelectingRange()) {
    ArrayNode arrayNode=new ArrayNode(JsonUtil.NODE_FACTORY);
    for (int index=this.startIndex; index < this.endIndex; index++)     arrayNode.add(node.get(index));
    return arrayNode;
  }
  return node.get(this.startIndex);
}
