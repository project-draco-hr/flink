{
  Entry[] children=x.children;
  if (ht == 0) {
    for (int j=0; j < x.m; j++) {
      if (eq(key,children[j].key))       return (Value)children[j].value;
    }
  }
 else {
    for (int j=0; j < x.m; j++) {
      if (j + 1 == x.m || less(key,children[j + 1].key))       return search(children[j].next,key,ht - 1);
    }
  }
  return null;
}
