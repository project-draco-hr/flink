{
  long curMask=0;
  long tmp=0;
  int offset=0;
  int len=0;
  for (int k=0; k < mask.length; k++) {
    curMask=mask[k];
    offset=k * Long.SIZE;
    len=((offset + Long.SIZE) < this.numFields) ? Long.SIZE : this.numFields - offset;
    for (int l=0; l < len; l++) {
      tmp=(1L << l) & curMask;
      if (tmp != 0) {
        internallySetField(offset + l,null);
      }
    }
  }
}
