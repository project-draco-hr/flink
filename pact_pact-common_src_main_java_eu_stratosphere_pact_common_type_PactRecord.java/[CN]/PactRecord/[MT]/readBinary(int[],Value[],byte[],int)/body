{
  int val=binData[offset++] & 0xff;
  if (val >= MAX_BIT) {
    int shift=7;
    int curr;
    val=val & 0x7f;
    while ((curr=binData[offset++] & 0xff) >= MAX_BIT) {
      val|=(curr & 0x7f) << shift;
      shift+=7;
    }
    val|=curr << shift;
  }
  this.binaryData=binData;
  initFields(binData,offset,val);
  return getFieldsInto(fields,holders);
}
