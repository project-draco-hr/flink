{
  long curMask=0;
  int offset=0;
  long tmp=0;
  int len=0;
  int index=0;
  int oldNumFields=this.numFields;
  for (int k=0; k < mask.length; k++) {
    curMask=mask[k];
    offset=k * Long.SIZE;
    len=((offset + Long.SIZE) < oldNumFields) ? Long.SIZE : oldNumFields - offset;
    for (int l=0; l < len; l++) {
      tmp=(1L << l) & curMask;
      if (tmp == 0) {
        this.removeField(index);
      }
 else {
        index++;
      }
    }
  }
}
