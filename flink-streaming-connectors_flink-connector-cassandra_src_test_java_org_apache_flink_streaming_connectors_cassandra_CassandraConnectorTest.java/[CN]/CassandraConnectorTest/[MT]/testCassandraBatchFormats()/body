{
  ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  env.setParallelism(1);
  DataSet<Tuple3<String,Integer,Integer>> dataSet=env.fromCollection(collection);
  dataSet.output(new CassandraOutputFormat<Tuple3<String,Integer,Integer>>(INSERT_DATA_QUERY,builder));
  env.execute("Write data");
  DataSet<Tuple3<String,Integer,Integer>> inputDS=env.createInput(new CassandraInputFormat<Tuple3<String,Integer,Integer>>(SELECT_DATA_QUERY,builder),new TupleTypeInfo(Tuple3.class,BasicTypeInfo.STRING_TYPE_INFO,BasicTypeInfo.INT_TYPE_INFO,BasicTypeInfo.INT_TYPE_INFO));
  long count=inputDS.count();
  Assert.assertEquals(count,20L);
}
