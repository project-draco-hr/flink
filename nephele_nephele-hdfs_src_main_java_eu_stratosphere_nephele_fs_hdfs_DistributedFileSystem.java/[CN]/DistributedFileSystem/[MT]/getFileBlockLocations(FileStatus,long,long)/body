{
  if (!(file instanceof DistributedFileStatus))   throw new IOException("file is not an instance of DistributedFileStatus");
  DistributedFileStatus f=(DistributedFileStatus)file;
  org.apache.hadoop.fs.BlockLocation[] blkLocations=fs.getFileBlockLocations(f.getInternalFileStatus(),start,len);
  DistributedBlockLocation[] distBlkLocations=new DistributedBlockLocation[blkLocations.length];
  for (int i=0; i < distBlkLocations.length; i++)   distBlkLocations[i]=new DistributedBlockLocation(blkLocations[i]);
  return distBlkLocations;
}
