{
  final SopremoTestPlan plan=new SopremoTestPlan(new TestOperator());
  plan.getInput(0).add(this.createPoint("sample_1")).add(this.createPoint("sample_2"));
  plan.getInput(1).add(this.createPoint("rest_1"));
  plan.run();
  for (  final IJsonNode outputNode : plan.getActualOutput(0))   System.out.println(outputNode);
}
