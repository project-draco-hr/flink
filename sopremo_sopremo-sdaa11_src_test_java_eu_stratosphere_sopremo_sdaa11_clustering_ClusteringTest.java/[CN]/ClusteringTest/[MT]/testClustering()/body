{
  final Clustering clustering=new Clustering();
  final ElementarySopremoModule module=clustering.asElementaryOperators();
  module.inferSchema(new NaiveSchemaFactory());
  final SopremoTestPlan plan=new SopremoTestPlan(clustering);
  final List<IJsonNode> input1Nodes=Points.loadPoints(Points.POINTS1_PATH);
  for (  final IJsonNode inputNode : input1Nodes)   plan.getInput(0).add(inputNode);
  final List<IJsonNode> input2Nodes=Points.loadPoints(Points.POINTS2_PATH);
  for (  final IJsonNode inputNode : input2Nodes)   plan.getInput(1).add(inputNode);
  plan.run();
  for (  final IJsonNode outputNode : plan.getActualOutput(0))   System.out.println(outputNode);
}
