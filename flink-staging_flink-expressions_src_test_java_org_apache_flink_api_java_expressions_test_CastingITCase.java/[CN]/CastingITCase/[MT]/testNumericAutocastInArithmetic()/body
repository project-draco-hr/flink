{
  ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  DataSource<Tuple7<Byte,Short,Integer,Long,Float,Double,String>> input=env.fromElements(new Tuple7<Byte,Short,Integer,Long,Float,Double,String>((byte)1,(short)1,1,1L,1.0f,1.0d,"Hello"));
  ExpressionOperation<JavaBatchTranslator> expressionOperation=ExpressionUtil.from(input);
  ExpressionOperation<JavaBatchTranslator> result=expressionOperation.select("f0 + 1, f1 +" + " 1, f2 + 1L, f3 + 1.0f, f4 + 1.0d, f5 + 1");
  DataSet<Row> ds=ExpressionUtil.toSet(result,Row.class);
  ds.writeAsText(resultPath,FileSystem.WriteMode.OVERWRITE);
  env.execute();
  expected="2,2,2,2.0,2.0,2.0";
}
