{
  DataStream<T> returnStream=new DataStream<T>(this);
  ByteArrayOutputStream baos=new ByteArrayOutputStream();
  ObjectOutputStream oos;
  try {
    oos=new ObjectOutputStream(baos);
    oos.writeObject(sinkFunction);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  jobGraphBuilder.setSink(returnStream.getId(),new SinkInvokable<T>(sinkFunction),"sink",baos.toByteArray(),parallelism,(int)Math.ceil(parallelism / clusterSize));
  connectGraph(inputStream,returnStream.getId());
  return returnStream;
}
