{
  DataStream<R> returnStream=new DataStream<R>(this);
  ByteArrayOutputStream baos=new ByteArrayOutputStream();
  ObjectOutputStream oos;
  try {
    oos=new ObjectOutputStream(baos);
    oos.writeObject(reducer);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  jobGraphBuilder.setTask(returnStream.getId(),new BatchReduceInvokable<T,R>(reducer),"batchReduce",baos.toByteArray());
  connectGraph(inputStream,returnStream.getId());
  return returnStream;
}
