{
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple1<Integer>> model=env.addSource(new TrainingDataSource()).map(new PartialModelBuilder()).broadcast();
  DataStream<Tuple1<Integer>> prediction=env.addSource(new NewDataSource()).connectWith(model).map(new Predictor()).addSink(new IMLSink());
  env.execute();
}
