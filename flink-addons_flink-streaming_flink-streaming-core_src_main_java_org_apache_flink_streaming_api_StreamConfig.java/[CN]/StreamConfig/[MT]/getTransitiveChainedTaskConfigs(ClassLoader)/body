{
  try {
    Map<String,StreamConfig> confs=(Map<String,StreamConfig>)InstantiationUtil.readObjectFromConfig(this.config,CHAINED_TASK_CONFIG,cl);
    return confs == null ? new HashMap<String,StreamConfig>() : confs;
  }
 catch (  Exception e) {
    throw new RuntimeException("Could not instantiate configuration.");
  }
}
