{
  TypeWrapper<T> typeWrapper;
  try {
    typeWrapper=(TypeWrapper<T>)InstantiationUtil.readObjectFromConfig(this.config,key,cl);
  }
 catch (  Exception e) {
    throw new RuntimeException("Cannot load typeinfo");
  }
  if (typeWrapper != null) {
    return typeWrapper.getTypeInfo();
  }
 else {
    return null;
  }
}
