{
  if (this == obj) {
    return true;
  }
 else   if (obj == null) {
    return false;
  }
 else   if (getClass() != obj.getClass()) {
    return false;
  }
  GlobalProperties other=(GlobalProperties)obj;
  if (ordering.equals(other.getOrdering()) && partitioning == other.getPartitioning() && partitionedFields.equals(other.getPartitionedFiels())) {
    return true;
  }
 else {
    return false;
  }
}
