{
  if (obj != null && obj instanceof GlobalProperties) {
    GlobalProperties other=(GlobalProperties)obj;
    return (ordering == other.getOrdering() || (ordering != null && ordering.equals(other.getOrdering()))) && (partitioning == other.getPartitioning()) && (partitioningFields == other.partitioningFields || (partitioningFields != null && partitioningFields.equals(other.getPartitionedFields())));
  }
 else {
    return false;
  }
}
