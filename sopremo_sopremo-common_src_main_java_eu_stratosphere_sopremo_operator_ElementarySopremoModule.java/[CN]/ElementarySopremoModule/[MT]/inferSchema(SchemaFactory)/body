{
  final Set<EvaluationExpression> keyExpressions=new HashSet<EvaluationExpression>();
  for (  final ElementaryOperator<?> operator : this.getReachableNodes())   for (  final List<? extends EvaluationExpression> expressions : operator.getAllKeyExpressions())   keyExpressions.addAll(expressions);
  this.schema=schemaFactory.create(keyExpressions);
}
