{
  OneTimeTraverser.INSTANCE.traverse(ElementarySopremoModule.this.getAllOutputs(),OperatorNavigator.INSTANCE,new GraphTraverseListener<Operator<?>>(){
    @Override public void nodeTraversed(    final Operator<?> node){
      final PactModule module=node.asPactModule(PactAssembler.this.context);
      PactAssembler.this.modules.put(node,module);
      final List<FileDataSink> outputStubs=module.getOutputs();
      final List<List<Contract>> outputContracts=new ArrayList<List<Contract>>();
      for (      FileDataSink fileDataSink : outputStubs)       outputContracts.add(fileDataSink.getInputs());
      PactAssembler.this.operatorOutputs.put(node,outputContracts);
    }
  }
);
  for (  final PactModule module : this.modules.values())   module.validate();
}
