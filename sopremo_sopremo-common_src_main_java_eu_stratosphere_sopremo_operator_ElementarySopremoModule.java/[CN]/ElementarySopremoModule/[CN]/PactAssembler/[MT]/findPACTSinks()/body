{
  final List<Contract> pactSinks=new ArrayList<Contract>();
  for (  final Operator<?> sink : ElementarySopremoModule.this.getAllOutputs()) {
    for (    final FileDataSink outputStub : this.modules.get(sink).getAllOutputs())     if (sink instanceof Sink)     pactSinks.add(outputStub);
 else     pactSinks.addAll(outputStub.getInputs());
  }
  return pactSinks;
}
