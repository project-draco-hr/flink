{
  final ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  DataSet<Tuple5<Integer,Long,String,Long,Integer>> tupleDs=env.fromCollection(emptyTupleData,tupleTypeInfo);
  try {
    tupleDs.project(true,true,false,true,false);
  }
 catch (  Exception e) {
    Assert.fail();
  }
  try {
    tupleDs.project(false,true);
  }
 catch (  Exception e) {
    Assert.fail();
  }
  try {
    tupleDs.project(true,true,true,true,true,true,true,true,true,true,true,true,true,true,true,true,true,true,true,true,true,true,true);
    Assert.fail();
  }
 catch (  IllegalArgumentException iae) {
  }
catch (  Exception e) {
    Assert.fail();
  }
  try {
    tupleDs.project(false,false,false,false,false);
    Assert.fail();
  }
 catch (  IllegalArgumentException iae) {
  }
catch (  Exception e) {
    Assert.fail();
  }
  DataSet<Long> longDs=env.fromCollection(emptyLongData,BasicTypeInfo.LONG_TYPE_INFO);
  try {
    longDs.project(false,true);
    Assert.fail();
  }
 catch (  UnsupportedOperationException uoe) {
  }
catch (  Exception e) {
    Assert.fail();
  }
}
