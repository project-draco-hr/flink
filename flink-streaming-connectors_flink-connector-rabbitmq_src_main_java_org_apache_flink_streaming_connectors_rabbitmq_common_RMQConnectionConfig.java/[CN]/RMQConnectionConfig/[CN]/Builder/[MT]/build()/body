{
  if (this.uri != null) {
    return new RMQConnectionConfig(this.uri,this.networkRecoveryInterval,this.automaticRecovery,this.topologyRecovery,this.connectionTimeout,this.requestedChannelMax,this.requestedFrameMax,this.requestedHeartbeat);
  }
 else {
    return new RMQConnectionConfig(this.host,this.port,this.virtualHost,this.username,this.password,this.networkRecoveryInterval,this.automaticRecovery,this.topologyRecovery,this.connectionTimeout,this.requestedChannelMax,this.requestedFrameMax,this.requestedHeartbeat);
  }
}
