{
  return new NamedChildIterator("elementExpr","setExpr"){
    @Override protected void set(    int index,    EvaluationExpression childExpression){
      if (index == 0)       ElementInSetExpression.this.elementExpr=childExpression;
 else       ElementInSetExpression.this.setExpr=childExpression;
    }
    @Override protected EvaluationExpression get(    int index){
      if (index == 0)       return ElementInSetExpression.this.elementExpr;
      return ElementInSetExpression.this.setExpr;
    }
  }
;
}
