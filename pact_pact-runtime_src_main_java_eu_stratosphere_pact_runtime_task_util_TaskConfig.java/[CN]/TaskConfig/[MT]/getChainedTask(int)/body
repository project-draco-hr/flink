{
  String className=this.config.getString(CHAINING_TASK_PREFIX + chainPos,null);
  if (className == null)   throw new IllegalStateException("Chained Task Class missing");
  @SuppressWarnings("unchecked") final Class<ChainedTask<?,?>> clazz=(Class<ChainedTask<?,?>>)(Class<?>)ChainedTask.class;
  return Class.forName(className).asSubclass(clazz);
}
