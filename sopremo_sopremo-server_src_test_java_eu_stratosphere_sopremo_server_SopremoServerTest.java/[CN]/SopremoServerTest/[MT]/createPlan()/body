{
  final SopremoPlan plan=new SopremoPlan();
  final Source input=new Source(SopremoTest.createTemporaryFile("input"));
  final Selection selection=new Selection().withCondition(new OrExpression(new UnaryExpression(JsonUtil.createPath("0","mgr")),new ComparativeExpression(JsonUtil.createPath("0","income"),BinaryOperator.GREATER,new ConstantExpression(30000)))).withInputs(input);
  final Sink output=new Sink(SopremoTest.createTemporaryFile("output")).withInputs(selection);
  plan.setSinks(output);
  return plan;
}
