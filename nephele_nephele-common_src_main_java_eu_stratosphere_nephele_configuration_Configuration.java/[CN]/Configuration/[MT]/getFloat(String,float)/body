{
  final String str=this.confData.get(key);
  if (str == null) {
    return defaultValue;
  }
  try {
    return Float.parseFloat(str);
  }
 catch (  NumberFormatException nfe) {
    if (LOG.isDebugEnabled()) {
      LOG.debug(StringUtils.stringifyException(nfe));
    }
  }
  return defaultValue;
}
