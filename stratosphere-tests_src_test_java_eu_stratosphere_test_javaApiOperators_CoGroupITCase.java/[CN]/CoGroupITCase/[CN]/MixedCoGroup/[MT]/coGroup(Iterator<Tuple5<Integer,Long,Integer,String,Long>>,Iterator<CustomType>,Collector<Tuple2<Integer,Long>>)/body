{
  long sum=0;
  int id=0;
  while (first.hasNext()) {
    Tuple5<Integer,Long,Integer,String,Long> element=first.next();
    sum+=element.f2;
    id=element.f0;
  }
  while (second.hasNext()) {
    CustomType element=second.next();
    id=element.myInt;
    sum+=element.myLong;
  }
  out.collect(new Tuple2<Integer,Long>(id,sum));
}
