{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment();
  DataStream<Tuple2<String,Integer>> dataStream=env.readTextStream("/home/strato/stratosphere-distrib/resources/hamlet.txt",4).flatMap(new WordCountPerformanceSplitter(),2).partitionBy(0).map(new WordCountCounter(),2).addSink(new Mysink(),2);
  env.executeCluster();
}
