{
  if (fileOutput) {
    return Graph.fromCsvReader(verticesInputPath,edgesInSSSPInputPath,env).lineDelimiterEdges("\n").typesEdges(Long.class,Double.class).typesVertices(Long.class,Double.class);
  }
 else {
    System.err.println("Usage: IncrementalSSSP <vertex path> <edge path> <edges in SSSP> " + "<src id edge to be removed> <trg id edge to be removed> <val edge to be removed> " + "<output path> <max iterations>");
    return Graph.fromDataSet(IncrementalSSSPData.getDefaultVertexDataSet(env),IncrementalSSSPData.getDefaultEdgesInSSSP(env),env);
  }
}
