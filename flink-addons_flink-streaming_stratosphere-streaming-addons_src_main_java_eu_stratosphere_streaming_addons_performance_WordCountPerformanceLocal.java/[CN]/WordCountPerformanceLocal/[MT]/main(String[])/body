{
  TestDataUtil.downloadIfNotExists("hamlet.txt");
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple2<String,Integer>> dataStream=env.readTextStream("src/test/resources/testdata/hamlet.txt").flatMap(new WordCountPerformanceSplitter(),1).broadcast().map(new WordCountCounter(),3);
  dataStream.print();
  env.execute();
}
