{
  final String accessKey=System.getenv("AK");
  final String secretKey=System.getenv("SK");
  if (accessKey != null || secretKey != null) {
    Configuration conf=new Configuration();
    if (accessKey != null)     conf.setString(S3FileSystem.S3_ACCESS_KEY_KEY,accessKey);
    if (secretKey != null)     conf.setString(S3FileSystem.S3_SECRET_KEY_KEY,secretKey);
    GlobalConfiguration.includeConfiguration(conf);
  }
}
