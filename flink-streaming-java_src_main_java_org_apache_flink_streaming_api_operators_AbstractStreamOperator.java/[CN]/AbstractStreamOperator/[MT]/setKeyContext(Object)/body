{
  if (stateBackend != null) {
    try {
      stateBackend.setCurrentKey(key);
    }
 catch (    Exception e) {
      throw new RuntimeException("Exception occurred while setting the current key context.",e);
    }
  }
 else {
    throw new RuntimeException("Could not set the current key context, because the " + "AbstractStateBackend has not been initialized.");
  }
}
