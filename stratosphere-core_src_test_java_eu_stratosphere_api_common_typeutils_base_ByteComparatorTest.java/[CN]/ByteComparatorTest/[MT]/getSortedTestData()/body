{
  Random rnd=new Random(874597969123412338L);
  int rndByte=rnd.nextInt(Byte.MAX_VALUE);
  if (rndByte < 0) {
    rndByte=-rndByte;
  }
  if (rndByte == Byte.MAX_VALUE) {
    rndByte-=3;
  }
  if (rndByte <= 2) {
    rndByte+=3;
  }
  return new Byte[]{new Byte(Byte.MIN_VALUE),new Byte(new Integer(-rndByte).byteValue()),new Byte(new Integer(-1).byteValue()),new Byte(new Integer(0).byteValue()),new Byte(new Integer(1).byteValue()),new Byte(new Integer(2).byteValue()),new Byte(new Integer(rndByte).byteValue()),new Byte(Byte.MAX_VALUE)};
}
