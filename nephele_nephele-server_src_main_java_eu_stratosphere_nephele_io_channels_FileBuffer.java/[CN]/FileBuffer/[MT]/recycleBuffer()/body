{
  try {
    if (this.fileChannel != null) {
      this.fileBufferManager.releaseFileChannelForReading(this.ownerID,this.fileID);
      this.fileChannel=null;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  this.fileBufferManager.decreaseBufferCounter(this.ownerID,this.fileID);
}
