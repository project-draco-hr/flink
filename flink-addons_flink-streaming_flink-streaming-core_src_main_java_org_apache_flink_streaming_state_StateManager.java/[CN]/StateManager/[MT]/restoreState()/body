{
  ObjectInputStream ois=null;
  try {
    ois=new ObjectInputStream(new FileInputStream(filename));
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  for (  Object state : stateList) {
    try {
      state=ois.readObject();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
