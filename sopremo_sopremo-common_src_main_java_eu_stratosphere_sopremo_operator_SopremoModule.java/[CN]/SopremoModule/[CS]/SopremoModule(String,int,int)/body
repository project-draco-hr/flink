{
  super(name,numberOfInputs,numberOfOutputs,OperatorNavigator.INSTANCE);
  for (int index=0; index < numberOfInputs; index++)   setInput(index,new Source(String.format("%s %d",name,index)));
  for (int index=0; index < numberOfOutputs; index++)   setOutput(index,new Sink(String.format("%s %d",name,index)));
}
