{
  if (this.jobID == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    this.jobID.write(out);
  }
  if (this.executionVertexID == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    this.executionVertexID.write(out);
  }
  EnumUtils.writeEnum(out,this.checkpointState);
}
