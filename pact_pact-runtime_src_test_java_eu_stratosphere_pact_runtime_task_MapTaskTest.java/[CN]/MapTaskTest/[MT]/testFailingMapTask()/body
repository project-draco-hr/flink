{
  final int keyCnt=100;
  final int valCnt=20;
  addInput(new UniformPactRecordGenerator(keyCnt,valCnt,false));
  addOutput(this.outList);
  final MapDriver<PactRecord,PactRecord> testTask=new MapDriver<PactRecord,PactRecord>();
