{
  final ExecutionGroupVertexIterator it=new ExecutionGroupVertexIterator(this.executionGraph,true,this.executionGraph.getIndexOfCurrentExecutionStage());
  while (it.hasNext()) {
    final ExecutionGroupVertex groupVertex=it.next();
    for (int i=0; i < groupVertex.getCurrentNumberOfGroupMembers(); i++) {
      final ExecutionVertex executionVertex=groupVertex.getGroupMember(i);
      if (instanceProfilingData.getInstanceConnectionInfo().equals(executionVertex.getAllocatedResource().getInstance().getInstanceConnectionInfo())) {
        this.collectedInstanceProfilingData.put(instanceProfilingData.getInstanceConnectionInfo(),instanceProfilingData);
        return true;
      }
    }
  }
  return false;
}
