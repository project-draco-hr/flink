{
  return new TuplePairComparator<Tuple3<Integer,String,Double>,Tuple4<Integer,Float,Long,Double>>(new int[]{0,2},new int[]{0,3},new TypeComparator[]{new IntComparator(ascending),new DoubleComparator(ascending)},new TypeComparator[]{new IntComparator(ascending),new DoubleComparator(ascending)});
}
