{
  StringBuilder b=new StringBuilder("[");
  for (int i=0; i < nodes.length - 1; i++) {
    b.append(Integer.toString(nodes[i].id()));
    b.append(',');
  }
  if (nodes.length > 0) {
    b.append(Integer.toString(nodes[nodes.length - 1].id()));
    b.append(',');
  }
  b.append("]");
  return b.toString();
}
