{
  return (((long)this.memory[index] & 0xff) << 56) | (((long)this.memory[index + 1] & 0xff) << 48) | (((long)this.memory[index + 2] & 0xff) << 40)| (((long)this.memory[index + 3] & 0xff) << 32)| (((long)this.memory[index + 4] & 0xff) << 24)| (((long)this.memory[index + 5] & 0xff) << 16)| (((long)this.memory[index + 6] & 0xff) << 8)| (((long)this.memory[index + 7] & 0xff));
}
