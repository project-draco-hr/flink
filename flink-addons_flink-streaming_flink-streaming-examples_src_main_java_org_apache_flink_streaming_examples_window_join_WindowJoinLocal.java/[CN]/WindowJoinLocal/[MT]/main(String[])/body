{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.getExecutionEnvironment();
  DataStream<Tuple2<String,Integer>> grades=env.addSource(new GradeSource());
  DataStream<Tuple2<String,Integer>> salaries=env.addSource(new SalarySource());
  DataStream<Tuple2<Tuple2<String,Integer>,Tuple2<String,Integer>>> joinedStream=grades.windowJoin(salaries,1000,1000,0,0);
  joinedStream.print();
  env.execute();
}
