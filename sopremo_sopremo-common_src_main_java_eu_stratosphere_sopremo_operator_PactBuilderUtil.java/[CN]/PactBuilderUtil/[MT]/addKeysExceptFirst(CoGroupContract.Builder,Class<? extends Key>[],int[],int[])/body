{
  if (keyClasses.length != keyIndices1.length || keyClasses.length != keyIndices2.length) {
    throw new IllegalArgumentException("Lenght of keyClasses and keyIndices must match.");
  }
  for (int i=1; i < keyClasses.length; ++i) {
    builder.keyField(keyClasses[i],keyIndices1[i],keyIndices2[i]);
  }
}
