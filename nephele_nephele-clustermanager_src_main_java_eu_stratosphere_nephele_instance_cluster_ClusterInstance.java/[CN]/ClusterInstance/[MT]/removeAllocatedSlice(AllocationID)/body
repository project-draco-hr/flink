{
  final AllocatedSlice slice=this.allocatedSlices.remove(allocationID);
  if (slice != null) {
    this.remainingCapacity=new InstanceType(this.remainingCapacity.getIdentifier(),this.remainingCapacity.getNumberOfComputeUnits() + slice.getType().getNumberOfComputeUnits(),this.remainingCapacity.getNumberOfCores() + slice.getType().getNumberOfCores(),this.remainingCapacity.getMemorySize() + slice.getType().getMemorySize(),this.remainingCapacity.getDiskCapacity() + slice.getType().getDiskCapacity(),this.remainingCapacity.getPricePerHour());
  }
  return slice;
}
