{
  LogUtils.initializeDefaultConsoleLogger(Level.DEBUG,Level.INFO);
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple3<String,String,Integer>> source1=env.addSource(new JoinSourceOne(),SOURCE_PARALELISM);
  @SuppressWarnings("unused") DataStream<Tuple3<String,Integer,Integer>> source2=env.addSource(new JoinSourceTwo(),SOURCE_PARALELISM).connectWith(source1).partitionBy(1).flatMap(new JoinTask(),PARALELISM).addSink(new JoinSink());
  env.execute();
}
