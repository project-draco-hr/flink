{
  PactRecord next=records.next();
  out.collect(new PactRecord(new PactInteger(100 + next.getField(0,PactInteger.class).getValue()),next.getField(1,PactInteger.class)));
  while (records.hasNext()) {
    out.collect(records.next());
  }
  next=records2.next();
  out.collect(new PactRecord(new PactInteger(100 + next.getField(0,PactInteger.class).getValue()),next.getField(1,PactInteger.class)));
  while (records2.hasNext()) {
    out.collect(records2.next());
  }
}
