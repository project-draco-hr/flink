{
  if (this.combiner == null) {
    throw new RuntimeException("No combiner has been specified in Hadoop job. Flink reduce function is" + "declared combinable. Invalid behaviour.");
  }
 else {
    combineCollector.setFlinkCollector(out);
    iterator.set(values.iterator());
    combiner.reduce(iterator.getKey(),iterator,combineCollector,reporter);
  }
}
