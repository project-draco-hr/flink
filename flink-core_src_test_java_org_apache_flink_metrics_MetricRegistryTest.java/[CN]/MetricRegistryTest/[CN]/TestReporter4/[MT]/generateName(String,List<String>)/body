{
  if (name.compareTo("rootCounter") == 0) {
    Assert.assertEquals("host",scope.get(0));
    return "success";
  }
 else   if (name.compareTo("topCounter") == 0) {
    Assert.assertEquals("host",scope.get(0));
    Assert.assertEquals("taskmanager",scope.get(1));
    return "success";
  }
 else {
    Assert.fail();
    return null;
  }
}
