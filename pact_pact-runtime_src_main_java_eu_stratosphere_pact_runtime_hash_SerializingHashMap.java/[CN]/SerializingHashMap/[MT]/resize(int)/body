{
  int[] oldOffsets=offsets;
  int oldCapacity=oldOffsets.length;
  if (oldCapacity == MAXIMUM_CAPACITY) {
    threshold=Integer.MAX_VALUE;
    return;
  }
 else {
    int[] newOffsets=new int[newCapacity];
    transfer(newOffsets);
    offsets=newOffsets;
    threshold=(int)(newCapacity * loadFactor);
  }
}
