{
  super(pKeys);
  Preconditions.checkArgument(pMethod != PartitionMethod.RANGE || pKeys.toFieldList().isExactMatch(ordering.getInvolvedIndexes()),"Partition keys must match the given ordering.");
  this.pMethod=pMethod;
  this.customPartitioner=customPartitioner;
  this.distribution=distribution;
  this.ordering=ordering;
}
