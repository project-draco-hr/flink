{
  ArrayList<Boolean> fields=new ArrayList<Boolean>();
  int highestNum=0;
  for (int i=0; i < 64; i++) {
    long bitMask=0x1l << i;
    if ((mask & bitMask) != 0) {
      fields.add(true);
      highestNum=i;
    }
 else {
      fields.add(false);
    }
  }
  this.includedMask=new boolean[highestNum + 1];
  for (int i=0; i < highestNum; i++) {
    this.includedMask[i]=fields.get(i);
  }
  return this;
}
