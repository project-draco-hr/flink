{
  try {
    new CollectionDataSource("test_1d_valid_array","a","b","c");
  }
 catch (  Exception e) {
    e.printStackTrace();
    Assert.fail(e.getMessage());
  }
  try {
    new CollectionDataSource("test_2d_valid_array",new Object[][]{{1,"a"},{2,"b"},{3,"c"}});
  }
 catch (  Exception e) {
    e.printStackTrace();
    Assert.fail(e.getMessage());
  }
  try {
    new CollectionDataSource("test_1d_invalid_array",1,"b","c");
    Assert.fail("input type is different");
  }
 catch (  Exception e) {
  }
  try {
    new CollectionDataSource("test_2d_invalid_array",new Object[][]{{1,"a"},{2,"b"},{3,4}});
    Assert.fail("input type is different");
  }
 catch (  Exception e) {
  }
}
