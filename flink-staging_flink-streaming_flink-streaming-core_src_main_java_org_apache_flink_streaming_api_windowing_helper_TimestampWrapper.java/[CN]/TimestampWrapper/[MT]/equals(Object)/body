{
  if (other == null || !(other instanceof TimestampWrapper)) {
    return false;
  }
 else {
    try {
      @SuppressWarnings("unchecked") TimestampWrapper<T> otherTSW=(TimestampWrapper<T>)other;
      if (timestamp instanceof SystemTimestamp && otherTSW.timestamp instanceof SystemTimestamp) {
        return true;
      }
 else {
        return startTime == otherTSW.startTime && timestamp.getClass() == otherTSW.timestamp.getClass();
      }
    }
 catch (    ClassCastException e) {
      return false;
    }
  }
}
