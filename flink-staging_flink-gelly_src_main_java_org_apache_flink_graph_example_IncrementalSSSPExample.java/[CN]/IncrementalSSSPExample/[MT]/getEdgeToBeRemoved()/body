{
  if (fileOutput) {
    String[] edgeComponents=edgeToBeRemoved.split(",");
    return new Edge<Long,Double>(Long.parseLong(edgeComponents[0]),Long.parseLong(edgeComponents[1]),Double.parseDouble(edgeComponents[2]));
  }
 else {
    System.err.println("Usage: IncrementalSSSP <vertex path> <edge path> <edges in SSSP> <edge to be removed> " + "<output path> <max iterations>");
    return IncrementalSSSPData.getDefaultEdgeToBeRemoved();
  }
}
