{
  if (fileOutput) {
    return env.readCsvFile(edgesInSSSPInputPath).lineDelimiter("\n").types(Long.class,Long.class,Double.class).map(new MapFunction<Tuple3<Long,Long,Double>,Edge<Long,Double>>(){
      @Override public Edge<Long,Double> map(      Tuple3<Long,Long,Double> tuple3) throws Exception {
        return new Edge(tuple3.f0,tuple3.f1,tuple3.f2);
      }
    }
);
  }
 else {
    System.err.println("Usage: IncrementalSSSP <vertex path> <edge path> <edges in SSSP> <edge to be removed> " + "<output path> <max iterations>");
    return IncrementalSSSPData.getDefaultEdgesInSSSP(env);
  }
}
