{
  StringBuilder bld=new StringBuilder();
  bld.append(getDescriptionPrefix()).append(' ').append('[');
  for (  Integer i : this.collection) {
    bld.append(i);
    bld.append(',');
    bld.append(' ');
  }
  if (this.collection.size() > 0) {
    bld.setLength(bld.length() - 2);
  }
  bld.append(']');
  return bld.toString();
}
