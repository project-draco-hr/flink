{
  addVertex(vertexName,CoStreamVertex.class,taskInvokableObject,operatorName,serializedFunction,parallelism);
  addTypeSerializers(vertexName,new StreamRecordSerializer<IN1>(in1TypeInfo),new StreamRecordSerializer<IN2>(in2TypeInfo),new StreamRecordSerializer<OUT>(outTypeInfo),null);
  if (LOG.isDebugEnabled()) {
    LOG.debug("CO-TASK: {}",vertexName);
  }
}
