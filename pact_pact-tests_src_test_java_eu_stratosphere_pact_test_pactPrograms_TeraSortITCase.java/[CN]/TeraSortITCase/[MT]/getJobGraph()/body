{
  URL fileURL=getClass().getResource(INPUT_DATA_FILE);
  String inPath="file://" + fileURL.getPath();
  TeraSort ts=new TeraSort();
  Plan plan=ts.getPlan(this.config.getString("TeraSortITCase#NoSubtasks","1"),inPath,getFilesystemProvider().getURIPrefix() + resultPath);
  PactCompiler pc=new PactCompiler(new DataStatistics());
  OptimizedPlan op=pc.compile(plan);
  NepheleJobGraphGenerator jgg=new NepheleJobGraphGenerator();
  return jgg.compileJobGraph(op);
}
