{
  CoGroupContract<PactJsonObject.Key,PactJsonObject,PactJsonObject,PactJsonObject.Key,PactJsonObject> intersection=new CoGroupContract<PactJsonObject.Key,PactJsonObject,PactJsonObject,PactJsonObject.Key,PactJsonObject>(TwoInputIntersection.class);
  intersection.setFirstInput(leftInput);
  intersection.setSecondInput(rightInput);
