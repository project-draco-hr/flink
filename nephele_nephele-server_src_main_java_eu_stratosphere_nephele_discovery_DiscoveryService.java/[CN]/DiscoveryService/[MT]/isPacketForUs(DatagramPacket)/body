{
  final byte[] data=packet.getData();
  if (data == null) {
    return false;
  }
  if (packet.getLength() < (MAGIC_NUMBER_OFFSET + 4)) {
    return false;
  }
  if (byteArrayToInteger(data,MAGIC_NUMBER_OFFSET) != GlobalConfiguration.getInteger(MAGICNUMBER_KEY,DEFAULT_MAGICNUMBER_VALUE)) {
    return false;
  }
  return true;
}
