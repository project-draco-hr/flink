{
  if ((a == null) || (b == null)) {
    return false;
  }
  byte[] A=a.getAddress();
  byte[] B=b.getAddress();
  if (A.length != B.length) {
    return false;
  }
  byte[] prefix=getNetworkPrefix(A.length * 8,networkPrefixLength);
  for (int byteNr=0; byteNr < A.length; ++byteNr) {
    if ((A[byteNr] & prefix[byteNr]) != (B[byteNr] & prefix[byteNr])) {
      return false;
    }
  }
  return true;
}
