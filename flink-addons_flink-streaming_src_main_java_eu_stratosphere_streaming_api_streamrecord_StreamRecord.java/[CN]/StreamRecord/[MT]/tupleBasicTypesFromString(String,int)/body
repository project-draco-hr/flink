{
  String[] types=representation.split(",");
  @SuppressWarnings("rawtypes") Class[] basicTypes=new Class[types.length];
  for (int i=0; i < types.length; i++) {
    try {
      basicTypes[i]=Class.forName(types[i]);
    }
 catch (    ClassNotFoundException e) {
      e.printStackTrace();
    }
  }
  return basicTypes;
}
