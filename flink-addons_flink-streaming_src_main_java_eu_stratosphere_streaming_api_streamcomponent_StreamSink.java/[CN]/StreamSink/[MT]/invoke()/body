{
  boolean hasInput=true;
  while (hasInput) {
    hasInput=false;
    for (    RecordReader<Record> input : inputs) {
      if (input.hasNext()) {
        hasInput=true;
        StreamRecord rec=new StreamRecord(input.next());
        String id=rec.getId();
        userFunction.invoke(rec.getRecord());
        input.publishEvent(new AckEvent(id));
      }
    }
  }
}
