{
  int val1Cnt=0;
  while (records1.hasNext()) {
    val1Cnt++;
    records1.next();
  }
  while (records2.hasNext()) {
    Record record2=records2.next();
    if (val1Cnt == 0) {
      if (++this.cnt >= 10) {
        throw new ExpectedTestException();
      }
      out.collect(record2);
    }
 else {
      for (int i=0; i < val1Cnt; i++) {
        if (++this.cnt >= 10) {
          throw new ExpectedTestException();
        }
        out.collect(record2);
      }
    }
  }
}
