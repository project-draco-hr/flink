{
  if (out != null) {
    return true;
  }
  try {
    delivery=consumer.nextDelivery();
  }
 catch (  Exception e) {
    throw new RuntimeException("Error while reading message from RMQ source from " + QUEUE_NAME + " at "+ HOST_NAME,e);
  }
  out=schema.deserialize(delivery.getBody());
  if (schema.isEndOfStream(out)) {
    out=null;
    return false;
  }
  return true;
}
