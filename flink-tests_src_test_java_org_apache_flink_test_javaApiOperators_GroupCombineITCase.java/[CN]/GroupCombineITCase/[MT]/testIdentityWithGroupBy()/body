{
  final ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  DataSet<Tuple3<Integer,Long,String>> ds=CollectionDataSets.get3TupleDataSet(env);
  DataSet<Tuple3<Integer,Long,String>> reduceDs=ds.groupBy(1).combineGroup(new IdentityFunction()).reduceGroup(new IdentityFunction());
  List<Tuple3<Integer,Long,String>> result=reduceDs.collect();
  compareResultAsTuples(result,identityResult);
}
