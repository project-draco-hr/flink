{
  int result;
switch (selectedStrategy) {
case MIN:
    result=Integer.MAX_VALUE;
  for (  Integer item : items) {
    if (result > item) {
      result=item;
    }
  }
return result;
case MAX:
result=0;
for (Integer item : items) {
if (result < item) {
result=item;
}
}
return result;
case SUM:
result=0;
for (Integer item : items) {
result+=item;
}
return result;
case PRIORITY:
for (Integer item : items) {
if (item > 0) {
return item;
}
}
return 0;
default :
return 0;
}
}
