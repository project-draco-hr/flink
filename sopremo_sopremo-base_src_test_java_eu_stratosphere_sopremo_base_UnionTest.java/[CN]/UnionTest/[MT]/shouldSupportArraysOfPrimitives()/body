{
  final SopremoTestPlan sopremoPlan=new SopremoTestPlan(2,1);
  final Union union=new Union();
  union.setInputs(sopremoPlan.getInputOperators(0,2));
  sopremoPlan.getOutputOperator(0).setInputs(union);
  sopremoPlan.getInput(0).add(createPactJsonArray(1,2)).add(createPactJsonArray(3,4)).add(createPactJsonArray(5,6));
  sopremoPlan.getInput(1).add(createPactJsonArray(1,2)).add(createPactJsonArray(3,4)).add(createPactJsonArray(7,8));
  sopremoPlan.getExpectedOutput(0).add(createPactJsonArray(1,2)).add(createPactJsonArray(3,4)).add(createPactJsonArray(5,6)).add(createPactJsonArray(7,8));
  sopremoPlan.run();
}
