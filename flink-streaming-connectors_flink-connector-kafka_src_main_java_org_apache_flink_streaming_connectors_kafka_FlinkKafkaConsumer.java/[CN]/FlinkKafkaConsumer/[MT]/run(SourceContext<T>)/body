{
  if (fetcher != null) {
    fetcher.run(sourceContext,valueDeserializer,lastOffsets);
  }
 else {
    final Object waitLock=new Object();
    while (running) {
      try {
synchronized (waitLock) {
          waitLock.wait();
        }
      }
 catch (      InterruptedException e) {
      }
    }
  }
  sourceContext.close();
}
