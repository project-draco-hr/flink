{
  KMeansIterative kmi=new KMeansIterative();
  Plan plan=kmi.getPlan(config.getString("IterativeKMeansITCase#NoSubtasks","1"),getFilesystemProvider().getURIPrefix() + dataPath,getFilesystemProvider().getURIPrefix() + clusterPath,getFilesystemProvider().getURIPrefix() + resultPath,config.getString("IterativeKMeansITCase#NumIterations","1"));
  PactCompiler pc=new PactCompiler(new DataStatistics());
  OptimizedPlan op=pc.compile(plan);
  NepheleJobGraphGenerator jgg=new NepheleJobGraphGenerator();
  return jgg.compileJobGraph(op);
}
