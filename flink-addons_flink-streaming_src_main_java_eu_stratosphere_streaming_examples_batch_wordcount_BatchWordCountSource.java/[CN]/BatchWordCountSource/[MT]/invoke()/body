{
  timestamp=0L;
  outRecord=new StreamRecord(2);
  line=br.readLine().replaceAll("[\\-\\+\\.\\^:,]","");
  while (line != null) {
    if (line != "") {
      outRecord.addRecord(new Tuple2<String,Long>(line,timestamp));
      timestamp++;
      if (timestamp % BATCH_SIZE == 0) {
        emit(outRecord);
        outRecord=new StreamRecord(2);
      }
    }
    line=br.readLine();
  }
}
