{
  assertEquals(new CountTriggerPolicy<Integer>(5,5),new CountTriggerPolicy<Integer>(5,5));
  assertEquals(new CountTriggerPolicy<Integer>(5,5),new CountTriggerPolicy<Integer>(5,5));
  assertEquals(new CountTriggerPolicy<Integer>(5),new CountTriggerPolicy<Integer>(5));
  assertNotEquals(new CountTriggerPolicy<Integer>(4,5),new CountTriggerPolicy<Integer>(5,5));
  assertNotEquals(new CountTriggerPolicy<Integer>(5,5),new CountTriggerPolicy<Integer>(5,4));
}
