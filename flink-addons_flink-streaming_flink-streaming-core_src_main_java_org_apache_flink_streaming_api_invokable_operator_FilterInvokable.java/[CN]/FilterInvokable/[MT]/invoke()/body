{
  if (this.isMutable) {
    while (recordIterator.next(reuse) != null) {
      if (filterFunction.filter(reuse.getTuple())) {
        collector.collect(reuse.getTuple());
      }
    }
  }
 else {
    while (recordIterator.next(reuse) != null) {
      if (filterFunction.filter(reuse.getTuple())) {
        collector.collect(reuse.getTuple());
      }
      resetReuse();
    }
  }
}
