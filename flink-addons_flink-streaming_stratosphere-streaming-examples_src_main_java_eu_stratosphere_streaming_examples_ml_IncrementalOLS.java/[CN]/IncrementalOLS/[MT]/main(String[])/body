{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment(PARALLELISM);
  DataStream<Tuple2<Boolean,Double[]>> model=env.addSource(new TrainingDataSource(),SOURCE_PARALLELISM).map(new PartialModelBuilder()).broadcast();
  DataStream<Tuple1<Double>> prediction=env.addSource(new NewDataSource(),SOURCE_PARALLELISM).connectWith(model).map(new Predictor());
  prediction.print();
  env.execute();
}
