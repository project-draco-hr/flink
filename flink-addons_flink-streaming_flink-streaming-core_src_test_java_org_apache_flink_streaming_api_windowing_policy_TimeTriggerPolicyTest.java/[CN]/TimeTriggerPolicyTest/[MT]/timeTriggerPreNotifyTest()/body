{
  Integer[] times={1,3,20,26};
  @SuppressWarnings("serial") TimeStamp<Integer> timeStamp=new TimeStamp<Integer>(){
    @Override public long getTimestamp(    Integer value){
      return value;
    }
    @Override public long getStartTime(){
      return 0;
    }
  }
;
  TimeTriggerPolicy<Integer> policy=new TimeTriggerPolicy<Integer>(5,timeStamp,new Extractor<Long,Integer>(){
    private static final long serialVersionUID=1L;
    @Override public Integer extract(    Long in){
      return in.intValue();
    }
  }
);
  Integer[][] result={{},{},{5,10,15},{25}};
  for (int i=0; i < times.length; i++) {
    arrayEqualityCheck(result[i],policy.preNotifyTrigger(times[i]));
    policy.notifyTrigger(times[i]);
  }
}
