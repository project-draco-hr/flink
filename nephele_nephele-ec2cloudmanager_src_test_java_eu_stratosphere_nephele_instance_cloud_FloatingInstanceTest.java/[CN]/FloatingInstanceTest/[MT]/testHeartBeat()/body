{
  FloatingInstance fi=new FloatingInstance("i-1234ABCD",new InstanceConnectionInfo(new InetSocketAddress("localhost",6122).getAddress(),6122,6121),System.currentTimeMillis());
  long lastHeartBeat=fi.getLastReceivedHeartBeat();
  try {
    Thread.sleep(1);
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
  fi.updateLastReceivedHeartBeat();
  assertTrue(fi.getLastReceivedHeartBeat() - lastHeartBeat > 0);
}
