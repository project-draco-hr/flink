{
  function=input.getExecutionEnvironment().clean(function);
  String callLocation=Utils.getCallLocationName();
  String udfName="MapWindow at " + callLocation;
  DataStream<R> result=createFastTimeOperatorIfValid(function,resultType,udfName);
  if (result != null) {
    return result;
  }
  String opName="TriggerWindow(" + windowAssigner + ", "+ trigger+ ", "+ udfName+ ")";
  OneInputStreamOperator<T,R> operator;
  if (evictor != null) {
    operator=new EvictingNonKeyedWindowOperator<>(windowAssigner,new HeapWindowBuffer.Factory<T>(),function,trigger,evictor);
  }
 else {
    operator=new NonKeyedWindowOperator<>(windowAssigner,new HeapWindowBuffer.Factory<T>(),function,trigger);
  }
  return input.transform(opName,resultType,operator).setParallelism(1);
}
