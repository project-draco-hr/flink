{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (getClass() != obj.getClass())   return false;
  Aggregation other=(Aggregation)obj;
  if (!groupings.equals(other.groupings))   return false;
  for (int index=0; index < groupings.size(); index++)   if (!groupings.get(index).deepEquals(other.groupings.get(index)))   return false;
  return true;
}
