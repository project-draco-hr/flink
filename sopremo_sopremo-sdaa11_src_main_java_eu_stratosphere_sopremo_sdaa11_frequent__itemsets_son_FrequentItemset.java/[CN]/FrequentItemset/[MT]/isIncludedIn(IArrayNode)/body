{
  int i=0;
  for (  final IJsonNode itemNode : items) {
    final String item=((TextNode)itemNode).getTextValue();
    final int diff=this.items[i].compareTo(item);
    if (diff < 0)     return false;
 else     if (diff == 0) {
      i++;
      if (i >= this.items.length)       return true;
    }
  }
  return false;
}
