{
  final Iterator<ManagementGroupVertex> it=this.groupVertices.iterator();
  while (it.hasNext()) {
    final ManagementGroupVertex groupVertex=it.next();
    if (groupVertex.isInputVertex()) {
      if (index == 0) {
        return groupVertex;
      }
 else {
        --index;
      }
    }
  }
  return null;
}
