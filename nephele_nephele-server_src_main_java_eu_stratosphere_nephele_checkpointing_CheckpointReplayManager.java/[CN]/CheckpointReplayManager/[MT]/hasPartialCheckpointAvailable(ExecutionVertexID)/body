{
  File file=new File(this.checkpointDirectory + File.separator + METADATA_PREFIX+ "_"+ vertexID+ "_0");
  if (file.exists()) {
    return true;
  }
  file=new File(this.checkpointDirectory + File.separator + METADATA_PREFIX+ "_"+ vertexID+ "_part");
  if (file.exists()) {
    return true;
  }
  return false;
}
