{
  if (LOG.isInfoEnabled())   LOG.info(getLogString("Start PACT code."));
  boolean stubOpen=false;
  try {
    try {
      prepare();
    }
 catch (    Throwable t) {
      try {
        cleanup();
      }
 catch (      Throwable t2) {
      }
      throw new Exception("The data preparation for task '" + this.getEnvironment().getTaskName() + "' , caused an error: "+ t.getMessage(),t);
    }
    try {
      this.stub.open(this.config.getStubParameters());
      stubOpen=true;
    }
 catch (    Throwable t) {
      throw new Exception("The user defined 'open()' method caused an exception: " + t.getMessage(),t);
    }
    run();
    if (this.running) {
      this.stub.close();
      stubOpen=false;
    }
  }
 catch (  Exception ex) {
    if (stubOpen) {
      try {
        this.stub.close();
      }
 catch (      Throwable t) {
      }
    }
    if (this.running) {
      if (LOG.isErrorEnabled())       LOG.error(getLogString("Unexpected ERROR in PACT code."));
      throw ex;
    }
  }
 finally {
    try {
      this.output.close();
    }
 catch (    Throwable t) {
    }
    cleanup();
  }
  if (this.running) {
    if (LOG.isInfoEnabled())     LOG.info(getLogString("Finished PACT code."));
  }
 else {
    if (LOG.isWarnEnabled())     LOG.warn(getLogString("PACT code cancelled."));
  }
}
