{
  List<Operator<?>> unmatchingOperators=actualPlan.getUnmatchingOperators(expectedPlan);
  if (unmatchingOperators != null) {
    if (unmatchingOperators.get(0).getClass() == unmatchingOperators.get(1).getClass())     Assert.failNotEquals("operators are different","\n" + unmatchingOperators.get(1),"\n" + unmatchingOperators.get(0));
 else     Assert.failNotEquals("plans are different",expectedPlan,actualPlan);
  }
}
