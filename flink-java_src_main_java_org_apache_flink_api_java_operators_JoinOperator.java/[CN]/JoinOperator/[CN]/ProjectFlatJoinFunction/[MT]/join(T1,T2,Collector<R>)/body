{
  for (int i=0; i < fields.length; i++) {
    if (isFromFirst[i]) {
      if (fields[i] >= 0) {
        outTuple.setField(((Tuple)in1).getField(fields[i]),i);
      }
 else {
        outTuple.setField(in1,i);
      }
    }
 else {
      if (fields[i] >= 0) {
        outTuple.setField(((Tuple)in2).getField(fields[i]),i);
      }
 else {
        outTuple.setField(in2,i);
      }
    }
  }
  out.collect(outTuple);
}
