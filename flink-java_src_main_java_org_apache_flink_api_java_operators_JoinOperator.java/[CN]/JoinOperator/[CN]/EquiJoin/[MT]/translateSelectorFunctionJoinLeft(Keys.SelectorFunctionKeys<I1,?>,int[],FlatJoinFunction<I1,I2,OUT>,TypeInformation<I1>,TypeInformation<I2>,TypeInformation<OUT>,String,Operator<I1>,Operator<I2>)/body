{
  if (!inputType2.isTupleType()) {
    throw new InvalidParameterException("Should not happen.");
  }
  @SuppressWarnings("unchecked") final Keys.SelectorFunctionKeys<I1,K> keys1=(Keys.SelectorFunctionKeys<I1,K>)rawKeys1;
