{
  ByteBuf result=null;
  try {
    result=allocateBuffer(allocator,ID,16 + 16 + 4+ 16);
    producerExecutionId.writeTo(result);
    partitionId.writeTo(result);
    result.writeInt(queueIndex);
    receiverId.writeTo(result);
    return result;
  }
 catch (  Throwable t) {
    if (result != null) {
      result.release();
    }
    throw new IOException(t);
  }
}
