{
  SopremoPlan actualPlan=parseScript("$employees = read 'employees.json';" + "$result = group $employees into count($);" + "write $result to 'output.json'; ");
  SopremoPlan expectedPlan=new SopremoPlan();
  Source input=new Source("employees.json");
  Grouping selection=new Grouping().withResultProjection(new MethodCall("count",new InputSelection(0))).withInputs(input);
  Sink output=new Sink("output.json").withInputs(selection);
  expectedPlan.setSinks(output);
  assertEquals(expectedPlan,actualPlan);
}
