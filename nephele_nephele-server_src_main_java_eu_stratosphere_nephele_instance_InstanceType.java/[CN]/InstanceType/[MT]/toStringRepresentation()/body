{
  StringBuilder bld=new StringBuilder(32);
  bld.append(identifier);
  bld.append(',');
  bld.append(numberOfComputeUnits);
  bld.append(',');
  bld.append(numberOfCores);
  bld.append(',');
  bld.append(memorySize);
  bld.append(',');
  bld.append(diskCapacity);
  bld.append(',');
  bld.append(pricePerHour);
  return bld.toString();
}
