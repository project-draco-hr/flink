{
  numberOfInputs=getTaskConfiguration().getInteger("numberOfInputs",0);
  for (int i=0; i < numberOfInputs; i++) {
    inputs.add(new RecordReader<Record>(this,Record.class));
  }
  Class<? extends UserSinkInvokable> userFunctionClass;
