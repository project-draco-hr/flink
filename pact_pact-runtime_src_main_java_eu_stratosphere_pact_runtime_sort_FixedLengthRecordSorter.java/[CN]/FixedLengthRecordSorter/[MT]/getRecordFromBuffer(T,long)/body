{
  this.sortBufferInputView.setReadPosition(pointer);
  this.serializer.deserialize(target,this.sortBufferInputView);
  this.sortBufferInputView.setReadPosition(pointer);
  this.comparator.readFromNormalizedKey(target,this.sortBufferInputView.getCurrentSegment().getBackingArray(),this.sortBufferInputView.getCurrentPositionInSegment(),this.numKeyBytes);
}
