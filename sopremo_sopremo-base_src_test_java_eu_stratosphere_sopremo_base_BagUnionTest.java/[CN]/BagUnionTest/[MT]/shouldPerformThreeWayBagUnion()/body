  sopremoPlan.getInput(2).add(createValueNode((Object)6)).add(createValueNode((Object)7)).add(createValueNode((Object)8));
  sopremoPlan.getExpectedOutput(0).add(createValueNode((Object)1)).add(createValueNode((Object)2)).add(createValueNode((Object)3)).add(createValueNode((Object)4)).add(createValueNode((Object)5)).add(createValueNode((Object)6)).add(createValueNode((Object)7)).add(createValueNode((Object)8));
  sopremoPlan.run();
