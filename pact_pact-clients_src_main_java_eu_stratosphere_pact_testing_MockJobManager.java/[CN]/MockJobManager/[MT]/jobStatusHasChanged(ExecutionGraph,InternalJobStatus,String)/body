{
  if (newJobStatus == InternalJobStatus.CANCELING || newJobStatus == InternalJobStatus.FAILING)   this.cancelJob(executionGraph);
  if (newJobStatus == InternalJobStatus.FINISHED || newJobStatus == InternalJobStatus.CANCELED || newJobStatus == InternalJobStatus.FAILED)   MockTaskManager.INSTANCE.cleanupJob(executionGraph);
}
