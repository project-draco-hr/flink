{
  if (isCountOnly(trigger,eviction)) {
    long slide=getSlideSize(trigger);
    long window=getWindowSize(eviction);
    return slide < window && ((CountTriggerPolicy<?>)trigger).getStart() == ((CountEvictionPolicy<?>)eviction).getStart() && ((CountEvictionPolicy<?>)eviction).getDeleteOnEviction() == 1;
  }
 else {
    return false;
  }
}
