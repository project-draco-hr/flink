{
  if (eviction instanceof TumblingEvictionPolicy || eviction instanceof KeepAllEvictionPolicy) {
    return true;
  }
 else   if (isTimeOnly(trigger,eviction)) {
    long slide=getSlideSize(trigger);
    long window=getWindowSize(eviction);
    return slide == window && getTimeStampWrapper(trigger).equals(getTimeStampWrapper(eviction));
  }
 else   if (isCountOnly(trigger,eviction)) {
    long slide=getSlideSize(trigger);
    long window=getWindowSize(eviction);
    return slide == window && ((CountTriggerPolicy<?>)trigger).getStart() == ((CountEvictionPolicy<?>)eviction).getStart() && ((CountEvictionPolicy<?>)eviction).getDeleteOnEviction() == 1;
  }
 else {
    return false;
  }
}
