{
  SON son=new SON();
  son.setParallelism(2);
  son.setMinSupport(200);
  son.setMaxSetSize(10);
  final SopremoTestPlan plan=new SopremoTestPlan(son);
  final List<IJsonNode> baskets=Baskets.loadBaskets(Baskets.BASKETS2_PATH);
  for (  final IJsonNode basket : baskets)   plan.getInput(0).add(basket);
  plan.run();
  for (  final IJsonNode outputNode : plan.getActualOutput(0))   System.out.println(outputNode);
}
