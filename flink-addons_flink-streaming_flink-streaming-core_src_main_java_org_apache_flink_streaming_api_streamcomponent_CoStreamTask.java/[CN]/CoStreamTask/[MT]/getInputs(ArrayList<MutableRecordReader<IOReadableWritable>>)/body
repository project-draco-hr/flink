{
  if (inputList.size() == 1) {
    return inputList.get(0);
  }
 else   if (inputList.size() > 1) {
    MutableRecordReader<IOReadableWritable>[] inputArray=inputList.toArray(new MutableRecordReader[inputList.size()]);
    return new MutableUnionRecordReader<IOReadableWritable>(inputArray);
  }
  return null;
}
