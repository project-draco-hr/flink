{
  T nextElement;
  if (hasNext()) {
    nextElement=subIterator.next().getObject();
    if (!subIterator.hasNext()) {
      if (iterator.hasNext()) {
        subIterator=iterator.next().iterator();
      }
    }
  }
 else {
    throw new NoSuchElementException("There is no more element in the current batch");
  }
  return nextElement;
}
