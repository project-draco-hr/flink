{
  if (shipType == null) {
    return "(null)";
  }
switch (shipType) {
case NONE:
    return "(none)";
case FORWARD:
  return "Forward";
case BROADCAST:
return "Broadcast";
case PARTITION_HASH:
return "Hash Partition";
case PARTITION_RANGE:
return "Range Partition";
case PARTITION_RANDOM:
return "Redistribute";
case PARTITION_FORCED_REBALANCE:
return "Rebalance";
case PARTITION_CUSTOM:
return "Custom Partition";
default :
return shipType.name();
}
}
