{
  this.inputs[index].setOperator(operator);
  if (operator.getType() == PersistenceType.ADHOC)   for (  final JsonNode node : (ArrayNode)operator.getAdhocValues())   this.inputs[index].add(new PactJsonObject(node));
 else {
    final TestPairs<PactJsonObject.Key,PactJsonObject> testPairs=new TestPairs<PactJsonObject.Key,PactJsonObject>();
    testPairs.fromFile(JsonInputFormat.class,operator.getInputName());
    for (    final KeyValuePair<PactJsonObject.Key,PactJsonObject> kvPair : testPairs)     this.inputs[index].add(kvPair.getValue());
    testPairs.close();
  }
}
