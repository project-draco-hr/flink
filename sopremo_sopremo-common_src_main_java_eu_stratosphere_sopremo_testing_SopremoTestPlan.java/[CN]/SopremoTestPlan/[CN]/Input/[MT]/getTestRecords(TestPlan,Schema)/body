{
  int sourceIndex=-1;
  final List<FileDataSource> sources=testPlan.getSources();
  for (int index=0; index < sources.size(); index++)   if (sources.get(index).getName().equals(this.getOperator().getInputPath())) {
    sourceIndex=index;
    break;
  }
  return testPlan.getInput(sourceIndex == -1 ? this.getIndex() : sourceIndex);
}
