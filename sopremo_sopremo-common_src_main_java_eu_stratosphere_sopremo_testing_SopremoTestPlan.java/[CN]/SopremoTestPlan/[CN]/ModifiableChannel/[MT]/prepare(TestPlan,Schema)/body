{
  if (this.operator instanceof MockupSource) {
    final TestRecords testRecords=this.getTestRecords(testPlan,schema);
    testRecords.setSchema(schema.getPactSchema());
    if (this.isEmpty())     testRecords.setEmpty();
 else     if (this.file != null)     testRecords.fromFile(JsonInputFormat.class,this.file);
 else     for (    final IJsonNode node : this.values)     testRecords.add(schema.jsonToRecord(node,null,null));
  }
}
