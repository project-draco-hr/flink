{
  try {
    if (!FileSystem.get(new URI(this.file)).exists(new Path(this.file)))     throw new FileNotFoundException();
  }
 catch (  URISyntaxException e) {
    throw new IllegalArgumentException(String.format("File %s is not a valid URI",file));
  }
  this.empty=false;
  this.values.clear();
  this.file=file;
}
