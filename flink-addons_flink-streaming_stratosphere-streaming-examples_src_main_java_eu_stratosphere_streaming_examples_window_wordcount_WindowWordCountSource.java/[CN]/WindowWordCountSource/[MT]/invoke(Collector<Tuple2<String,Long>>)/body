{
  BufferedReader br=new BufferedReader(new FileReader("src/test/resources/testdata/hamlet.txt"));
  while (true) {
    line=br.readLine();
    if (line == null) {
      break;
    }
    if (line != "") {
      line=line.replaceAll("[\\-\\+\\.\\^:,]","");
      outRecord.f0=line;
      outRecord.f1=timestamp;
      collector.collect(outRecord);
      timestamp++;
    }
  }
}
