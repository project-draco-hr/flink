{
  if (value == null)   throw new NullPointerException();
  if (offset < 0 || len < 0 || offset > value.length() - len)   throw new IndexOutOfBoundsException();
  ensureSize(len);
  this.len=len;
  for (int i=0; i < len; i++)   this.value[i]=value.charAt(offset + i);
  this.len=len;
  this.hashCode=0;
}
