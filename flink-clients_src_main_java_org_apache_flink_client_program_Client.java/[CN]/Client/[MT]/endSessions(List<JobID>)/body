{
  if (jobIds == null) {
    throw new IllegalArgumentException("The JobIDs must not be null");
  }
  for (  JobID jid : jobIds) {
    if (jid != null) {
      LOG.info("Telling job manager to end the session {}.",jid);
      jobManagerGateway.tell(new JobManagerMessages.RemoveCachedJob(jid));
    }
  }
}
