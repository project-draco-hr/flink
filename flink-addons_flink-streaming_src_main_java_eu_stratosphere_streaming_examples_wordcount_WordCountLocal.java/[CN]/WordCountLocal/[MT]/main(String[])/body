{
  StreamExecutionEnvironment context=new StreamExecutionEnvironment();
  @SuppressWarnings("unused") DataStream<Tuple2<String,Integer>> dataStream=context.readTextFile("src/test/resources/testdata/hamlet.txt").flatMap(new WordCountSplitter()).partitionBy(0).map(new WordCountCounter()).addSink(new WordCountSink());
  context.execute();
}
