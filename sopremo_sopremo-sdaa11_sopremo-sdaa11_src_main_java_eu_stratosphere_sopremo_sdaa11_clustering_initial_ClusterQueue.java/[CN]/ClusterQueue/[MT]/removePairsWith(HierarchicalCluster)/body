{
  final Set<Integer> keysToRemove=new HashSet<Integer>();
  for (  final Integer key : this.distancedPairs.keySet()) {
    final Set<ClusterPair> originalPairs=this.distancedPairs.get(key);
    final Set<ClusterPair> pairs=new HashSet<ClusterPair>(originalPairs);
    for (    final ClusterPair clusterPair : pairs)     if (clusterPair.contains(cluster))     originalPairs.remove(clusterPair);
    if (originalPairs.isEmpty())     keysToRemove.add(key);
  }
  for (  final Integer key : keysToRemove)   this.distancedPairs.remove(key);
}
