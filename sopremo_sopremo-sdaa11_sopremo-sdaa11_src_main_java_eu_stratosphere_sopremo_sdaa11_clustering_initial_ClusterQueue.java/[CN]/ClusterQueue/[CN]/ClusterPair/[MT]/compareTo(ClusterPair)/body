{
  int difference=this.distance - otherPair.distance;
  if (difference != 0)   return difference;
  difference=FastStringComparator.INSTANCE.compare(this.cluster1.getId(),otherPair.cluster1.getId());
  if (difference != 0)   return difference;
  return FastStringComparator.INSTANCE.compare(this.cluster2.getId(),otherPair.cluster2.getId());
}
