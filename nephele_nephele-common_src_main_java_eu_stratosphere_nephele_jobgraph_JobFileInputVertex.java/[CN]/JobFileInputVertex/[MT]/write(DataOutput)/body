{
  super.write(out);
  if (this.inputClass == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    StringRecord.writeString(out,this.inputClass.getName());
  }
  if (this.path == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    this.path.write(out);
  }
}
