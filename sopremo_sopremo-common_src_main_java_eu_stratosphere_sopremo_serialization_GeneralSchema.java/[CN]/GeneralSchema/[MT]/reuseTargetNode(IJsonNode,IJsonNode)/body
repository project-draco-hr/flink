{
  target.clear();
  if (target.isObject()) {
    ((IObjectNode)target).putAll((IObjectNode)source);
  }
 else   if (target.isArray()) {
    ((IArrayNode)target).addAll((IArrayNode)source);
  }
 else {
    if (source.getClass() != target.getClass()) {
      target=source;
    }
 else {
      target=this.reusePrimitive(source,target,source.getClass());
    }
  }
  return target;
}
