{
  if (target.isObject()) {
    ((IObjectNode)target).removeAll();
    ((IObjectNode)target).putAll((IObjectNode)source);
  }
 else   if (target.isArray()) {
    ((IArrayNode)target).clear();
    ((IArrayNode)target).addAll((IArrayNode)source);
  }
 else {
    target=source;
  }
  return target;
}
