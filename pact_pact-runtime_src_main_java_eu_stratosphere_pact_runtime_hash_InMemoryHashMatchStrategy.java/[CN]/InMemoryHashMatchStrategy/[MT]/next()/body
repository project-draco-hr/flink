{
  while (readerProbe.hasNext()) {
    KeyValuePair<K,VP> pair=readerProbe.next();
    K key=pair.getKey();
    if (hashMap.contains(key)) {
      currentKey=key;
      currentBuildValuesIterable=hashMap.get(currentKey);
      currentProbeValuesIterable.value=pair.getValue();
      return true;
    }
  }
  return false;
}
