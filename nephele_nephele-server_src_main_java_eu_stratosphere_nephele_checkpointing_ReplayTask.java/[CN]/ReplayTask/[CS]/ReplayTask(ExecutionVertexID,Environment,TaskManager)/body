{
  this.vertexID=vertexID;
  this.environment=new CheckpointEnvironment(this.vertexID,environment,CheckpointUtils.hasLocalCheckpointAvailable(this.vertexID),CheckpointUtils.hasCompleteCheckpointAvailable(this.vertexID),this.outputBrokerMap);
  this.environment.setExecutionObserver(new ReplayTaskExecutionObserver(null));
  this.encapsulatedTask=null;
  this.taskManager=taskManager;
}
