{
  Condition selectionCondition=new Condition(new BooleanExpression(createPath("$","name"),BinaryOperator.NOT_EQUAL,new JsonPath.Constant("")));
  Selection selection=new Selection(selectionCondition,new Source(DataType.HDFS,"in.json"));
  assertParseResult(new Sink(DataType.HDFS,"out.json",selection),"read(hdfs(\"in.json\")) -> filter $.name != \"\" -> write(hdfs(\"out.json\"))");
}
