{
  Condition selectionCondition=new Condition(Combination.OR,new UnaryExpression(createPath("$","mgr")),new Comparison(createPath("$","income"),BinaryOperator.GREATER,new Constant(30000L)));
  assertParseResult(new Selection(selectionCondition,employeeSource()),employeeJaql() + "employees -> filter each emp emp.mgr or emp.income > 30000;");
}
