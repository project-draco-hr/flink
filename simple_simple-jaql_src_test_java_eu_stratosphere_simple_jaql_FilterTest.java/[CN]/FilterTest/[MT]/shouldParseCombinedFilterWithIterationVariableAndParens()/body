{
  ConditionalExpression selectionCondition=new ConditionalExpression(Combination.OR,new UnaryExpression(createPath("$","mgr")),new ComparativeExpression(createPath("$","income"),BinaryOperator.GREATER,new ConstantExpression(30000L)));
  assertParseResult(new Selection(selectionCondition,employeeSource()),employeeJaql() + "employees -> filter each emp (emp.mgr or emp.income > 30000);");
}
