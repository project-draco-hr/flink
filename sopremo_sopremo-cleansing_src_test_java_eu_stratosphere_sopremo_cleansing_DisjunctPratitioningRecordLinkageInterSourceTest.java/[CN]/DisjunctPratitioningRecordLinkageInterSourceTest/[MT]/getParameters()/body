{
  EvaluationExpression[] projections={null,getAggregativeProjection()};
  double[] thresholds={0.0,0.4,0.8,1.0};
  ArrayList<Object[]> parameters=new ArrayList<Object[]>();
  for (  EvaluationExpression projection : projections)   for (  double threshold : thresholds)   for (  String[][] combinedBlockingKeys : CombinedBlockingKeys)   parameters.add(new Object[]{threshold,projection,combinedBlockingKeys});
  return parameters;
}
