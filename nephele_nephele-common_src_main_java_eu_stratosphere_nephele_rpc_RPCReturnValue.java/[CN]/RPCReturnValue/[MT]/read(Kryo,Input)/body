{
  super.read(kryo,input);
  if (!input.readBoolean()) {
    this.retVal=null;
    return;
  }
  Class<?> clazz=null;
  try {
    clazz=Class.forName(input.readString());
  }
 catch (  ClassNotFoundException cnfe) {
    throw new RuntimeException(cnfe);
  }
  this.retVal=kryo.readObject(input,clazz);
}
