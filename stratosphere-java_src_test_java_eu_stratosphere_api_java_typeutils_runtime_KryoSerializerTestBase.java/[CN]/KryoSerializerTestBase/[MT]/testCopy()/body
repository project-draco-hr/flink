{
  try {
    TypeSerializer<String> serializer=getSerializer();
    String testData=getData();
    String copy=serializer.copy(testData,serializer.createInstance());
    assertEquals("Copied element is not equal to the original element.",testData,copy);
  }
 catch (  Exception e) {
    System.err.println(e.getMessage());
    e.printStackTrace();
    fail("Exception in test: " + e.getMessage());
  }
}
