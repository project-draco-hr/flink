{
  if (heapMemory != null) {
    in.readFully(heapMemory,offset,length);
  }
 else {
    while (length >= 8) {
      putLongBigEndian(offset,in.readLong());
      offset+=8;
      length-=8;
    }
    while (length > 0) {
      put(offset,in.readByte());
      offset++;
      length--;
    }
  }
}
