{
  for (  MemoryPoolMXBean pool : ManagementFactory.getMemoryPoolMXBeans()) {
    for (    String s : TENURED_POOL_NAMES) {
      if (pool.getName().equals(s)) {
        if (pool.getType() == MemoryType.HEAP && pool.isUsageThresholdSupported()) {
          return pool;
        }
      }
    }
  }
  return null;
}
