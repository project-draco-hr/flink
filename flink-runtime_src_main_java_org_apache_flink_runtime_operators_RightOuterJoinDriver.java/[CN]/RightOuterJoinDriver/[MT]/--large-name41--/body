{
switch (driverStrategy) {
case RIGHT_OUTER_MERGE:
    return new NonReusingMergeOuterJoinIterator<>(OuterJoinType.RIGHT,in1,in2,serializer1,comparator1,serializer2,comparator2,pairComparatorFactory.createComparator12(comparator1,comparator2),memoryManager,ioManager,numPages,super.taskContext.getOwningNepheleTask());
default :
  throw new Exception("Unsupported driver strategy for right outer join driver: " + driverStrategy.name());
}
}
