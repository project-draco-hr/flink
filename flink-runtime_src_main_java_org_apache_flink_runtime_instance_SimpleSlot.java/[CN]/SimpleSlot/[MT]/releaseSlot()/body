{
  if (!isCanceled()) {
    Execution exec=this.executedTask;
    if (exec != null && !exec.isFinished()) {
      exec.fail(new Exception("The slot in which the task was executed has been released. Probably loss of TaskManager " + getInstance()));
    }
    if (getParent() == null) {
      if (markCancelled()) {
        getInstance().returnAllocatedSlot(this);
      }
    }
 else {
      getParent().releaseChild(this);
    }
  }
}
