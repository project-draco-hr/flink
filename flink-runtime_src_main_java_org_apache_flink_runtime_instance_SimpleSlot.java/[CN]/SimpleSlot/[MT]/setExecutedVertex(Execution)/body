{
  if (executedVertex == null) {
    throw new NullPointerException();
  }
  if (getStatus() != ALLOCATED_AND_ALIVE) {
    return false;
  }
  if (!VERTEX_UPDATER.compareAndSet(this,null,executedVertex)) {
    return false;
  }
  if (getStatus() != ALLOCATED_AND_ALIVE) {
    this.executedTask=null;
    return false;
  }
  return true;
}
