{
  long numKey1=0;
  for (  PactConnection c : this.input1) {
    long keys=c.getSourcePact().estimatedKeyCardinality;
    if (keys == -1) {
      numKey1=-1;
      break;
    }
    numKey1+=keys;
  }
  long numKey2=0;
  for (  PactConnection c : this.input2) {
    long keys=c.getSourcePact().estimatedKeyCardinality;
    if (keys == -1) {
      numKey2=-1;
      break;
    }
    numKey2+=keys;
  }
switch (this.getOutputContract()) {
case SameKeyFirst:
    return numKey1;
case SameKeySecond:
  return numKey2;
}
if (numKey1 == -1 || numKey2 == -1) return -1;
return numKey1 * numKey2;
}
