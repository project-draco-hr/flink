{
  long numRecords1=0;
  for (  PactConnection c : this.input1) {
    long recs=c.getSourcePact().estimatedNumRecords;
    if (recs == -1) {
      return -1;
    }
    numRecords1+=recs;
  }
  long numRecords2=0;
  for (  PactConnection c : this.input2) {
    long recs=c.getSourcePact().estimatedNumRecords;
    if (recs == -1) {
      return -1;
    }
    numRecords2+=recs;
  }
  return numRecords1 * numRecords2;
}
