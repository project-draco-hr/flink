{
  List<InterestingProperties> thisNodesIntProps=getInterestingProperties();
  List<InterestingProperties> props1=InterestingProperties.filterByNodesConstantSet(thisNodesIntProps,this,0);
  List<InterestingProperties> props2=InterestingProperties.filterByNodesConstantSet(thisNodesIntProps,this,1);
  for (  PactConnection c : this.input1) {
    if (props1.isEmpty() == false) {
      c.addAllInterestingProperties(props1);
    }
 else {
      c.setNoInterestingProperties();
    }
  }
  for (  PactConnection c : this.input1) {
    if (props2.isEmpty() == false) {
      c.addAllInterestingProperties(props2);
    }
 else {
      c.setNoInterestingProperties();
    }
  }
}
