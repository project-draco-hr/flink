{
  StringBuilder bld=new StringBuilder();
  bld.append("[");
  Iterator<ExecutionGraph> it=graphs.iterator();
  while (it.hasNext()) {
    ExecutionGraph graph=it.next();
    writeJsonForJob(bld,graph);
    if (it.hasNext()) {
      bld.append(",");
    }
  }
  bld.append("]");
  return bld.toString();
}
