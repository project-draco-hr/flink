{
  if (!repeatKey) {
    if (valCnt >= numVals) {
      return false;
    }
    target.setKey(keyCnt++);
    target.setValue(valCnt);
    if (keyCnt == numKeys) {
      keyCnt=0;
      valCnt++;
    }
  }
 else {
    if (keyCnt >= numKeys) {
      return false;
    }
    target.setKey(keyCnt);
    target.setValue(valCnt++);
    if (valCnt == numVals) {
      valCnt=0;
      keyCnt++;
    }
  }
  return true;
}
