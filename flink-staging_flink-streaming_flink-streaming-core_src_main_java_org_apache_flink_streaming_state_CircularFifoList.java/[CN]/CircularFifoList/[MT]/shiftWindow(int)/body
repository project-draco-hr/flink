{
  if (numberOfSlides <= slideSizes.size()) {
    for (int i=0; i < numberOfSlides; i++) {
      Long firstSlideSize=slideSizes.remove();
      for (int j=0; j < firstSlideSize; j++) {
        queue.remove();
      }
    }
  }
 else {
    slideSizes.clear();
    queue.clear();
    counter=0;
  }
}
