{
  final List<Operator> passes=new ArrayList<Operator>();
  if (inputs.size() == 1)   for (int index=0; index < this.passPartitionKeys.size(); index++)   passes.add(this.createSinglePassIntraSource(this.passPartitionKeys.get(index)[0],similarityCondition,inputs.get(0),idProjections,duplicateProjection));
 else   for (int index=0; index < this.passPartitionKeys.size(); index++)   passes.add(this.createSinglePassInterSource(this.passPartitionKeys.get(index),similarityCondition,inputs.get(0),inputs.get(1),idProjections,duplicateProjection));
  return SopremoModule.valueOf(this.toString(),new Union(passes));
}
