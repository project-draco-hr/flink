{
  if (index < 0 || index >= this.size())   return MissingNode.getInstance();
  final int size=this.size();
  final int recordPosition=this.schema.getTailSize() - size + index;
  if (recordPosition < 0)   return this.getOtherField().remove(index);
  final IJsonNode oldNode=SopremoUtil.unwrap(this.record.getField(recordPosition + 1,JsonNodeWrapper.class));
  for (int i=recordPosition; i > 0; i--)   if (!this.record.isNull(i))   this.record.setField(i + 1,this.record.getField(i,JsonNodeWrapper.class));
 else {
    this.record.setNull(i + 1);
    return oldNode;
  }
  if (size > this.schema.getTailSize())   this.record.setField(1,this.getOtherField().remove(size - this.schema.getTailSize()));
 else   this.record.setNull(1);
  return oldNode;
}
