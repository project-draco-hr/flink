{
  MemorySegmentDescriptor descriptor=descriptorReference.get();
  if (position >= 0 && position + 7 < descriptor.end) {
    return (((long)descriptor.memory[position++] & 0xff) << 56) | (((long)descriptor.memory[position++] & 0xff) << 48) | (((long)descriptor.memory[position++] & 0xff) << 40)| (((long)descriptor.memory[position++] & 0xff) << 32)| (((long)descriptor.memory[position++] & 0xff) << 24)| (((long)descriptor.memory[position++] & 0xff) << 16)| (((long)descriptor.memory[position++] & 0xff) << 8)| (((long)descriptor.memory[position++] & 0xff) << 0);
  }
 else {
    throw new EOFException();
  }
}
