{
  MemorySegmentDescriptor descriptor=descriptorReference.get();
  if (position >= 0 && position + 3 < descriptor.end) {
    return ((descriptor.memory[position++] & 0xff) << 24) | ((descriptor.memory[position++] & 0xff) << 16) | ((descriptor.memory[position++] & 0xff) << 8)| ((descriptor.memory[position++] & 0xff) << 0);
  }
 else {
    throw new EOFException();
  }
}
