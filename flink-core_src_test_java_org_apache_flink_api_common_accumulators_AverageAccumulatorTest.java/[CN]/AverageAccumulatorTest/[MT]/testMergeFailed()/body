{
  AverageAccumulator average=new AverageAccumulator();
  Accumulator<Double,Double> averageNew=null;
  average.add(1);
  try {
    average.merge(averageNew);
  }
 catch (  Exception e) {
    assertTrue(e.toString().indexOf("The merged accumulator must be AverageAccumulator.") != -1);
    return;
  }
  fail();
}
