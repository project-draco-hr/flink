{
  if (out != null) {
    return true;
  }
  try {
    delivery=consumer.nextDelivery();
  }
 catch (  Exception e) {
    if (LOG.isErrorEnabled()) {
      LOG.error("Cannot recieve RMQ message {} at {}",QUEUE_NAME,HOST_NAME);
    }
  }
  out=schema.deserialize(delivery.getBody());
  if (schema.isEndOfStream(out)) {
    out=null;
    return false;
  }
  return true;
}
