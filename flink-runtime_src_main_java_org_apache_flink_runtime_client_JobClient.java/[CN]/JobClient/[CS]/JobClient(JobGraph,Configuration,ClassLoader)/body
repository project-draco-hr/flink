{
  final String address=configuration.getString(ConfigConstants.JOB_MANAGER_IPC_ADDRESS_KEY,null);
  final int port=configuration.getInteger(ConfigConstants.JOB_MANAGER_IPC_PORT_KEY,ConfigConstants.DEFAULT_JOB_MANAGER_IPC_PORT);
  final InetSocketAddress inetaddr=new InetSocketAddress(address,port);
  this.jobManager=AkkaUtils.getReference(inetaddr);
  this.jobGraph=jobGraph;
  this.configuration=configuration;
  this.userCodeClassLoader=userCodeClassLoader;
}
