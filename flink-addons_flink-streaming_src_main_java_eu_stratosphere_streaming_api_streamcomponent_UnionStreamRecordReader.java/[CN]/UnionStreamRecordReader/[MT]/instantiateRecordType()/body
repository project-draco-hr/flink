{
  try {
    return this.recordType.newInstance();
  }
 catch (  InstantiationException e) {
    throw new RuntimeException("Cannot instantiate class '" + this.recordType.getName() + "'.",e);
  }
catch (  IllegalAccessException e) {
    throw new RuntimeException("Cannot instantiate class '" + this.recordType.getName() + "'.",e);
  }
}
