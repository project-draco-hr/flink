{
  if (!parseParameters(args)) {
    return;
  }
  StreamExecutionEnvironment env=StreamExecutionEnvironment.getExecutionEnvironment();
  setInputStreams(env);
  DataStream<Tuple3<String,Integer,Integer>> joinedStream=grades.join(salaries).onWindow(1,new MyTimestamp(0),new MyTimestamp(0)).where(0).equalTo(0).with(new MyJoinFunction());
  if (fileOutput) {
    joinedStream.writeAsText(outputPath,1);
  }
 else {
    joinedStream.print();
  }
  env.execute("Windowed Join Example");
}
