{
  if (!parseParameters(args)) {
    return;
  }
  StreamExecutionEnvironment env=StreamExecutionEnvironment.getExecutionEnvironment();
  DataStream<Tuple2<String,Integer>> grades=env.addSource(new GradeSource());
  DataStream<Tuple2<String,Integer>> salaries=env.addSource(new SalarySource());
  DataStream<Tuple3<String,Integer,Integer>> joinedStream=grades.join(salaries).onWindow(1,TimeUnit.SECONDS).where(0).equalTo(0).with(new MyJoinFunction());
  if (fileOutput) {
    joinedStream.writeAsText(outputPath,1);
  }
 else {
    joinedStream.print();
  }
  env.execute("Windowed Join Example");
}
