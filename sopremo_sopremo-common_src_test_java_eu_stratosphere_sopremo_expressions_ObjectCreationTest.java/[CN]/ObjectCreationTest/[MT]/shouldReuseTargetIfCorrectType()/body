{
  ObjectNode target=new ObjectNode();
  final ObjectCreation object=new ObjectCreation(new ObjectCreation.FieldAssignment("name",new ConstantExpression(TextNode.valueOf("testperson"))),new ObjectCreation.FieldAssignment("age",new ConstantExpression(IntNode.valueOf(30))));
  IJsonNode result=object.evaluate(IntNode.valueOf(0),target,this.context);
  Assert.assertSame(target,result);
}
