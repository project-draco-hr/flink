{
  final IJsonNode result=new ObjectCreation(new ObjectCreation.FieldAssignment("name",new ConstantExpression(TextNode.valueOf("testperson"))),new ObjectCreation.FieldAssignment("age",new ConstantExpression(IntNode.valueOf(30)))).evaluate(IntNode.valueOf(0),null,this.context);
  Assert.assertEquals(createObjectNode("name","testperson","age",30),result);
}
