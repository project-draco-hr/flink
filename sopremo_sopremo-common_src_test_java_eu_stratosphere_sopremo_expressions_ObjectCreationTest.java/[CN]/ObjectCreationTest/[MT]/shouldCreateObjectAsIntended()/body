{
  final JsonNode result=new ObjectCreation(new ObjectCreation.Mapping("name",new ConstantExpression(TextNode.valueOf("testperson"))),new ObjectCreation.Mapping("age",new ConstantExpression(IntNode.valueOf(30)))).evaluate(IntNode.valueOf(0),this.context);
  Assert.assertEquals(createObjectNode("name","testperson","age",30),result);
}
