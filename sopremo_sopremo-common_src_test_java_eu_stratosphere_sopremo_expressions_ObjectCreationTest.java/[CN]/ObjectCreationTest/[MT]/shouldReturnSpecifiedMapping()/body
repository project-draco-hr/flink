{
  final ObjectCreation.Mapping<?> mapping=new ObjectCreation(new ObjectCreation.FieldAssignment("name",new ConstantExpression(TextNode.valueOf("testperson"))),new ObjectCreation.FieldAssignment("age",new ConstantExpression(IntNode.valueOf(30)))).getMapping(1);
  Assert.assertEquals(new ObjectCreation.FieldAssignment("age",new ConstantExpression(IntNode.valueOf(30))),mapping);
}
