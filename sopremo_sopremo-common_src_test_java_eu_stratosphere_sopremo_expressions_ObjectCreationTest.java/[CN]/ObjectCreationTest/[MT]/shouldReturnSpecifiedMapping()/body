{
  final ObjectCreation.Mapping mapping=new ObjectCreation(new ObjectCreation.Mapping("name",new ConstantExpression(TextNode.valueOf("testperson"))),new ObjectCreation.Mapping("age",new ConstantExpression(IntNode.valueOf(30)))).getMapping(1);
  Assert.assertEquals(new ObjectCreation.Mapping("age",new ConstantExpression(IntNode.valueOf(30))),mapping);
}
