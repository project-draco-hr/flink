{
  inputStream1.getType();
  inputStream2.getType();
  TwoInputTransformation<IN1,IN2,OUT> transform=new TwoInputTransformation<>(inputStream1.getTransformation(),inputStream2.getTransformation(),functionName,operator,outTypeInfo,environment.getParallelism());
  @SuppressWarnings({"unchecked","rawtypes"}) SingleOutputStreamOperator<OUT,?> returnStream=new SingleOutputStreamOperator(environment,transform);
  getExecutionEnvironment().addOperator(transform);
  return returnStream;
}
