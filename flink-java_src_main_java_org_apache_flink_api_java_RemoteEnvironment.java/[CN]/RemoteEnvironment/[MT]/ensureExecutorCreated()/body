{
  if (executor == null) {
    executor=PlanExecutor.createRemoteExecutor(host,port,clientConfiguration,jarFiles);
    executor.setPrintStatusDuringExecution(getConfig().isSysoutLoggingEnabled());
  }
  if (getSessionTimeout() > 0 && !executor.isRunning()) {
    executor.start();
    installShutdownHook();
  }
}
