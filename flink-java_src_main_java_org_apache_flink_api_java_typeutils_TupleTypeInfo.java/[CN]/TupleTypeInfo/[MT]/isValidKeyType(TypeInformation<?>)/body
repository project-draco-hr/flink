{
  if (typeInfo instanceof TupleTypeInfo) {
    TupleTypeInfo<?> tupleType=((TupleTypeInfo<?>)typeInfo);
    for (int i=0; i < tupleType.getArity(); i++) {
      if (!isValidKeyType(tupleType.getTypeAt(i))) {
        return false;
      }
    }
    return true;
  }
 else   if (typeInfo.isKeyType()) {
    return true;
  }
 else {
    return false;
  }
}
