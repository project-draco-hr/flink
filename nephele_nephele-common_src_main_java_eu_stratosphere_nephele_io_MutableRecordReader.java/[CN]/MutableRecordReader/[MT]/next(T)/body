{
  if (this.endOfStream) {
    return false;
  }
  while (true) {
    InputChannelResult result=this.inputGate.readRecord(target);
switch (result) {
case INTERMEDIATE_RECORD_FROM_BUFFER:
case LAST_RECORD_FROM_BUFFER:
      return true;
case EVENT:
    handleEvent(this.inputGate.getCurrentEvent());
  break;
case END_OF_STREAM:
this.endOfStream=true;
return false;
default :
;
}
}
}
