{
  final ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  DataSet<CustomType> pojoDs=env.fromCollection(pojoData);
  try {
    pojoDs.sortPartition("myLong",Order.ASCENDING).sortPartition("myString",Order.DESCENDING).writeAsText("/tmp/willNotHappen");
  }
 catch (  Exception e) {
    Assert.fail();
  }
}
