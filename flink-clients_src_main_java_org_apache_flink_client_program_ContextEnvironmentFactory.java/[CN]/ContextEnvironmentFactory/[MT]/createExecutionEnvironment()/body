{
  if (!wait && lastEnvCreated != null) {
    throw new InvalidProgramException("Multiple enviornments cannot be created in detached mode");
  }
  lastEnvCreated=wait ? new ContextEnvironment(client,jarFilesToAttach,classpathsToAttach,userCodeClassLoader,savepointPath) : new DetachedEnvironment(client,jarFilesToAttach,classpathsToAttach,userCodeClassLoader,savepointPath);
  if (defaultParallelism > 0) {
    lastEnvCreated.setParallelism(defaultParallelism);
  }
  return lastEnvCreated;
}
