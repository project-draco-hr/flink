{
  if (ee.hasUnboundInputGates() || ee.hasUnboundOutputGates())   TestPlan.fail("Task with ID " + id + " has unbound gates",ee,id);
  for (int i=0; i < ee.getNumberOfInputGates(); i++)   this.registerInputChannels(ee.getInputGate(i));
  for (int i=0; i < ee.getNumberOfOutputGates(); i++)   this.registerOutputChannels(ee.getOutputGate(i));
}
