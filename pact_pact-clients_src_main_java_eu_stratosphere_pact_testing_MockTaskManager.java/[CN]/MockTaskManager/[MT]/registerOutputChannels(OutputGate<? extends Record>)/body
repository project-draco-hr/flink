{
  for (int i=0; i < eog.getNumberOfOutputChannels(); i++) {
    final AbstractOutputChannel<? extends Record> eoc=eog.getOutputChannel(i);
    if (eoc instanceof InMemoryOutputChannel<?>)     this.directChannelManager.registerDirectOutputChannel((AbstractDirectOutputChannel<? extends Record>)eoc);
 else     throw new ChannelSetupException("Type of output channel " + eoc.getType() + " is not supported");
  }
}
