{
  for (int i=0; i < eig.getNumberOfInputChannels(); i++) {
    final AbstractInputChannel<? extends Record> eic=eig.getInputChannel(i);
    if (eic instanceof InMemoryInputChannel<?>)     this.directChannelManager.registerDirectInputChannel((AbstractDirectInputChannel<? extends Record>)eic);
 else     throw new ChannelSetupException("Type of input channel " + eic.getType() + " is not supported");
  }
}
