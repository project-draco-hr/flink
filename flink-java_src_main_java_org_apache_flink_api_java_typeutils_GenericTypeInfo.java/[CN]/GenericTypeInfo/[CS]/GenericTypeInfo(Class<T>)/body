{
  this.typeClass=typeClass;
  for (  Class<?> unsupported : unsupportedByAvro) {
    if (unsupported.isAssignableFrom(typeClass)) {
      throw new RuntimeException("The type '" + typeClass + "' is currently not supported "+ "by the Avro Serializer that Flink is using for serializing "+ "arbitrary objects");
    }
  }
}
