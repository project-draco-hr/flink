{
  if (index >= 0 && index < this.size - 3) {
    return ((this.memory[this.offset + index + 0] & 0xff) << 24) | ((this.memory[this.offset + index + 1] & 0xff) << 16) | ((this.memory[this.offset + index + 2] & 0xff) << 8)| ((this.memory[this.offset + index + 3] & 0xff) << 0);
  }
 else {
    throw new IndexOutOfBoundsException();
  }
}
