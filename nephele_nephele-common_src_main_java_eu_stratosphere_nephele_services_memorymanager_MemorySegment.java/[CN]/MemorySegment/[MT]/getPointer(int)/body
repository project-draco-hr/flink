{
  if (index >= 0 && index < this.size - 4) {
    return (((long)this.memory[this.offset + index + 0] & 0xff) << 32) | (((long)this.memory[this.offset + index + 1] & 0xff) << 24) | (((long)this.memory[this.offset + index + 2] & 0xff) << 16)| (((long)this.memory[this.offset + index + 3] & 0xff) << 8)| (((long)this.memory[this.offset + index + 4] & 0xff) << 0);
  }
 else {
    throw new IndexOutOfBoundsException();
  }
}
