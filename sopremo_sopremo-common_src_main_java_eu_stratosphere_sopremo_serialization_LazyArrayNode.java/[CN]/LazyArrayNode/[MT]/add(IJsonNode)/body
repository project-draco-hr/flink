{
  if (node == null) {
    throw new NullPointerException();
  }
  for (int i=0; i < this.schema.getHeadSize(); i++) {
    if (this.record.isNull(i) && !node.isMissing()) {
      this.record.setField(i,new JsonNodeWrapper(node));
      return this;
    }
  }
  this.getOtherField().add(node);
  return this;
}
