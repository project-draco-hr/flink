{
  JoinWindowFunction<I1,I2,OUT> joinWindowFunction=new JoinWindowFunction<I1,I2,OUT>(predicate.keys1,predicate.keys2,joinFunction);
  StreamJoinOperator<I1,I2> op=predicate.op;
  TypeInformation<OUT> outType=TypeExtractor.getJoinReturnTypes(joinFunction,op.input1.getType(),op.input2.getType());
  return op.input1.connect(op.input2).addGeneralWindowCombine(joinWindowFunction,outType,op.windowSize,op.slideInterval,op.timeStamp1,op.timeStamp2);
}
