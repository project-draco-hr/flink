{
  if (outputs == null)   throw new NullPointerException("outputs must not be null");
  if (this.minOutputs > outputs.size() || outputs.size() > this.maxOutputs)   throw new IndexOutOfBoundsException();
  this.outputs.clear();
  for (  final JsonStream output : outputs) {
    this.checkOutput(output);
    this.outputs.add(output == null ? null : output.getSource());
  }
}
