{
  int index=this.schema.hasMapping(fieldName);
  if (this.fieldInSchema(index)) {
    IJsonNode node;
    if (this.record.isNull(index))     node=MissingNode.getInstance();
 else     node=SopremoUtil.unwrap(this.record.getField(index,JsonNodeWrapper.class));
    return node;
  }
  return this.getOtherField().get(fieldName);
}
