{
  this.count=getRuntimeContext().getOperatorState("count",0L,false,new StateCheckpointer<Long,String>(){
    @Override public String snapshotState(    Long state,    long id,    long ts){
      return state.toString();
    }
    @Override public Long restoreState(    String stateSnapshot){
      return Long.parseLong(stateSnapshot);
    }
  }
);
}
