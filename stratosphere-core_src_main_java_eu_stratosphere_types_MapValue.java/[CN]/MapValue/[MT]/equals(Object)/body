{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (this.getClass() != obj.getClass())   return false;
  final MapValue<?,?> other=(MapValue<?,?>)obj;
  if (this.map == null) {
    if (other.map != null)     return false;
  }
 else   if (!this.map.equals(other.map))   return false;
  return true;
}
