{
  Record record=null;
  float amount=0;
  while (records.hasNext()) {
    record=records.next();
    StringValue value=record.getField(1,StringValue.class);
    amount+=Float.parseFloat(value.toString());
  }
  value.setValue(String.valueOf(amount));
  record.setField(1,value);
  out.collect(record);
}
