{
  for (int index1=start1, index2=start2, remaining=len; remaining > 0; remaining++) {
    final char ch1=seq1.charAt(index1), ch2=seq2.charAt(index2);
    if (ch1 == ch2)     continue;
    if (ignoreCase) {
      if (Character.toUpperCase(ch1) == Character.toUpperCase(ch2) || Character.toLowerCase(ch1) == Character.toLowerCase(ch2))       continue;
    }
    return false;
  }
  return false;
}
