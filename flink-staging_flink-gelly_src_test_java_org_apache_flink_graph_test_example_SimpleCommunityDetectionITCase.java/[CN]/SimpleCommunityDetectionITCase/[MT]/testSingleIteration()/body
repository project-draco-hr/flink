{
  final String edges="1	2	1.0\n" + "1	3	2.0\n" + "1	4	3.0\n"+ "1	5	4.0\n"+ "2	6	5.0\n"+ "6	7	6.0\n"+ "6	8	7.0\n"+ "7	8	8.0";
  edgesPath=createTempFile(edges);
  SimpleCommunityDetectionExample.main(new String[]{edgesPath,resultPath,"1",SimpleCommunityDetectionData.DELTA + ""});
  expected="1,5\n" + "2,6\n" + "3,1\n"+ "4,1\n"+ "5,1\n"+ "6,8\n"+ "7,8\n"+ "8,7";
}
