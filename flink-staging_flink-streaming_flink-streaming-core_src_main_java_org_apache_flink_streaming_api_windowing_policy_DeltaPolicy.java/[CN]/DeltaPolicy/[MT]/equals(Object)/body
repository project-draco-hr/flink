{
  if (other == null || !(other instanceof DeltaPolicy)) {
    return false;
  }
 else {
    try {
      @SuppressWarnings("unchecked") DeltaPolicy<DATA> otherPolicy=(DeltaPolicy<DATA>)other;
      return threshold == otherPolicy.threshold && deltaFuntion.getClass() == otherPolicy.deltaFuntion.getClass() && triggerDataPoint.equals(otherPolicy.triggerDataPoint);
    }
 catch (    ClassCastException e) {
      return false;
    }
  }
}
