{
  final BufferPairResponse bufferPairResponse=this.byteBufferedChannelManager.requestEmptyWriteBuffers(this,byteBufferPair);
  if (bufferPairResponse.getCompressedDataBuffer() != null || bufferPairResponse.getUncompressedDataBuffer() != null) {
    this.channelsWithWriteBuffers.add(wrapper);
  }
  return bufferPairResponse;
}
