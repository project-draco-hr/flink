{
{
    try {
      outputs.get(outputChannel).emit(record);
      if (log.isWarnEnabled()) {
        log.warn("RE-EMITTED: " + record.getId() + " "+ outputChannel);
      }
    }
 catch (    Exception e) {
      if (log.isErrorEnabled()) {
        log.error("RE-EMIT FAILED, avoiding record: " + record.getId());
      }
    }
  }
}
