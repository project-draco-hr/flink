{
  for (  RecordWriter<StreamRecord> output : outputs) {
    try {
      output.emit(record);
      if (log.isWarnEnabled()) {
        log.warn("RE-EMITTED: " + record.getId());
      }
    }
 catch (    Exception e) {
      if (log.isErrorEnabled()) {
        log.error("RE-EMIT FAILED, avoiding record: " + record.getId());
      }
    }
  }
}
