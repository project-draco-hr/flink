{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Condition other=(Condition)obj;
  if (chainedCondition == null) {
    if (other.chainedCondition != null)     return false;
  }
 else   if (!chainedCondition.equals(other.chainedCondition))   return false;
  if (combination == null) {
    if (other.combination != null)     return false;
  }
 else   if (!combination.equals(other.combination))   return false;
  return comparison.equals(other.comparison);
}
