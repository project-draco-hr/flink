{
  final FileDataSource read=new FileDataSource(CsvInputFormat.class,this.getResource("SopremoTestPlan/restaurant_short.csv"),"Input");
  final MapContract map=MapContract.builder(IdentityMap.class).name("Map").build();
  map.setInput(read);
  final FileDataSink output=this.createOutput(map,SequentialOutputFormat.class);
  final TestPlan testPlan=new TestPlan(output);
  testPlan.getExpectedOutput(output,SCHEMA.getPactSchema()).fromFile(JsonInputFormat.class,this.getResource("SopremoTestPlan/restaurant_short.json"));
  testPlan.run();
}
