{
  ForExpr forExpr=(ForExpr)expr;
  Expr[] exprs=buildExprList(forExpr.binding(),forExpr.collectExpr());
  if (exprs == null) {
    return false;
  }
  UnionFn merge=new UnionFn(exprs);
  if (forExpr.parent() instanceof UnrollLoopPragma) {
    expr=forExpr.parent();
  }
  expr.replaceInParent(merge);
  return true;
}
