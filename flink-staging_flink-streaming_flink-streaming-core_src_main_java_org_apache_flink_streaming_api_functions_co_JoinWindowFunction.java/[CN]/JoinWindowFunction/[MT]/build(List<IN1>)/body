{
  Map<Object,List<IN1>> map=new HashMap<Object,List<IN1>>();
  for (  IN1 record : records) {
    Object key=keySelector1.getKey(record);
    List<IN1> current=map.get(key);
    if (current == null) {
      current=new LinkedList<IN1>();
      map.put(key,current);
    }
    current.add(record);
  }
  return map;
}
