{
  try {
    Execution ev=mock(Execution.class);
    SimpleSlot slot=getSlot();
    assertTrue(slot.setExecutedVertex(ev));
    assertEquals(ev,slot.getExecution());
    slot.cancel();
    slot.releaseSlot();
    slot.cancel();
    verify(ev,times(1)).fail(Matchers.any(Throwable.class));
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail(e.getMessage());
  }
}
