{
  IntPair result=new IntPair();
  if (!repeatKey) {
    if (valCnt >= numVals) {
      return null;
    }
    result.setKey(keyCnt++);
    result.setValue(valCnt);
    if (keyCnt == numKeys) {
      keyCnt=0;
      valCnt++;
    }
  }
 else {
    if (keyCnt >= numKeys) {
      return null;
    }
    result.setKey(keyCnt);
    result.setValue(valCnt++);
    if (valCnt == numVals) {
      valCnt=0;
      keyCnt++;
    }
  }
  return result;
}
