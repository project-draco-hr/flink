{
  final List<OutputType> childTypes=new ArrayList<OutputType>();
  if (converterInfo == null || !converterInfo.isStopRecursion())   for (  final InputType child : navigator.getConnectedNodes(root)) {
    final OutputType handledResult=this.convertGraph(child,navigator);
    if (this.flattenCollection && handledResult instanceof Collection<?>)     childTypes.addAll((Collection<? extends OutputType>)handledResult);
 else     if (handledResult != null)     childTypes.add(handledResult);
  }
  childTypes.addAll(this.lastChildren);
  return childTypes;
}
