{
  this.defaultMaxFan=config.getInteger(PactConfigConstants.DEFAULT_SPILLING_MAX_FAN_KEY,PactConfigConstants.DEFAULT_SPILLING_MAX_FAN);
  this.defaultSortSpillingThreshold=config.getFloat(PactConfigConstants.DEFAULT_SORT_SPILLING_THRESHOLD_KEY,PactConfigConstants.DEFAULT_SORT_SPILLING_THRESHOLD);
