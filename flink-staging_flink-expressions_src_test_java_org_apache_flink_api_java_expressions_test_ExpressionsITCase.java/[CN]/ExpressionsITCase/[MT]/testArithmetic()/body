{
  ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  DataSource<Tuple2<Integer,Integer>> input=env.fromElements(new Tuple2<Integer,Integer>(5,10));
  ExpressionOperation<JavaBatchTranslator> expressionOperation=ExpressionUtil.from(input,"a, b");
  ExpressionOperation<JavaBatchTranslator> result=expressionOperation.select("a - 5, a + 5, a / 2, a * 2, a % 2, -a");
  DataSet<Row> ds=ExpressionUtil.toSet(result,Row.class);
  ds.writeAsText(resultPath,FileSystem.WriteMode.OVERWRITE);
  env.execute();
  expected="0,10,2,10,1,-5";
}
