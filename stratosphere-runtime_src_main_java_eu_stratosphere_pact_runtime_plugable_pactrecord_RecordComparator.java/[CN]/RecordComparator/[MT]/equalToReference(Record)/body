{
  for (int i=0; i < this.keyFields.length; i++) {
    final Key k=candidate.getField(this.keyFields[i],this.transientKeyHolders[i]);
    if (k == null) {
      throw new NullKeyFieldException(this.keyFields[i]);
    }
 else     if (!k.equals(this.keyHolders[i])) {
      return false;
    }
  }
  return true;
}
