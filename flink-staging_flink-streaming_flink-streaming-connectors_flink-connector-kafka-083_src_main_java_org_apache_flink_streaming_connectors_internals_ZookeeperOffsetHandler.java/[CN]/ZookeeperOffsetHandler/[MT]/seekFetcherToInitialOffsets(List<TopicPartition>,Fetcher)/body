{
  for (  TopicPartition tp : partitions) {
    long offset=getOffsetFromZooKeeper(zkClient,groupId,tp.topic(),tp.partition());
    if (offset != OFFSET_NOT_SET) {
      LOG.info("Offset for partition {} was set to {} in ZooKeeper. Seeking fetcher to that position.",tp.partition(),offset);
      fetcher.seek(tp,offset + 1);
    }
  }
}
