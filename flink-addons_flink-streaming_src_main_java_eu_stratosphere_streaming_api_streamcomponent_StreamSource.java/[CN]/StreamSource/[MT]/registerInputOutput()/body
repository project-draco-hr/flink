{
  Configuration taskConfiguration=getTaskConfiguration();
  StreamComponentFactory.setConfigOutputs(this,taskConfiguration,outputs,partitioners);
  recordBuffer=new FaultTolerancyBuffer(outputs,sourceInstanceID);
  userFunction=(UserSourceInvokable)StreamComponentFactory.setUserFunction(taskConfiguration,outputs,sourceInstanceID,recordBuffer);
  StreamComponentFactory.setAckListener(recordBuffer,sourceInstanceID,outputs);
  StreamComponentFactory.setFailListener(recordBuffer,sourceInstanceID,outputs);
}
