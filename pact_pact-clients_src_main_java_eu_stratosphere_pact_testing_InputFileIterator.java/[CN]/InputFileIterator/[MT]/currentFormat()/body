{
  InputFormat<K,V> format=this.currentFormat;
  try {
    while (format != null && format.reachedEnd())     if (this.formatIterator.hasNext())     format=this.currentFormat=this.formatIterator.next();
 else     format=this.currentFormat=null;
  }
 catch (  final IOException e) {
    throw new IllegalStateException("cannot verify end of format " + format,e);
  }
  return format;
}
