{
  checkErroneous();
  byte[] serializedKey=schema.serializeKey(next);
  byte[] serializedValue=schema.serializeValue(next);
  ProducerRecord<byte[],byte[]> record=new ProducerRecord<>(topicId,partitioner.partition(next,partitions.length),serializedKey,serializedValue);
  producer.send(record,callback);
}
