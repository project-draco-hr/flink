{
  for (  final KeyValuePair<JsonNode,JsonNode> left : input) {
    boolean skipPairs=true;
    for (    final KeyValuePair<JsonNode,JsonNode> right : input) {
      if (left == right) {
        skipPairs=false;
        continue;
      }
 else       if (skipPairs)       continue;
      this.emitCandidate(left,right);
    }
  }
}
