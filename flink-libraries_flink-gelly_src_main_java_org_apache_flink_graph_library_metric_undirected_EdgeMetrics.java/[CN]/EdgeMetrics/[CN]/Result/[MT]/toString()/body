{
  NumberFormat nf=NumberFormat.getInstance();
  return "vertex count: " + nf.format(vertexCount) + "; edge count: "+ nf.format(edgeCount)+ "; triangle triplet count: "+ nf.format(triangleTripletCount)+ "; rectangle triplet count: "+ nf.format(rectangleTripletCount)+ "; triplet count: "+ nf.format(tripletCount)+ "; maximum degree: "+ nf.format(maximumDegree)+ "; maximum triangle triplets: "+ nf.format(maximumTriangleTriplets)+ "; maximum rectangle triplets: "+ nf.format(maximumRectangleTriplets)+ "; maximum triplets: "+ nf.format(maximumTriplets);
}
