{
  if (typeHierarchy.size() > 0 && typeHierarchy.get(0) == curT && isClassType(curT)) {
    curT=typeToClass(curT).getGenericSuperclass();
  }
  while (!(isClassType(curT) && typeToClass(curT).equals(stopAtClass))) {
    typeHierarchy.add(curT);
    curT=typeToClass(curT).getGenericSuperclass();
  }
  return curT;
}
