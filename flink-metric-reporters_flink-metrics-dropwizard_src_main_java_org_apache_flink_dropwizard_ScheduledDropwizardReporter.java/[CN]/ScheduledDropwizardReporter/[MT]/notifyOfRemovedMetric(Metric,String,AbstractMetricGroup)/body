{
synchronized (this) {
    String fullName;
    if (metric instanceof Counter) {
      fullName=counters.remove(metric);
    }
 else     if (metric instanceof Gauge) {
      fullName=gauges.remove(metric);
    }
 else {
      fullName=null;
    }
    if (fullName != null) {
      registry.remove(fullName);
    }
  }
}
