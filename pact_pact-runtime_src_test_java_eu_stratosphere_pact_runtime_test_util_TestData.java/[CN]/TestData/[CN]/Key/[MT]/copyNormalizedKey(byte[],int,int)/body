{
  if (len >= 4) {
    target[offset]=(byte)((key >>> 24) & 0xff);
    target[offset + 1]=(byte)((key >>> 16) & 0xff);
    target[offset + 2]=(byte)((key >>> 8) & 0xff);
    target[offset + 3]=(byte)((key) & 0xff);
    return 4;
  }
 else {
    for (int i=0; len > 0; len--, i++) {
      target[offset + i]=(byte)((key >>> ((3 - i) << 3)) & 0xff);
    }
    return len;
  }
}
