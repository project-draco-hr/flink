{
  final int rowsum=JsonUtil2.getField(node,JSON_KEY_ROWSUM,IntNode.class).getIntValue();
  INode subnode;
  if (JsonUtil2.getField(node,JSON_KEY_HAS_LEAF,BooleanNode.class).getBooleanValue())   subnode=this.tree.createLeaf(null,null);
 else   subnode=this.tree.createInnerNode();
  subnode.read(JsonUtil2.getField(node,JSON_KEY_CHILD,IJsonNode.class));
  return new Entry(subnode,rowsum);
}
