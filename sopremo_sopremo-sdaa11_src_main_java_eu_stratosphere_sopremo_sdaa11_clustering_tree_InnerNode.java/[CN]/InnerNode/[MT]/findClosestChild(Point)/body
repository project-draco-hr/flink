{
  int minDistance=-1;
  INode subnode=null;
  for (  final Entry entry : this.entries)   if (entry != null) {
    final int tempDistance=point.getDistance(entry.getPoint());
    if (minDistance < 0 || tempDistance < minDistance) {
      minDistance=tempDistance;
      subnode=entry.subnode;
    }
  }
  if (subnode == null)   throw new IllegalStateException("Did not find a subnode");
  return subnode;
}
