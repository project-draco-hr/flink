{
  Random rnd=new Random(874597969123412338L);
  float rndFloat=rnd.nextFloat();
  if (rndFloat < 0) {
    rndFloat=-rndFloat;
  }
  if (rndFloat == Float.MAX_VALUE) {
    rndFloat-=3;
  }
  if (rndFloat <= 2) {
    rndFloat+=3;
  }
  return new Float[]{new Float(-rndFloat),new Float(-1.0F),new Float(0.0F),new Float(2.0F),new Float(rndFloat),new Float(Float.MAX_VALUE)};
}
