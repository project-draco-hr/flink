{
  if (inputs.size() <= 1)   return new GroupProjection(this.resultProjection,inputs.get(0));
  if (inputs.size() == 2)   return new CoGroupProjection(this.resultProjection,inputs.get(0),inputs.get(1));
  final UnionAll union=new UnionAll(inputs);
  return new GroupProjection(new PathExpression(new AggregationExpression(new ArrayUnion()),this.resultProjection),union);
}
