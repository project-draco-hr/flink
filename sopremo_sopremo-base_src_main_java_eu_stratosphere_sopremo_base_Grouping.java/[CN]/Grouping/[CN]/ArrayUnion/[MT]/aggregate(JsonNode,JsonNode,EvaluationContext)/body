{
  ArrayNode arrayNode=((ArrayNode)array);
  for (int index=0; index < arrayNode.size(); index++) {
    if (((ArrayNode)mergedArray).size() <= index)     ((ArrayNode)mergedArray).add(new ArrayNode());
    if (!arrayNode.get(index).isNull())     ((ArrayNode)(((ArrayNode)mergedArray).get(index))).add(arrayNode.get(index));
  }
  return mergedArray;
}
