{
  if (queueIterator == null) {
    LOG.debug("Requesting LOCAL queue {} of partition {}.",subpartitionIndex,partitionId);
    queueIterator=partitionManager.getSubpartition(partitionId,subpartitionIndex,Optional.of(inputGate.getBufferProvider()));
    if (queueIterator == null) {
      throw new IOException("Error requesting sub partition.");
    }
    getNextLookAhead();
  }
}
