{
  final ResultSubpartitionView view=subpartitionView;
  if (view == null) {
    return;
  }
  while (true) {
    lookAhead=view.getNextBuffer();
    if (lookAhead != null) {
      notifyAvailableBuffer();
      break;
    }
    if (view.registerListener(this) || view.isReleased()) {
      return;
    }
  }
}
