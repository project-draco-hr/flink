{
  if (queueIterator == null) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Requesting LOCAL queue {} from partition {} produced by {}.",queueIndex,partitionId,producerExecutionId);
    }
    queueIterator=partitionManager.getIntermediateResultPartitionIterator(producerExecutionId,partitionId,queueIndex,Optional.of(inputGate.getBufferProvider()));
    getNextLookAhead();
  }
}
