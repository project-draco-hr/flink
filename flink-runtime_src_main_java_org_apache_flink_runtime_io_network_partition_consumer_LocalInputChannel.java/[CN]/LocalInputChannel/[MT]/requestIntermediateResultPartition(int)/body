{
  if (queueIterator == null) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Requesting queue {} from LOCAL partition {}.",partitionId,queueIndex);
    }
    queueIterator=reader.getIntermediateResultPartitionProvider().getIntermediateResultPartitionIterator(producerExecutionId,partitionId,queueIndex,Optional.of(reader.getBufferProvider()));
    getNextLookAhead();
  }
}
