{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.getExecutionEnvironment();
  @SuppressWarnings("unused") DataStream<String> stream1=env.addSource(new KafkaSource<String>("localhost:2181","group","test",new SimpleStringScheme())).addSink(new MyKafkaPrintSink());
  @SuppressWarnings("unused") DataStream<String> stream2=env.addSource(new MySource()).addSink(new KafkaSink<String,String>("test","localhost:9092",new SimpleStringScheme()));
  env.execute();
}
