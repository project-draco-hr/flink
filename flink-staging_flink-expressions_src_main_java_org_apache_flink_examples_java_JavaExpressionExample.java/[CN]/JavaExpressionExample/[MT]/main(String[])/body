{
  ExecutionEnvironment env=ExecutionEnvironment.createCollectionsEnvironment();
  DataSet<WC> input=env.fromElements(new WC("Hello",1),new WC("Ciao",1),new WC("Hello",1));
  ExpressionOperation expr=ExpressionUtil.from(input);
  ExpressionOperation filtered=expr.groupBy("word").select("word.count as count, word").filter("count = 2");
  DataSet<WC> result=ExpressionUtil.toSet(filtered,WC.class);
  result.print();
  env.execute();
}
