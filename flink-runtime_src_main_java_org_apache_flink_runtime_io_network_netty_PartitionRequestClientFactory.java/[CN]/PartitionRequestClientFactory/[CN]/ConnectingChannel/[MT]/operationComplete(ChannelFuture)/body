{
  if (future.isSuccess()) {
    handInChannel(future.channel());
  }
 else   if (future.cause() != null) {
    notifyOfError(new RemoteTransportException("Connecting to remote task manager + '" + connectionId.getAddress() + "' has failed. This might indicate that the remote task "+ "manager has been lost.",future.cause(),connectionId.getAddress()));
  }
 else {
    notifyOfError(new LocalTransportException("Connecting to remote task manager + '" + connectionId.getAddress() + "' has been cancelled."));
  }
}
