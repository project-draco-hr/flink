{
  DataOutputBuffer buffer=new DataOutputBuffer(16);
  try {
    one.write(buffer);
    byte[] data=new byte[16];
    System.arraycopy(buffer.getData().array(),0,data,0,16);
    buffer.reset();
    two.write(buffer);
    byte[] twoData=buffer.getData().array();
    for (int i=0; i < 16; i++) {
      data[i]=(byte)(data[i] ^ twoData[i]);
    }
    return data;
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return null;
}
