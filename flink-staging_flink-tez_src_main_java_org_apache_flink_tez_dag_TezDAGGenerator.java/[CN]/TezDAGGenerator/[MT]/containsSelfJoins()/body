{
  for (  FlinkVertex v : vertices.values()) {
    ArrayList<FlinkVertex> predecessors=new ArrayList<FlinkVertex>();
    for (    FlinkEdge e : edges) {
      if (e.target == v) {
        if (predecessors.contains(e.source)) {
          return true;
        }
        predecessors.add(e.source);
      }
    }
  }
  return false;
}
