{
  checkNotNull(vertexId);
  checkNotNull(executionId);
  checkNotNull(name);
synchronized (this) {
    if (!isClosed()) {
      TaskMetricGroup task=new TaskMetricGroup(registry,this,vertexId,executionId,subtaskIndex,name);
      tasks.put(executionId,task);
      return task;
    }
 else {
      return null;
    }
  }
}
