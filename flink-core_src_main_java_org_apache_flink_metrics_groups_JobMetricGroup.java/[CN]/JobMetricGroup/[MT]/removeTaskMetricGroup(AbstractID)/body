{
  checkNotNull(executionId);
  boolean removeFromParent=false;
synchronized (this) {
    if (!isClosed() && tasks.remove(executionId) != null && tasks.isEmpty()) {
      removeFromParent=true;
      close();
    }
  }
  if (removeFromParent) {
    ((TaskManagerMetricGroup)parent()).removeJobMetricsGroup(jobId,this);
  }
}
