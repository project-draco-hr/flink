{
  List<TwoSourceJoin> joins=new ArrayList<TwoSourceJoin>();
  for (  BooleanExpression expression : condition.getExpressions())   if (expression instanceof ComparativeExpression)   joins.add(new ComparisonJoin((ComparativeExpression)expression));
 else   if (expression instanceof ElementInSetExpression)   joins.add(new ElementInSetJoin((ElementInSetExpression)expression));
 else   throw new UnsupportedOperationException();
  return joins;
}
