{
  if (streamVertex.configuration.getDirectedEmit()) {
    OutputSelector<OUT> outputSelector=streamVertex.configuration.getOutputSelector();
    collector=new DirectedStreamCollector<OUT>(streamVertex.getInstanceID(),outSerializationDelegate,outputSelector);
  }
 else {
    collector=new StreamCollector<OUT>(streamVertex.getInstanceID(),outSerializationDelegate);
  }
  return collector;
}
