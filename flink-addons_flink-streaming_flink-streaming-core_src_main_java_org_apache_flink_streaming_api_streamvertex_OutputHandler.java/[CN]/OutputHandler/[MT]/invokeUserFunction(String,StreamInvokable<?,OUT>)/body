{
  if (LOG.isDebugEnabled()) {
    LOG.debug("{} {} invoked with instance id {}",componentTypeName,streamVertex.getName(),streamVertex.getInstanceID());
  }
  try {
    streamVertex.invokeUserFunction(userInvokable);
  }
 catch (  Exception e) {
    flushOutputs();
    throw new RuntimeException(e);
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("{} {} invoke finished instance id {}",componentTypeName,streamVertex.getName(),streamVertex.getInstanceID());
  }
  flushOutputs();
}
