{
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple1<String>> stream1=env.addSource(new KafkaSource("localhost:2181","group","test",1),SOURCE_PARALELISM).print();
  DataStream<Tuple1<String>> stream2=env.addSource(new MySource(),1).addSink(new KafkaSink("test","localhost:9092"));
  env.execute();
}
