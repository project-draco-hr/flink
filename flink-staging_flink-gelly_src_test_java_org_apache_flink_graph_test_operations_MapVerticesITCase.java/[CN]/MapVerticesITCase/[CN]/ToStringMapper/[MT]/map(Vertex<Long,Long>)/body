{
  String stringValue;
  if (vertex.getValue() == 1) {
    stringValue="one";
  }
 else   if (vertex.getValue() == 2) {
    stringValue="two";
  }
 else   if (vertex.getValue() == 3) {
    stringValue="three";
  }
 else   if (vertex.getValue() == 4) {
    stringValue="four";
  }
 else   if (vertex.getValue() == 5) {
    stringValue="five";
  }
 else {
    stringValue="";
  }
  return stringValue;
}
