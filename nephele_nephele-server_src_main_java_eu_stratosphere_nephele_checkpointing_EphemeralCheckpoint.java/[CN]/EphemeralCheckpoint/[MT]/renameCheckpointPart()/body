{
  final Path oldFile=this.checkpointPath.suffix(Path.SEPARATOR + CheckpointUtils.METADATA_PREFIX + "_"+ this.task.getVertexID()+ "_part");
  final Path newFile=this.checkpointPath.suffix(Path.SEPARATOR + CheckpointUtils.METADATA_PREFIX + "_"+ this.task.getVertexID()+ "_"+ this.metaDataSuffix);
  if (!this.fileSystem.rename(oldFile,newFile)) {
    LOG.error("Unable to rename " + oldFile + " to "+ newFile);
    return false;
  }
  return true;
}
