{
  Assert.assertEquals((LATE_EVENTS_PER_SESSION + 1) * NUMBER_OF_SESSIONS * EVENTS_PER_SESSION,getRuntimeContext().getIntCounter(SESSION_COUNTER_ON_TIME_KEY).getLocalValuePrimitive());
  Assert.assertEquals(NUMBER_OF_SESSIONS * (LATE_EVENTS_PER_SESSION * (LATE_EVENTS_PER_SESSION + 1) / 2),getRuntimeContext().getIntCounter(SESSION_COUNTER_LATE_KEY).getLocalValuePrimitive());
}
