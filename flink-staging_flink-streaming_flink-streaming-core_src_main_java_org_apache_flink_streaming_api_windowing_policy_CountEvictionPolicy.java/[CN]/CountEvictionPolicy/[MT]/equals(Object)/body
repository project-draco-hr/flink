{
  if (other == null || !(other instanceof CountEvictionPolicy)) {
    return false;
  }
 else {
    try {
      @SuppressWarnings("unchecked") CountEvictionPolicy<IN> otherPolicy=(CountEvictionPolicy<IN>)other;
      return startValue == otherPolicy.startValue && deleteOnEviction == otherPolicy.deleteOnEviction && maxElements == otherPolicy.maxElements;
    }
 catch (    ClassCastException e) {
      return false;
    }
  }
}
