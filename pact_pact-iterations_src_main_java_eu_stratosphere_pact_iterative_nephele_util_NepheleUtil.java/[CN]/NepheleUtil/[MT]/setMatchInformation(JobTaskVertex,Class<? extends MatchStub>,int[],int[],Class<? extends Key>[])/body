{
  TaskConfig config=new TaskConfig(match.getConfiguration());
  config.setStubClass(stubClass);
  PactRecordComparatorFactory.writeComparatorSetupToConfig(config.getConfiguration(),config.getPrefixForInputParameters(0),keyPosA,keyClasses);
  PactRecordComparatorFactory.writeComparatorSetupToConfig(config.getConfiguration(),config.getPrefixForInputParameters(1),keyPosB,keyClasses);
}
