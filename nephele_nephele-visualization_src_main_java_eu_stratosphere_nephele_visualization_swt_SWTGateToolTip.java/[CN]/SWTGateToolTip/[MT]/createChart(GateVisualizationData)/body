{
  final String yAxisLabel=this.managementGate.isInputGate() ? "No data available/sec." : "Capacity limit reached/sec.";
  final JFreeChart chart=ChartFactory.createXYLineChart(null,"Time [sec.]",yAxisLabel,visualizationData.getChartCollection(),PlotOrientation.VERTICAL,false,false,false);
  final XYPlot xyPlot=chart.getXYPlot();
  xyPlot.getDomainAxis().setAutoRange(true);
  xyPlot.getDomainAxis().setAutoRangeMinimumSize(60);
  xyPlot.getRangeAxis().setAutoRange(true);
  return new ChartComposite(getShell(),SWT.NONE,chart,true);
}
