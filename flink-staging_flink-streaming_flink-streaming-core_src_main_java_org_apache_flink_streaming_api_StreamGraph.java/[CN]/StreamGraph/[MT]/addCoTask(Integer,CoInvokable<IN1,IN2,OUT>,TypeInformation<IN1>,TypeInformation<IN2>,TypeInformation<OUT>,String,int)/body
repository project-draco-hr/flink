{
  addVertex(vertexID,CoStreamVertex.class,taskInvokableObject,operatorName,parallelism);
  StreamRecordSerializer<OUT> outSerializer=(outTypeInfo != null) && !(outTypeInfo instanceof MissingTypeInfo) ? new StreamRecordSerializer<OUT>(outTypeInfo,executionConfig) : null;
  addTypeSerializers(vertexID,new StreamRecordSerializer<IN1>(in1TypeInfo,executionConfig),new StreamRecordSerializer<IN2>(in2TypeInfo,executionConfig),outSerializer,null);
  if (LOG.isDebugEnabled()) {
    LOG.debug("CO-TASK: {}",vertexID);
  }
}
