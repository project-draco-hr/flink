{
  addVertex(vertexID,CoStreamVertex.class,taskInvokableObject,operatorName,parallelism);
  addTypeSerializers(vertexID,new StreamRecordSerializer<IN1>(in1TypeInfo,executionConfig),new StreamRecordSerializer<IN2>(in2TypeInfo,executionConfig),new StreamRecordSerializer<OUT>(outTypeInfo,executionConfig),null);
  if (LOG.isDebugEnabled()) {
    LOG.debug("CO-TASK: {}",vertexID);
  }
}
