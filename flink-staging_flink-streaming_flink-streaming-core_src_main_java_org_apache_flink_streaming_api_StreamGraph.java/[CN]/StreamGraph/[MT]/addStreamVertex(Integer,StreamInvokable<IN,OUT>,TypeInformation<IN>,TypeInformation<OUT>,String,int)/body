{
  addVertex(vertexID,StreamVertex.class,invokableObject,operatorName,parallelism);
  StreamRecordSerializer<IN> inSerializer=inTypeInfo != null ? new StreamRecordSerializer<IN>(inTypeInfo,executionConfig) : null;
  StreamRecordSerializer<OUT> outSerializer=outTypeInfo != null ? new StreamRecordSerializer<OUT>(outTypeInfo,executionConfig) : null;
  addTypeSerializers(vertexID,inSerializer,null,outSerializer,null);
  if (LOG.isDebugEnabled()) {
    LOG.debug("Vertex: {}",vertexID);
  }
}
