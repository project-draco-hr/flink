{
  TypeInformation<OUT> outType=TypeExtractor.getJoinReturnTypes(joinFunction,predicate.op.input1.getType(),predicate.op.input2.getType());
  CoStreamWindow<I1,I2,OUT> operator=new CoStreamWindow<I1,I2,OUT>(getJoinWindowFunction(joinFunction,predicate),predicate.op.windowSize,predicate.op.slideInterval,predicate.op.timeStamp1,predicate.op.timeStamp2);
  streamGraph.setOperator(id,operator);
  return ((SingleOutputStreamOperator<OUT,?>)this).returns(outType);
}
