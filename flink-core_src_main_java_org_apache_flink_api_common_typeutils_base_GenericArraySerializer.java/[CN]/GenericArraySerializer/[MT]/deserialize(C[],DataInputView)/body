{
  int len=source.readInt();
  if (reuse.length != len) {
    reuse=create(len);
  }
  for (int i=0; i < len; i++) {
    boolean isNonNull=source.readBoolean();
    if (isNonNull) {
      reuse[i]=componentSerializer.deserialize(componentSerializer.createInstance(),source);
    }
 else {
      reuse[i]=null;
    }
  }
  return reuse;
}
