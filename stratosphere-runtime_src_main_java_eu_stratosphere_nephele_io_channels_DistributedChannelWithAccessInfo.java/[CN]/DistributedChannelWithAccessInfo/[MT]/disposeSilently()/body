{
  this.referenceCounter.set(Integer.MIN_VALUE);
  this.reservedWritePosition.set(Long.MIN_VALUE);
  if (this.channel.isOpen()) {
    try {
      this.channel.close();
      if (this.deleteOnClose.get()) {
        this.fs.delete(this.checkpointFile,false);
      }
    }
 catch (    Throwable t) {
    }
  }
}
