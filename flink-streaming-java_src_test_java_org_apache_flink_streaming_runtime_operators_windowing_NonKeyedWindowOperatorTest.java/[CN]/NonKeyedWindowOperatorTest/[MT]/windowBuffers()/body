{
  TupleSerializer<Tuple2> tuple2TupleSerializer=new TupleSerializer<>(Tuple2.class,new TypeSerializer<?>[]{StringSerializer.INSTANCE,IntSerializer.INSTANCE});
  return Arrays.asList(new WindowBufferFactory[]{new ReducingWindowBuffer.Factory(new SumReducer(),tuple2TupleSerializer)},new WindowBufferFactory[]{new ListWindowBuffer.Factory(tuple2TupleSerializer)});
}
