{
  BufferedWriter tmpBufferedWriter=null;
  try {
    tmpBufferedWriter=new BufferedWriter(new FileWriter(GlobalConfiguration.getString(ConfigConstants.TASK_MANAGER_TMP_DIR_KEY,ConfigConstants.DEFAULT_TASK_MANAGER_TMP_PATH) + File.separator + filePrefix));
  }
 catch (  IOException ioe) {
    LOG.error(StringUtils.stringifyException(ioe));
  }
  this.bufferedWriter=tmpBufferedWriter;
}
