{
synchronized (lock) {
    if (discarded) {
      return false;
    }
    ExecutionVertex vertex=notYetAcknowledgedTasks.remove(attemptID);
    if (vertex != null) {
      if (state != null) {
        collectedStates.add(new StateForTask(state,stateSize,vertex.getJobvertexId(),vertex.getParallelSubtaskIndex(),System.currentTimeMillis() - checkpointTimestamp));
      }
      numAcknowledgedTasks++;
      return true;
    }
 else {
      return false;
    }
  }
}
