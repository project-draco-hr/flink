{
  BatchReduceInvokable<Integer,String> invokable=new BatchReduceInvokable<Integer,String>(new MySlidingBatchReduce(),SLIDING_BATCH_SIZE,SLIDE_SIZE);
  List<String> expected=Arrays.asList("1","2","3",END_OF_BATCH,"3","4","5",END_OF_BATCH,"5","6","7",END_OF_BATCH);
  List<String> actual=MockInvokable.createAndExecute(invokable,Arrays.asList(1,2,3,4,5,6,7));
  assertEquals(expected,actual);
}
