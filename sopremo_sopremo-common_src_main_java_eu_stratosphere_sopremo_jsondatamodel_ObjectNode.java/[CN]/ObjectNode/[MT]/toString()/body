{
  final StringBuilder sb=new StringBuilder();
  sb.append('{');
  int count=0;
  for (  final Map.Entry<String,JsonNode> en : this.children.entrySet()) {
    if (count > 0)     sb.append(',');
    ++count;
    TextNode.appendQuoted(sb,en.getKey());
    sb.append(':').append(en.getValue().toString());
  }
  sb.append('}');
  return sb.toString();
}
