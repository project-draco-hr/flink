{
  int numberOfNodesOnLevel=0;
  if (level == getDepth()) {
    ++numberOfNodesOnLevel;
  }
  if (level > getDepth()) {
    final Iterator<SWTNetworkNode> it=this.childNodes.iterator();
    while (it.hasNext()) {
      numberOfNodesOnLevel+=it.next().getNumberOfNodesOnLevel(level);
    }
  }
  return numberOfNodesOnLevel;
}
