{
  if (cnt > failCnt) {
    throw new RuntimeException("This is a test exception!");
  }
  int totalReadCnt=0;
  do {
    int readCnt=super.extProcOutStream.read(buf,totalReadCnt,buf.length - totalReadCnt);
    if (readCnt == -1) {
      this.end=true;
      return null;
    }
 else {
      totalReadCnt+=readCnt;
    }
  }
 while (totalReadCnt != 8);
  int v1=0;
  v1=v1 | (0xFF & buf[0]);
  v1=(v1 << 8) | (0xFF & buf[1]);
  v1=(v1 << 8) | (0xFF & buf[2]);
  v1=(v1 << 8) | (0xFF & buf[3]);
  int v2=0;
  v2=v2 | (0xFF & buf[4]);
  v2=(v2 << 8) | (0xFF & buf[5]);
  v2=(v2 << 8) | (0xFF & buf[6]);
  v2=(v2 << 8) | (0xFF & buf[7]);
  reuse.setField(0,new IntValue(v1));
  reuse.setField(1,new IntValue(v2));
  this.cnt++;
  return reuse;
}
