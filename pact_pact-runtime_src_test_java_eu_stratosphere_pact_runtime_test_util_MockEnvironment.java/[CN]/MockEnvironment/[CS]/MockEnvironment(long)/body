{
  this.config=new Configuration();
  this.inputs=new ArrayList<InputGate<KeyValuePair<PactInteger,PactInteger>>>();
  this.outputs=new ArrayList<OutputGate<KeyValuePair<PactInteger,PactInteger>>>();
