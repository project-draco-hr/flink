{
  TaskOperationProtocol tm=this.taskManager;
  if (tm == null) {
synchronized (this) {
      if (this.taskManager == null) {
        this.taskManager=RPC.getProxy(TaskOperationProtocol.class,new InetSocketAddress(getInstanceConnectionInfo().address(),getInstanceConnectionInfo().ipcPort()),NetUtils.getSocketFactory());
      }
      tm=this.taskManager;
    }
  }
  return tm;
}
