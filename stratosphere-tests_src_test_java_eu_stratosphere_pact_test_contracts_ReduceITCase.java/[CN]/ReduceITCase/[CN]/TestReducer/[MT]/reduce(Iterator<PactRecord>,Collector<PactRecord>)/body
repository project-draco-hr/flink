{
  int sum=0;
  PactRecord record=new PactRecord();
  while (records.hasNext()) {
    record=records.next();
    reduceValue=record.getField(1,reduceValue);
    sum+=Integer.parseInt(reduceValue.toString());
    LOG.debug("Processed: [" + record.getField(0,PactString.class).toString() + ","+ reduceValue.toString()+ "]");
  }
  record.setField(1,new PactInteger(sum));
  out.collect(record);
}
