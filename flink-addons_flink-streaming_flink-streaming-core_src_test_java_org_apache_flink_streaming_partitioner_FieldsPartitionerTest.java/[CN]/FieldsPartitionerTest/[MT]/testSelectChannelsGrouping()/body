{
  sd1.setInstance(streamRecord1);
  sd2.setInstance(streamRecord2);
  assertArrayEquals(fieldsPartitioner.selectChannels(sd1,1),fieldsPartitioner.selectChannels(sd2,1));
  assertArrayEquals(fieldsPartitioner.selectChannels(sd1,2),fieldsPartitioner.selectChannels(sd2,2));
  assertArrayEquals(fieldsPartitioner.selectChannels(sd1,1024),fieldsPartitioner.selectChannels(sd2,1024));
}
