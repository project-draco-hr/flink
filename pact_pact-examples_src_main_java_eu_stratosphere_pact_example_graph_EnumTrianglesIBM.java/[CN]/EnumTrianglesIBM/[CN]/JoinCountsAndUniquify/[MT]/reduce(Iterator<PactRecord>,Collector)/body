{
  PactRecord rec=null;
  int c1=0, c2=0;
  while (records.hasNext()) {
    rec=records.next();
    int f1=rec.getField(2,PactInteger.class).getValue();
    int f2=rec.getField(3,PactInteger.class).getValue();
    if (c1 == 0 && f1 != 0)     c1+=f1;
    if (c2 == 0 && f2 != 0)     c2+=f2;
  }
  count1.setValue(c1);
  count2.setValue(c2);
  rec.setField(2,count1);
  rec.setField(3,count2);
  out.collect(rec);
}
