{
  final ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  env.setDegreeOfParallelism(2);
  DataSet<Tuple5<Integer,Long,Integer,String,Long>> ds=CollectionDataSets.get5TupleDataSet(env);
  ds.map(new IdMapper()).setParallelism(2).sortPartition(4,Order.ASCENDING).sortPartition(2,Order.DESCENDING).mapPartition(new OrderCheckMapper<Tuple5<Integer,Long,Integer,String,Long>>(new Tuple5Checker())).distinct().writeAsText(resultPath);
  env.execute();
  expected="(true)\n";
}
