{
  final ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  env.setParallelism(3);
  DataSet<POJO> ds=CollectionDataSets.getMixedPojoDataSet(env);
  ds.map(new IdMapper()).setParallelism(1).sortPartition("nestedTupleWithCustom.f1.myString",Order.ASCENDING).sortPartition("number",Order.DESCENDING).mapPartition(new OrderCheckMapper<POJO>(new PojoChecker())).distinct().writeAsText(resultPath);
  env.execute();
  expected="(true)\n";
}
