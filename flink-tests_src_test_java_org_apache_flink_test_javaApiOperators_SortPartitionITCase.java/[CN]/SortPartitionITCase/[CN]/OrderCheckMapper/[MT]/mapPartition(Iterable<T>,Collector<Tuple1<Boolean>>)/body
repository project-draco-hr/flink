{
  Iterator<T> it=values.iterator();
  if (!it.hasNext()) {
    out.collect(new Tuple1<>(true));
    return;
  }
 else {
    T last=it.next();
    while (it.hasNext()) {
      T next=it.next();
      if (!checker.inOrder(last,next)) {
        out.collect(new Tuple1<>(false));
        return;
      }
      last=next;
    }
    out.collect(new Tuple1<>(true));
  }
}
