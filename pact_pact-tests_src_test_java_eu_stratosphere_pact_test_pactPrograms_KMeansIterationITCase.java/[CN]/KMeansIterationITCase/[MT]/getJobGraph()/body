{
  KMeansIteration kmi=new KMeansIteration();
  Plan plan=kmi.getPlan(config.getString("KMeansIterationTest#NoSubtasks","1"),getFilesystemProvider().getURIPrefix() + dataPath,getFilesystemProvider().getURIPrefix() + clusterPath,getFilesystemProvider().getURIPrefix() + resultPath);
  setParameterToCross(plan,config.getString("KMeansIterationTest#ForwardSide",null),PactCompiler.HINT_SHIP_STRATEGY_FORWARD);
  PactCompiler pc=new PactCompiler(new DataStatistics());
  OptimizedPlan op=pc.compile(plan);
  NepheleJobGraphGenerator jgg=new NepheleJobGraphGenerator();
  return jgg.compileJobGraph(op);
}
