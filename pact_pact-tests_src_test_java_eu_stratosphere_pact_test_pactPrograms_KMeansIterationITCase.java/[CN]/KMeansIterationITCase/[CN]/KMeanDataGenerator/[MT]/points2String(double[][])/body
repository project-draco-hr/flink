{
  StringBuilder sb=new StringBuilder();
  DecimalFormat df=new DecimalFormat("#.00");
  for (int i=0; i < points.length; i++) {
    sb.append(i);
    sb.append('|');
    for (int j=0; j < points[i].length; j++) {
      sb.append(df.format(points[i][j]));
      sb.append('|');
    }
    sb.append('\n');
  }
  return sb.toString();
}
