{
  try {
    CheckpointCoordinator coord=new CheckpointCoordinator(new JobID(),200000L,200000L,new ExecutionVertex[]{mock(ExecutionVertex.class)},new ExecutionVertex[]{mock(ExecutionVertex.class)},new ExecutionVertex[0],cl,new StandaloneCheckpointIDCounter(),new StandaloneCompletedCheckpointStore(1,cl),RecoveryMode.STANDALONE);
