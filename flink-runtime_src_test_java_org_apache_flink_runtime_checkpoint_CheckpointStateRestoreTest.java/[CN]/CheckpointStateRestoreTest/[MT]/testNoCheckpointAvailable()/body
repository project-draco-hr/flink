      coord.restoreLatestCheckpointedState(new HashMap<JobVertexID,ExecutionJobVertex>(),true,false);
      fail("this should throw an exception");
    }
 catch (    IllegalStateException e) {
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail(e.getMessage());
  }
}
