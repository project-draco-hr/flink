{
synchronized (lock) {
    try {
      target.trigger(timestamp);
    }
 catch (    Throwable t) {
      if (task.isRunning) {
        LOG.error("Caught exception while processing timer.",t);
      }
      if (task.asyncException == null) {
        task.asyncException=new TimerException(t);
      }
    }
  }
}
