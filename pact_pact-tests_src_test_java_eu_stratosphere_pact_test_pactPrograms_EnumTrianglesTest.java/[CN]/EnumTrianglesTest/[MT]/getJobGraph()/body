{
  EnumTriangles enumTriangles=new EnumTriangles();
  Plan plan=enumTriangles.getPlan(config.getString("EnumTrianglesTest#NoSubtasks","1"),getFilesystemProvider().getURIPrefix() + edgesPath,getFilesystemProvider().getURIPrefix() + getFilesystemProvider().getTempDirPath() + "/triangles.txt");
  PactCompiler pc=new PactCompiler();
  OptimizedPlan op=pc.compile(plan);
  JobGraphGenerator jgg=new JobGraphGenerator();
  return jgg.compileJobGraph(op);
}
