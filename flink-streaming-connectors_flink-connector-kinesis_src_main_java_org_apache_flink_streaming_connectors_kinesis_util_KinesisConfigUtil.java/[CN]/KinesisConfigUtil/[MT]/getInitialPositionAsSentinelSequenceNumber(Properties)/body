{
  InitialPosition initialPosition=InitialPosition.valueOf(config.getProperty(KinesisConfigConstants.CONFIG_STREAM_INIT_POSITION_TYPE,InitialPosition.LATEST.toString()));
switch (initialPosition) {
case TRIM_HORIZON:
    return SentinelSequenceNumber.SENTINEL_EARLIEST_SEQUENCE_NUM;
case LATEST:
default :
  return SentinelSequenceNumber.SENTINEL_LATEST_SEQUENCE_NUM;
}
}
