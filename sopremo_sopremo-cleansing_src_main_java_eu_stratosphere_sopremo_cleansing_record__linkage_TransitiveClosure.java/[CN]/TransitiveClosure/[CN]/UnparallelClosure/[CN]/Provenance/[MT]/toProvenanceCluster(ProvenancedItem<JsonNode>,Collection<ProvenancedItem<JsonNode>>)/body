{
  final ArrayNode[] provenanceCluster=new ArrayNode[this.sourceCount];
  for (int index=0; index < provenanceCluster.length; index++)   provenanceCluster[index]=new ArrayNode(null);
  provenanceCluster[row.getSourceIndex()].add(row.getNode());
  for (  ProvenancedItem<JsonNode> node : cluster)   provenanceCluster[node.getSourceIndex()].add(node.getNode());
  return JsonUtil.asArray(provenanceCluster);
}
