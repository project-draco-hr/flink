{
  registry.register(JavaFunctions.class);
  Assert.assertSame(SUM_NODE,registry.evaluate("sum",JsonUtils.asArray(JsonUtils.NODE_FACTORY.numberNode(1),JsonUtils.NODE_FACTORY.numberNode(2),JsonUtils.NODE_FACTORY.numberNode(3))));
  Assert.assertSame(SUM_NODE,registry.evaluate("sum",JsonUtils.asArray(JsonUtils.NODE_FACTORY.numberNode(1))));
}
