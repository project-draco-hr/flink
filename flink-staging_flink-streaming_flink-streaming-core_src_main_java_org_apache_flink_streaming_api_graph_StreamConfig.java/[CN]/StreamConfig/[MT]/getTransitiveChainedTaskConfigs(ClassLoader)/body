{
  try {
    Map<Integer,StreamConfig> confs=(Map<Integer,StreamConfig>)InstantiationUtil.readObjectFromConfig(this.config,CHAINED_TASK_CONFIG,cl);
    return confs == null ? new HashMap<Integer,StreamConfig>() : confs;
  }
 catch (  Exception e) {
    throw new StreamTaskException("Could not instantiate configuration.",e);
  }
}
