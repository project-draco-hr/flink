{
  WordCountArrayTuples wc=new WordCountArrayTuples();
  Plan plan=wc.getPlan(config.getString("WordCountTest#NoSubtasks","1"),getFilesystemProvider().getURIPrefix() + textPath,getFilesystemProvider().getURIPrefix() + resultPath);
  PactCompiler pc=new PactCompiler(new DataStatistics());
  OptimizedPlan op=pc.compile(plan);
  NepheleJobGraphGenerator jgg=new NepheleJobGraphGenerator();
  return jgg.compileJobGraph(op);
}
