{
  final MemorySegment s=buffer.dispose();
  this.returnBuffers.add(s);
  if (this.closed) {
synchronized (this.closeLock) {
      int num=this.requestsNotReturned.decrementAndGet();
      if (num == 0) {
        this.closeLock.notifyAll();
      }
    }
  }
 else {
    this.requestsNotReturned.decrementAndGet();
  }
}
