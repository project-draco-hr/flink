{
  double rankSum=0.0;
  double rank;
  Record rec=null;
  while (records.hasNext()) {
    rec=records.next();
    rank=rec.getField(1,DoubleValue.class).getValue();
    rankSum+=rank;
  }
  if (Math.abs(rankSum) > 0.00001) {
    newRank.setValue(rankSum);
    rec.setField(1,newRank);
    out.collect(rec);
  }
}
