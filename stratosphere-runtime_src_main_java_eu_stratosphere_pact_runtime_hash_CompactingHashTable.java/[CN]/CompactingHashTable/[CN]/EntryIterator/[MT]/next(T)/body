{
  if (done) {
    return null;
  }
 else   if (!cache.isEmpty()) {
    reuse=cache.remove(cache.size() - 1);
    return reuse;
  }
 else {
    while (!done && cache.isEmpty()) {
      done=!fillCache();
    }
    if (!done) {
      reuse=cache.remove(cache.size() - 1);
      return reuse;
    }
 else {
      return null;
    }
  }
}
