{
  int minDistance=Integer.MAX_VALUE;
  int minCount=0;
  T minConstructor=null;
  for (  final Entry<T,Integer> entry : candidateDistances.entrySet())   if (entry.getValue() < minDistance) {
    minDistance=entry.getValue();
    minConstructor=entry.getKey();
    minCount=1;
  }
 else   if (entry.getValue() == minDistance)   minCount++;
  return minCount == 1 ? minConstructor : null;
}
