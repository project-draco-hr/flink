{
  for (int i=0; i < 1000000; i++) {
    if (i % 50000 == 0) {
      time=System.currentTimeMillis();
      System.out.println("Source:\t" + i + "\t----Time: "+ (time - prevTime));
      prevTime=time;
    }
    if (i % 2 == 0) {
      lineValue.setValue("Gyula Marci");
    }
 else {
      lineValue.setValue("Gabor Gyula");
    }
    record.setRecord(lineValue);
    emit(record);
  }
}
