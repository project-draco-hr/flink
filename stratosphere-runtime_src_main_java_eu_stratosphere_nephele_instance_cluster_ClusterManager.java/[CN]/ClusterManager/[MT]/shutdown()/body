{
synchronized (this.lock) {
    if (this.shutdown) {
      return;
    }
    this.cleanupStaleMachines.cancel();
    Iterator<ClusterInstance> it=this.registeredHosts.values().iterator();
    while (it.hasNext()) {
      it.next().destroyProxies();
    }
    this.registeredHosts.clear();
    this.shutdown=true;
  }
}
