{
synchronized (this.lock) {
    for (    InstanceType i : availableInstanceTypes) {
      if (i.getNumberOfComputeUnits() >= minNumComputeUnits && i.getNumberOfCores() >= minNumCPUCores && i.getMemorySize() >= minMemorySize && i.getDiskCapacity() >= minDiskCapacity && i.getPricePerHour() <= maxPricePerHour) {
        return i;
      }
    }
  }
  return null;
}
