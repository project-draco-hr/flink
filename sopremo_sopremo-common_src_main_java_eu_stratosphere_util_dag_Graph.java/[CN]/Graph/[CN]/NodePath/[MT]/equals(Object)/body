{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (this.getClass() != obj.getClass())   return false;
  @SuppressWarnings("unchecked") final NodePath other=(NodePath)obj;
  return this.getOuterType().equals(other.getOuterType()) && this.index == other.index && (this.node == null ? other.node == null : this.node.equals(other.node)) && (this.parentPath == null ? other.parentPath == null : this.parentPath.equals(other.parentPath));
}
