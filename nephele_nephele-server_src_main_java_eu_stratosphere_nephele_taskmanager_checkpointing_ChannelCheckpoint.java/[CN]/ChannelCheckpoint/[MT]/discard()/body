{
  final Iterator<TransferEnvelope> it=this.queuedEnvelopes.iterator();
  while (it.hasNext()) {
    final TransferEnvelope transferEnvelope=it.next();
  }
  this.queuedEnvelopes.clear();
  final File file=new File(getFilename());
  if (file.exists()) {
    try {
      file.delete();
    }
 catch (    SecurityException e) {
      LOG.error(e);
    }
  }
}
