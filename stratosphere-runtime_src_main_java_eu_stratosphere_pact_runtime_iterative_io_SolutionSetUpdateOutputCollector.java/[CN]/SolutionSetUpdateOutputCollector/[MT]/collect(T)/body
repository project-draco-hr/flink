{
  try {
    MutableHashTable.HashBucketIterator<T,T> bucket=solutionSet.getMatchesFor(record);
    if (bucket.next(buildSideRecord)) {
      bucket.writeBack(record);
      if (delegate != null) {
        delegate.collect(record);
      }
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
