{
  ensureNumBuffersReturned(numPartitions);
  this.currentEnumerator=this.ioManager.createChannelEnumerator();
  this.partitionsBeingBuilt.clear();
  for (int i=0; i < numPartitions; i++) {
    HashPartition<BT,PT> p=new HashPartition<BT,PT>(this.buildSideSerializer,this.probeSideSerializer,i,recursionLevel,this.availableMemory.remove(this.availableMemory.size() - 1),this,this.segmentSize);
    this.partitionsBeingBuilt.add(p);
  }
}
