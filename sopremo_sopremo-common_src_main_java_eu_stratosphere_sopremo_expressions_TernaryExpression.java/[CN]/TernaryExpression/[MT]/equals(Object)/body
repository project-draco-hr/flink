{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  TernaryExpression other=(TernaryExpression)obj;
  if (ifClause == null) {
    if (other.ifClause != null)     return false;
  }
 else   if (!ifClause.equals(other.ifClause))   return false;
  if (ifExpression == null) {
    if (other.ifExpression != null)     return false;
  }
 else   if (!ifExpression.equals(other.ifExpression))   return false;
  if (thenExpression == null) {
    if (other.thenExpression != null)     return false;
  }
 else   if (!thenExpression.equals(other.thenExpression))   return false;
  return true;
}
