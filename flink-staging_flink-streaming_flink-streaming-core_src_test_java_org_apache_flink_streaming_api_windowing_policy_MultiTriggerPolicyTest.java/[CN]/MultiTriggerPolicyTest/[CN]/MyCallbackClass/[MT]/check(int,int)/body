{
  int totalTime=0;
  while (totalTime <= timeout) {
    boolean result=true;
    for (    int id : ids) {
      if (!received.contains(id)) {
        result=false;
      }
    }
    if (result) {
      return true;
    }
 else {
      try {
        Thread.sleep(1000);
        totalTime+=1000;
      }
 catch (      InterruptedException e) {
      }
    }
  }
  return false;
}
