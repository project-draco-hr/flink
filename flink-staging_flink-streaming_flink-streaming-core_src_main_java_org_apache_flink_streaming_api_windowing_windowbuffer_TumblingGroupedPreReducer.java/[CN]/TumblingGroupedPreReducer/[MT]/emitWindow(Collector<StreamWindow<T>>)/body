{
  if (!reducedValues.isEmpty()) {
    StreamWindow<T> currentWindow=createEmptyWindow();
    currentWindow.addAll(reducedValues.values());
    collector.collect(currentWindow);
  }
 else   if (emitEmpty) {
    collector.collect(createEmptyWindow());
  }
  if (evict) {
    reducedValues.clear();
  }
}
