{
  int expected=15;
  String jsonText="{\"key\":" + expected + "}";
  String searchedField="key";
  try {
    JSONParser parser=new JSONParser(jsonText);
    JSONObject jo=parser.parse(searchedField);
    assertEquals(expected,jo.getInt("retValue"));
  }
 catch (  JSONException e) {
    fail();
  }
}
