{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment(1);
  DataStream<Tuple1<String>> dataStream1=env.addSource(new MyZeroMQSource("tcp://localhost:5555","pushpull")).print();
  DataStream<Tuple1<String>> dataStream2=env.fromElements("one","two","three","four","five","q").addSink(new MyZeroMQSink("tcp://*:5555","pushpull"));
  DataStream<Tuple1<String>> dataStream3=env.fromElements("apple","Hello","World","Hello","World").addSink(new MyZeroMQSink("tcp://*:5563","pubsub"));
  DataStream<Tuple1<String>> dataStream4=env.addSource(new MyZeroMQSource1("tcp://localhost:5563","pubsub","H")).addSink(new PrintSink1());
  DataStream<Tuple1<String>> dataStream5=env.addSource(new MyZeroMQSource2("tcp://localhost:5563","pubsub","W")).addSink(new PrintSink2());
  env.execute();
}
