{
  for (  final OptimizerNode node : optimizedPlan.getAllNodes()) {
    for (    final List<PactConnection> listPactConnection : node.getIncomingConnections())     for (    final PactConnection pactConnection : listPactConnection)     pactConnection.setShipStrategy(ShipStrategy.FORWARD);
    for (    final PactConnection pactConnection : node.getOutgoingConnections())     pactConnection.setShipStrategy(ShipStrategy.FORWARD);
  }
}
