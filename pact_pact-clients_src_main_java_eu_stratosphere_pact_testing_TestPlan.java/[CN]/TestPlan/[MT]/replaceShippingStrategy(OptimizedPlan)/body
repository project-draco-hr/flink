{
  for (  final OptimizerNode node : optimizedPlan.getAllNodes()) {
    for (    final PactConnection pactConnection : node.getIncomingConnections())     pactConnection.setShipStrategy(ShipStrategyType.FORWARD);
    for (    final PactConnection pactConnection : node.getOutgoingConnections())     pactConnection.setShipStrategy(ShipStrategyType.FORWARD);
  }
}
