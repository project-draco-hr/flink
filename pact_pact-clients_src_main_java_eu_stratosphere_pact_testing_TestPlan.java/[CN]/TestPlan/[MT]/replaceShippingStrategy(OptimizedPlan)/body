{
  for (  final OptimizerNode node : optimizedPlan.getAllNodes()) {
    for (    final PactConnection pactConnection : node.getIncomingConnections())     pactConnection.setShipStrategy(new ForwardSS());
    for (    final PactConnection pactConnection : node.getOutConns())     pactConnection.setShipStrategy(new ForwardSS());
  }
}
