{
  for (  final OptimizerNode node : optimizedPlan.getAllNodes()) {
    for (    final List<PactConnection> pactConnections : node.getIncomingConnections())     for (    PactConnection pactConnection : pactConnections)     pactConnection.setShipStrategy(ShipStrategy.FORWARD);
    for (    final PactConnection pactConnection : node.getOutgoingConnections())     pactConnection.setShipStrategy(ShipStrategy.FORWARD);
  }
}
