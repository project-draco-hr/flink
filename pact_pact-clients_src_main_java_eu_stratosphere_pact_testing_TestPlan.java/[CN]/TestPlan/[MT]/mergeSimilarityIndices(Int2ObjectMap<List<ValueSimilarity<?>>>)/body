{
  similarityMap=new Int2ObjectOpenHashMap<List<ValueSimilarity<?>>>(similarityMap);
  for (  Entry<List<ValueSimilarity<?>>> entry : this.getFuzzySimilarityIndexMap(ALL_SINKS).int2ObjectEntrySet())   if (!entry.getValue().isEmpty()) {
    List<ValueSimilarity<?>> existingList=similarityMap.get(entry.getKey());
    if (existingList == null)     similarityMap.put(entry.getKey(),entry.getValue());
 else     existingList.addAll(entry.getValue());
  }
  return similarityMap;
}
