{
  super(combinable ? new TupleUnwrappingFlatCombinableGroupReducer<IN,OUT,K1,K2>((RichGroupReduceFunction<IN,OUT>)udf) : new TupleUnwrappingNonCombinableGroupReducer<IN,OUT,K1,K2>(udf),new UnaryOperatorInformation<Tuple3<K1,K2,IN>,OUT>(typeInfoWithKey,outType),groupingKey.computeLogicalKeyPositions(),name);
  super.setCombinable(combinable);
}
