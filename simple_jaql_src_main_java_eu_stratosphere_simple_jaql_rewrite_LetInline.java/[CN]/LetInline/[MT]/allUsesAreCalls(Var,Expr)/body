{
  ExprWalker walker=engine.walker;
  walker.reset(expr);
  Expr e;
  while ((e=walker.next()) != null) {
    if (e instanceof VarExpr) {
      VarExpr ve=(VarExpr)e;
      if (ve.var() == var) {
        if (ve.getChildSlot() != 0 || !(ve.parent() instanceof FunctionCallExpr)) {
          return false;
        }
      }
    }
  }
  return true;
}
