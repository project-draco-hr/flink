{
  super();
  this.remote=remote;
  this.port=port;
  this.rmiId=rmiId;
  if (this.remote == null) {
    throw new IllegalStateException(String.format("No remote configured for %s.",this));
  }
  if (this.rmiId == null) {
    throw new IllegalStateException(String.format("No registry ID configured for %s.",this));
  }
}
