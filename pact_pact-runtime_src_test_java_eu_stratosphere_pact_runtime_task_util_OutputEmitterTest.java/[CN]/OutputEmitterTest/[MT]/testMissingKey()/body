{
  @SuppressWarnings("unchecked") OutputEmitter oe1=new OutputEmitter(ShipStrategy.PARTITION_HASH,new JobID(),new int[]{1},new Class[]{PactInteger.class});
  PactRecord rec=new PactRecord(0);
  rec.setField(0,new PactInteger(1));
  boolean correctException=false;
  try {
    oe1.selectChannels(rec,100);
  }
 catch (  RuntimeException re) {
    if (re.getMessage().equals("Key field 1 is of our bounds of record."))     correctException=true;
  }
  assertTrue(correctException);
}
