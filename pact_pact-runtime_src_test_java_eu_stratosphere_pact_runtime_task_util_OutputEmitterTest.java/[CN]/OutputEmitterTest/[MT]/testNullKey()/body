{
  @SuppressWarnings("unchecked") OutputEmitter oe1=new OutputEmitter(ShipStrategy.PARTITION_HASH,new int[]{0},new Class[]{PactInteger.class});
  PactRecord rec=new PactRecord(2);
  rec.setField(1,new PactInteger(1));
  boolean correctException=false;
  try {
    oe1.selectChannels(rec,100);
  }
 catch (  RuntimeException re) {
    if (re.getMessage().equals("Key field 0 is null."))     correctException=true;
  }
  assertTrue(correctException);
}
