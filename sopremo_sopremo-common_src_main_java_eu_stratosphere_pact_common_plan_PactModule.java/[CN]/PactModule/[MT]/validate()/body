{
  for (int index=0; index < this.outputStubs.length; index++)   if (this.outputStubs[index].getInput() == null)   throw new IllegalStateException(String.format("%d. output is not connected",index));
  final Collection<Contract> visitedContracts=this.getAllContracts();
  for (int index=0; index < this.inputStubs.length; index++)   if (!visitedContracts.contains(this.inputStubs[index]))   throw new IllegalStateException(String.format("%d. input is not connected",index));
}
