{
  super(name,new DataSourceContract[numberOfInputs],new DataSinkContract[numberOfOutputs],ContractNavigator.INSTANCE);
  for (int index=0; index < this.inputNodes.length; index++)   this.inputNodes[index]=new DataSourceContract<PactJsonObject.Key,PactJsonObject>(JsonInputFormat.class,String.format("%s %d",name,index));
  for (int index=0; index < this.outputNodes.length; index++)   this.outputNodes[index]=new DataSinkContract<PactJsonObject.Key,PactJsonObject>(JsonOutputFormat.class,String.format("%s %d",name,index));
}
