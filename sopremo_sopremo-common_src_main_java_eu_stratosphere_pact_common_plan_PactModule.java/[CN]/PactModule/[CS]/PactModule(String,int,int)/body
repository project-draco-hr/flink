{
  super(name,numberOfInputs,numberOfOutputs,ContractNavigator.INSTANCE);
  for (int index=0; index < numberOfInputs; index++)   setInput(index,new FileDataSource(SequentialInputFormat.class,String.format("%s %d",name,index)));
  for (int index=0; index < numberOfOutputs; index++)   setOutput(index,new FileDataSink(SequentialOutputFormat.class,String.format("%s %d",name,index)));
}
