{
  this.inputStubs=new DataSourceContract[numberOfInputs];
  for (int index=0; index < this.inputStubs.length; index++)   this.inputStubs[index]=new DataSourceContract<PactNull,PactJsonObject>(JsonInputFormat.class,String.valueOf(index));
  this.outputStubs=new DataSinkContract[numberOfOutputs];
  for (int index=0; index < this.outputStubs.length; index++)   this.outputStubs[index]=new DataSinkContract<PactNull,PactJsonObject>(JsonOutputFormat.class,String.valueOf(index));
}
