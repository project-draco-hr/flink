{
  final List<Operator<?>.Output> inputs=this.getInputs();
  final SopremoModule module=new SopremoModule(this.getName(),inputs.size(),1);
  Operator<?> leftInput=module.getInput(0);
  for (int index=1; index < inputs.size(); index++)   leftInput=new TwoInputUnionAll().withInputs(leftInput,module.getInput(index));
  module.getOutput(0).setInput(0,leftInput);
  return module;
}
