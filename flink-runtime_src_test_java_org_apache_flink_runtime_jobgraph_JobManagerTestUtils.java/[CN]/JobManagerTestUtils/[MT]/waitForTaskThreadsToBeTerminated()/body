{
  Thread[] threads=new Thread[Thread.activeCount()];
  Thread.enumerate(threads);
  for (  Thread t : threads) {
    if (t == null) {
      continue;
    }
    ThreadGroup tg=t.getThreadGroup();
    if (tg != null && tg.getName() != null && tg.getName().equals("Task Threads")) {
      t.join();
    }
  }
}
