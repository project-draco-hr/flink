{
  TPCHQueryAsterix tpchBench=new TPCHQueryAsterix();
  Plan plan=tpchBench.getPlan(config.getString("TPCHQueryAsterix#NoSubtasks","1"),getFilesystemProvider().getURIPrefix() + ordersPath,getFilesystemProvider().getURIPrefix() + custPath,getFilesystemProvider().getURIPrefix() + resultPath);
  PactCompiler pc=new PactCompiler();
  OptimizedPlan op=pc.compile(plan);
  NepheleJobGraphGenerator jgg=new NepheleJobGraphGenerator();
  return jgg.compileJobGraph(op);
}
