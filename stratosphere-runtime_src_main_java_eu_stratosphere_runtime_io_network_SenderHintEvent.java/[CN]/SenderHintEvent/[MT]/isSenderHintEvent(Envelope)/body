{
  if (envelope.getSequenceNumber() != SENDER_HINT_SEQUENCE_NUMBER) {
    return false;
  }
  if (envelope.getBuffer() != null) {
    return false;
  }
  List<? extends AbstractEvent> events=envelope.deserializeEvents();
  if (events.size() != 1) {
    return false;
  }
  if (!(events.get(0) instanceof SenderHintEvent)) {
    return false;
  }
  return true;
}
