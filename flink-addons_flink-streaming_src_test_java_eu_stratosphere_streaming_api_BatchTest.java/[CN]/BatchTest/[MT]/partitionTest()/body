{
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple1<String>> dataStream=env.addSource(new MySource(),SOURCE_PARALELISM).flatMap(new MyMap(),PARALLELISM).batch(4).partitionBy(0).addSink(new MyPartitionSink(),SINK_PARALELISM);
  env.execute(SINK_PARALELISM);
  assertTrue(partitionCorrect);
}
