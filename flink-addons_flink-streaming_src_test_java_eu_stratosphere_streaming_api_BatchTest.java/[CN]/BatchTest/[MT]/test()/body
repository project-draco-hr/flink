{
  StreamExecutionEnvironment context=new StreamExecutionEnvironment();
  DataStream<Tuple1<String>> dataStream=context.addSource(new MySource(),SOURCE_PARALELISM).flatMap(new MyMap(),PARALELISM).batch(4).flatMap(new MyMap(),PARALELISM).batch(2).flatMap(new MyMap(),PARALELISM).batch(5).flatMap(new MyMap(),PARALELISM).batch(4).addSink(new MySink());
  context.execute();
  assertEquals(20,count);
}
