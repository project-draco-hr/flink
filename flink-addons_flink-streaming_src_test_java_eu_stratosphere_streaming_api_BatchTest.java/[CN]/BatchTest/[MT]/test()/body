{
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple1<String>> dataStream=env.addSource(new MySource(),SOURCE_PARALELISM).flatMap(new MyMap(),PARALELISM).batch(4).flatMap(new MyMap(),PARALELISM).batch(2).flatMap(new MyMap(),PARALELISM).batch(5).flatMap(new MyMap(),PARALELISM).batch(4).addSink(new MySink());
  env.execute();
  assertEquals(20,count);
}
