{
  int sum=0;
  PactRecord record=new PactRecord();
  while (records.hasNext()) {
    record=records.next();
    record.getField(1,combineValue);
    sum+=Integer.parseInt(combineValue.toString());
    LOG.debug("Processed: [" + record.getField(0,PactString.class).toString() + ","+ combineValue.toString()+ "]");
  }
  combineValue.setValue(sum + "");
  record.setField(1,combineValue);
  out.collect(record);
}
