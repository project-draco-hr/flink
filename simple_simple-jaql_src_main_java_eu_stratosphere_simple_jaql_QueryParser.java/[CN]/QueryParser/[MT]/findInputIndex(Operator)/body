{
  if (this.operatorInputs.isEmpty())   return -1;
  Iterator<List<Operator>> iterator=this.operatorInputs.descendingIterator();
  while (iterator.hasNext()) {
    List<Operator> inputs=iterator.next();
    for (int index=0; index < inputs.size(); index++)     if (inputs.get(index) == input)     return index;
  }
  return -1;
}
