{
  List<Expr> children=new ArrayList<Expr>();
  for (int index=0; index < node.numChildren(); index++) {
    Expr child=node.child(index);
    if (!(child instanceof ConstExpr) && !(child instanceof VarExpr) && !(child instanceof PathReturn)&& !(child instanceof NameValueBinding)&& !(child instanceof PathExpr)&& !(child instanceof RecordExpr)&& !(child instanceof CompareExpr)&& !(child instanceof CopyField)&& !(child instanceof FixedRecordExpr)&& !(child instanceof CopyRecord)&& !(child instanceof PathFieldValue))     children.add(node.child(index));
  }
  return children;
}
