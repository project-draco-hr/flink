{
  Random rnd=new Random(874597969123412338L);
  int rndInt=rnd.nextInt();
  if (rndInt < 0) {
    rndInt=-rndInt;
  }
  if (rndInt == Integer.MAX_VALUE) {
    rndInt-=3;
  }
  if (rndInt <= 2) {
    rndInt+=3;
  }
  return new Integer[]{new Integer(Integer.MIN_VALUE),new Integer(-rndInt),new Integer(-1),new Integer(0),new Integer(1),new Integer(2),new Integer(rndInt),new Integer(Integer.MAX_VALUE)};
}
