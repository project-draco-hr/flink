{
  for (  String s : IN.split("\n")) {
    String[] fields=s.split(" ");
    input.add(new Tuple2<String,String>(fields[0],fields[1]));
  }
  for (  String s : RESULT.split("\n")) {
    String[] fields=s.split(" ");
    expected.add(new Tuple2<String,Integer>(fields[0],Integer.parseInt(fields[1])));
  }
}
