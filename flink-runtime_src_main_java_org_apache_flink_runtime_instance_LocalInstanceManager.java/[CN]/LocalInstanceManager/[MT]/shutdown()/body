{
  try {
    for (    TaskManager taskManager : taskManagers) {
      try {
        taskManager.shutdown();
      }
 catch (      Throwable t) {
        LogFactory.getLog(LocalInstanceManager.class).error("Error shutting down local embedded TaskManager.",t);
      }
    }
  }
  finally {
    this.taskManagers.clear();
    super.shutdown();
  }
}
