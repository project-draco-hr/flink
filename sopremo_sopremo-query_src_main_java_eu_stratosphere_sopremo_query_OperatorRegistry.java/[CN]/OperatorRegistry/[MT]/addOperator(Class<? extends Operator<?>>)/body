{
  String name;
  final Name nameAnnotation=ReflectUtil.getAnnotation(operatorClass,Name.class);
  if (nameAnnotation != null)   name=this.operatorNameChooser.choose(nameAnnotation.noun(),nameAnnotation.verb(),nameAnnotation.adjective(),nameAnnotation.preposition());
 else   name=operatorClass.getSimpleName();
  if (this.operators.containsKey(name))   throw new IllegalStateException("Duplicate operator " + name);
  this.operators.put(name,new OperatorInfo(operatorClass,name,this.propertyNameChooser));
}
