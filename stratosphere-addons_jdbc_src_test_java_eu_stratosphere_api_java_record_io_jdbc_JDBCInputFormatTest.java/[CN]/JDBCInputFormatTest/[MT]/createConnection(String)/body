{
  try {
    Class.forName("org.apache.derby.jdbc.EmbeddedDriver");
    conn=DriverManager.getConnection(dbURL);
    createTable();
    insertDataToSQLTables();
    conn.close();
  }
 catch (  ClassNotFoundException e) {
    e.printStackTrace();
    Assert.fail();
  }
catch (  SQLException e) {
    e.printStackTrace();
    Assert.fail();
  }
}
