{
  int[] ids=this.ids;
  Arrays.sort(ids,0,this.len);
  int k=0;
  for (int curr=-1, i=0; i < this.len; i++) {
    int val=ids[i];
    if (val != curr) {
      curr=val;
      ids[k]=ids[i];
      k++;
    }
 else {
      ids[k]=ids[i];
    }
  }
  if (ids.length >= (k << 1)) {
    int[] na=new int[k];
    System.arraycopy(ids,0,na,0,k);
    this.ids=na;
  }
  this.len=k;
  this.degrees=new int[this.len + 10];
}
