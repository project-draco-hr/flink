{
switch (this.localStrategy) {
case SORT_BOTH_MERGE:
    return 2;
case SORT_FIRST_MERGE:
  return 1;
case SORT_SECOND_MERGE:
return 1;
case MERGE:
return 0;
case HYBRIDHASH_FIRST:
return 1;
case HYBRIDHASH_SECOND:
return 1;
case MMHASH_FIRST:
return 1;
case MMHASH_SECOND:
return 1;
case SORT_SELF_NESTEDLOOP:
return 2;
case SELF_NESTEDLOOP:
return 1;
default :
return 0;
}
}
