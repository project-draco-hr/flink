{
  out.writeInt(this.returnCode.ordinal());
  if (this.remoteTarget != null) {
    out.writeBoolean(true);
    this.remoteTarget.write(out);
  }
 else {
    out.writeBoolean(false);
  }
  if (this.localTarget != null) {
    out.writeBoolean(true);
    this.localTarget.write(out);
  }
 else {
    out.writeBoolean(false);
  }
}
