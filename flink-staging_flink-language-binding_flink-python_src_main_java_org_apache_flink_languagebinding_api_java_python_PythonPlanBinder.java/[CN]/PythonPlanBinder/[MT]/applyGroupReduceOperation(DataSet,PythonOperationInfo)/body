{
  if (info.combine) {
    return op1.reduceGroup(new PythonCombineIdentity(info.setID,info.combineOperator,info.meta)).setCombinable(true).name("PythonCombine").mapPartition(new PythonMapPartition(info.setID * -1,info.operator,info.types,info.meta)).name(info.name);
  }
 else {
    return op1.reduceGroup(new PythonCombineIdentity()).setCombinable(false).name("PythonGroupReducePreStep").mapPartition(new PythonMapPartition(info.setID * -1,info.operator,info.types,info.meta)).name(info.name);
  }
}
