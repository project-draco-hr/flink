{
  org.apache.flink.api.java.hadoop.mapreduce.HadoopInputFormat<K,V> hadoopInputFormat=new org.apache.flink.api.java.hadoop.mapreduce.HadoopInputFormat<K,V>(mapredInputFormat,key,value,job);
