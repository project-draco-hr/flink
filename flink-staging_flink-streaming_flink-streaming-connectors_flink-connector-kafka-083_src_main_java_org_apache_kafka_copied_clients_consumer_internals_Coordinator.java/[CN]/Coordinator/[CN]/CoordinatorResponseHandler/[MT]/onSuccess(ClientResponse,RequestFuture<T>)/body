{
  this.response=clientResponse;
  if (clientResponse.wasDisconnected()) {
    int correlation=response.request().request().header().correlationId();
    log.debug("Cancelled request {} with correlation id {} due to coordinator {} being disconnected",response.request(),correlation,response.request().request().destination());
    coordinatorDead();
    future.raise(new DisconnectException());
    return;
  }
  R response=parse(clientResponse);
  handle(response,future);
}
