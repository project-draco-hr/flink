{
  PactRecord toUse=this.copier;
  val1.copyTo(toUse);
  matchFunction.match(toUse,firstValN,collector);
  boolean more=true;
  do {
    PactRecord nRec=valsN.next();
    if (valsN.hasNext()) {
      val1.copyTo(toUse);
    }
 else {
      toUse=val1;
      more=false;
    }
    matchFunction.match(toUse,nRec,collector);
  }
 while (more);
}
