{
  return new Iterator<V>(){
    boolean first=true;
    boolean last=false;
    @Override public boolean hasNext(){
      if (first) {
        first=false;
        return true;
      }
 else       if (last) {
        return false;
      }
 else {
        if (!iterator.hasNext()) {
          return false;
        }
        KeyValuePair<K,V> prev=next;
        next=iterator.next();
        if (next.getKey().compareTo(prev.getKey()) == 0) {
          return true;
        }
 else {
          last=true;
          nextKey=true;
          return false;
        }
      }
    }
    @Override public V next(){
      return next.getValue();
    }
    @Override public void remove(){
    }
  }
;
}
