{
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple4<Boolean,Integer,Long,Integer>> querySource=env.addSource(new QuerySource(),SOURCE_PARALELISM);
  DataStream<Tuple1<String>> stream=env.addSource(new InfoSource(),SOURCE_PARALELISM).connectWith(querySource).partitionBy(1).flatMap(new CellTask(),PARALELISM);
  stream.print();
  env.execute();
}
