{
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple4<Boolean,Integer,Long,Integer>> querySource=env.addSource(new QuerySource());
  DataStream<Tuple1<String>> stream=env.addSource(new InfoSource()).connectWith(querySource).partitionBy(1).flatMap(new CellTask()).addDummySink();
  env.execute();
}
