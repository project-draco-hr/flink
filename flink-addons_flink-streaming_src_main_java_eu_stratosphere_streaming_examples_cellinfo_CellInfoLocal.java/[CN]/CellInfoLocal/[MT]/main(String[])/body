{
  StreamExecutionEnvironment context=new StreamExecutionEnvironment();
  DataStream<Tuple4<Boolean,Integer,Long,Integer>> querySource=context.addSource(new QuerySource());
  DataStream<Tuple1<String>> stream=context.addSource(new InfoSource()).connectWith(querySource).partitionBy(1).flatMap(new CellTask()).addDummySink();
  context.execute();
}
