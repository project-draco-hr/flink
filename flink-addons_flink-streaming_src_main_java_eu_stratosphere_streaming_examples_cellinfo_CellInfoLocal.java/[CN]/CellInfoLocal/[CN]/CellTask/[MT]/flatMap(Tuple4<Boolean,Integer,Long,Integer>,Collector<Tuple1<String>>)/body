{
  cellID=value.f1;
  timeStamp=value.f2;
  if (value.f0) {
    lastMillis=value.f3;
    outTuple.f0="QUERY:\t" + cellID + ": "+ engine.get(timeStamp,lastMillis,cellID);
    out.collect(outTuple);
  }
 else {
    engine.put(cellID,timeStamp);
    outTuple.f0="INFO:\t" + cellID + " @ "+ timeStamp;
    out.collect(outTuple);
  }
}
