{
  buffer.setLength(0);
  buffer.append(id);
  buffer.append(DELIMITER);
  for (int j=0; j < coordinates.length; j++) {
    buffer.append(FORMAT.format(coordinates[j]));
    if (j < coordinates.length - 1) {
      buffer.append(DELIMITER);
    }
  }
  out.write(buffer.toString());
  out.newLine();
}
