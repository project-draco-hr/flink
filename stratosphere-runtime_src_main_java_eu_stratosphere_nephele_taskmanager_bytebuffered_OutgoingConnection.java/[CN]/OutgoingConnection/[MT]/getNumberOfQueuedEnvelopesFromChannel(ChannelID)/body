{
synchronized (this.queuedEnvelopes) {
    int number=0;
    final Iterator<TransferEnvelope> it=this.queuedEnvelopes.iterator();
    while (it.hasNext()) {
      final TransferEnvelope te=it.next();
      if (sourceChannelID.equals(te.getSource())) {
        number++;
      }
    }
    return number;
  }
}
