{
  final long edge=pair.getValue().getValue();
  final int e1=pair.getKey().getValue();
  final int e2=(int)(edge >>> 32);
  final int e3=(int)(edge & 0xffffffff);
  this.line.setLength(0);
  this.line.append(e1);
  this.line.append(',');
  this.line.append(e2);
  this.line.append(',');
  this.line.append(e3);
  this.line.append('\n');
  return line.toString().getBytes();
}
