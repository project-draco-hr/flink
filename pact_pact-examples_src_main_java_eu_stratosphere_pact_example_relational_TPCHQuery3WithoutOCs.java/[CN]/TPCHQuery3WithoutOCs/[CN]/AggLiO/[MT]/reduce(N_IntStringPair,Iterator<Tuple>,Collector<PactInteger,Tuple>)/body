{
  long partExtendedPriceSum=0;
  Tuple value=null;
  while (values.hasNext()) {
    value=values.next();
    partExtendedPriceSum+=((long)Double.parseDouble(value.getStringValueAt(2)));
  }
  if (value != null) {
    value.project(3);
    value.addAttribute(partExtendedPriceSum + "");
    out.collect(oKeyShipPrio.getFirst(),value);
  }
}
