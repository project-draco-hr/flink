{
  StringBuilder builder=new StringBuilder();
  builder.append("LatencyPath[");
  ManagementVertex previous=null;
  for (  ManagementVertex vertex : pathVertices) {
    if (previous != null) {
      builder.append("->");
    }
    builder.append(vertex);
    previous=vertex;
  }
  builder.append("]");
  return builder.toString();
}
