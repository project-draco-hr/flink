{
  final List<Field> fields=ReflectUtil.getFields(javaConstants,null,Modifier.STATIC | Modifier.FINAL | Modifier.PUBLIC);
  for (  final Field field : fields)   if (EvaluationExpression.class.isAssignableFrom(field.getType()))   try {
    this.put(field.getName(),(EvaluationExpression)field.get(null));
  }
 catch (  Exception e) {
    SopremoUtil.LOG.warn(String.format("Cannot access constant %s: %s",field,e));
  }
  if (ConstantRegistryCallback.class.isAssignableFrom(javaConstants))   ((ConstantRegistryCallback)ReflectUtil.newInstance(javaConstants)).registerConstants(this);
}
