{
  Map<String,Object> newConfigs=new HashMap<String,Object>();
  newConfigs.putAll(configs);
  if (keyDeserializer != null)   newConfigs.put(KEY_DESERIALIZER_CLASS_CONFIG,keyDeserializer.getClass());
  if (keyDeserializer != null)   newConfigs.put(VALUE_DESERIALIZER_CLASS_CONFIG,valueDeserializer.getClass());
  return newConfigs;
}
