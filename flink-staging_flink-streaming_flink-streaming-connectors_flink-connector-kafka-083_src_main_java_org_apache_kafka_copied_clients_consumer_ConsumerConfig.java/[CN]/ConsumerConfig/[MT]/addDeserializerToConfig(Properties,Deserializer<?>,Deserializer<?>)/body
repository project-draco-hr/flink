{
  Properties newProperties=new Properties();
  newProperties.putAll(properties);
  if (keyDeserializer != null)   newProperties.put(KEY_DESERIALIZER_CLASS_CONFIG,keyDeserializer.getClass().getName());
  if (keyDeserializer != null)   newProperties.put(VALUE_DESERIALIZER_CLASS_CONFIG,valueDeserializer.getClass().getName());
  return newProperties;
}
