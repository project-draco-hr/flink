{
  BufferOrEvent bufferOrEvent=getNonProcessed();
  if (bufferOrEvent != null) {
    return bufferOrEvent;
  }
 else {
    while (true) {
      bufferOrEvent=inputGate.getNextBufferOrEvent();
      if (isBlocked(bufferOrEvent.getChannelIndex())) {
        store(bufferOrEvent);
      }
 else {
        return bufferOrEvent;
      }
    }
  }
}
