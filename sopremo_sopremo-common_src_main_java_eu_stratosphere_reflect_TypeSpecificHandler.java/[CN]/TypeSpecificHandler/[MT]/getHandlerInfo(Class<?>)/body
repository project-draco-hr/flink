{
  HandlerInfo<InputType,OutputBase,TypeHandler<InputType,OutputBase>> handlerInfo=this.handlerInfos.get(clazz);
  if (handlerInfo == null && clazz.getSuperclass() != null) {
    handlerInfo=this.getHandlerInfo(clazz.getSuperclass());
    if (handlerInfo != null)     this.handlerInfos.put((Class<? extends InputType>)clazz,handlerInfo);
  }
  return handlerInfo;
}
