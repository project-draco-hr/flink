{
  HandlerInfo<InputType,OutputBase,Handler> handlerInfo=new HandlerInfo<InputType,OutputBase,Handler>(handler);
  for (  Class<? extends Type> type : types)   this.handlerInfos.put(type,handlerInfo);
  if (this.outputBase == null) {
    BoundType bounds=ReflectUtil.getBindingOfSuperclass(handler.getClass(),TypeHandler.class);
    this.outputBase=bounds.getParameters()[1].getType();
  }
}
