{
  DataInputStream dis=new DataInputStream(new ByteArrayInputStream(data.array()));
  int id=dis.readInt();
  if (LOG.isDebugEnabled())   LOG.debug(" got #" + id);
  IOReadableWritable invocation=newInstance(invocationClass);
  invocation.read(dis);
  Call call=new Call(id,invocation,this);
  callQueue.put(call);
}
