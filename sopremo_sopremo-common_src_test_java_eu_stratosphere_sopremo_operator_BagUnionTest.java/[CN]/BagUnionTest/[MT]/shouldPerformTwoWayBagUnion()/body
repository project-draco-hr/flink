{
  SopremoTestPlan sopremoPlan=new SopremoTestPlan(2,1);
  BagUnion union=new BagUnion(sopremoPlan.getInputOperators(0,2));
  sopremoPlan.getOutputOperator(0).setInputs(union);
  sopremoPlan.getInput(0).add(createJsonValue(1)).add(createJsonValue(2));
  sopremoPlan.getInput(1).add(createJsonValue(3)).add(createJsonValue(4)).add(createJsonValue(5));
  sopremoPlan.getExpectedOutput(0).add(createJsonValue(1)).add(createJsonValue(2)).add(createJsonValue(3)).add(createJsonValue(4)).add(createJsonValue(5));
  sopremoPlan.run();
}
