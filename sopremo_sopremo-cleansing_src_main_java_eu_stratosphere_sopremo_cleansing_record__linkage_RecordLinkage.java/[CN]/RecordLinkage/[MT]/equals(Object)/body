{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (this.getClass() != obj.getClass())   return false;
  final RecordLinkage other=(RecordLinkage)obj;
  return this.linkageMode == other.linkageMode && this.threshold == other.threshold && this.algorithm.equals(other.algorithm) && this.similarityExpression.equals(other.similarityExpression) && this.recordLinkageInputs.equals(other.recordLinkageInputs);
}
