{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (this.getClass() != obj.getClass())   return false;
  final RecordLinkage other=(RecordLinkage)obj;
  return this.retainNonDuplicates == other.retainNonDuplicates && this.transitive == other.transitive && this.algorithm.equals(other.algorithm) && this.similarityCondition.equals(other.similarityCondition) && this.recordLinkageInputs.equals(other.recordLinkageInputs);
}
