{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (this.getClass() != obj.getClass())   return false;
  final RecordLinkage<?> other=(RecordLinkage<?>)obj;
  return this.linkageMode == other.linkageMode && this.algorithm.equals(other.algorithm) && this.duplicateCondition.equals(other.duplicateCondition) && this.recordLinkageInputs.equals(other.recordLinkageInputs);
}
