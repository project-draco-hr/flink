{
  if (!parseParameters(args)) {
    return;
  }
  StreamExecutionEnvironment env=StreamExecutionEnvironment.getExecutionEnvironment();
  DataStream<String> stream=env.addSource(new BasicSource()).groupBy(0).window(Count.of(2)).every(Count.of(3),Count.of(5)).reduceGroup(new Concat());
  if (fileOutput) {
    stream.writeAsText(outputPath,1);
  }
 else {
    stream.print();
  }
  env.execute("Multiple Policies Example");
}
