{
  IN_RESTORE_LATCH.trigger();
  try {
synchronized (this) {
      wait();
    }
  }
 catch (  InterruptedException e) {
    while (!closed) {
      try {
synchronized (this) {
          wait();
        }
      }
 catch (      InterruptedException ignored) {
      }
    }
  }
  return new SerializableObject();
}
