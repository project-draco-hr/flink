{
  int hashCode=0;
  for (int i=0; i < this.buildSideKeyFields.length; i++) {
    Key k=this.keyHolders[i];
    if ((k=record.getField(this.buildSideKeyFields[i],k)) != null) {
      hashCode^=hash(k.hashCode(),recursionLevel);
    }
 else {
      throw new NullKeyFieldException();
    }
  }
  return hashCode;
}
