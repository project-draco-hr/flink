{
  int hashCode=0;
  for (int i=0; i < this.buildSideKeyFields.length; i++) {
    Key k=record.getField(this.buildSideKeyFields[i],this.keyClasses[i]);
    if (k != null) {
      hashCode^=hash(k.hashCode(),recursionLevel);
    }
 else {
      throw new NullKeyFieldException(i);
    }
  }
  return hashCode;
}
