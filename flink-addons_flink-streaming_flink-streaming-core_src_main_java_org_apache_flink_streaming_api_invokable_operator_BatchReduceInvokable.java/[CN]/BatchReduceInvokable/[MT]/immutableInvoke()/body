{
  if ((reuse=recordIterator.next(reuse)) == null) {
    throw new RuntimeException("DataStream must not be empty");
  }
  while (reuse != null) {
    StreamBatch batch=getBatch(reuse);
    batch.reduceToBuffer(reuse.getObject());
    resetReuse();
    reuse=recordIterator.next(reuse);
  }
  reduceLastBatch();
}
