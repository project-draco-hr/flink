{
  if (getNextRecord() == null) {
    throw new RuntimeException("DataStream must not be empty");
  }
  initializeAtFirstRecord();
  while (reuse != null && !isStateFull()) {
    collectOneUnit();
  }
  reduce();
  while (reuse != null) {
    for (int i=0; i < slideSize / granularity; i++) {
      if (reuse != null) {
        collectOneUnit();
      }
    }
    reduce();
  }
}
