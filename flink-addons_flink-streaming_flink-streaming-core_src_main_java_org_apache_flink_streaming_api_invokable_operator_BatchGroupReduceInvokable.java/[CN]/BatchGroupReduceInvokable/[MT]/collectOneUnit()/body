{
  ArrayList<StreamRecord<IN>> list;
  if (!batchNotFull()) {
    list=new ArrayList<StreamRecord<IN>>();
  }
 else {
    list=new ArrayList<StreamRecord<IN>>(listSize);
    do {
      list.add(reuse);
      resetReuse();
    }
 while (getNextRecord() != null && batchNotFull());
  }
  state.pushBack(list);
}
