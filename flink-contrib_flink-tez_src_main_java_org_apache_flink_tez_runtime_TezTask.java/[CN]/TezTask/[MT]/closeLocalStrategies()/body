{
  if (this.localStrategies != null) {
    for (int i=0; i < this.localStrategies.length; i++) {
      if (this.localStrategies[i] != null) {
        try {
          this.localStrategies[i].close();
        }
 catch (        Throwable t) {
          LOG.error("Error closing local strategy for input " + i,t);
        }
      }
    }
  }
}
