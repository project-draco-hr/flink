{
  target.setVid(source.readLong());
  target.setCid(source.readLong());
  final int num=source.readInt();
  final long[] n;
  if (target.getNeighbors().length >= num) {
    n=target.getNeighbors();
    target.setNeighbors(n,num);
  }
 else {
    n=new long[num];
    target.setNeighbors(n,num);
  }
  for (int i=0; i < num; i++) {
    n[i]=source.readLong();
  }
}
