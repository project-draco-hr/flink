{
  try {
    if (base == null) {
      base=objectReuseEnabled ? record : serializer.copy(record);
    }
 else {
      base=objectReuseEnabled ? reducer.reduce(base,record) : serializer.copy(reducer.reduce(base,record));
    }
  }
 catch (  Exception e) {
    throw new ExceptionInChainedStubException(taskName,e);
  }
}
