{
  UserFunction=getTaskConfiguration().getClass("userfunction",TestSourceInvokable.class,UserSourceInvokable.class);
  numberOfOutputs=getTaskConfiguration().getInteger("numberOfOutputs",0);
  for (int i=1; i <= numberOfOutputs; i++) {
    Partitioner=getTaskConfiguration().getClass("partitioner_" + i,DefaultPartitioner.class,ChannelSelector.class);
    try {
      partitioners.add(Partitioner.newInstance());
    }
 catch (    Exception e) {
    }
  }
  try {
    userFunction=UserFunction.newInstance();
  }
 catch (  Exception e) {
  }
}
