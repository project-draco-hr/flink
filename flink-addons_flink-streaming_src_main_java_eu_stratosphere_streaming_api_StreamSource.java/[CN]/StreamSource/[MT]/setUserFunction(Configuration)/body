{
  Class<? extends UserSourceInvokable> userFunctionClass=taskConfiguration.getClass("userfunction",DefaultSourceInvokable.class,UserSourceInvokable.class);
  try {
    userFunction=userFunctionClass.newInstance();
    userFunction.declareOutputs(outputs,sourceInstanceID,recordBuffer);
  }
 catch (  Exception e) {
  }
}
