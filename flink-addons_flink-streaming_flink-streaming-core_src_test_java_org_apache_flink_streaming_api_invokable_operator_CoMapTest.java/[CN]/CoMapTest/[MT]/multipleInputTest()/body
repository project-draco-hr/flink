{
  LogUtils.initializeDefaultConsoleLogger(Level.OFF,Level.OFF);
  result=new HashSet<String>();
  LocalStreamEnvironment env=StreamExecutionEnvironment.createLocalEnvironment(1);
  DataStream<Tuple1<Integer>> ds1=env.fromElements(1,3);
  @SuppressWarnings("unchecked") DataStream<Tuple1<Integer>> ds2=env.fromElements(2,4).connectWith(ds1);
  DataStream<Tuple1<String>> ds3=env.fromElements("a","b");
  @SuppressWarnings({"unused","unchecked"}) DataStream<Tuple1<Boolean>> ds4=env.fromElements("c").connectWith(ds3).coMapWith(new MyCoMap(),ds2).addSink(new EmptySink());
  env.executeTest(32);
  Assert.assertArrayEquals(expected.toArray(),result.toArray());
}
