{
  try {
    for (    IN rec : tupleList) {
      format.writeRecord(rec);
    }
  }
 catch (  Exception ex) {
    try {
      if (!cleanupCalled && format instanceof CleanupWhenUnsuccessful) {
        cleanupCalled=true;
        ((CleanupWhenUnsuccessful)format).tryCleanupOnError();
      }
    }
 catch (    Throwable t) {
      LOG.error("Cleanup on error failed.",t);
    }
  }
  resetParameters();
}
