{
  Random rnd=new Random(874597969123412341L);
  double rndDouble=rnd.nextDouble() * Double.MAX_VALUE;
  return new Double[]{Double.valueOf(0),Double.valueOf(1),Double.valueOf(-1),Double.valueOf(Double.MAX_VALUE),Double.valueOf(Double.MIN_VALUE),Double.valueOf(rndDouble),Double.valueOf(-rndDouble),Double.valueOf(Double.NaN),Double.valueOf(Double.NEGATIVE_INFINITY),Double.valueOf(Double.POSITIVE_INFINITY)};
}
