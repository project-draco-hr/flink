{
  List<InterestingProperties> thisNodesIntProps=getInterestingProperties();
  List<InterestingProperties> props1=InterestingProperties.createInterestingPropertiesForInput(thisNodesIntProps,this,0);
  List<InterestingProperties> props2=InterestingProperties.createInterestingPropertiesForInput(thisNodesIntProps,this,1);
  createInterestingProperties(this.input1,props1,estimator,0);
  this.input1.addAllInterestingProperties(props1);
  createInterestingProperties(this.input2,props2,estimator,1);
  this.input2.addAllInterestingProperties(props2);
}
