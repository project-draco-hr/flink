{
  ObjectNode left=createObjectNode("names",new String[]{"Joe","J.","Joseph"});
  ObjectNode right=createObjectNode("names2",new String[]{"Joseph"});
  SimmetricFunction baseMeasure=new SimmetricFunction(new JaroWinkler(),new InputSelection(0),new InputSelection(1));
  MongeElkanSimilarity mongeElkanSimilarity=new MongeElkanSimilarity(baseMeasure,new PathExpression(new InputSelection(0),new ObjectAccess("names")),new PathExpression(new InputSelection(1),new ObjectAccess("names2")));
  Assert.assertEquals(0.8,mongeElkanSimilarity.evaluate(JsonUtil.asArray(left,right),new EvaluationContext()).getDoubleValue(),0.1);
}
