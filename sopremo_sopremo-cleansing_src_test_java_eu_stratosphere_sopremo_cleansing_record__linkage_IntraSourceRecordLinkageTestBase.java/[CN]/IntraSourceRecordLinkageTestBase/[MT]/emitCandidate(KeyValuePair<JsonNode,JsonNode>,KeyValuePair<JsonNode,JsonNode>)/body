{
  EvaluationExpression resultProjection=this.resultProjection;
  if (resultProjection == null)   resultProjection=EvaluationExpression.VALUE;
  final EvaluationContext context=this.getContext();
  JsonNode smaller=left.getValue(), bigger=right.getValue();
  if (bigger.compareTo(smaller) < 0) {
    JsonNode temp=smaller;
    smaller=bigger;
    bigger=temp;
  }
  this.sopremoTestPlan.getExpectedOutput(0).addArray(resultProjection.evaluate(smaller,context),resultProjection.evaluate(bigger,context));
}
