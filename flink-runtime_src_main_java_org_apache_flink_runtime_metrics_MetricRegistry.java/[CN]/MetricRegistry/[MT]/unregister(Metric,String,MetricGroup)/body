{
  try {
    if (reporters != null) {
      for (      MetricReporter reporter : reporters) {
        if (reporter != null) {
          reporter.notifyOfRemovedMetric(metric,metricName,group);
        }
      }
    }
  }
 catch (  Exception e) {
    LOG.error("Error while registering metric.",e);
  }
}
