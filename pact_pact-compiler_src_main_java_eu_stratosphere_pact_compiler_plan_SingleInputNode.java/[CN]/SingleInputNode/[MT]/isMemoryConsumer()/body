{
  for (  DriverPropertiesSingle dps : this.possibleProperties) {
    if (dps.getStrategy().damsInput(0)) {
      return true;
    }
    for (    RequestedLocalProperties rlp : dps.getPossibleLocalProperties()) {
      if (!rlp.isTrivial()) {
        return true;
      }
    }
  }
  return false;
}
