{
  if (input != 0) {
    throw new IndexOutOfBoundsException();
  }
switch (constantSetMode) {
case Constant:
    return (constantSet != null && Arrays.binarySearch(constantSet,fieldNumber) >= 0);
case Update:
  return (updateSet == null || Arrays.binarySearch(updateSet,fieldNumber) < 0);
default :
return false;
}
}
