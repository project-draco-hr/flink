{
  if (input != 0) {
    throw new IndexOutOfBoundsException();
  }
  if (implOpMode == null) {
    return false;
  }
switch (implOpMode) {
case Projection:
    return (explCopies == null ? false : explCopies.contains(fieldNumber));
case Copy:
  return (explProjections == null || explWrites == null ? false : !((new FieldSet(explWrites,explProjections)).contains(fieldNumber)));
default :
return false;
}
}
