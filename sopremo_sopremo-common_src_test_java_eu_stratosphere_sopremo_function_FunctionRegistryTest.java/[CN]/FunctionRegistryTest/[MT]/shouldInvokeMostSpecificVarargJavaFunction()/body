{
  this.registry.register(JavaFunctions.class);
  Assert.assertSame(ONE_INT_VARARG_NODE,this.registry.evaluate("count",JsonUtils.asArray(JsonUtils.NODE_FACTORY.numberNode(1),JsonUtils.NODE_FACTORY.numberNode(2),JsonUtils.NODE_FACTORY.numberNode(3)),this.context));
  Assert.assertSame(ONE_INT_VARARG_NODE,this.registry.evaluate("count",JsonUtils.asArray(JsonUtils.NODE_FACTORY.numberNode(1)),this.context));
}
