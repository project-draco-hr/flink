{
  registry.register(JavaFunctions.class);
  Assert.assertEquals("should have been 2 functions",2,registry.getRegisteredFunctions().size());
  for (  Function function : registry.getRegisteredFunctions().values())   Assert.assertEquals("should have been a java function",JavaFunction.class,function.getClass());
  Assert.assertEquals("should have been 5 count signatures",5,((JavaFunction)registry.getFunction("count")).getSignatures().size());
  Assert.assertEquals("should have been 1 sum signatures",1,((JavaFunction)registry.getFunction("sum")).getSignatures().size());
}
