{
switch (executionState) {
case SCHEDULED:
    return true;
case ASSIGNED:
  return true;
case READY:
return true;
case STARTING:
return false;
case RUNNING:
return false;
case FINISHING:
return false;
case FINISHED:
return true;
case CANCELING:
return false;
case CANCELED:
return true;
case FAILED:
return false;
case REPLAYING:
return true;
}
throw new IllegalStateException("No mapping for execution state " + executionState);
}
