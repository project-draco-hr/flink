{
  final int size1=((ArrayNode)value1).size(), size2=((ArrayNode)value2).size();
  if (size1 != size2)   return size1 - size2;
  for (int index=0, size=size1; index < size; index++) {
    final int comparisonResult=JsonNodeComparator.INSTANCE.compare(((ArrayNode)value1).get(index),((ArrayNode)value2).get(index));
    if (comparisonResult != 0)     return comparisonResult;
  }
  return 0;
}
