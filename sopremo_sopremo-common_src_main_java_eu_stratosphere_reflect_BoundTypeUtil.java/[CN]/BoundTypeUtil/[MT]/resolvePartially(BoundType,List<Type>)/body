{
  if (hierarchy.isEmpty())   return boundType;
  Type type=hierarchy.get(0);
  if (type instanceof Class<?>)   return resolvePartially(BoundType.of((Class<?>)type,boundType.getParameters()),hierarchy.subList(1,hierarchy.size()));
  return resolvePartially(getBindingsOfDirectSuperclass(boundType,(ParameterizedType)type),hierarchy.subList(1,hierarchy.size()));
}
