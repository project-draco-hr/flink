{
  final long threadId=thread.getId();
  final ThreadInfo threadInfo=tmx.getThreadInfo(threadId);
  if (threadInfo == null) {
    return null;
  }
  return new CPUUtilizationSnapshot(timestamp,tmx.getThreadCpuTime(threadId) / NANO_TO_MILLISECONDS,tmx.getThreadUserTime(threadId) / NANO_TO_MILLISECONDS,threadInfo.getWaitedTime(),threadInfo.getBlockedTime());
}
