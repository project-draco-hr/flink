{
  try {
    see.execute(name);
  }
 catch (  JobExecutionException good) {
    Throwable t=good.getCause();
    int limit=0;
    while (!(t instanceof SuccessException)) {
      if (t == null) {
        LOG.warn("Test failed with exception",good);
        Assert.fail("Test failed with: " + good.getMessage());
      }
      t=t.getCause();
      if (limit++ == 20) {
        LOG.warn("Test failed with exception",good);
        Assert.fail("Test failed with: " + good.getMessage());
      }
    }
  }
}
