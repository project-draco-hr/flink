{
  for (  RecordDeserializer<?> deserializer : reader1RecordDeserializers) {
    Buffer buffer=deserializer.getCurrentBuffer();
    if (buffer != null && !buffer.isRecycled()) {
      buffer.recycle();
    }
  }
  for (  RecordDeserializer<?> deserializer : reader2RecordDeserializers) {
    Buffer buffer=deserializer.getCurrentBuffer();
    if (buffer != null && !buffer.isRecycled()) {
      buffer.recycle();
    }
  }
}
