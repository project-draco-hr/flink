{
  ShipStrategy[] innerStartStrategies=null;
  if (innerLoopStarts != null) {
    innerStartStrategies=new ShipStrategy[innerLoopStarts.length];
    Arrays.fill(innerStartStrategies,ShipStrategy.FORWARD);
  }
  connectBoundedRoundsIterationLoop(iterationInput,iterationOutput,innerLoopStarts,innerStartStrategies,innerLoopEnd,iterationHead,iterationInputShipStrategy,numRounds,graph,false);
}
