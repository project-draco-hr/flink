{
  if (typeInfo instanceof TupleTypeInfoBase) {
    TupleTypeInfoBase<?> tupleType=((TupleTypeInfoBase<?>)typeInfo);
    for (int i=0; i < tupleType.getArity(); i++) {
      if (!isValidKeyType(tupleType.getTypeAt(i))) {
        return false;
      }
    }
    return true;
  }
 else {
    return typeInfo.isKeyType();
  }
}
