{
  if (!(obj instanceof LocalBlockLocation)) {
    return false;
  }
  final LocalBlockLocation lbl=(LocalBlockLocation)obj;
  if (this.length != lbl.length) {
    return false;
  }
  if (this.hosts[0] == null) {
    if (lbl.hosts[0] != null) {
      return false;
    }
 else {
      return true;
    }
  }
 else {
    if (lbl.hosts[0] == null) {
      return false;
    }
 else {
      return this.hosts[0].equals(lbl.hosts[0]);
    }
  }
}
