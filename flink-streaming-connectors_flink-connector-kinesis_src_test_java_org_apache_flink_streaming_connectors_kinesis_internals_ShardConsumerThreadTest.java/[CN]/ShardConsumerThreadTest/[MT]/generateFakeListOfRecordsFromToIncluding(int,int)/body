{
  List<Record> fakeListOfRecords=new LinkedList<>();
  for (int i=startingSeq; i <= endingSeq; i++) {
    fakeListOfRecords.add(new Record().withData(ByteBuffer.wrap(String.valueOf(i).getBytes())).withPartitionKey(UUID.randomUUID().toString()).withSequenceNumber(String.valueOf(i)));
  }
  return fakeListOfRecords;
}
