{
  AbstractStateBackend.CheckpointStateOutputStream stream=new FsStateBackend.FsCheckpointStateOutputStream(TEMP_DIR_PATH,FileSystem.getLocalFileSystem(),1024,512);
  StreamStateHandle handle=stream.closeAndGetHandle();
  assertTrue(handle instanceof ByteStreamStateHandle);
  InputStream inStream=handle.getState(ClassLoader.getSystemClassLoader());
  assertEquals(-1,inStream.read());
}
