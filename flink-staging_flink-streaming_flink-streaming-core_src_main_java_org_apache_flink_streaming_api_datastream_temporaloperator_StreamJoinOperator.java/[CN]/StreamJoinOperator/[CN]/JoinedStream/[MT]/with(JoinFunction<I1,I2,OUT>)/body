{
  TypeInformation<OUT> outType=TypeExtractor.getJoinReturnTypes(joinFunction,predicate.op.input1.getType(),predicate.op.input2.getType());
  CoWindowInvokable<I1,I2,OUT> invokable=new CoWindowInvokable<I1,I2,OUT>(getJoinWindowFunction(joinFunction,predicate),predicate.op.windowSize,predicate.op.slideInterval,predicate.op.timeStamp1,predicate.op.timeStamp2);
  streamGraph.setInvokable(id,invokable);
  return setType(outType);
}
