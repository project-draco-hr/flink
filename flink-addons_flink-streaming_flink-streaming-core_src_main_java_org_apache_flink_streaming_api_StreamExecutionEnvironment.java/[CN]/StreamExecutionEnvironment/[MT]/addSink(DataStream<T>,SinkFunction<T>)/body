{
  DataStream<T> returnStream=new DataStream<T>(this,"sink");
  try {
    jobGraphBuilder.addSink(returnStream.getId(),new SinkInvokable<T>(sinkFunction),"sink",SerializationUtils.serialize(sinkFunction),degreeOfParallelism);
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize SinkFunction");
  }
  connectGraph(inputStream,returnStream.getId());
  return returnStream;
}
