{
  DataStream<Tuple1<X>> returnStream=new DataStream<Tuple1<X>>(this,"elements");
  try {
    jobGraphBuilder.addSource(returnStream.getId(),new FromElementsFunction<X>(data),"elements",SerializationUtils.serialize(data[0]),1);
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize elements");
  }
  return returnStream;
}
