{
  DataStream<X> returnStream=new DataStream<X>(this,"elements");
  try {
    SourceFunction<X> function=new FromElementsFunction<X>(data);
    jobGraphBuilder.addSource(returnStream.getId(),new SourceInvokable<X>(function),new ObjectTypeWrapper<X,Tuple,X>(data[0],null,data[0]),"source",SerializationUtils.serialize(function),1);
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize elements");
  }
  return returnStream;
}
