{
  DataStream<Tuple1<X>> returnStream=new DataStream<Tuple1<X>>(this,"elements");
  try {
    SourceFunction<Tuple1<X>> function=new FromElementsFunction<X>(data);
    jobGraphBuilder.addSource(returnStream.getId(),new SourceInvokable<Tuple1<X>>(function),new ObjectTypeWrapper<Tuple1<X>,Tuple,Tuple1<X>>(data[0],null,data[0]),"source",SerializationUtils.serialize(function),1);
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize elements");
  }
  return returnStream;
}
