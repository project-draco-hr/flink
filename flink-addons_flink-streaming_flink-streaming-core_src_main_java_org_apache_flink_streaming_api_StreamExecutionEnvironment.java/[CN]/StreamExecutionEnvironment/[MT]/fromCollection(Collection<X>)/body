{
  DataStream<X> returnStream=new DataStream<X>(this,"elements");
  if (data.isEmpty()) {
    throw new RuntimeException("Collection must not be empty");
  }
  try {
    SourceFunction<X> function=new FromElementsFunction<X>(data);
    jobGraphBuilder.addSource(returnStream.getId(),new SourceInvokable<X>(new FromElementsFunction<X>(data)),new ObjectTypeWrapper<X,Tuple,X>((X)data.toArray()[0],null,(X)data.toArray()[0]),"source",SerializationUtils.serialize(function),1);
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize collection");
  }
  return returnStream;
}
