{
  DataStream<Tuple1<X>> returnStream=new DataStream<Tuple1<X>>(this,"elements");
  if (data.isEmpty()) {
    throw new RuntimeException("Collection must not be empty");
  }
  try {
    SourceFunction<Tuple1<X>> function=new FromElementsFunction<X>(data);
    jobGraphBuilder.addSource(returnStream.getId(),new SourceInvokable<Tuple1<X>>(new FromElementsFunction<X>(data)),new ObjectTypeWrapper<Tuple1<X>,Tuple,Tuple1<X>>(data.toArray()[0],null,data.toArray()[0]),"source",SerializationUtils.serialize(function),1);
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize collection");
  }
  return returnStream;
}
