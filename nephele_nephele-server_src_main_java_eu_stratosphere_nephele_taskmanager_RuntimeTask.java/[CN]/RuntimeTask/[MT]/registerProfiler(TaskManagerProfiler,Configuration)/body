{
  taskManagerProfiler.registerExecutionListener(this,jobConfiguration);
  for (int i=0; i < this.environment.getNumberOfInputGates(); i++) {
    taskManagerProfiler.registerInputGateListener(this.vertexID,jobConfiguration,this.environment.getInputGate(i));
  }
  for (int i=0; i < this.environment.getNumberOfOutputGates(); i++) {
    taskManagerProfiler.registerOutputGateListener(this.vertexID,jobConfiguration,this.environment.getOutputGate(i));
  }
}
