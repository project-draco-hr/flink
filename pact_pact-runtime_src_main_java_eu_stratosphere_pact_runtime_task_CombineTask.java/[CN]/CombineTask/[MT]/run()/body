{
  if (LOG.isDebugEnabled())   LOG.debug(getLogString("Preprocessing done, iterator obtained."));
  final KeyGroupedIterator<T> iter=new KeyGroupedIterator<T>(this.input.getIterator(),this.serializer,this.comparator);
  final GenericReducer<T,?> stub=this.stub;
  final Collector<T> output=this.output;
  while (this.running && iter.nextKey()) {
    stub.combine(iter.getValues(),output);
  }
}
