{
  if (LOG.isDebugEnabled())   LOG.debug(getLogString("Preprocessing done, iterator obtained."));
  final KeyGroupedIterator iter=new KeyGroupedIterator(this.input.getIterator(),this.keyPositions,this.keyClasses);
  final ReduceStub stub=this.stub;
  final Collector output=this.output;
  if (this.stub.getClass().isAnnotationPresent(ForceCheckpoint.class)) {
    getEnvironment().isForced(this.stub.getClass().getAnnotation(ForceCheckpoint.class).checkpoint());
  }
  while (this.running && iter.nextKey()) {
    stub.combine(iter.getValues(),output);
  }
}
