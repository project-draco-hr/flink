{
  record.setId(channelID);
  if (useFaultTolerance) {
    emittedRecords.addRecord(record,outputChannel);
  }
  try {
    outputs.get(outputChannel).emit(record);
  }
 catch (  Exception e) {
    if (log.isWarnEnabled()) {
      log.warn("EMIT ERROR: " + e.getClass().getSimpleName() + " -- "+ name);
    }
  }
}
