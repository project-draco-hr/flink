{
  if (keys == null) {
    keys=new HashSet<>();
  }
  if (nfaOperatorState == null) {
    nfaOperatorState=this.createKeyValueState(NFA_OPERATOR_STATE_NAME,new KryoSerializer<NFA<IN>>((Class<NFA<IN>>)(Class<?>)NFA.class,getExecutionConfig()),null);
  }
  if (priorityQueueOperatorState == null) {
    priorityQueueOperatorState=this.createKeyValueState(PRIORIRY_QUEUE_STATE_NAME,new PriorityQueueSerializer<StreamRecord<IN>>(new StreamRecordSerializer<IN>(getInputSerializer()),new PriorityQueueStreamRecordFactory<IN>()),null);
  }
}
