{
  groupCounter=getRuntimeContext().getOperatorState("groupCounter",0,true,new StateCheckpointer<Integer,String>(){
    @Override public String snapshotState(    Integer state,    long checkpointId,    long checkpointTimestamp){
      return state.toString();
    }
    @Override public Integer restoreState(    String stateSnapshot){
      return Integer.parseInt(stateSnapshot);
    }
  }
);
}
