{
  org.junit.Assume.assumeThat(mode,new IsNot(new IsEqual<ExecutionMode>(ExecutionMode.COLLECTION)));
  final ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  env.setDegreeOfParallelism(2);
  DataSet<Tuple3<Integer,Long,String>> ds=CollectionDataSets.get3TupleDataSet(env);
  DataSet<Tuple2<Integer,String>> reduceDs=ds.groupBy(1).reduceGroup(new Tuple3GroupReduceWithCombine());
  reduceDs.writeAsCsv(resultPath);
  env.execute();
  expected="1,test1\n" + "5,test2\n" + "15,test3\n"+ "34,test4\n"+ "65,test5\n"+ "111,test6\n";
}
