{
  if (this.children.size() > 0) {
    sb.append("STRUCT ");
    sb.append(this.nodeConnectionInfo);
    for (    TreeNode n : this.children) {
      sb.append(" ");
      sb.append(n.getConnectionInfo().toString());
    }
    sb.append("\n");
    for (    TreeNode n : this.children) {
      n.printRecursiveTree(sb);
    }
  }
}
