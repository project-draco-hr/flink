{
  List<InterestingProperties> thisNodesIntProps=getInterestingProperties();
  for (int i=0; i < inConns.size(); i++) {
    List<InterestingProperties> inputProps=InterestingProperties.createInterestingPropertiesForInput(thisNodesIntProps,this,i);
    inConns.get(i).addAllInterestingProperties(inputProps);
  }
}
