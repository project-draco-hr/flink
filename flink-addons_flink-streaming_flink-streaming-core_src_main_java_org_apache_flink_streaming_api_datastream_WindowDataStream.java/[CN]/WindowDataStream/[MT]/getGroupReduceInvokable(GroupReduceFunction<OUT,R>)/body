{
  BatchGroupReduceInvokable<OUT,R> invokable;
  if (isGrouped) {
    invokable=new GroupedWindowGroupReduceInvokable<OUT,R>(reducer,batchSize,slideSize,keyPosition,timeStamp);
  }
 else {
    invokable=new WindowGroupReduceInvokable<OUT,R>(reducer,batchSize,slideSize,timeStamp);
  }
  return invokable;
}
