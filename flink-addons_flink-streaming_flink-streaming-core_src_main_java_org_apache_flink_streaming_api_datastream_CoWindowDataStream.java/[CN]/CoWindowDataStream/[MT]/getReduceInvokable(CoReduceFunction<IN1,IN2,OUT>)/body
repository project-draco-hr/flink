{
  CoWindowReduceInvokable<IN1,IN2,OUT> invokable;
  if (isGrouped) {
    invokable=new CoGroupedWindowReduceInvokable<IN1,IN2,OUT>(coReducer,batchSize1,batchSize2,slideSize1,slideSize2,keyPosition1,keyPosition2,timeStamp1,timeStamp2);
  }
 else {
    invokable=new CoWindowReduceInvokable<IN1,IN2,OUT>(coReducer,batchSize1,batchSize2,slideSize1,slideSize2,timeStamp1,timeStamp2);
  }
  return invokable;
}
