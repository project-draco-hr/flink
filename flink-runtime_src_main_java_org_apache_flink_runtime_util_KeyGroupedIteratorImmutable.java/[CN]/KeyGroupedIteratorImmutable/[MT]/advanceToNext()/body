{
  try {
    E next=this.iterator.next(serializer.createInstance());
    if (next != null) {
      if (comparator.equalToReference(next)) {
        return next;
      }
 else {
        this.lookahead=next;
        return null;
      }
    }
 else {
      this.done=true;
      return null;
    }
  }
 catch (  IOException e) {
    throw new RuntimeException("An error occurred while reading the next record.",e);
  }
}
