{
  if (this == obj) {
    return true;
  }
  if (obj.getClass() == PactString.class) {
    final PactString other=(PactString)obj;
    int len=this.len;
    if (len == other.len) {
      final char[] tc=this.value;
      final char[] oc=other.value;
      int i=0, j=0;
      while (len-- != 0) {
        if (tc[i++] != oc[j++])         return false;
      }
      return true;
    }
  }
  return false;
}
