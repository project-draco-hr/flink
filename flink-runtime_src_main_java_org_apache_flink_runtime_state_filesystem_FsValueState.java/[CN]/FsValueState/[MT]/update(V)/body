{
  if (currentKey == null) {
    throw new RuntimeException("No key available.");
  }
  if (value == null) {
    clear();
    return;
  }
  if (currentNSState == null) {
    currentNSState=new HashMap<>();
    state.put(currentNamespace,currentNSState);
  }
  currentNSState.put(currentKey,value);
}
