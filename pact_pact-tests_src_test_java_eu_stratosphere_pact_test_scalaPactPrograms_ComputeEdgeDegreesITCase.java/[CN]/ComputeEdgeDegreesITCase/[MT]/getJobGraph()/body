{
  ComputeEdgeDegrees computeDegrees=new ComputeEdgeDegrees();
  Plan plan=computeDegrees.getScalaPlan(config.getInteger("ComputeEdgeDegreesTest#NoSubtasks",4),getFilesystemProvider().getURIPrefix() + edgesPath,getFilesystemProvider().getURIPrefix() + resultPath);
  PactCompiler pc=new PactCompiler(new DataStatistics());
  OptimizedPlan op=pc.compile(plan);
  NepheleJobGraphGenerator jgg=new NepheleJobGraphGenerator();
  return jgg.compileJobGraph(op);
}
