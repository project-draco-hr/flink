{
  long weight=Long.MIN_VALUE;
  long vertexId=-1;
  long i=0;
  for (  Tuple2<Long,Edge<Long,Long>> edge : edges) {
    if (edge.f1.getValue() > weight) {
      weight=edge.f1.getValue();
    }
    if (i == 0) {
      vertexId=edge.f0;
    }
    i++;
  }
  return new Tuple2<Long,Long>(vertexId,weight);
}
