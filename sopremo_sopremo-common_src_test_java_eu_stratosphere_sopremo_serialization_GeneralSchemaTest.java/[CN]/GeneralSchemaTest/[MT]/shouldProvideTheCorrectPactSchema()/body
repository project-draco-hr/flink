{
  final int count=3;
  @SuppressWarnings("unchecked") final Class<? extends Value>[] expected=new Class[count];
  for (int i=0; i < count; i++)   expected[i]=JsonNodeWrapper.class;
  final Class<? extends Value>[] result=this.schema.getPactSchema();
  Assert.assertEquals(expected.length,result.length);
  for (int i=0; i < expected.length; i++)   Assert.assertEquals(expected[i],result[i]);
}
