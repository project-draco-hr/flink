{
  List<EvaluationExpression> mappings=new LinkedList<EvaluationExpression>();
  mappings.add(new ArithmeticExpression(new ArrayAccess(0),ArithmeticExpression.ArithmeticOperator.ADDITION,new ArrayAccess(1)));
  mappings.add(new ArithmeticExpression(new ArrayAccess(0),ArithmeticExpression.ArithmeticOperator.MULTIPLICATION,new ArrayAccess(1)));
  this.array=new ArrayNode(IntNode.valueOf(3),IntNode.valueOf(4));
  this.schema=new GeneralSchema();
  this.schema.setMappings(mappings);
  this.context=new EvaluationContext();
}
