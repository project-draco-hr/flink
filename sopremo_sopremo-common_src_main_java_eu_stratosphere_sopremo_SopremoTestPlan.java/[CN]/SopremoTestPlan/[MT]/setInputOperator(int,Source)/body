{
  this.inputs[index].setOperator(operator);
  if (operator.getType() == PersistenceType.ADHOC)   for (  JsonNode node : operator.getAdhocValues())   this.inputs[index].add(new PactJsonObject(node));
 else {
    TestPairs<PactJsonObject.Key,PactJsonObject> testPairs=new TestPairs<PactJsonObject.Key,PactJsonObject>();
    testPairs.fromFile(JsonInputFormat.class,operator.getInputName());
    for (    KeyValuePair<PactJsonObject.Key,PactJsonObject> kvPair : testPairs)     this.inputs[index].add(kvPair.getValue());
  }
}
