{
  StreamInvokable<?,?> headInvokable=streamGraph.getInvokable(vertexID);
  StreamInvokable<?,?> outInvokable=streamGraph.getInvokable(outName);
  return streamGraph.getInEdges(outName).size() == 1 && outInvokable != null && outInvokable.getChainingStrategy() == ChainingStrategy.ALWAYS && (headInvokable.getChainingStrategy() == ChainingStrategy.HEAD || headInvokable.getChainingStrategy() == ChainingStrategy.ALWAYS) && streamGraph.getEdge(vertexID,outName).getPartitioner().getStrategy() == PartitioningStrategy.FORWARD && streamGraph.getParallelism(vertexID) == streamGraph.getParallelism(outName) && streamGraph.chaining;
}
