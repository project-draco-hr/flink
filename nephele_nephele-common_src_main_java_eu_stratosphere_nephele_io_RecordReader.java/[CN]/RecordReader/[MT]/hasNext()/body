{
  if (this.noMoreRecordsWillFollow) {
    return false;
  }
  if (this.lastRead == null) {
    try {
      this.lastRead=getInputGate().readRecord(null);
      if (this.lastRead == null) {
        return false;
      }
    }
 catch (    IOException e) {
      this.ioException=e;
      return true;
    }
catch (    InterruptedException e) {
      this.interruptedException=e;
      return true;
    }
  }
  return true;
}
