{
  CrossWindowFunction<I1,I2,Tuple2<I1,I2>> crossWindowFunction=new CrossWindowFunction<I1,I2,Tuple2<I1,I2>>(clean(new CrossOperator.DefaultCrossFunction<I1,I2>()));
  return (CrossWindow<I1,I2,R>)new CrossWindow<I1,I2,Tuple2<I1,I2>>(op,op.input1.connect(op.input2).addGeneralWindowCombine(crossWindowFunction,new TupleTypeInfo<Tuple2<I1,I2>>(op.input1.getType(),op.input2.getType()),op.windowSize,length,op.timeStamp1,op.timeStamp2));
}
