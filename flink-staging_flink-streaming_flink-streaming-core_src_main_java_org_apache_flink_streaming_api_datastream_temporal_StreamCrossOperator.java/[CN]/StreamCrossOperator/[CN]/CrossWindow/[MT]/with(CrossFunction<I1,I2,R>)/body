{
  TypeInformation<R> outTypeInfo=TypeExtractor.getCrossReturnTypes(function,op.input1.getType(),op.input2.getType());
  CoStreamWindow<I1,I2,R> operator=new CoStreamWindow<I1,I2,R>(new CrossWindowFunction<I1,I2,R>(clean(function)),op.windowSize,op.slideInterval,op.timeStamp1,op.timeStamp2);
  streamGraph.setOperator(id,operator);
  return ((SingleOutputStreamOperator<R,?>)this).returns(outTypeInfo);
}
