{
  final int numInputs=this.getInputOperators().size();
  final SopremoModule module=new SopremoModule(this.getName(),numInputs,1);
  final List<Operator> inputs=new ArrayList<Operator>();
  for (int index=0; index < numInputs; index++)   inputs.add(new Projection(this.getKeyProjection(index),this.getValueProjection(index),module.getInput(index)));
  final Operator lastOperator=this.createElementaryOperations(inputs);
  module.getOutput(0).setInput(0,new Projection(EvaluationExpression.NULL,EvaluationExpression.SAME_VALUE,lastOperator));
  return module;
}
