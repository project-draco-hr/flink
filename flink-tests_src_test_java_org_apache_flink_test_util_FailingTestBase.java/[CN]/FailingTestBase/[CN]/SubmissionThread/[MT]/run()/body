{
  try {
    this.executor.submitJobAndWait(this.failingJob,false);
    this.error=new Exception("The job did not fail.");
  }
 catch (  JobExecutionException jee) {
  }
catch (  Exception e) {
    this.error=e;
  }
  try {
    this.executor.submitJobAndWait(this.job,false);
  }
 catch (  Exception e) {
    this.error=e;
  }
  timeoutThread.interrupt();
}
