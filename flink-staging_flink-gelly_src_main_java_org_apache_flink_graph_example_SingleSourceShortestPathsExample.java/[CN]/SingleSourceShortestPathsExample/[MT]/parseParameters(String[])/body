{
  if (args.length > 0) {
    if (args.length == 5) {
      fileOutput=true;
      srcVertexId=Long.parseLong(args[0]);
      verticesInputPath=args[1];
      edgesInputPath=args[2];
      outputPath=args[3];
      maxIterations=Integer.parseInt(args[4]);
    }
 else {
      System.err.println("Usage: SingleSourceShortestPaths <source vertex id>" + " <input vertices path> <input edges path> <output path> <num iterations>");
      return false;
    }
  }
  return true;
}
