{
  ClosableManager closableManager=new ClosableManager();
  for (  TestPairs<?,?> pairs : inputs.values())   closableManager.add(pairs);
  for (  TestPairs<?,?> pairs : actualOutputs.values())   closableManager.add(pairs);
  for (  TestPairs<?,?> pairs : expectedOutputs.values())   closableManager.add(pairs);
  closableManager.close();
}
