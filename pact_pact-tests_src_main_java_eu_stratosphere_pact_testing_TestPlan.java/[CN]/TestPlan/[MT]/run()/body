{
  try {
    final ExecutionGraph eg=this.getExecutionGraph();
    final LocalScheduler localScheduler=new LocalScheduler(new SchedulingListener(){
      @Override public void jobRemovedFromScheduler(      final ExecutionGraph executionGraph){
      }
    }
,this.instanceManager);
    localScheduler.schedulJob(eg);
    this.execute(eg,localScheduler);
  }
 catch (  final Exception e) {
    fail(e,"plan scheduling");
  }
  this.validateResults();
}
