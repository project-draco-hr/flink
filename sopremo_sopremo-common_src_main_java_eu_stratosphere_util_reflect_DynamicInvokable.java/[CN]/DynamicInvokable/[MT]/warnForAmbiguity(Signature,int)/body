{
  final List<Signature> ambigiousSignatures=new ArrayList<Signature>();
  for (  final Entry<Signature,MemberType> originalSignature : this.originalSignatures.entrySet()) {
    final int distance=originalSignature.getKey().getDistance(signature);
    if (distance == minDistance)     ambigiousSignatures.add(originalSignature.getKey());
  }
  LOG.warn(String.format("multiple matching signatures found for the member %s and parameters types %s: %s",this.getName(),Arrays.toString(signature.getParameterTypes()),ambigiousSignatures));
}
