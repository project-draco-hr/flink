{
  RangeCalculator[] rt={new LastBitsToRange(8),new LastBitsToRange(3),new LastBitsToRange(3)};
  HashTester ht=new HashTester(new RotateHashFunction(),new StepRangeIterator(-300000,300000,19),rt);
  BucketBoundaries[] boundaries={new BucketBoundaries(90,150,5,0.05),new BucketBoundaries(0,30,5,0.1),new BucketBoundaries(0,10,5000,0.1)};
  ht.runTest(boundaries);
}
