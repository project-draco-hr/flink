{
  ScatterGatherIteration<K,VV,M,EV> iteration=ScatterGatherIteration.withEdges(edges,vertexUpdateFunction,messagingFunction,maximumNumberOfIterations);
  iteration.configure(parameters);
  DataSet<Vertex<K,VV>> newVertices=this.getVertices().runOperation(iteration);
  return new Graph<K,VV,EV>(newVertices,this.edges,this.context);
}
