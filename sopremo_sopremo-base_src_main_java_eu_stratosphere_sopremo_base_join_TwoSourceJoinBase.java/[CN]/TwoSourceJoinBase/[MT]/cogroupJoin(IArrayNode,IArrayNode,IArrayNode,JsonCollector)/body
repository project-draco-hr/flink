{
  final ArrayList<IJsonNode> firstSourceNodes=new ArrayList<IJsonNode>();
  for (  final IJsonNode value : values1)   firstSourceNodes.add(value);
  for (  final IJsonNode secondSourceNode : values2)   for (  final IJsonNode firstSourceNode : firstSourceNodes) {
    result.set(0,firstSourceNode);
    result.set(1,secondSourceNode);
    out.collect(result);
  }
}
