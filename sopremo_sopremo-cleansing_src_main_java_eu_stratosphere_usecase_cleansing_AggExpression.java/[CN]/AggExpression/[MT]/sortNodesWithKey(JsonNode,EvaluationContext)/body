{
  final List<CompactArrayNode> nodes=new ArrayList<CompactArrayNode>();
  for (  final JsonNode jsonNode : node)   nodes.add(JsonUtil.asArray(this.groupingExpression.evaluate(jsonNode,context),jsonNode));
  Collections.sort(nodes,new Comparator<CompactArrayNode>(){
    @Override public int compare(    CompactArrayNode o1,    CompactArrayNode o2){
      return JsonNodeComparator.INSTANCE.compare(o1.get(0),o2.get(0));
    }
  }
);
  return nodes;
}
