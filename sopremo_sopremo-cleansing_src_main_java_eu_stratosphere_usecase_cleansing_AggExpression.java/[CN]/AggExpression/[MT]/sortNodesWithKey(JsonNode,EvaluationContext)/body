{
  final List<ArrayNode> nodes=new ArrayList<ArrayNode>();
  for (  final JsonNode jsonNode : (ArrayNode)node)   nodes.add(JsonUtil.asArray(this.groupingExpression.evaluate(jsonNode,context),jsonNode));
  Collections.sort(nodes,new Comparator<ArrayNode>(){
    @Override public int compare(    ArrayNode o1,    ArrayNode o2){
      return JsonNodeComparator.INSTANCE.compare(o1.get(0),o2.get(0));
    }
  }
);
  return nodes;
}
