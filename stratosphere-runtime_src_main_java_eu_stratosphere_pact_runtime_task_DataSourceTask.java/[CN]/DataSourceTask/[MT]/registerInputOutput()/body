{
  if (LOG.isDebugEnabled()) {
    LOG.debug(getLogString("Start registering input and output"));
  }
  if (this.userCodeClassLoader == null) {
    try {
      this.userCodeClassLoader=LibraryCacheManager.getClassLoader(getEnvironment().getJobID());
    }
 catch (    IOException ioe) {
      throw new RuntimeException("Usercode ClassLoader could not be obtained for job: " + getEnvironment().getJobID(),ioe);
    }
  }
  Configuration taskConf=getTaskConfiguration();
  taskConf.setClassLoader(this.userCodeClassLoader);
  this.config=new TaskConfig(taskConf);
  initInputFormat(this.userCodeClassLoader);
  try {
    initOutputs(this.userCodeClassLoader);
  }
 catch (  Exception ex) {
    throw new RuntimeException("The initialization of the DataSource's outputs caused an error: " + ex.getMessage(),ex);
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug(getLogString("Finished registering input and output"));
  }
}
