{
  if (obj instanceof ExecutionAttempt) {
    ExecutionAttempt other=(ExecutionAttempt)obj;
    return this.executionId.equals(other.executionId) && this.vertexId.equals(other.vertexId) && this.subtaskIndex == other.subtaskIndex && this.attempt == other.attempt;
  }
 else {
    return false;
  }
}
