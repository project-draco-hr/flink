{
  final int size=conn.size();
  Class<? extends DataDistribution> distribution=conn.get(0).getTargetPact().getPactContract().getCompilerHints().getInputDistributionClass();
  for (int i=1; i < size; ++i) {
    if (!conn.get(i).getTargetPact().getPactContract().getCompilerHints().getInputDistributionClass().equals(distribution))     return false;
  }
  return true;
}
