{
  if (len == 1) {
    int highByte=this.value & 0xff;
    highByte-=Byte.MIN_VALUE;
    target.put(offset,(byte)highByte);
  }
 else   if (len <= 0) {
  }
 else {
    int highByte=this.value & 0xff;
    highByte-=Byte.MIN_VALUE;
    target.put(offset,(byte)highByte);
    for (int i=1; i < len; i++) {
      target.put(offset + i,(byte)0);
    }
  }
}
