{
  try {
    if (encoded == null) {
      return null;
    }
    byte[] bytes=Base64.decodeBase64(encoded);
    return InstantiationUtil.deserializeObject(bytes,cl);
  }
 catch (  IOException e) {
    e.printStackTrace();
    System.exit(-1);
    throw new RuntimeException();
  }
catch (  ClassNotFoundException e) {
    e.printStackTrace();
    System.exit(-1);
    throw new RuntimeException();
  }
}
