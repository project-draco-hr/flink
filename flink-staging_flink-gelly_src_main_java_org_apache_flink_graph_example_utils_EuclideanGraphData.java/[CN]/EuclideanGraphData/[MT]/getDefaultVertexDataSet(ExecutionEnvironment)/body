{
  List<Vertex<Long,EuclideanGraphExample.Point>> vertices=new ArrayList<Vertex<Long,EuclideanGraphExample.Point>>();
  for (int i=1; i <= NUM_VERTICES; i++) {
    vertices.add(new Vertex<Long,EuclideanGraphExample.Point>(new Long(i),new EuclideanGraphExample.Point(new Double(i),new Double(i))));
  }
  return env.fromCollection(vertices);
}
