{
  List<Vertex<Long,EuclideanGraphWeighing.Point>> vertices=new ArrayList<Vertex<Long,EuclideanGraphWeighing.Point>>();
  for (int i=1; i <= NUM_VERTICES; i++) {
    vertices.add(new Vertex<Long,EuclideanGraphWeighing.Point>(new Long(i),new EuclideanGraphWeighing.Point(new Double(i),new Double(i))));
  }
  return env.fromCollection(vertices);
}
