{
  DataStreamSource<OUT> source;
  if (inputFormat instanceof FileInputFormat) {
    FileInputFormat<OUT> format=(FileInputFormat<OUT>)inputFormat;
    source=createFileInput(format,typeInfo,"Custom File source",FileProcessingMode.PROCESS_ONCE,FilePathFilter.createDefaultFilter(),-1);
  }
 else {
    source=createInput(inputFormat,typeInfo,"Custom Source");
  }
  return source;
}
