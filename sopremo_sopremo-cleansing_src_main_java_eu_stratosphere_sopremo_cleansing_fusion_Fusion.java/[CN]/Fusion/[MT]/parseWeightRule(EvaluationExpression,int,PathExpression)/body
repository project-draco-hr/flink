{
  if (value instanceof ArithmeticExpression) {
    this.setWeight(((NumericNode)((ConstantExpression)((ArithmeticExpression)value).getFirstOperand()).getConstant()).getDoubleValue(),index,path);
    this.parseWeightRule(((ArithmeticExpression)value).getSecondOperand(),index,new PathExpression(path));
  }
 else   this.setWeight(((NumericNode)((ConstantExpression)value).getConstant()).getDoubleValue(),index,path);
}
