{
  EvictionPolicy<Integer> policy=new TumblingEvictionPolicy<Integer>();
  int counter=0;
  for (int i=0; i < 10; i++) {
    for (int j=0; j < i; j++) {
      assertEquals(0,policy.notifyEviction(0,false,counter++));
    }
    assertEquals(counter,policy.notifyEviction(0,true,counter));
    counter=1;
  }
}
