{
  int numOutputs=this.config.getNumOutputs();
  OutputCollectorV2 output=new OutputCollectorV2();
  outputAccessors=new TypeSerializer[numOutputs];
  for (int i=0; i < numOutputs; i++) {
    initOutputAccessor(i);
    final OutputEmitterV2 oe=new OutputEmitterV2(config.getOutputShipStrategy(i),getEnvironment().getJobID(),(TypeSerializer<Value>)outputAccessors[i]);
    output.addWriter(new RecordWriter<Value>((AbstractTask)this,Value.class,oe));
  }
  this.output=output;
}
