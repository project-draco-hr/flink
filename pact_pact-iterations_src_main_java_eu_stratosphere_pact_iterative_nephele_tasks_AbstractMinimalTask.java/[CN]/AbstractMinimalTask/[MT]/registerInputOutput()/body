{
  config=new TaskConfig(getTaskConfiguration());
  try {
    classLoader=LibraryCacheManager.getClassLoader(getEnvironment().getJobID());
  }
 catch (  IOException ioe) {
    throw new RuntimeException("The ClassLoader for the user code could not be instantiated from the library cache.",ioe);
  }
  memorySize=config.getMemorySize();
  initInputs();
  initOutputs();
  initInternal();
  initTask();
}
