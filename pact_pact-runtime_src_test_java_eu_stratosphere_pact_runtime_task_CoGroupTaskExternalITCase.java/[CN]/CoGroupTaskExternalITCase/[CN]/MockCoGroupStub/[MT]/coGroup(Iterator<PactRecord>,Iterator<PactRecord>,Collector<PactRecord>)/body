{
  int val1Cnt=0;
  int val2Cnt=0;
  while (records1.hasNext()) {
    val1Cnt++;
    records1.next();
  }
  while (records2.hasNext()) {
    val2Cnt++;
    records2.next();
  }
  if (val1Cnt == 0) {
    for (int i=0; i < val2Cnt; i++) {
      out.collect(this.res);
    }
  }
 else   if (val2Cnt == 0) {
    for (int i=0; i < val1Cnt; i++) {
      out.collect(this.res);
    }
  }
 else {
    for (int i=0; i < val2Cnt * val1Cnt; i++) {
      out.collect(this.res);
    }
  }
}
