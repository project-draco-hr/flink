{
  Iterator<? extends Node> iterator1=nodes1.iterator();
  Iterator<? extends Node> iterator2=nodes2.iterator();
  while (iterator1.hasNext() && iterator2.hasNext()) {
    Node node1=iterator1.next();
    Node node2=iterator2.next();
    if (!node1.equals(node2))     return node1;
    Node unmatching=this.getUnmatchingNode(this.navigator.getConnectedNodes(node1),this.navigator.getConnectedNodes(node2),seen);
    if (unmatching != null)     return unmatching;
  }
  return null;
}
