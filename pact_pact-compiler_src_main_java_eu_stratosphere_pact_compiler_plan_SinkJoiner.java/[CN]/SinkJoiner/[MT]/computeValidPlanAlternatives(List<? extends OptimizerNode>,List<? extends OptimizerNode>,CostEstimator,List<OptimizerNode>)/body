{
  for (  OptimizerNode subPlan1 : altSubPlans1) {
    for (    OptimizerNode subPlan2 : altSubPlans2) {
      if (!areBranchCompatible(subPlan1,subPlan2)) {
        continue;
      }
      SinkJoiner n=new SinkJoiner(this,subPlan1,subPlan2);
      estimator.costOperator(n);
      outputPlans.add(n);
    }
  }
}
