{
  Record r1=null;
  if (records1.hasNext()) {
    r1=records1.next();
  }
  Record r2=null;
  if (records2.hasNext()) {
    r2=records2.next();
  }
  if (r1 != null) {
    r1.getField(1,SBookAvroValue.class).datum();
  }
  if (r2 != null) {
    r2.getField(1,SBookAuthorValue.class).datum();
  }
}
