{
  if (this.path == null) {
    return 0;
  }
  try {
    final FileSystem fs=path.getFileSystem();
    try {
      final FileStatus f=fs.getFileStatus(path);
      if (f == null) {
        return 1;
      }
      if (f.isDir()) {
        return -1;
      }
    }
 catch (    FileNotFoundException fnfex) {
      if (getNumberOfSubtasks() > 1) {
        fs.mkdirs(path);
        return -1;
      }
 else {
        return 1;
      }
    }
  }
 catch (  IOException e) {
    return 1;
  }
  return 1;
}
