{
  PactRecord record=this.generateFilledDenseRecord(58);
  long mask=generateRandomBitmask(58);
  record.setNull(mask);
  for (int i=0; i < 58; i++) {
    if (((1l << i) & mask) != 0) {
      assertTrue(record.getField(i,PactInteger.class) == null);
    }
  }
  assertTrue(record.getNumFields() == 58);
}
