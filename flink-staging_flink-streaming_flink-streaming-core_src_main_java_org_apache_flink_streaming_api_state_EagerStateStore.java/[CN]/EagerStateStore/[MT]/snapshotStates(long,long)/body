{
  Map<Serializable,StateHandle<C>> handles=new HashMap<Serializable,StateHandle<C>>();
  for (  Entry<Serializable,S> stateEntry : fetchedState.entrySet()) {
    handles.put(stateEntry.getKey(),provider.createStateHandle(checkpointer.snapshotState(stateEntry.getValue(),checkpointId,checkpointTimestamp)));
  }
  return handles;
}
