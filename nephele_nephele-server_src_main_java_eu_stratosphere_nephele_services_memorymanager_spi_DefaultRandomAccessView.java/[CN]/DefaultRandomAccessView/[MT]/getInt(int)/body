{
  if (index >= 0 && index + 3 < this.size) {
    return (((int)this.memory[this.offset + index + 0] & 0xff) << 24) | (((int)this.memory[this.offset + index + 1] & 0xff) << 16) | (((int)this.memory[this.offset + index + 2] & 0xff) << 8)| (((int)this.memory[this.offset + index + 3] & 0xff) << 0);
  }
 else {
    throw new IndexOutOfBoundsException();
  }
}
