{
  br=new BufferedReader(new FileReader("src/test/resources/testdata/hamlet.txt"));
  while (true) {
    line=br.readLine();
    if (line == null) {
      break;
    }
    if (line != "") {
      line=line.replaceAll("[\\-\\+\\.\\^:,]","");
      for (      String word : line.split(" ")) {
        outRecord.setString(0,word);
        System.out.println("word=" + word);
        emit(outRecord);
        performanceCounter.count();
      }
    }
  }
}
