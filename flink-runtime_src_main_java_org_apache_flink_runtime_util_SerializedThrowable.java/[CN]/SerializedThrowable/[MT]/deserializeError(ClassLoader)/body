{
  Throwable cached=cachedException == null ? null : cachedException.get();
  if (cached == null) {
    try {
      cached=InstantiationUtil.deserializeObject(serializedException,classloader);
      cachedException=new WeakReference<Throwable>(cached);
    }
 catch (    Exception e) {
      return placeholder;
    }
  }
  return cached;
}
