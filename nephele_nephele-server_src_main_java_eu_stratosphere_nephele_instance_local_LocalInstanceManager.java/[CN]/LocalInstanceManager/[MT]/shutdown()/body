{
  if (this.localTaskManagerThread != null) {
    this.localTaskManagerThread.interrupt();
    while (!this.localTaskManagerThread.isTaskManagerShutDown()) {
      try {
        Thread.sleep(100);
      }
 catch (      InterruptedException e) {
        break;
      }
    }
    this.instanceTypeDescriptionMap.clear();
  }
}
