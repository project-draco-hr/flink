{
  if (input < 0 || input > 1)   throw new IndexOutOfBoundsException("TwoInputNode has inputs 0 or 1");
  if (input == 0) {
    if (this.reads1 != null && !inputSchema.containsAll(this.reads1))     return false;
    if (this.keySet1 != null && !inputSchema.containsAll(this.keySet1))     return false;
    if (this.implOpMode1 == null) {
      return false;
    }
    if (this.implOpMode1 == ImplicitOperationMode.Copy && !inputSchema.containsAll(this.explProjections1))     return false;
    if (this.implOpMode1 == ImplicitOperationMode.Projection && !inputSchema.containsAll(this.explCopies1))     return false;
  }
 else {
    if (this.reads2 != null && !inputSchema.containsAll(this.reads2))     return false;
    if (this.keySet2 != null && !inputSchema.containsAll(this.keySet2))     return false;
    if (this.implOpMode2 == null) {
      return false;
    }
    if (this.implOpMode2 == ImplicitOperationMode.Copy && !inputSchema.containsAll(this.explProjections2))     return false;
    if (this.implOpMode2 == ImplicitOperationMode.Projection && !inputSchema.containsAll(this.explCopies2))     return false;
  }
  return true;
}
