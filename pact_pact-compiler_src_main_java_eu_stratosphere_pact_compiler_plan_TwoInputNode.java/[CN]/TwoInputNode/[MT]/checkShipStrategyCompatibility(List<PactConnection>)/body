{
  ShipStrategy ss=ShipStrategy.NONE;
  final int size=input.size();
  int i;
  for (i=0; i < size; ++i) {
    ss=input.get(i).getShipStrategy();
    if (ss != ShipStrategy.NONE) {
      ++i;
      break;
    }
  }
  if (ss != ShipStrategy.NONE) {
    for (; i < size; ++i) {
      ShipStrategy s=input.get(i).getShipStrategy();
      if (s != ShipStrategy.NONE && s != ss)       return null;
    }
  }
  return ss;
}
