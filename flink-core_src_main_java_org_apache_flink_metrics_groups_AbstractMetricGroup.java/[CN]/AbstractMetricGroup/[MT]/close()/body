{
synchronized (this) {
    if (!closed) {
      closed=true;
      for (      MetricGroup group : groups.values()) {
        group.close();
      }
      groups.clear();
      for (      Map.Entry<String,Metric> metric : metrics.entrySet()) {
        registry.unregister(metric.getValue(),metric.getKey(),this);
      }
      metrics.clear();
    }
  }
}
