{
  if (metrics.containsKey(name)) {
    LOG.warn("Detected metric name collision. This group already contains a metric for the given group name." + this.generateScope().toString() + "."+ name);
  }
  if (!groups.containsKey(name)) {
    groups.put(name,new GenericMetricGroup(registry,this,name));
  }
  return groups.get(name);
}
