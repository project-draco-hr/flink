{
  try {
    final int typeIndex=in.readByte();
    this.value=Type.values()[typeIndex].getClazz().newInstance();
    this.value.read(in);
    this.value=this.value.canonicalize();
  }
 catch (  final InstantiationException e) {
    e.printStackTrace();
  }
catch (  final IllegalAccessException e) {
    e.printStackTrace();
  }
}
