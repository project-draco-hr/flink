{
  return dirToFilter.listFiles(new FilenameFilter(){
    @Override public boolean accept(    final File dir,    final String name){
      for (      String suffix : suffixes) {
        if (dir.equals(dirToFilter) && name != null && name.endsWith(suffix)) {
          return true;
        }
      }
      return false;
    }
  }
);
}
