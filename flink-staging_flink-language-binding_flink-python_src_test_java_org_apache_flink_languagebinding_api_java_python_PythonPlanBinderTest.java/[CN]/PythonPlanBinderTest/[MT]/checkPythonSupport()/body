{
  try {
    Runtime.getRuntime().exec("python");
  }
 catch (  IOException ex) {
    python2Supported=false;
    LOG.info("No Python 2 runtime detected.");
  }
  try {
    Runtime.getRuntime().exec("python3");
  }
 catch (  IOException ex) {
    python3Supported=false;
    LOG.info("No Python 3 runtime detected.");
  }
}
