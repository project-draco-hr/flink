{
  if (target == null)   target=new PactRecord(new JsonNodeWrapper());
 else   if (target.getNumFields() != 1)   target.setField(0,new JsonNodeWrapper());
  final JsonNodeWrapper wrapper=target.getField(0,JsonNodeWrapper.class);
  wrapper.setValue(value);
  target.setField(0,wrapper);
  return target;
}
