{
  final Map<String,T> combinedBindings=new HashMap<String,T>();
  final Iterator<Map<String,Object>> iterator=this.bindings.descendingIterator();
  while (iterator.hasNext())   for (  final Map.Entry<String,Object> binding : iterator.next().entrySet())   if (!combinedBindings.containsKey(binding.getKey()) && expectedType.isInstance(binding.getValue()))   combinedBindings.put(binding.getKey(),(T)binding.getValue());
  return combinedBindings;
}
