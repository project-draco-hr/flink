{
  DataStream<T> returnStream=new DataStream<T>(this);
  jobGraphBuilder.setSink(returnStream.getId(),new SinkInvokable<T>(sinkFunction),"sink",serializeToByteArray(sinkFunction),parallelism,(int)Math.ceil(parallelism / clusterSize));
  connectGraph(inputStream,returnStream.getId());
  return returnStream;
}
