{
  ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  TableEnvironment tableEnv=new TableEnvironment();
  DataSet<Tuple2<String,Integer>> ds=env.fromElements(new Tuple2<String,Integer>("AAAA",2),new Tuple2<String,Integer>("BBBB",1));
  Table in=tableEnv.fromDataSet(ds,"a, b");
  Table result=in.select("a.substring(0, b)");
  DataSet<Row> resultSet=tableEnv.toDataSet(result,Row.class);
  resultSet.writeAsText(resultPath,FileSystem.WriteMode.OVERWRITE);
  env.execute();
  expected="AA\nB";
}
