{
  if (!parseParameters(args)) {
    return;
  }
  StreamExecutionEnvironment env=StreamExecutionEnvironment.getExecutionEnvironment();
  DataStream<String> stream=env.addSource(new CountingSource()).window(Count.of(5).withDelete(4)).every(Count.of(4).startingAt(-1)).reduce(new Concat());
  if (fileOutput) {
    stream.writeAsText(outputPath,1);
  }
 else {
    stream.print();
  }
  env.execute("Sliding Example");
}
