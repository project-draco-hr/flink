{
  try {
    int ret=cli.run(new String[]{"-m",hostPort,getTestJarPath()});
    System.out.flush();
    assert(ret == 0 && checkForLogs(stream.toString()));
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail(e.getMessage());
  }
 finally {
    if (cluster != null) {
      cluster.shutdown();
    }
  }
}
