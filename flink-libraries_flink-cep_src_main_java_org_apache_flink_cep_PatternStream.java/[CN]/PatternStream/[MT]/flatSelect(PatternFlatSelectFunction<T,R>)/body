{
  TypeInformation<R> outTypeInfo=TypeExtractor.getUnaryOperatorReturnType(patternFlatSelectFunction,PatternFlatSelectFunction.class,1,0,inputType,null,false);
  return patternStream.flatMap(new PatternFlatSelectMapper<T,R>(patternStream.getExecutionEnvironment().clean(patternFlatSelectFunction))).returns(outTypeInfo);
