{
  this.parent=parent;
  @SuppressWarnings("unchecked") final FlatCombineFunction<IN,OUT> combiner=RegularPactTask.instantiateUserCode(this.config,userCodeClassLoader,FlatCombineFunction.class);
  this.combiner=combiner;
  FunctionUtils.setFunctionRuntimeContext(combiner,getUdfRuntimeContext());
