{
  this.parent=parent;
  @SuppressWarnings("unchecked") final GroupCombineFunction<IN,OUT> combiner=RegularPactTask.instantiateUserCode(this.config,userCodeClassLoader,GroupCombineFunction.class);
  this.combiner=combiner;
  FunctionUtils.setFunctionRuntimeContext(combiner,getUdfRuntimeContext());
