{
synchronized (count) {
    if (count.get(new ImmutablePair<JobID,String>(jobID,name)) != oldCount) {
      return;
    }
  }
  Path tmp=getTempDir(jobID,"");
  try {
    if (lfs.exists(tmp)) {
      lfs.delete(tmp,true);
    }
  }
 catch (  IOException e) {
    LOG.error("Could not delete file from local file cache.",e);
  }
}
