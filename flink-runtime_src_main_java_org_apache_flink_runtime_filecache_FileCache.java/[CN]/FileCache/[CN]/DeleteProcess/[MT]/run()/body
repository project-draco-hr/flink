{
synchronized (count) {
    if (count.get(new ImmutablePair<JobID,String>(jobID,name)) != oldCount) {
      return;
    }
  }
  Path tmp=getTempDir(jobID,"");
  try {
    if (lfs.exists(tmp)) {
      lfs.delete(tmp,true);
    }
  }
 catch (  IOException e1) {
    throw new RuntimeException("Error deleting the file",e1);
  }
}
