{
  if (isAllFieldsConstant()) {
    return new FieldSet(sourceField);
  }
  if (this.nonForwardedFields2 == null) {
    return super.getForwardedField2(sourceField);
  }
 else {
    if (this.nonForwardedFields2.contains(sourceField)) {
      return null;
    }
 else {
      return new FieldSet(sourceField);
    }
  }
}
