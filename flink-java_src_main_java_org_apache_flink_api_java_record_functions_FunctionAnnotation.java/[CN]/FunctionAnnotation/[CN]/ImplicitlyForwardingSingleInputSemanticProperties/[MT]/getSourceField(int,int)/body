{
  if (input != 0) {
    throw new IndexOutOfBoundsException();
  }
  if (isAllFieldsConstant()) {
    return new FieldSet(field);
  }
  if (this.nonForwardedFields == null) {
    return super.getSourceField(input,field);
  }
  if (this.nonForwardedFields.contains(field)) {
    return null;
  }
 else {
    return new FieldSet(field);
  }
}
