{
  if (input != 0) {
    throw new IndexOutOfBoundsException();
  }
  if (nonForwardedFields == null) {
    return super.getForwardingSourceField(input,targetField);
  }
 else {
    if (this.nonForwardedFields.contains(targetField)) {
      return -1;
    }
 else {
      return targetField;
    }
  }
}
