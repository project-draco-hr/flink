{
  if (this.byteBufferedOutputChannel.getType() == ChannelType.FILE) {
    return getNext().hasDataLeft();
  }
  if (this.closeAcknowledgmentReceived) {
    return getNext().hasDataLeft();
  }
  if ((this.lastSequenceNumberWithReceiverNotFound + 1) == this.sequenceNumber) {
    return getNext().hasDataLeft();
  }
  return true;
}
