{
  final Matcher matcher=this.pattern.matcher(text);
  tokens.clear();
  if (!matcher.find()) {
    addToken(tokens,text,0,text.length());
    return;
  }
  int start=0, end=0;
  do {
    end=matcher.start();
    if (end > start) {
      addToken(tokens,text,start,end);
    }
    start=matcher.end();
  }
 while (matcher.find());
  addToken(tokens,text,start,text.length());
}
