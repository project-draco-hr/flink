{
  ObjectSchema schema=new ObjectSchema();
  schema.setMappings("firstName","lastName","age");
  PactRecord record=new PactRecord();
  schema.jsonToRecord(new ObjectNode().put("firstName",TextNode.valueOf("Hans")).put("age",IntNode.valueOf(25)).put("gender",TextNode.valueOf("male")),record);
  this.node=new LazyObjectNode(record,schema);
}
