{
  if (exclusiveIndex == 0) {
    return null;
  }
  final Ordering newOrdering=new Ordering(this.indexes.get(0),this.types.get(0),this.orders.get(0));
  for (int i=1; i < exclusiveIndex; i++) {
    newOrdering.appendOrdering(this.indexes.get(i),this.types.get(i),this.orders.get(i));
  }
  return newOrdering;
}
