{
  if (fields.size() == 0) {
    return "(none)";
  }
  StringBuffer buf=new StringBuffer();
  for (int i=0; i < fields.size(); i++) {
    if (buf.length() == 0) {
      buf.append("[");
    }
 else {
      buf.append(",");
    }
    buf.append(fields.get(i));
    buf.append(":");
    buf.append(orders.get(i).name());
  }
  buf.append("]");
  return buf.toString();
}
