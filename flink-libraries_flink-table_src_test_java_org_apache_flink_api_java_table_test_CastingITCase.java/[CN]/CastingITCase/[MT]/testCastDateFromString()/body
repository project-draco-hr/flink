{
  ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  TableEnvironment tableEnv=new TableEnvironment();
  DataSource<Tuple4<String,String,String,String>> input=env.fromElements(new Tuple4<>("2011-05-03","15:51:36","2011-05-03 15:51:36.000","1446473775"));
  Table table=tableEnv.fromDataSet(input);
  Table result=table.select("f0.cast(DATE) AS f0, f1.cast(DATE) AS f1, f2.cast(DATE) AS f2, f3.cast(DATE) AS f3").select("f0.cast(STRING), f1.cast(STRING), f2.cast(STRING), f3.cast(STRING)");
}
