{
  CuratorFramework client=startCuratorFramework(configuration);
  String latchPath=configuration.getString(ConfigConstants.ZOOKEEPER_LATCH_PATH,ConfigConstants.DEFAULT_ZOOKEEPER_LATCH_PATH);
  String leaderPath=configuration.getString(ConfigConstants.ZOOKEEPER_LEADER_PATH,ConfigConstants.DEFAULT_ZOOKEEPER_LEADER_PATH);
  return new ZooKeeperLeaderElectionService(client,latchPath,leaderPath);
}
