{
  if (this.connectionFailureException != null) {
    throw this.connectionFailureException;
  }
  if (!this.closeRequested) {
    return false;
  }
  if (this.connectedDirectInputChannel == null) {
    return true;
  }
  if (!this.connectedDirectInputChannel.allQueuedRecordsConsumed()) {
    return false;
  }
  return true;
}
