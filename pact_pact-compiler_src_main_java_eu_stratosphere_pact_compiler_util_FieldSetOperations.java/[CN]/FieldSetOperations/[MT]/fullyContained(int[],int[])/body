{
  if (b.length > a.length)   return false;
  int r=0;
  int d=0;
  while (r < a.length && d < b.length) {
    if (a[r] < b[d]) {
      r++;
    }
 else     if (a[r] > b[d]) {
      return false;
    }
 else {
      r++;
      d++;
    }
  }
  if (d == b.length) {
    return true;
  }
 else {
    return false;
  }
}
