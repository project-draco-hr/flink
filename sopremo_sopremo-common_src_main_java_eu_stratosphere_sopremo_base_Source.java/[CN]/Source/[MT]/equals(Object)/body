{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (this.getClass() != obj.getClass())   return false;
  Source other=(Source)obj;
  if (this.type != other.type)   return false;
  if (this.inputName == null) {
    if (other.inputName != null)     return false;
  }
 else   if (!this.inputName.equals(other.inputName))   return false;
  if (this.adhocValue == null) {
    if (other.adhocValue != null)     return false;
  }
 else   if (!this.adhocValue.equals(other.adhocValue))   return false;
  return true;
}
