{
  StringBuilder returnString=new StringBuilder(fromVertex + "|" + toVertex+ "|"+ distance+ "|");
  for (int i=0; i < hopCnt; i++) {
    returnString.append(hopsList[i]);
    if (i + 1 < hopCnt) {
      returnString.append(' ');
    }
  }
  returnString.append('|');
  return returnString.toString();
}
