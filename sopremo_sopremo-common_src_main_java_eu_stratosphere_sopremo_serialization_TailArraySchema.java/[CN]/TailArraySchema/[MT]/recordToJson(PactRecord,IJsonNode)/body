{
  if (this.getTailSize() + 1 != record.getNumFields())   throw new IllegalStateException("Schema does not match to record!");
  if (target == null)   target=new ArrayNode();
 else   ((IArrayNode)target).clear();
  JsonNodeWrapper recordElement;
  ((IArrayNode)target).addAll((IArrayNode)SopremoUtil.unwrap(record.getField(0,JsonNodeWrapper.class)));
  for (int i=1; i <= this.getTailSize(); i++) {
    recordElement=record.getField(i,JsonNodeWrapper.class);
    if (recordElement != null)     ((IArrayNode)target).add(SopremoUtil.unwrap(recordElement));
  }
  return target;
}
