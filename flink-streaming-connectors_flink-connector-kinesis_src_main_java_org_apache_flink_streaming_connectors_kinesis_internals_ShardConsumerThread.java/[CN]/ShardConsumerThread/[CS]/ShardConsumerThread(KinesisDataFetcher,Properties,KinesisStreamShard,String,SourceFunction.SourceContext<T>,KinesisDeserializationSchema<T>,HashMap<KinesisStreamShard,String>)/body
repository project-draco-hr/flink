{
  this.ownerRef=checkNotNull(ownerRef);
  this.assignedShard=checkNotNull(assignedShard);
  this.lastSequenceNum=checkNotNull(lastSequenceNum);
  this.sourceContext=checkNotNull(sourceContext);
  this.deserializer=checkNotNull(deserializer);
  this.seqNoState=checkNotNull(seqNumState);
  this.kinesisProxy=new KinesisProxy(props);
  this.maxNumberOfRecordsPerFetch=Integer.valueOf(props.getProperty(KinesisConfigConstants.CONFIG_SHARD_RECORDS_PER_GET,Integer.toString(KinesisConfigConstants.DEFAULT_SHARD_RECORDS_PER_GET)));
}
