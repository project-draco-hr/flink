{
  TernaryExpression ternaryExpression=new TernaryExpression(new InputSelection(0),new ConstantExpression(TextNode.valueOf("if")),new ConstantExpression(TextNode.valueOf("else")));
  final IJsonNode result=ternaryExpression.evaluate(createArrayNode(BooleanNode.TRUE,BooleanNode.FALSE),null,this.context);
  Assert.assertEquals(TextNode.valueOf("if"),result);
}
