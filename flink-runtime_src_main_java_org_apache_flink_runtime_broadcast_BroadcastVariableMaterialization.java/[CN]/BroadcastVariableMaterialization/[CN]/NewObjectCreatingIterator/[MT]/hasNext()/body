{
  if (next != null) {
    return true;
  }
 else   if (iterator != null) {
    try {
      next=iterator.next(serializer.createInstance());
      if (next != null) {
        return true;
      }
 else {
        iterator=null;
        return false;
      }
    }
 catch (    IOException e) {
      throw new RuntimeException("Error getting element for broadcast variable.",e);
    }
  }
 else {
    return false;
  }
}
