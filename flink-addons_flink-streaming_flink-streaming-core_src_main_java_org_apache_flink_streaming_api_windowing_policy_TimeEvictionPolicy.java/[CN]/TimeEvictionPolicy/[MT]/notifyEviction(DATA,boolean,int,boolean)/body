{
  while (bufferSize < this.buffer.size()) {
    this.buffer.removeFirst();
  }
  int counter=0;
  while (!buffer.isEmpty()) {
    if (timestamp.getTimestamp(buffer.getFirst()) < timestamp.getTimestamp(datapoint) - granularity) {
      buffer.removeFirst();
      counter++;
    }
 else {
      break;
    }
  }
  if (!isFake) {
    buffer.add(datapoint);
  }
  return counter;
}
