{
  if (closure == null) {
    con2node=new HashMap<Operator<?>,OptimizerNode>();
  }
 else {
    con2node=closure;
  }
  this.sinks=new ArrayList<DataSinkNode>(2);
  this.defaultParallelism=defaultParallelism;
  this.parent=parent;
  this.defaultDataExchangeMode=dataExchangeMode;
  this.forceDOP=forceDOP;
}
