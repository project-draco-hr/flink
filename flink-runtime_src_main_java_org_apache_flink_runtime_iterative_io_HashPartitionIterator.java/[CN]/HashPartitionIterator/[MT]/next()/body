{
  if (currentPartition == null) {
    if (!partitions.hasNext()) {
      return null;
    }
    currentPartition=partitions.next();
    currentPartition.setReadPosition(0);
  }
  try {
    return serializer.deserialize(currentPartition);
  }
 catch (  EOFException e) {
    return advanceAndRead();
  }
}
