{
  final Integer priority=Integer.valueOf(executionStateListener.getPriority());
  if (priority.intValue() < 0) {
    LOG.error("Priority for execution state listener " + executionStateListener.getClass() + " must be non-negative.");
    return;
  }
  final ExecutionStateListener previousValue=this.executionStateListeners.putIfAbsent(priority,executionStateListener);
  if (previousValue != null) {
    LOG.error("Cannot register " + executionStateListener.getClass() + " as an execution listener. Priority "+ priority.intValue()+ " is already taken.");
  }
}
