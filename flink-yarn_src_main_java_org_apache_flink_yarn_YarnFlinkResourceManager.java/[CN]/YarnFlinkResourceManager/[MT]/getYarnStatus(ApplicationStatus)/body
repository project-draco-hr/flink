{
  if (status == null) {
    return FinalApplicationStatus.UNDEFINED;
  }
 else {
switch (status) {
case SUCCEEDED:
      return FinalApplicationStatus.SUCCEEDED;
case FAILED:
    return FinalApplicationStatus.FAILED;
case CANCELED:
  return FinalApplicationStatus.KILLED;
default :
return FinalApplicationStatus.UNDEFINED;
}
}
}
