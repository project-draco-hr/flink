{
  int numBytes=in.readInt();
  if (numBytes > 0) {
    bytes=new byte[numBytes];
    in.readFully(bytes);
    numCols=in.readInt() + 1;
    offsets=new short[numCols + 1];
    for (int i=1; i < numCols; i++) {
      offsets[i]=in.readShort();
    }
    offsets[numCols]=(short)numBytes;
  }
 else {
    numCols=0;
  }
}
