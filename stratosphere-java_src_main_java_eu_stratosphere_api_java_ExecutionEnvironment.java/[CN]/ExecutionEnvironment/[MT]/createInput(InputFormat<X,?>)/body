{
  if (inputFormat == null) {
    throw new IllegalArgumentException("InputFormat must not be null.");
  }
  @SuppressWarnings("unchecked") TypeInformation<X> producedType=(inputFormat instanceof Typed) ? (TypeInformation<X>)((Typed)inputFormat).getProducedType() : TypeExtractor.extractInputFormatTypes(inputFormat);
  return createInput(inputFormat,producedType);
}
