{
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  @SuppressWarnings("unused") DataStream<Tuple3<String,Integer,Long>> dataStream=env.addSource(new WindowWordCountSource(),SOURCE_PARALELISM).flatMap(new WindowWordCountSplitter(),PARALELISM).partitionBy(0).flatMap(new WindowWordCountCounter(),PARALELISM).addSink(new WindowWordCountSink());
  env.execute();
}
