{
  EvaluableExpression[] elements=new EvaluableExpression[constants.length];
  for (int index=0; index < elements.length; index++)   if (constants[index] instanceof EvaluableExpression)   elements[index]=(EvaluableExpression)constants[index];
 else   elements[index]=new Constant(constants[index]);
  return new ArrayCreation(elements);
}
