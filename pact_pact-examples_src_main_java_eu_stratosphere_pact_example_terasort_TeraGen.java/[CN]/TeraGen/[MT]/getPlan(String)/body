{
  int noSubTasks=1;
  String dataInput="hdfs://hadoop-dev.local:50040/dummyinput";
  String dataOutput="hdfs://hadoop-dev.local:50040/tsinput";
  DataSourceContract<TeraKey,TeraValue> source=new DataSourceContract<TeraKey,TeraValue>(TeraInputFormat.class,dataInput,"Data Source");
  source.setDegreeOfParallelism(noSubTasks);
  DataSinkContract<TeraKey,TeraValue> sink=new DataSinkContract<TeraKey,TeraValue>(TeraOutputFormat.class,dataOutput);
  sink.setDegreeOfParallelism(noSubTasks);
  sink.setInput(source);
  return new Plan(sink,"TeraSort Generator");
}
