{
  Class<? extends StreamInvokable> userFunctionClass=taskConfiguration.getClass("userfunction",DefaultTaskInvokable.class,StreamInvokable.class);
  StreamInvokable userFunction=null;
  try {
    userFunction=userFunctionClass.newInstance();
    userFunction.declareOutputs(outputs,instanceID,recordBuffer);
  }
 catch (  Exception e) {
  }
  return userFunction;
}
