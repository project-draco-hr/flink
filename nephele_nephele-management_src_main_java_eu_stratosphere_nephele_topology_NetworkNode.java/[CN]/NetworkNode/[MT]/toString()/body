{
  String str;
  if (this.childNodes.isEmpty()) {
    str=this.name;
  }
 else {
    str="[";
    final Iterator<NetworkNode> it=this.childNodes.iterator();
    while (it.hasNext()) {
      str+=it.next().toString();
      if (it.hasNext()) {
        str+=", ";
      }
    }
    str+="]";
  }
  return str;
}
