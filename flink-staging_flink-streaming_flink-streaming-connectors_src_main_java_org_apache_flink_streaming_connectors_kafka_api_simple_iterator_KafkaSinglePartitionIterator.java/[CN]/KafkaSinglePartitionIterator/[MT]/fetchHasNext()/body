{
synchronized (fetchResponse) {
    if (!iter.hasNext()) {
      try {
        resetFetchResponse(readOffset);
      }
 catch (      ClosedChannelException e) {
        if (LOG.isWarnEnabled()) {
          LOG.warn("Got ClosedChannelException, trying to find new leader.");
        }
        findNewLeader();
      }
      return iter.hasNext();
    }
 else {
      return true;
    }
  }
}
