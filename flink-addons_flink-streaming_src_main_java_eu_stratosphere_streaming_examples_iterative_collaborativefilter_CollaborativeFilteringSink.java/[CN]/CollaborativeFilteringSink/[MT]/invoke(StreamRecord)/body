{
  System.out.println("received record...");
  int tupleNum=record.getNumOfTuples();
  System.out.println("============================================");
  for (int i=0; i < tupleNum; ++i) {
    System.out.println("name=" + record.getField(i,0) + ", grade="+ record.getField(i,1)+ ", salary="+ record.getField(i,2));
  }
  System.out.println("============================================");
}
