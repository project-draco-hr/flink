{
  long count=0;
  Tuple t=null;
  while (values.hasNext()) {
    t=values.next();
    count++;
  }
  if (t != null) {
    t.addAttribute("" + count);
  }
  if (LOGGER.isDebugEnabled()) {
    LOGGER.debug("Constructed tuple " + t.toString() + " in "+ context+ "() as result");
  }
  out.collect(key,t);
}
