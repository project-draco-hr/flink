{
  if (ioReader != null) {
    try {
      ioReader.close();
      ioReader.deleteChannel();
    }
 catch (    IOException ioex) {
      throw new RuntimeException();
    }
  }
  memoryManager.release(memorySegments);
  LOG.debug("Iterator closed. Deserialized " + count + " objects in last run.");
}
