{
  TypeInformation<R> outTypeInfo=TypeExtractor.getCrossReturnTypes(function,op.input1.getType(),op.input2.getType());
  CrossWindowFunction<I1,I2,R> crossWindowFunction=new CrossWindowFunction<I1,I2,R>(clean(function));
  return op.input1.connect(op.input2).addGeneralWindowCombine(crossWindowFunction,outTypeInfo,op.windowSize,op.slideInterval,op.timeStamp1,op.timeStamp2);
}
