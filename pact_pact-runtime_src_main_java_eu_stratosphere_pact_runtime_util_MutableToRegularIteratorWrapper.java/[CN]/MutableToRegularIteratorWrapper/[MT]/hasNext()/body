{
  if (this.staged == null) {
    try {
      if (this.source.next(this.instance)) {
        this.staged=this.instance;
        return true;
      }
 else {
        return false;
      }
    }
 catch (    IOException ioex) {
      throw new RuntimeException("Error reading next record: " + ioex.getMessage(),ioex);
    }
  }
 else {
    return true;
  }
}
