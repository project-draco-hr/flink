{
  try {
    resultSet.close();
  }
 catch (  SQLException se) {
    LOG.info("Inputformat couldn't be closed - " + se.getMessage());
  }
catch (  NullPointerException npe) {
  }
  try {
    statement.close();
  }
 catch (  SQLException se) {
    LOG.info("Inputformat couldn't be closed - " + se.getMessage());
  }
