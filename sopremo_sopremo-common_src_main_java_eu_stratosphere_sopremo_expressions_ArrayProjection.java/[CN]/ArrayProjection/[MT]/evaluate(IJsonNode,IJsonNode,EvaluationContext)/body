{
  final IArrayNode array=(IArrayNode)node;
  target=SopremoUtil.reinitializeTarget(target,this.expectedTarget);
  for (int index=0, size=array.size(); index < size; index++)   ((IArrayNode)target).add(this.expression.evaluate(array.get(index),((IArrayNode)target).get(index),context));
  return target;
}
