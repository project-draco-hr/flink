{
switch (connection.getTargetPact().getPactType()) {
case Match:
case Cross:
    break;
default :
  throw new CompilerException("ShipStrategy " + connection.getShipStrategy().name() + " does not suit PACT "+ connection.getTargetPact().getPactType().name());
}
connectJobVertices(connection,inputNumber,outputVertex,outputConfig,inputVertex,inputConfig);
}
