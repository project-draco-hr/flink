{
  if (node instanceof Placeholder)   this.appender.append(String.format(GraphPrinter.this.widthString,((Placeholder)node).toString(GraphPrinter.this.connectorProvider)));
 else {
    String nodeString=GraphPrinter.this.nodePrinter.toString((Node)node);
    if (nodeString.length() > GraphPrinter.this.width)     nodeString=nodeString.substring(0,GraphPrinter.this.width);
    this.appender.append(String.format(GraphPrinter.this.widthString,nodeString));
  }
}
