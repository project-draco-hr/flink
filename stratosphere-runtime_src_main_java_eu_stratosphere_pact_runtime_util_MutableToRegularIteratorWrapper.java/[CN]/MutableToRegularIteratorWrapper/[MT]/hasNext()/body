{
  if (currentIsAvailable) {
    return true;
  }
 else {
    try {
      if (source.next(next)) {
        T tmp=current;
        current=next;
        next=tmp;
        currentIsAvailable=true;
        return true;
      }
 else {
        return false;
      }
    }
 catch (    IOException ioex) {
      throw new RuntimeException("Error reading next record: " + ioex.getMessage(),ioex);
    }
  }
}
