{
  if (!parseParameters(args)) {
    return;
  }
  StreamExecutionEnvironment env=StreamExecutionEnvironment.getExecutionEnvironment();
  DataStream<String> stream=env.addSource(new BasicSource()).groupBy(new KeySelector<String,String>(){
    private static final long serialVersionUID=1L;
    @Override public String getKey(    String value) throws Exception {
      return value;
    }
  }
).window(Count.of(2)).every(Count.of(3),Count.of(5)).reduceGroup(new Concat());
  if (fileOutput) {
    stream.writeAsText(outputPath,1);
  }
 else {
    stream.print();
  }
  env.execute("Multiple Policies Example");
}
