{
  final ArrayList<EvaluationExpression> expressions=new ArrayList<EvaluationExpression>();
  this.transformRecursively(new TransformFunction(){
    @Override public EvaluationExpression call(    final EvaluationExpression evaluationExpression){
      if (predicate.isTrue(evaluationExpression))       expressions.add(evaluationExpression);
      return evaluationExpression;
    }
  }
);
  return expressions;
}
