{
  for (int index=0; index < level; index++)   builder.append(' ');
  builder.append(getClass().getSimpleName()).append(' ');
  toString(builder);
  builder.append('\n');
  if (this instanceof ExpressionParent)   for (  EvaluationExpression child : ((ExpressionParent)this))   child.printAsTree(builder,level + 1);
}
