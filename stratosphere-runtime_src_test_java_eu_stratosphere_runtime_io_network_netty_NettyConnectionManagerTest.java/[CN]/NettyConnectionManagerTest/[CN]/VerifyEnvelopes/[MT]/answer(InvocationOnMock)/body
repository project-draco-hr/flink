{
  Envelope env=(Envelope)invocation.getArguments()[0];
  ChannelID channelId=env.getSource();
  int seqNum=env.getSequenceNumber();
  if (seqNum == 0) {
    Assert.assertNull(String.format("Received envelope from %s before, but current seq num is 0",channelId),this.received.putIfAbsent(channelId,seqNum));
  }
 else {
    Assert.assertTrue(String.format("Received seq num %d from %s, but previous was not %d",seqNum,channelId,seqNum - 1),this.received.replace(channelId,seqNum - 1,seqNum));
  }
  if (seqNum == numToSendPerSubtask - 1) {
    this.latch.countDown();
  }
  return null;
}
