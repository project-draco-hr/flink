{
  if (target == null || !(target instanceof IArrayNode)) {
    final ArrayNode arrayNode=new ArrayNode();
    for (    final EvaluationExpression expression : this.elements)     arrayNode.add(expression.evaluate(node,null,context));
    return arrayNode;
  }
 else {
    ((IArrayNode)target).clear();
    for (    final EvaluationExpression expression : this.elements)     ((IArrayNode)target).add(expression.evaluate(node,null,context));
    return target;
  }
}
