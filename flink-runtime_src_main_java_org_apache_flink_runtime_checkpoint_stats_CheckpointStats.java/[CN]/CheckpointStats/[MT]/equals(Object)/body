{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  CheckpointStats that=(CheckpointStats)o;
  return checkpointId == that.checkpointId && triggerTimestamp == that.triggerTimestamp && duration == that.duration && stateSize == that.stateSize;
}
