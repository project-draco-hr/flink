{
  boolean modified=false;
  Iterator<E> e=iterator();
  findUnmatchedElement:   while (e.hasNext()) {
    E element=e.next();
    Iterator<?> otherIterator=c.iterator();
    while (otherIterator.hasNext())     if (element == otherIterator.next())     continue findUnmatchedElement;
    e.remove();
    modified=true;
  }
  return modified;
}
