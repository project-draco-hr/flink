{
  try {
    final ParameterTool params=ParameterTool.fromArgs(args);
    final String zkConfigFile=params.getRequired("zkConfigFile");
    final int peerId=params.getInt("peerId");
    runFlinkZkQuorumPeer(zkConfigFile,peerId);
  }
 catch (  Throwable t) {
    t.printStackTrace();
    System.exit(-1);
  }
}
