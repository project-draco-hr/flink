{
  Random rnd=new Random(874597969123412341L);
  float rndFloat=rnd.nextFloat() * Float.MAX_VALUE;
  return new Float[]{new Float(0),new Float(1),new Float(-1),new Float(Float.MAX_VALUE),new Float(Float.MIN_VALUE),new Float(rndFloat),new Float(-rndFloat),new Float(Float.NaN),new Float(Float.NEGATIVE_INFINITY),new Float(Float.POSITIVE_INFINITY)};
}
