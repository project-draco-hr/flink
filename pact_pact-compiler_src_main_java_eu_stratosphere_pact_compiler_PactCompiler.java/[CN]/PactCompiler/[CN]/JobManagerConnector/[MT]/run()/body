{
  RPCService rpcService=null;
  try {
    rpcService=new RPCService();
    ExtendedManagementProtocol jobManagerConnection=rpcService.getProxy(this.jobManagerAddress,ExtendedManagementProtocol.class);
    this.instances=jobManagerConnection.getMapOfAvailableInstanceTypes();
    if (this.instances == null) {
      throw new IOException("Returned instance map was <null>");
    }
  }
 catch (  Throwable t) {
    this.error=t;
  }
 finally {
synchronized (this.lock) {
      this.lock.notifyAll();
    }
    if (rpcService != null) {
      rpcService.shutDown();
    }
    rpcService=null;
  }
}
