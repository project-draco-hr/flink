{
  if (isTempingConnection(conn)) {
    return false;
  }
  if (conn.getSourcePact().getOutConns().size() > 1) {
    return true;
  }
  for (  PactConnection inConn : conn.getSourcePact().getIncomingConnections()) {
    if (mayCauseDeadlock(inConn) == true) {
      return true;
    }
  }
  return false;
}
