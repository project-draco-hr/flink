{
  if ((reuse=loadNextRecord()) == null) {
    throw new RuntimeException("DataStream must not be empty");
  }
  while (reuse != null && !state.isFull()) {
    collectOneTimeUnit();
  }
  reduce();
  while (reuse != null) {
    for (int i=0; i < slideInterval / timeUnitInMillis; i++) {
      collectOneTimeUnit();
    }
    reduce();
  }
}
