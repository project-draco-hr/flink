{
  OUT reduced=null;
  if (batchNotFull()) {
    reduced=reuse.getObject();
    resetReuse();
    while (getNextRecord() != null && batchNotFull()) {
      reduced=reducer.reduce(reduced,reuse.getObject());
      resetReuse();
    }
  }
  if (reduced != null) {
    state.pushBack(reduced);
  }
 else {
    state.pushBack(nullElement);
  }
}
