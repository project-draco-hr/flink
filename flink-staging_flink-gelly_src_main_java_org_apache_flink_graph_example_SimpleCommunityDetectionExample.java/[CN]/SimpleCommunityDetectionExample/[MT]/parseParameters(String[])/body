{
  if (args.length > 0) {
    if (args.length != 4) {
      System.err.println("Usage SimpleCommunityDetection <edge path> <output path> " + "<num iterations> <delta>");
      return false;
    }
    fileOutput=true;
    edgeInputPath=args[0];
    outputPath=args[1];
    maxIterations=Integer.parseInt(args[2]);
    delta=Double.parseDouble(args[3]);
  }
 else {
    System.out.println("Executing SimpleCommunityDetection example with default parameters and built-in default data.");
    System.out.println("Provide parameters to read input data from files.");
    System.out.println("Usage SimpleCommunityDetection <edge path> <output path> " + "<num iterations> <delta>");
  }
  return true;
}
