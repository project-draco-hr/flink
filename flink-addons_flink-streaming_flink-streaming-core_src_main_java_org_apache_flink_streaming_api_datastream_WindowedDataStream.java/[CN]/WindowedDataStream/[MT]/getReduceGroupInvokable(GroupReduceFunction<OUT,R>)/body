{
  StreamInvokable<OUT,R> invokable;
  if (isGrouped) {
    invokable=new GroupedWindowInvokable<OUT,R>(reducer,keySelector,getDistributedTriggers(),getDistributedEvicters(),getCentralTriggers(),getCentralEvicters());
  }
 else {
    invokable=new WindowGroupReduceInvokable<OUT,R>(reducer,getTriggers(),getEvicters());
  }
  return invokable;
}
