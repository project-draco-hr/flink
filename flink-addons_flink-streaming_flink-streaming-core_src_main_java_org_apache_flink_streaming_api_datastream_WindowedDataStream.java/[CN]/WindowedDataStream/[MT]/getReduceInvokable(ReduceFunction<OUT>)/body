{
  StreamInvokable<OUT,OUT> invokable;
  if (isGrouped) {
    invokable=new GroupedWindowInvokable<OUT,OUT>(reducer,keySelector,getDistributedTriggers(),getDistributedEvicters(),getCentralTriggers());
  }
 else {
    invokable=new WindowReduceInvokable<OUT>(reducer,getTriggers(),getEvicters());
  }
  return invokable;
}
