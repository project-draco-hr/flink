{
  try {
    return getReceiverList(envelope.getJobID(),envelope.getSource(),reportException);
  }
 catch (  IOException e) {
    releaseEnvelope(envelope);
    throw e;
  }
catch (  CancelTaskException e) {
    releaseEnvelope(envelope);
    throw e;
  }
catch (  Throwable t) {
    releaseEnvelope(envelope);
    ExceptionUtils.rethrow(t,"Error while requesting receiver list.");
    return null;
  }
}
