{
  out.writeInt(size());
  for (  Map.Entry<K,V> entry : entrySet()) {
    final K key=entry.getKey();
    final V value=entry.getValue();
    StringUtils.writeNullableString(key.getClass().getName(),out);
    key.write(out);
    StringUtils.writeNullableString(value.getClass().getName(),out);
    value.write(out);
  }
}
