{
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple2<Boolean,Double[]>> model=env.addSource(new TrainingDataSource(),SOURCE_PARALELISM).map(new PartialModelBuilder(),PARALELISM).broadcast();
  DataStream<Tuple1<Double>> prediction=env.addSource(new NewDataSource(),SOURCE_PARALELISM).connectWith(model).map(new Predictor(),PARALELISM).addSink(new IncOLSSink());
  env.execute();
}
