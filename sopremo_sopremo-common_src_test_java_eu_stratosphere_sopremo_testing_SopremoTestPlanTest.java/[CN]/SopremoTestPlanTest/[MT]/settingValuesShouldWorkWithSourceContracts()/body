{
  CartesianProduct cartesianProduct=new CartesianProduct(null,null);
  SopremoTestPlan testPlan=new SopremoTestPlan(cartesianProduct);
  testPlan.getInputForStream(cartesianProduct.getInput(0)).add(createPactJsonValue("test1")).add(createPactJsonValue("test2"));
  testPlan.getInputForStream(cartesianProduct.getInput(1)).add(createPactJsonValue("test3")).add(createPactJsonValue("test4"));
  testPlan.getExpectedOutputForStream(cartesianProduct.getOutput(0)).add(createPactJsonArray(null,null),createPactJsonArray("test1","test3")).add(createPactJsonArray(null,null),createPactJsonArray("test1","test4")).add(createPactJsonArray(null,null),createPactJsonArray("test2","test3")).add(createPactJsonArray(null,null),createPactJsonArray("test2","test4"));
  testPlan.run();
}
