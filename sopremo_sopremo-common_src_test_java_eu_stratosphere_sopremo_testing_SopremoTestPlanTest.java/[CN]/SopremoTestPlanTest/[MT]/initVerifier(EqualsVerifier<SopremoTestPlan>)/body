{
  super.initVerifier(equalVerifier);
  final ObjectSchema redSchema=new ObjectSchema("redField");
  equalVerifier.withPrefabValues(TestRecords.class,new TestRecords(redSchema.getPactSchema()).add(redSchema.jsonToRecord(JsonUtil.createObjectNode("color","red"),null,null)),new TestRecords(redSchema.getPactSchema()).add(redSchema.jsonToRecord(JsonUtil.createObjectNode("color","black"),null,null))).withPrefabValues(Schema.class,redSchema,new ObjectSchema("blackField")).withPrefabValues(SopremoTestPlan.ExpectedOutput.class,new SopremoTestPlan.ExpectedOutput(0).addValue(0),new SopremoTestPlan.ExpectedOutput(1).addValue(1)).withPrefabValues(SopremoTestPlan.Input.class,new SopremoTestPlan.Input(0).addValue(0),new SopremoTestPlan.Input(1).addValue(1));
}
