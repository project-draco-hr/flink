{
  if (excludeInPath == null) {
    for (    String n : names) {
      if (!name.contains(n)) {
        return false;
      }
    }
    return true;
  }
 else {
    for (    String n : names) {
      if (!name.contains(n)) {
        return false;
      }
    }
    return !dir.toString().contains(excludeInPath);
  }
}
