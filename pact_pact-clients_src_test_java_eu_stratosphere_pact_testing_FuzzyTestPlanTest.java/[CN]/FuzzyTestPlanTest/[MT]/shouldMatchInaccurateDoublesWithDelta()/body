{
  final MapContract map=new MapContract(DoubleTruncatingMap.class,"Map");
  TestPlan testPlan=new TestPlan(map);
  testPlan.setAllowedPactDoubleDelta(0.01,1);
  testPlan.getInput().add(new PactInteger(1),new PactDouble(1.2345)).add(new PactInteger(2),new PactDouble(2.3456));
  testPlan.getExpectedOutput(PactInteger.class,PactDouble.class).add(new PactInteger(1),new PactDouble(1.2345)).add(new PactInteger(2),new PactDouble(2.3456));
  testPlan.run();
}
