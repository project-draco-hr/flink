{
  JobGraph jobGraph;
  if (jobName == null) {
    jobGraph=this.jobGraphBuilder.getJobGraph();
  }
 else {
    jobGraph=this.jobGraphBuilder.getJobGraph(jobName);
  }
  for (  File file : jars) {
    jobGraph.addJar(new Path(file.getAbsolutePath()));
  }
  try {
    client.run(jobGraph,true);
  }
 catch (  Exception e) {
    throw e;
  }
}
