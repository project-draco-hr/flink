{
  final TransferEnvelope ephemeralTransferEnvelope=new TransferEnvelope(this.byteBufferedInputChannel.getID(),this.byteBufferedInputChannel.getConnectedChannelID(),new TransferEnvelopeProcessingLog(true,false));
  ephemeralTransferEnvelope.setSequenceNumber(0);
  ephemeralTransferEnvelope.addEvent(event);
  this.byteBufferedChannelManager.queueOutgoingTransferEnvelope(ephemeralTransferEnvelope);
}
