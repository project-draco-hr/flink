{
  super.open(parameters);
  final TopologyContext topologyContext=StormWrapperSetupHelper.convertToTopologyContext((StreamingRuntimeContext)super.runtimeContext,false);
  OutputCollector stormCollector=null;
  if (this.numberOfAttributes != -1) {
    stormCollector=new OutputCollector(new StormBoltCollector<OUT>(this.numberOfAttributes,super.output));
  }
  this.bolt.prepare(null,topologyContext,stormCollector);
}
