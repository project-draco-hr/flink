{
  int length=readInt();
  ByteBuffer result;
  if (old != null && length <= old.capacity() && old.hasArray()) {
    result=old;
    result.clear();
  }
 else {
    result=ByteBuffer.allocate(length);
  }
  readBytedChecked(result.array(),result.position(),length);
  result.limit(length);
  return result;
}
