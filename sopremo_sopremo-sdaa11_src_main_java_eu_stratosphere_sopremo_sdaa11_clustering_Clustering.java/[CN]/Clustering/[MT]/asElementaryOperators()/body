{
  final SopremoModule module=new SopremoModule(this.getName(),2,4);
  final Operator<?> sampleInput=module.getInput(SAMPLE_INPUT_INDEX);
  final Operator<?> restInput=module.getInput(REST_INPUT_INDEX);
