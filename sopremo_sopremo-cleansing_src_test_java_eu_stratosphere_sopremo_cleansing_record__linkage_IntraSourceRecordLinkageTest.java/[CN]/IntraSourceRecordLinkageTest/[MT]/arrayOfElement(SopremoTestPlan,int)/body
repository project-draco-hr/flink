{
  Object[] array=new JsonNode[ids.length];
  EvaluationExpression resultProjection=this.resultProjection;
  if (resultProjection == null)   resultProjection=EvaluationExpression.VALUE;
  for (int index=0; index < array.length; index++)   array[index]=resultProjection.evaluate(this.inputs.get(ids[index]),testPlan.getEvaluationContext());
  return createArrayNode(array);
}
