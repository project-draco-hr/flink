{
  IntraSourceRecordLinkage recordLinkage=new IntraSourceRecordLinkage(new Naive(),this.similarityFunction,0.7,null);
  recordLinkage.setLinkageMode(mode);
  Projection sortedArrays=new Projection(BuiltinFunctions.SORT.asExpression(),recordLinkage);
  final SopremoTestPlan sopremoTestPlan=new SopremoTestPlan(sortedArrays);
  if (this.useId)   recordLinkage.getRecordLinkageInput().setIdProjection(new ObjectAccess("id"));
  if (this.resultProjection != null)   recordLinkage.getRecordLinkageInput().setResultProjection(this.resultProjection);
  for (  JsonNode object : this.inputs)   sopremoTestPlan.getInput(0).add(object);
  return sopremoTestPlan;
}
