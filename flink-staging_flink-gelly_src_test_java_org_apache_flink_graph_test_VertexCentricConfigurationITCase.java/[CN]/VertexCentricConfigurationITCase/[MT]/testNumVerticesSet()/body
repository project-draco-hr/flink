{
  final ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  Graph<Long,Long,Long> graph=Graph.fromCollection(TestGraphUtils.getLongLongVertices(),TestGraphUtils.getLongLongEdges(),env);
  IterationConfiguration parameters=new IterationConfiguration();
  parameters.setOptNumVertices(true);
  DataSet<Vertex<Long,Long>> verticesWithNumVertices=graph.runVertexCentricIteration(new UpdateFunctionNumVertices(),new DummyMessageFunction(),5,parameters).getVertices();
  verticesWithNumVertices.writeAsCsv(resultPath,"\n","\t");
  env.execute();
  expectedResult="1	5\n" + "2	5\n" + "3	5\n"+ "4	5\n"+ "5	5";
}
