{
  try {
    setNewVertexValue(new Tuple3(vertex.getValue().f0,vertex.getValue().f1,(((VertexWithDegrees)vertex).getInDegree() == vertex.getValue().f0) && (((VertexWithDegrees)vertex).getOutDegree() == vertex.getValue().f1) && vertex.getValue().f2));
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
