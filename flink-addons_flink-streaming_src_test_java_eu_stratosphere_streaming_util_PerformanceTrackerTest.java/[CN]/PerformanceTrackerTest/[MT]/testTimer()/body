{
  PerformanceTimer pT=new PerformanceTimer("timer",true,"");
  pT.startTimer();
  Thread.sleep(100);
  pT.stopTimer();
  System.out.println(pT.values.get(0));
  assertEquals(1,pT.timeStamps.size());
  assertEquals(1,pT.values.size());
  assertTrue(pT.values.get(0) < 200);
  System.out.println(pT);
}
