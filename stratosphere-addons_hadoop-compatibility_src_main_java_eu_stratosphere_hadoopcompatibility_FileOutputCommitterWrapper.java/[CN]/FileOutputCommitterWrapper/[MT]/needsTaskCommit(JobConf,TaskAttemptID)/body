{
  try {
    Path taskOutputPath=getTempTaskOutputPath(conf,taskAttemptID);
    if (taskOutputPath != null) {
      FileSystem fs=taskOutputPath.getFileSystem(conf);
      if (fs.exists(taskOutputPath)) {
        return true;
      }
    }
  }
 catch (  IOException ioe) {
    throw ioe;
  }
  return false;
}
