{
  final Coercer selfCoercer=new Coercer(){
    @Override public JsonNode coerce(    final JsonNode node){
      return node;
    }
  }
;
  for (  final Entry<Class<? extends JsonNode>,Map<Class<? extends JsonNode>,Coercer>> toCoercers : this.coercers.entrySet())   toCoercers.getValue().put(toCoercers.getKey(),selfCoercer);
}
