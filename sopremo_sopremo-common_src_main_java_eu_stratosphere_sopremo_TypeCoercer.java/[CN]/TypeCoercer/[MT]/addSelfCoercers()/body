{
  final Coercer selfCoercer=new Coercer(){
    @Override public IJsonNode coerce(    final IJsonNode node){
      return node;
    }
  }
;
  for (  final Entry<Class<? extends IJsonNode>,Map<Class<? extends IJsonNode>,Coercer>> toCoercers : this.coercers.entrySet())   toCoercers.getValue().put(toCoercers.getKey(),selfCoercer);
}
