{
  if (expression instanceof FunctionCall && ((FunctionCall)expression).getFunction() instanceof AggregationFunction)   aggregatingFunctionCalls.put((FunctionCall)expression,parent);
  if (expression instanceof ExpressionParent)   for (  EvaluationExpression child : ((ExpressionParent)expression))   findAggregatingFunctionCalls(child,aggregatingFunctionCalls,expression);
}
