{
  try {
    return retry(new Callable<V>(){
      public V call() throws Exception {
        byte[] serializedKey=InstantiationUtil.serializeToByteArray(keySerializer,key);
        byte[] serializedVal=dbAdapter.lookupKey(kvStateId,selectStatements.getForKey(key),serializedKey,nextCheckpointId);
        return serializedVal != null ? InstantiationUtil.deserializeFromByteArray(valueSerializer,serializedVal) : null;
      }
    }
,numSqlRetries,sqlRetrySleep);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
