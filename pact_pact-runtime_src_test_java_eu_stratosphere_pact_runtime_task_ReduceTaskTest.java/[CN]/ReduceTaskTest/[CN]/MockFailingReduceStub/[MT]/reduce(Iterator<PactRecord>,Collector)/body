{
  PactRecord element=null;
  int valCnt=0;
  while (records.hasNext()) {
    element=records.next();
    valCnt++;
  }
  if (++cnt >= 10) {
    throw new RuntimeException("Expected Test Exception");
  }
  element.getField(0,key);
  value.setValue(valCnt - key.getValue());
  element.setField(1,value);
  out.collect(element);
}
