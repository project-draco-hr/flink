{
  PactModule module=new PactModule(1,1);
  MapContract<PactNull,PactJsonObject,PactNull,PactJsonObject> projectionMap=new MapContract<PactNull,PactJsonObject,PactNull,PactJsonObject>(ProjectionStub.class);
