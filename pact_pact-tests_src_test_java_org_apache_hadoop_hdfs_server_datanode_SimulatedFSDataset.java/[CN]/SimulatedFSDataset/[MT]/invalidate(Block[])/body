{
  boolean error=false;
  if (invalidBlks == null) {
    return;
  }
  for (  Block b : invalidBlks) {
    if (b == null) {
      continue;
    }
    BInfo binfo=blockMap.get(b);
    if (binfo == null) {
      error=true;
      DataNode.LOG.warn("Invalidate: Missing block");
      continue;
    }
    storage.free(binfo.getlength());
    blockMap.remove(b);
  }
  if (error) {
    throw new IOException("Invalidate: Missing blocks.");
  }
}
