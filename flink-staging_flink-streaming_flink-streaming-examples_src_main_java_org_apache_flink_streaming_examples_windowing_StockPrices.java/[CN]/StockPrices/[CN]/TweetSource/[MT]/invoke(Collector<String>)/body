{
  random=new Random();
  stringBuilder=new StringBuilder();
  while (true) {
    stringBuilder.setLength(0);
    for (int i=0; i < 3; i++) {
      stringBuilder.append(" ");
      stringBuilder.append(SYMBOLS.get(random.nextInt(SYMBOLS.size())));
    }
    collector.collect(stringBuilder.toString());
    Thread.sleep(500);
  }
}
