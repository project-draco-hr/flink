{
  final Iterator<ExecutionVertex> it=new ExecutionGraphIterator(this,true);
  while (it.hasNext()) {
    final ExecutionState s=it.next().getExecutionState();
    if (s != ExecutionState.CREATED && s != ExecutionState.SCHEDULED && s != ExecutionState.ASSIGNING && s != ExecutionState.ASSIGNED && s != ExecutionState.READY) {
      return false;
    }
  }
  return true;
}
