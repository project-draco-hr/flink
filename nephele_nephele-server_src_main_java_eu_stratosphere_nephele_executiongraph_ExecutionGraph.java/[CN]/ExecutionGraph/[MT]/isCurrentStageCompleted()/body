{
  if (this.indexToCurrentExecutionStage >= this.stages.size()) {
    return true;
  }
  final Iterator<ExecutionGroupVertex> it=new ExecutionGroupVertexIterator(this,true,this.indexToCurrentExecutionStage);
  while (it.hasNext()) {
    final ExecutionGroupVertex groupVertex=it.next();
    if (groupVertex.getGroupExecutionState() != ExecutionState.FINISHED) {
      return false;
    }
  }
  return true;
}
