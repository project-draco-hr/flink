{
  addTestAppender(FlinkYarnClient.class,Level.WARN);
  runWithArgs(new String[]{"-j",flinkUberjar.getAbsolutePath(),"-n","1","-jm","512","-tm","1024","-qu","doesntExist"},"to unknown queue: doesntExist",RunTypes.YARN_SESSION);
