{
  String callLocation=Utils.getCallLocationName();
  TypeInformation<Type> inType=input.getType();
  TypeInformation<Result> resultType=TypeExtractor.getUnaryOperatorReturnType(function,KeyedWindowFunction.class,true,true,inType,null,false);
  return createWindowOperator(function,resultType,"KeyedWindowFunction at " + callLocation);
}
