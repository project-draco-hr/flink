{
  final Source source=new Source(PersistenceType.HDFS,getResourcePath("SopremoTestPlan/test.json"));
  final Projection projection=new Projection(EvaluableExpression.SAME_KEY,EvaluableExpression.SAME_VALUE,source);
  final Sink sink=new Sink(PersistenceType.HDFS,File.createTempFile("output",null).toURI().toString(),projection);
  SopremoTestPlan testPlan=new SopremoTestPlan(sink);
  testPlan.run();
  assertEquals("input and output should be equal in identity projection",testPlan.getInput(0),testPlan.getActualOutput(0));
}
