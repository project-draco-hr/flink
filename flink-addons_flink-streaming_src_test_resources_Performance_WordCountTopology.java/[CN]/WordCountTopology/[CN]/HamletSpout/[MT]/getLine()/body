{
  String line;
  try {
    line=br.readLine();
    if (line == null && numberOfHamlets < emitHamlets) {
      numberOfHamlets++;
      br=new BufferedReader(new FileReader(path));
      line=br.readLine();
    }
    return line;
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
