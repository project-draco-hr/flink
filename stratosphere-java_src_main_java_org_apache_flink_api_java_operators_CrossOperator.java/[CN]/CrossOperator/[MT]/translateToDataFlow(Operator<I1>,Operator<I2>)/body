{
  String name=getName() != null ? getName() : function.getClass().getName();
  CrossOperatorBase<I1,I2,OUT,GenericCrosser<I1,I2,OUT>> po=new CrossOperatorBase<I1,I2,OUT,GenericCrosser<I1,I2,OUT>>(function,new BinaryOperatorInformation<I1,I2,OUT>(getInput1Type(),getInput2Type(),getResultType()),name);
  po.setFirstInput(input1);
  po.setSecondInput(input2);
  po.setDegreeOfParallelism(this.getParallelism());
  return po;
}
