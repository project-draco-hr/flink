{
  String s="";
  Entry[] children=h.children;
  if (ht == 0) {
    for (int j=0; j < h.m; j++) {
      s+=indent + children[j].key + " "+ children[j].value+ "\n";
    }
  }
 else {
    for (int j=0; j < h.m; j++) {
      if (j > 0) {
        s+=indent + "(" + children[j].key+ ")\n";
      }
      s+=toString(children[j].next,ht - 1,indent + "     ");
    }
  }
  return s;
}
