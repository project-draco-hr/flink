{
  if (fileOutput) {
    TypeInformation<Sentence> sourceType=TypeExtractor.getForObject(new Sentence(""));
    return env.createInput(new CsvInputFormat<Sentence>(new Path(textPath),CsvInputFormat.DEFAULT_LINE_DELIMITER,CsvInputFormat.DEFAULT_LINE_DELIMITER,sourceType),sourceType);
  }
  return env.fromElements(WordCountDataPojos.SENTENCES);
}
