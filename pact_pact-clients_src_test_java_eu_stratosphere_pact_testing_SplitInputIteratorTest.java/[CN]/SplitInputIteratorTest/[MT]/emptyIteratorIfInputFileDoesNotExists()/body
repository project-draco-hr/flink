{
  String testPlanFile=TestPlan.getTestPlanFile("fileIteratorTest");
  SequentialInputFormat<Key,Value> inputFormat=FormatUtil.createInputFormat(SequentialInputFormat.class,testPlanFile,null);
  SplitInputIterator<Key,Value> inputFileIterator=new SplitInputIterator<Key,Value>(inputFormat);
  AssertUtil.assertIteratorEquals("input file iterator is not empty",Arrays.asList().iterator(),inputFileIterator);
}
