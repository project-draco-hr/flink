def __init__(self, info=None):
    if (info is None):
        self.identifier = (-1)
        self.parent = None
        self.other = None
        self.field = (-1)
        self.order = 0
        self.keys = ()
        self.key1 = ()
        self.key2 = ()
        self.types = None
        self.uses_udf = False
        self.name = None
        self.delimiter_line = '\n'
        self.delimiter_field = ','
        self.write_mode = WriteMode.NO_OVERWRITE
        self.path = ''
        self.count = 0
        self.values = []
        self.projections = []
        self.id = (-1)
        self.to_err = False
        self.parallelism = Value((-1))
        self.parent_set = None
        self.other_set = None
        self.chained_info = None
        self.bcvars = []
        self.sinks = []
        self.children = []
        self.operator = None
    else:
        self.__dict__.update(info.__dict__)
