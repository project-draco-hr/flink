def __init__(self, info=None):
    if (info is None):
        self.parent = None
        self.other = None
        self.identifier = None
        self.field = None
        self.order = None
        self.keys = None
        self.key1 = None
        self.key2 = None
        self.types = None
        self.operator = None
        self.name = None
        self.combine = False
        self.delimiter_line = '\n'
        self.delimiter_field = ','
        self.write_mode = WriteMode.NO_OVERWRITE
        self.sinks = []
        self.children = []
        self.combineop = None
        self.path = None
        self.values = []
        self.projections = []
        self.bcvars = []
        self.id = None
        self.to_err = False
    else:
        self.__dict__.update(info.__dict__)
