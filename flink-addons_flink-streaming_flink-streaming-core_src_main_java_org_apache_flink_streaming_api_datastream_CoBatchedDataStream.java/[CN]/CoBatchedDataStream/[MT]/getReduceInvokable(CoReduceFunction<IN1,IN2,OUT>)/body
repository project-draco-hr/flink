{
  CoBatchReduceInvokable<IN1,IN2,OUT> invokable;
  if (isGrouped) {
    invokable=new CoGroupedBatchReduceInvokable<IN1,IN2,OUT>(coReducer,batchSize1,batchSize2,slideSize1,slideSize2,keyPosition1,keyPosition2);
  }
 else {
    invokable=new CoBatchReduceInvokable<IN1,IN2,OUT>(coReducer,batchSize1,batchSize2,slideSize1,slideSize2);
  }
  return invokable;
}
