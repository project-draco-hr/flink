{
  CoGroupContract<PactJsonObject.Key,PactJsonObject,PactJsonObject,PactJsonObject.Key,PactJsonObject> union=new CoGroupContract<PactJsonObject.Key,PactJsonObject,PactJsonObject,PactJsonObject.Key,PactJsonObject>(TwoInputUnion.class);
