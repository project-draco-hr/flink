{
  if (setKeyExtractors == null)   throw new NullPointerException("setKeyExtractors must not be null");
  if (setKeyExtractors == BAG_SEMANTIC)   this.setKeyExtractors.clear();
 else {
    this.setKeyExtractors.addAll(Arrays.asList(new Path[Math.max(0,getInputs().size() - this.setKeyExtractors.size())]));
    for (    Path path : setKeyExtractors)     this.setKeyExtractors.set(SopremoUtil.getInputIndex(path),path);
    for (int index=0; index < this.setKeyExtractors.size(); index++)     if (this.setKeyExtractors.get(index) == null)     this.setKeyExtractors.set(index,new Path(new Input(index)));
  }
}
