{
  initializeConnection();
  try {
    channel.queueDeclare(QUEUE_NAME,false,false,false,null);
    String message=tuple.f0;
    channel.basicPublish("",QUEUE_NAME,null,message.getBytes());
  }
 catch (  IOException e1) {
    e1.printStackTrace();
  }
  try {
    channel.close();
    connection.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
