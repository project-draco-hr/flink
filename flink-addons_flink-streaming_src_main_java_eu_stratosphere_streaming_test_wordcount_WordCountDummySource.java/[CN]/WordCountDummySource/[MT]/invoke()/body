{
  line="first one";
  lineValue.setValue(line);
  values[0]=lineValue;
  StreamRecord record=new StreamRecord(lineValue);
  System.out.println(record.getNumOfRecords());
  record.copy();
  emit(record);
  line="second two";
  lineValue.setValue(line);
  values[0]=lineValue;
  record.setRecord(0,values);
  emit(record);
}
