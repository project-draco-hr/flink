{
  if (expr instanceof ConstExpr || expr instanceof BindingExpr || expr instanceof EnvExpr|| expr instanceof FieldExpr|| expr.isCompileTimeComputable().maybeNot()) {
    return false;
  }
  JsonValue value=expr.getEnvExpr().getEnv().eval(expr);
  ConstExpr c=new ConstExpr(value);
  expr.replaceInParent(c);
  return true;
}
