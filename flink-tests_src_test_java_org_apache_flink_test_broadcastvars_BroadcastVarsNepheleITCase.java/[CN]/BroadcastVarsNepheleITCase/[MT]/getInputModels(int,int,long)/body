{
  if (numModels < 1 || numModels > 100)   throw new IllegalArgumentException();
  Random r=new Random();
  StringBuilder bld=new StringBuilder(3 * (1 + numDimensions) * numModels);
  for (int i=1; i <= numModels; i++) {
    bld.append(i);
    bld.append(' ');
    r.setSeed(seed + 1000 * i);
    for (int j=1; j <= numDimensions; j++) {
      bld.append(r.nextInt(100));
      bld.append(' ');
    }
    bld.append('\n');
  }
  return bld.toString();
}
