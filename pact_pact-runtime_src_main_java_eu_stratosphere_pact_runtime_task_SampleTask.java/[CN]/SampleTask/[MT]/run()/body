{
  final MutableObjectIterator<PactRecord> input=this.inputs[0];
  final Collector output=this.output;
  final PactRecord record=new PactRecord();
  int counter=0;
  while (this.running && input.next(record)) {
    counter++;
    if ((counter % 10) == 0) {
      output.collect(record);
    }
  }
}
