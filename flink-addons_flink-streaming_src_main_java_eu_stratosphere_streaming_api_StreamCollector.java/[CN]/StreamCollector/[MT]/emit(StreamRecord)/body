{
  counter=0;
  streamRecord.setId(channelID);
  if (outputs == null) {
    System.out.println(streamRecord);
  }
 else {
    for (    RecordWriter<StreamRecord> output : outputs) {
      try {
        output.emit(streamRecord);
        output.flush();
      }
 catch (      Exception e) {
        e.printStackTrace();
        System.out.println("emit fail");
      }
    }
  }
}
