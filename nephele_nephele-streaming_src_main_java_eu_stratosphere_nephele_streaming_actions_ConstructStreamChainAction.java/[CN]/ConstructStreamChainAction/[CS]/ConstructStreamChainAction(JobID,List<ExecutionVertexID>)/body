{
  super(jobID);
  if (vertexIDs == null) {
    throw new IllegalArgumentException("Argument vertexIDs must not be null");
  }
  if (vertexIDs.size() < 2) {
    throw new IllegalArgumentException("Argument vertexIDs must be a list with at least two elements");
  }
  this.vertexIDs.addAll(vertexIDs);
}
