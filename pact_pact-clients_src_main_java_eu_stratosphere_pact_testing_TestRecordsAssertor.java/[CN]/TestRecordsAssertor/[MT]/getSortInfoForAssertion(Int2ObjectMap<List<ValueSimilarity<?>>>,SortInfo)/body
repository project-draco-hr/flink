{
  if (similarityMap.isEmpty())   return sortInfo;
  sortInfo=sortInfo.copy();
  for (  Entry<List<ValueSimilarity<?>>> similarityEntry : similarityMap.int2ObjectEntrySet())   if (similarityEntry.getIntKey() != TestRecords.ALL_VALUES && !similarityEntry.getValue().isEmpty())   sortInfo.remove(similarityEntry.getIntKey());
  return sortInfo;
}
