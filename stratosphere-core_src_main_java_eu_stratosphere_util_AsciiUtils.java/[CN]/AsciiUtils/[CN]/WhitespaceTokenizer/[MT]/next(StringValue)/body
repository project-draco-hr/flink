{
  final char[] data=this.toTokenize.getCharArray();
  final int limit=this.limit;
  int pos=this.pos;
  for (; pos < limit && Character.isWhitespace(data[pos]); pos++)   ;
  if (pos >= limit) {
    this.pos=pos;
    return null;
  }
  final int start=pos;
  for (; pos < limit && !Character.isWhitespace(data[pos]); pos++)   ;
  this.pos=pos;
  reuse.setValue(this.toTokenize,start,pos - start);
  return reuse;
}
