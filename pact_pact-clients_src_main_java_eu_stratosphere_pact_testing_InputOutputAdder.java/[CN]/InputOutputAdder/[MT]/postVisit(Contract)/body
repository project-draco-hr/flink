{
  List<List<Contract>> inputs=ContractUtil.getInputs(contract);
  for (int index=0; index < inputs.size(); index++)   if (inputs.get(index).isEmpty())   inputs.get(index).add(TestPlan.createDefaultSource(String.format("%s-input%d",contract.getName(),index)));
  ContractUtil.setInputs(contract,inputs);
}
