{
  if (contract instanceof FileDataSinkContract<?,?>)   ((FileDataSinkContract<?,?>)contract).setInput(TestPlan.createDefaultSource(contract.getName() + "-input"));
 else   if (contract instanceof SingleInputContract<?,?,?,?>)   ((SingleInputContract<?,?,?,?>)contract).setInput(TestPlan.createDefaultSource(contract.getName() + "-input"));
 else   if (contract instanceof DualInputContract<?,?,?,?,?,?>) {
    if (((DualInputContract<?,?,?,?,?,?>)contract).getFirstInput() == null)     ((DualInputContract<?,?,?,?,?,?>)contract).setFirstInput(TestPlan.createDefaultSource(contract.getName() + "-input1"));
    if (((DualInputContract<?,?,?,?,?,?>)contract).getSecondInput() == null)     ((DualInputContract<?,?,?,?,?,?>)contract).setSecondInput(TestPlan.createDefaultSource(contract.getName() + "-input2"));
  }
}
