{
  if (contract instanceof FileDataSinkContract<?,?>)   ((FileDataSinkContract<?,?>)contract).setInput(TestPlan.createDefaultSource(contract.getName() + "-input"));
 else   if (contract instanceof SingleInputContract<?,?,?,?>)   ((SingleInputContract<?,?,?,?>)contract).addInput(TestPlan.createDefaultSource(contract.getName() + "-input"));
 else   if (contract instanceof DualInputContract<?,?,?,?,?,?>) {
    if (((DualInputContract<?,?,?,?,?,?>)contract).getFirstInputs().size() == 0)     ((DualInputContract<?,?,?,?,?,?>)contract).addFirstInput(TestPlan.createDefaultSource(contract.getName() + "-input1"));
    if (((DualInputContract<?,?,?,?,?,?>)contract).getSecondInputs().size() == 0)     ((DualInputContract<?,?,?,?,?,?>)contract).addSecondInput(TestPlan.createDefaultSource(contract.getName() + "-input2"));
  }
}
