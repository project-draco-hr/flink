{
  ExecutionEnvironment env=ExecutionEnvironment.createLocalEnvironment();
  DataSet<Long> vertices=env.fromElements(1L);
  @SuppressWarnings("unchecked") DataSet<Tuple2<Long,Long>> edges=env.fromElements(new Tuple2<Long,Long>(1l,2l));
  ConnectedComponents.doConnectedComponents(vertices,edges,10).print();
  Plan p=env.createProgramPlan();
  dump(p);
}
