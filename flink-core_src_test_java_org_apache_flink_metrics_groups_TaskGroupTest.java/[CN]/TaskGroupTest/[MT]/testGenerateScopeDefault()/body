{
  MetricRegistry registry=new MetricRegistry(new Configuration());
  TaskMetricGroup operator=new TaskManagerMetricGroup(registry,"host","id").addJob(new JobID(),"job").addTask(new AbstractID(),new AbstractID(),0,"task");
  List<String> scope=operator.generateScope();
  assertEquals(5,scope.size());
  assertEquals("task",scope.get(4));
}
