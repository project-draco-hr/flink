{
  Source source=new Source(createJsonArray(createObject("a",1L,"b",2L),createObject("a",3L,"b",4L)));
  Transformation transformation=new Transformation();
  transformation.addMapping(new ValueAssignment("c",new JsonPath.Arithmetic(createPath("$","a"),ArithmeticOperator.MULTIPLY,createPath("$","b"))));
  assertParseResult(new Projection(transformation,source),"[{a: 1, b: 2}, {a: 3, b: 4}] -> transform { c: $.a * $.b }");
}
