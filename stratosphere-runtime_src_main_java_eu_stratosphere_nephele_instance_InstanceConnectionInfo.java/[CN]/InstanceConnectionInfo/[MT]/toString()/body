{
  String iaString;
  String portsString=" (ipcPort=" + ipcPort + ", dataPort="+ dataPort+ ")";
  if (this.hostName != null) {
    iaString=this.hostName + portsString;
  }
 else {
    iaString=inetAddress.toString();
    iaString=iaString.replace("/","");
    iaString+=portsString;
  }
  return iaString;
}
