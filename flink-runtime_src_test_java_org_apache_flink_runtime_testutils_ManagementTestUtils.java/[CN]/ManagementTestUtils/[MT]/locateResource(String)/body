{
  File file=new File(System.getProperty(USER_DIR_KEY) + File.separator + resourceName);
  if (file.exists()) {
    return file;
  }
  file=new File(System.getProperty(USER_DIR_KEY) + ECLIPSE_PATH_EXTENSION + File.separator+ resourceName);
  if (file.exists()) {
    return file;
  }
  return null;
}
