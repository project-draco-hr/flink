{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment();
  DataStream<Tuple2<Integer,Long>> dataStream=env.addSource(new WindowSumSource(),SOURCE_PARALELISM).map(new WindowSumMultiple(),PARALELISM).flatMap(new WindowSumAggregate(),PARALELISM).addSink(new WindowSumSink());
  env.execute();
}
