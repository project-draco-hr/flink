{
  while (currentIterator != null) {
    if (currentIterator.hasNext()) {
      return true;
    }
 else     if (nextIterator < furtherIterators.size()) {
      currentIterator=furtherIterators.get(nextIterator);
      nextIterator++;
    }
 else {
      currentIterator=null;
    }
  }
  return false;
}
