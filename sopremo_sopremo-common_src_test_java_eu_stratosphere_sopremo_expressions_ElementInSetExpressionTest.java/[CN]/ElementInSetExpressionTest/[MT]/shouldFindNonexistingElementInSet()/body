{
  final IJsonNode result=new ElementInSetExpression(new InputSelection(0),Quantor.EXISTS_NOT_IN,new InputSelection(1)).evaluate(createArrayNode(IntNode.valueOf(2),createArrayNode(IntNode.valueOf(1),IntNode.valueOf(2),IntNode.valueOf(3))),null,this.context);
  Assert.assertEquals(BooleanNode.FALSE,result);
}
