{
  JobManager jobManager;
  try {
    jobManager=initialize(args);
    jobManager.startInfoServer();
  }
 catch (  Exception e) {
    LOG.error(e.getMessage(),e);
    System.exit(FAILURE_RETURN_CODE);
  }
  Object w=new Object();
synchronized (w) {
    try {
      w.wait();
    }
 catch (    InterruptedException e) {
    }
  }
}
