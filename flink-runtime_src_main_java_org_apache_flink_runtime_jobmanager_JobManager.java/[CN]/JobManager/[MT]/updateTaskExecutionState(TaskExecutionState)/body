{
  Preconditions.checkNotNull(executionState);
  final ExecutionGraph eg=this.currentJobs.get(executionState.getJobID());
  if (eg == null) {
    LOG.debug("Orphaned execution task: UpdateTaskExecutionState call cannot find execution graph for ID " + executionState.getJobID() + " to change state to "+ executionState.getExecutionState());
    return false;
  }
  return eg.updateState(executionState);
}
