{
  if (LOG.isDebugEnabled())   LOG.debug(this.taskContext.formatLogString("Preprocessing done, iterator obtained."));
  final KeyGroupedIterator<T> iter=new KeyGroupedIterator<T>(this.input.getIterator(),this.serializerFactory.getSerializer(),this.comparator);
  final GenericGroupReduce<T,?> stub=this.taskContext.getStub();
  final Collector<T> output=this.taskContext.getOutputCollector();
  while (this.running && iter.nextKey()) {
    stub.combine(iter.getValues(),output);
  }
}
