{
  InetSocketAddress serverAddress=this.serverAddress;
  if (serverAddress == null) {
    String address=this.configuration.getString(SopremoConstants.SOPREMO_SERVER_IPC_ADDRESS_KEY,null);
    final int port=this.configuration.getInteger(SopremoConstants.SOPREMO_SERVER_IPC_PORT_KEY,SopremoConstants.DEFAULT_SOPREMO_SERVER_IPC_PORT);
    serverAddress=new InetSocketAddress(address,port);
  }
  try {
    this.executor=RPC.getProxy(SopremoExecutionProtocol.class,serverAddress,NetUtils.getSocketFactory());
  }
 catch (  IOException e) {
    this.dealWithError(progressListener,e,"Error while connecting to the server");
  }
}
