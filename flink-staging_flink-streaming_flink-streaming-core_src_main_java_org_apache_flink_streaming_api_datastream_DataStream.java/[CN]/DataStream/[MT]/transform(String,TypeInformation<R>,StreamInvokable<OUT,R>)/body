{
  DataStream<OUT> inputStream=this.copy();
  @SuppressWarnings({"unchecked","rawtypes"}) SingleOutputStreamOperator<R,?> returnStream=new SingleOutputStreamOperator(environment,operatorName,outTypeInfo,invokable);
  streamGraph.addStreamVertex(returnStream.getId(),invokable,getType(),outTypeInfo,operatorName,returnStream.getParallelism());
  connectGraph(inputStream,returnStream.getId(),0);
  return returnStream;
}
