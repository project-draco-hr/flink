{
  DataStream<OUT> inputStream=this.copy();
  @SuppressWarnings({"unchecked","rawtypes"}) SingleOutputStreamOperator<R,?> returnStream=new SingleOutputStreamOperator(environment,outTypeInfo,operator);
  streamGraph.addOperator(returnStream.getId(),operator,getType(),outTypeInfo,operatorName);
  connectGraph(inputStream,returnStream.getId(),0);
  if (iterationID != null) {
    addIterationSource(returnStream);
  }
  return returnStream;
}
