{
  return new NamedChildIterator("expr1","expr2"){
    @Override protected void set(    int index,    EvaluationExpression childExpression){
      if (index == 0)       ComparativeExpression.this.expr1=childExpression;
 else       ComparativeExpression.this.expr2=childExpression;
    }
    @Override protected EvaluationExpression get(    int index){
      if (index == 0)       return ComparativeExpression.this.expr1;
      return ComparativeExpression.this.expr2;
    }
  }
;
}
