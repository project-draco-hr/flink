{
  streamRecord.setId(channelID);
  for (  RecordWriter<StreamRecord<T>> output : outputs) {
    try {
      output.emit(streamRecord);
    }
 catch (    Exception e) {
      if (LOG.isErrorEnabled()) {
        LOG.error(String.format("Emit failed due to: %s",StringUtils.stringifyException(e)));
      }
    }
  }
}
