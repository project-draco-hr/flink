{
  outputs.add(output);
  for (  String outputName : outputNames) {
    if (outputName != null) {
      if (!outputMap.containsKey(outputName)) {
        outputMap.put(outputName,new ArrayList<RecordWriter<SerializationDelegate<StreamRecord<OUT>>>>());
        outputMap.get(outputName).add(output);
      }
 else {
        if (!outputMap.get(outputName).contains(output)) {
          outputMap.get(outputName).add(output);
        }
      }
    }
  }
}
