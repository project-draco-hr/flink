{
  outputs.add(output);
  if (outputName != null) {
    if (outputMap.containsKey(outputName)) {
      outputMap.get(outputName).add(output);
    }
 else {
      outputMap.put(outputName,new ArrayList<RecordWriter<SerializationDelegate<StreamRecord<T>>>>());
      outputMap.get(outputName).add(output);
    }
  }
}
