{
  final List<AbstractEvent> eventList=envelope.getEventList();
  if (eventList == null) {
    return null;
  }
  final Iterator<AbstractEvent> it=eventList.iterator();
  while (it.hasNext()) {
    final AbstractEvent event=it.next();
    if (event instanceof ByteBufferedChannelCloseEvent) {
      return event;
    }
  }
  return null;
}
