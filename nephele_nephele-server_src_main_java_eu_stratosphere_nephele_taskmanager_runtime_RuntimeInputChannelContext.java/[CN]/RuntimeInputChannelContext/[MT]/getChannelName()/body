{
  final StringBuilder sb=new StringBuilder(this.inputGateContext.getTaskName());
  sb.append(' ');
  sb.append('(');
  sb.append(this.byteBufferedInputChannel.getChannelIndex());
  sb.append(',');
  sb.append(' ');
  sb.append(this.byteBufferedInputChannel.getID());
  sb.append(')');
  return sb.toString();
}
