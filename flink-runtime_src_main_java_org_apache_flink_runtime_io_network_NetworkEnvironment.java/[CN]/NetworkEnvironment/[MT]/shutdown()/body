{
synchronized (lock) {
    if (isShutdown) {
      return;
    }
    try {
      disassociate();
    }
 catch (    Throwable t) {
      LOG.warn("Network services did not shut down properly: " + t.getMessage(),t);
    }
    try {
      networkBufferPool.destroy();
    }
 catch (    Throwable t) {
      LOG.warn("Network buffer pool did not shut down properly: " + t.getMessage(),t);
    }
    isShutdown=true;
  }
}
