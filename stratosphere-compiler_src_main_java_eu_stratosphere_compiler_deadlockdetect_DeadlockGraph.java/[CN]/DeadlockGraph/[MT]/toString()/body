{
  StringBuilder out=new StringBuilder();
  out.append("------------ GRAPH ------------\n");
  for (  DeadlockVertex n : vertices) {
    out.append("Node " + n + "_\n");
    for (    DeadlockEdge a : n.getOutEdges()) {
      out.append("\t->" + a.getDestination() + "\n");
    }
    out.append("\n");
  }
  return out.toString();
}
