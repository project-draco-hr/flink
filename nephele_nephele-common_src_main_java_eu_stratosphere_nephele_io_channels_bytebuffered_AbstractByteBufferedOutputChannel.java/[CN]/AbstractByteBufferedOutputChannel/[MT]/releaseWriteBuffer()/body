{
  if (getCompressionLevel() == CompressionLevel.DYNAMIC_COMPRESSION) {
    this.outputChannelBroker.transferEventToInputChannel(new CompressionEvent(this.compressor.getCurrentInternalCompressionLibraryIndex()));
  }
  this.amountOfDataTransmitted+=this.dataBuffer.size();
  this.outputChannelBroker.releaseWriteBuffer();
  this.dataBuffer=null;
}
