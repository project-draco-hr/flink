{
  VertexCentricIteration<K,Double,Double,Double> iteration=network.createVertexCentricIteration(new VertexRankUpdater<K>(beta),new RankMessenger<K>(),maxIterations);
  iteration.addBroadcastSetForUpdateFunction("numberOfVertices",network.numberOfVertices());
  return network.runVertexCentricIteration(iteration);
}
