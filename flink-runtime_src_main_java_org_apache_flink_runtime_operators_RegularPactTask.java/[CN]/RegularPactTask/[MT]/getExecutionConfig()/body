{
  try {
    ExecutionConfig c=(ExecutionConfig)InstantiationUtil.readObjectFromConfig(getOwningNepheleTask().getJobConfiguration(),ExecutionConfig.CONFIG_KEY,this.getClass().getClassLoader());
    if (c != null) {
      return c;
    }
 else {
      return new ExecutionConfig();
    }
  }
 catch (  Exception e) {
    throw new RuntimeException("Could not load ExecutionConfig from Job Configuration: " + e);
  }
}
