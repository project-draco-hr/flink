{
  final SopremoPlan actualPlan=this.parseScript("$users1 = read 'users1.json';\n" + "$users2 = read 'users2.json';\n" + "$allUsers = union $users1, $users2;\n"+ "write $allUsers to 'allUsers.json';");
  final SopremoPlan expectedPlan=new SopremoPlan();
  final Source users1=new Source("users1.json");
  final Source users2=new Source("users2.json");
  final Union union=new Union().withInputs(users1,users2);
  final Sink output=new Sink("allUsers.json").withInputs(union);
  expectedPlan.setSinks(output);
  assertEquals(expectedPlan,actualPlan);
}
