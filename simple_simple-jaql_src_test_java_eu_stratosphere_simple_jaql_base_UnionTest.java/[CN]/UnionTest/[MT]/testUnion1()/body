{
  SopremoPlan actualPlan=parseScript("$users1 = read 'users1.json';" + "$users2 = read 'users2.json';" + "$allUsers = union $users1, $users2;"+ "write $allUsers to 'allUsers.json';");
  SopremoPlan expectedPlan=new SopremoPlan();
  Source users1=new Source("users1.json");
  Source users2=new Source("users2.json");
  Union union=new Union().withInputs(users1,users2);
  Sink output=new Sink("allUsers.json").withInputs(union);
  expectedPlan.setSinks(output);
  assertEquals(expectedPlan,actualPlan);
}
