{
  for (  T elem : collection) {
    try {
      assertEquals(elem,sourceFunction.next());
    }
 catch (    Exception e) {
      e.printStackTrace();
      fail(e.getMessage());
    }
  }
  try {
    assertTrue(sourceFunction.reachedEnd());
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail(e.getMessage());
  }
}
