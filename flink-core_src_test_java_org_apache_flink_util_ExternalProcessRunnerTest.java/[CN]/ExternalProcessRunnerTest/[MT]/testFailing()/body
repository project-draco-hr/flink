{
  final ExternalProcessRunner runner=new ExternalProcessRunner(Failing.class.getName(),new String[]{});
  int result=runner.run();
  assertEquals(1,result);
  assertEquals(runner.getErrorOutput().toString(),"Exception in thread \"main\" java.lang.RuntimeException: HEHE, I'm failing.\n" + "\tat org.apache.flink.util.ExternalProcessRunnerTest$Failing.main(ExternalProcessRunnerTest.java:94)\n");
}
