{
  final Scrubbing scrubbing=new Scrubbing();
  final SopremoTestPlan sopremoTestPlan=new SopremoTestPlan(scrubbing);
  for (int index=0; index < this.path.size(); index++)   scrubbing.addRule(this.validationRules.get(index),this.path);
  Object[] fields={"stringInsteadOfInteger","12","outsideMonthRange",14,"shouldBeNonNull",null};
  sopremoTestPlan.getInput(0).addObject(fields);
  if (this.expectedObject == ERROR)   sopremoTestPlan.getExpectedOutput(0).setEmpty();
 else   sopremoTestPlan.getExpectedOutput(0).add(this.expectedObject);
  sopremoTestPlan.run();
}
