{
  StreamPartitioner<T> partitioner=null;
  try {
    partitioner=(StreamPartitioner<T>)InstantiationUtil.readObjectFromConfig(this.config,PARTITIONER_OBJECT + output,cl);
  }
 catch (  Exception e) {
    throw new RuntimeException("Partitioner could not be instantiated.");
  }
  if (partitioner != null) {
    return partitioner;
  }
 else {
    return new ShufflePartitioner<T>();
  }
}
