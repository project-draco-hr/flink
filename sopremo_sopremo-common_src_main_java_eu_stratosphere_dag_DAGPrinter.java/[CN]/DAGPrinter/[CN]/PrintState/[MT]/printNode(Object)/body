{
  if (node instanceof Placeholder)   this.appender.append(String.format(DAGPrinter.this.widthString,((Placeholder)node).toString(DAGPrinter.this.connectorProvider)));
 else {
    String nodeString=DAGPrinter.this.nodePrinter.toString((Node)node);
    if (nodeString.length() > DAGPrinter.this.width)     nodeString=nodeString.substring(0,DAGPrinter.this.width);
    this.appender.append(String.format(DAGPrinter.this.widthString,nodeString));
  }
}
