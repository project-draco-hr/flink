{
  EnumSet<Connector> connectorSet=EnumSet.of(connectors[0],connectors);
  if (connectorSet.contains(Connector.TOP_LEFT))   return "/";
  if (connectorSet.contains(Connector.TOP_RIGHT))   return "\\";
  if (connectorSet.contains(Connector.RIGHT_DOWN))   return "/";
  if (connectorSet.contains(Connector.LEFT_DOWN))   return "\\";
  if (connectorSet.contains(Connector.TOP_DOWN))   return "|";
  return "-";
}
