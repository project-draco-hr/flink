{
  try {
    int op=(Integer)this.OpField.get(expr);
    return new Comparison(ConditionParser.this.queryParser.parsePath(expr.child(0)),this.OperatorMapping[op],ConditionParser.this.queryParser.parsePath(expr.child(1)));
  }
 catch (  Exception e) {
    throw new IllegalArgumentException("Cannot parse " + expr,e);
  }
}
