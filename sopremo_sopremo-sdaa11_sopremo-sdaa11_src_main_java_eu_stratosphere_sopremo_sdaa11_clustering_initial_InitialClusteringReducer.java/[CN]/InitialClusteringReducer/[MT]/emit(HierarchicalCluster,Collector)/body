{
  if (cluster.isFinal()) {
    this.clusterName.setValue(cluster.getId());
    this.clusterPoints.clear();
    this.clusterPoints.addAll(Arrays.asList(cluster.getPoints()));
    this.outputRecord.clear();
    this.outputRecord.setField(PactRecordIndices.CLUSTER_ID,this.clusterName);
    this.outputRecord.setField(PactRecordIndices.CLUSTER_POINTS,this.clusterPoints);
    this.outputRecord.setField(PactRecordIndices.CLUSTER_CLUSTROID,cluster.getClustroid());
    out.collect(this.outputRecord);
  }
 else   for (  final HierarchicalCluster child : cluster.getChildren())   this.emit(child,out);
}
