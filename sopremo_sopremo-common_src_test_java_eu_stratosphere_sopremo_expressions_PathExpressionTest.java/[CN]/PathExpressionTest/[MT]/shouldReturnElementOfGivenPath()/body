{
  final IJsonNode result=new PathExpression(new ObjectAccess("glossary"),new ObjectAccess("GlossDiv"),new ObjectAccess("GlossList"),new ObjectAccess("GlossEntry"),new ObjectAccess("GlossDef"),new ObjectAccess("GlossSeeAlso"),new ArrayAccess(1)).evaluate(doc,this.context);
  Assert.assertEquals(TextNode.valueOf("XML"),result);
}
