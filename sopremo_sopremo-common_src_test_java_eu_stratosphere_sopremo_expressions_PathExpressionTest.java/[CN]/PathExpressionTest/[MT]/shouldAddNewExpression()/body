{
  PathExpression expr=new PathExpression(new ObjectAccess("glossary"),new ObjectAccess("GlossDiv"),new ObjectAccess("GlossList"),new ObjectAccess("GlossEntry"));
  expr.add(new ObjectAccess("ID"));
  final JsonNode result=expr.evaluate(doc,this.context);
  Assert.assertEquals(TextNode.valueOf("SGML"),result);
}
