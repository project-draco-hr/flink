{
  final PathExpression expr=new PathExpression(new ObjectAccess("glossary"),new ObjectAccess("GlossDiv"),new ObjectAccess("GlossList"),new ObjectAccess("GlossEntry"));
  expr.add(new ObjectAccess("ID"));
  final IJsonNode result=expr.evaluate(doc,null,this.context);
  Assert.assertEquals(TextNode.valueOf("SGML"),result);
}
