{
  try {
    JsonParser parser=JsonUtil.FACTORY.createJsonParser(new URL(getResourcePath("PathExpressionTest/test.json")));
    parser.setCodec(JsonUtil.OBJECT_MAPPER);
    return parser.readValueAsTree();
  }
 catch (  JsonParseException e) {
    e.printStackTrace();
  }
catch (  JsonProcessingException e) {
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
  return null;
}
