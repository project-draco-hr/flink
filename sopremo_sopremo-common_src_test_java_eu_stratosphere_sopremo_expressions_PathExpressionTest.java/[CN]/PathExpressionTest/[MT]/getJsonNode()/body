{
  try {
    final JsonParser parser=JsonUtil.FACTORY.createJsonParser(new URL(getResourcePath("PathExpressionTest/test.json")));
    parser.setCodec(JsonUtil.OBJECT_MAPPER);
    return parser.readValueAsTree();
  }
 catch (  final JsonParseException e) {
    e.printStackTrace();
  }
catch (  final JsonProcessingException e) {
    e.printStackTrace();
  }
catch (  final IOException e) {
    e.printStackTrace();
  }
  return null;
}
