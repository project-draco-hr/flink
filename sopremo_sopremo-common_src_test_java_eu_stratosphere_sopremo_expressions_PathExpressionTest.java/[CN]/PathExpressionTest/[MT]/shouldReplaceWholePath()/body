{
  EvaluationExpression expr=new PathExpression(new ObjectAccess("glossary"),new ObjectAccess("GlossDiv"),new ObjectAccess("GlossList"),new ObjectAccess("GlossEntry"),new ObjectAccess("ID"));
  expr=expr.replace(new PathExpression(new ObjectAccess("glossary"),new ObjectAccess("GlossDiv"),new ObjectAccess("GlossList"),new ObjectAccess("GlossEntry"),new ObjectAccess("ID")),new PathExpression(new ObjectAccess("glossary"),new ObjectAccess("title")));
  final IJsonNode result=expr.evaluate(doc,null,this.context);
  Assert.assertEquals(TextNode.valueOf("example glossary"),result);
}
