{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (this.getClass() != obj.getClass())   return false;
  final Source other=(Source)obj;
  return Equaler.SafeEquals.equal(this.inputPath,other.inputPath) && Equaler.SafeEquals.equal(this.inputFormat,other.inputFormat) && Equaler.SafeEquals.equal(this.adhocExpression,other.adhocExpression);
}
