{
  TypeInformation<String> outType=TypeExtractor.getForObject("A string");
  GroupedFoldInvokable<Integer,String> invokable1=new GroupedFoldInvokable<Integer,String>(new MyFolder(),new KeySelector<Integer,String>(){
    private static final long serialVersionUID=1L;
