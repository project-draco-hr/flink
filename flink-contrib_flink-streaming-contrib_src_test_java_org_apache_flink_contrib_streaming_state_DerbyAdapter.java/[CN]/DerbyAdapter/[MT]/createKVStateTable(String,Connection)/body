{
  validateStateId(stateId);
  try (Statement smt=con.createStatement()){
    smt.executeUpdate("CREATE TABLE kvstate_" + stateId + " ("+ "timestamp bigint, "+ "k varchar(256) for bit data, "+ "v blob, "+ "PRIMARY KEY (k, timestamp)"+ ")");
  }
 catch (  SQLException se) {
    if (se.getSQLState().equals("X0Y32")) {
    }
 else {
      throw se;
    }
  }
}
