{
  validateStateId(stateId);
  try (Statement smt=con.createStatement()){
    smt.executeUpdate("DELETE FROM kvstate_" + stateId + " t1"+ " WHERE EXISTS"+ " ("+ " 	SELECT * FROM kvstate_"+ stateId+ " t2"+ " 	WHERE t2.k = t1.k"+ "		AND t2.id > t1.id"+ " 	AND t2.id <="+ upperBound+ "		AND t2.id >= "+ lowerBound+ " )");
  }
 }
