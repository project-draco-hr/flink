{
  final SopremoTestPlan sopremoPlan=new SopremoTestPlan(2,1);
  sopremoPlan.getOutputOperator(0).setInputs(new ContextualProjection().withInputs(sopremoPlan.getInputOperators(0,2)));
  sopremoPlan.getInput(0).addObject("a",1,"b",4).addObject("a",2,"b",5).addObject("a",-1,"b",4);
  sopremoPlan.getInput(1).addValue(42);
  sopremoPlan.getExpectedOutput(0).addObject("a",1,"b",4,"context",42).addObject("a",2,"b",5,"context",42).addObject("a",-1,"b",4,"context",42);
  sopremoPlan.run();
}
