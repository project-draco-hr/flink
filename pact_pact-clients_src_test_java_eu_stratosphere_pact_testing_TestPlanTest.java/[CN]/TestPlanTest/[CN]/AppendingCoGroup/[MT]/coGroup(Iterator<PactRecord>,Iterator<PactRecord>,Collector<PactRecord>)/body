{
  StringList values=new StringList();
  PactRecord lastRecord=null;
  while (records1.hasNext())   values.add(new PactString((lastRecord=records1.next()).getField(1,PactString.class)));
  while (records2.hasNext())   values.add(new PactString((lastRecord=records2.next()).getField(1,PactString.class)));
  out.collect(new PactRecord(lastRecord.getField(0,PactInteger.class),values));
}
