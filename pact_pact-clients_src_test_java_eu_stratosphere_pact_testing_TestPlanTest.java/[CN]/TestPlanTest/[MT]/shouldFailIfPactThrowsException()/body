{
  final MapContract map=new MapContract.Builder(ErroneousPact.class).name("Map").build();
  TestPlan testPlan=new TestPlan(map);
  testPlan.getInput().add(new PactInteger(1),new PactString("test1")).add(new PactInteger(2),new PactString("test2"));
  testPlan.getExpectedOutput(PactInteger.class,PactString.class).setEmpty();
  assertTestRunFails(testPlan);
}
