{
  final FileDataSourceContract<PactLong,PactJsonObject> read=createInput(JsonInputFormat.class,"TestPlan/test.json");
  final MapContract<Key,Value,Key,Value> map=new MapContract<Key,Value,Key,Value>(IdentityMap.class,"Map");
  map.setInput(read);
  FileDataSinkContract<Key,Value> output=createOutput(map,SequentialOutputFormat.class);
  TestPlan testPlan=new TestPlan(output);
  testPlan.run();
  assertEquals("input and output should be equal in identity map",testPlan.getInput(),testPlan.getActualOutput());
}
