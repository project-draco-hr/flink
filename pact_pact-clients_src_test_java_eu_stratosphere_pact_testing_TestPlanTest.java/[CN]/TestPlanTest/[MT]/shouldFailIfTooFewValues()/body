{
  final MapContract map=new MapContract.Builder(IdentityMap.class).name("Map").build();
  TestPlan testPlan=new TestPlan(map);
  testPlan.getInput().add(new PactInteger(1),new PactString("test1")).add(new PactInteger(2),new PactString("test2"));
  testPlan.getExpectedOutput(PactInteger.class,PactString.class).add(new PactInteger(1),new PactString("test1")).add(new PactInteger(2),new PactString("test2")).add(new PactInteger(3),new PactString("test3"));
  assertTestRunFails(testPlan);
}
