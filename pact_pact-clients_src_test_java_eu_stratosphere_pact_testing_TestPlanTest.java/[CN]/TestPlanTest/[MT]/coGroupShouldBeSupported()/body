{
  CoGroupContract crossContract=new CoGroupContract.Builder(AppendingCoGroup.class,PactInteger.class,0,0).build();
  TestPlan testPlan=new TestPlan(crossContract);
  testPlan.getInput(0).add(new PactInteger(1),new PactString("test1")).add(new PactInteger(1),new PactString("test2")).add(new PactInteger(2),new PactString("test3"));
  testPlan.getInput(1).add(new PactInteger(1),new PactString("test4")).add(new PactInteger(3),new PactString("test5"));
  testPlan.getExpectedOutput(PactInteger.class,StringList.class).add(new PactInteger(1),new StringList("test1","test2","test4")).add(new PactInteger(2),new StringList("test3")).add(new PactInteger(3),new StringList("test5"));
  testPlan.run();
}
