{
  final FileDataSource read=createInput(IntegerInFormat.class,"TestPlan/test.txt");
  final MapContract map=new MapContract(IdentityMap.class,"Map");
  map.setInput(read);
  FileDataSink output=createOutput(map,RecordOutputFormat.class);
  output.getParameters().setInteger(RecordOutputFormat.NUM_FIELDS_PARAMETER,1);
  output.getParameters().setClass(RecordOutputFormat.FIELD_TYPE_PARAMETER_PREFIX + 0,PactInteger.class);
  TestPlan testPlan=new TestPlan(output);
  testPlan.getExpectedOutput(output,IntStringPair).fromFile(IntegerInFormat.class,getResourcePath("TestPlan/test.txt"));
  testPlan.run();
}
