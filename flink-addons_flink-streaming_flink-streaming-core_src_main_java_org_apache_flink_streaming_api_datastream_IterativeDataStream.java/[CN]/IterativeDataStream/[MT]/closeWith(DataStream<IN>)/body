{
  DataStream<IN> iterationSink=new DataStreamSink<IN>(environment,"iterationSink",null,null);
  streamGraph.addIterationTail(iterationSink.getId(),iterationTail.getId(),iterationID,iterationTail.getParallelism(),waitTime);
  streamGraph.setIterationSourceSettings(iterationID.toString(),iterationTail.getId());
  connectGraph(iterationTail.forward(),iterationSink.getId(),0);
  return iterationTail;
}
