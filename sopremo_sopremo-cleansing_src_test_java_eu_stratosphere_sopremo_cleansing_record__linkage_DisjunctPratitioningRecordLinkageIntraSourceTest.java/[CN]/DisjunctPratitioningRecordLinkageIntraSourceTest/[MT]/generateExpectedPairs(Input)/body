{
  for (  final KeyValuePair<JsonNode,JsonNode> left : input) {
    boolean skipPairs=true;
    for (    final KeyValuePair<JsonNode,JsonNode> right : input) {
      if (left == right) {
        skipPairs=false;
        continue;
      }
 else       if (skipPairs)       continue;
      boolean inSameBlockingBin=false;
      for (int index=0; index < this.blockingKeys.length && !inSameBlockingBin; index++)       if (this.blockingKeys[index].evaluate(left.getValue(),getContext()).equals(this.blockingKeys[index].evaluate(right.getValue(),getContext())))       inSameBlockingBin=true;
      if (inSameBlockingBin)       emitCandidate(left,right);
    }
  }
}
