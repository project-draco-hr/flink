{
  EvaluationExpression[] projections={null,getAggregativeProjection()};
  double[] thresholds={0.0,0.5,1.0};
  boolean[] useIds={false,true};
  ArrayList<Object[]> parameters=new ArrayList<Object[]>();
  for (  EvaluationExpression projection : projections)   for (  double threshold : thresholds)   for (  String[][] combinedBlockingKeys : CombinedBlockingKeys)   for (  boolean useId : useIds)   parameters.add(new Object[]{threshold,projection,useId,combinedBlockingKeys});
  return parameters;
}
