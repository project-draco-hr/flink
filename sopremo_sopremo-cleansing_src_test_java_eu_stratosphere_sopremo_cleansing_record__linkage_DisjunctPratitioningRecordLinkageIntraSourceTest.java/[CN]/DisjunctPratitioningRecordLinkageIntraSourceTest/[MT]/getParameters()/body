{
  final EvaluationExpression[] projections={null,getAggregativeProjection()};
  final boolean[] useIds={false,true};
  final ArrayList<Object[]> parameters=new ArrayList<Object[]>();
  for (  final EvaluationExpression projection : projections)   for (  final String[][] combinedBlockingKeys : CombinedBlockingKeys)   for (  final boolean useId : useIds)   parameters.add(new Object[]{projection,useId,combinedBlockingKeys});
  return parameters;
}
