{
  InetSocketAddress serverAddress=this.jobManagerAddress;
  if (serverAddress == null) {
    final String address=this.configuration.getString(ConfigConstants.JOB_MANAGER_IPC_ADDRESS_KEY,"localhost");
    final int port=this.configuration.getInteger(ConfigConstants.JOB_MANAGER_IPC_PORT_KEY,ConfigConstants.DEFAULT_JOB_MANAGER_IPC_PORT);
    serverAddress=new InetSocketAddress(address,port);
  }
  return serverAddress;
}
