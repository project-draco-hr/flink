{
  try {
    int[] partitions={4,52,17,1};
    for (int i=0; i < partitions.length; i++) {
      List<TopicPartition> parts=FlinkKafkaConsumer.assignPartitions(partitions,"test-topic",partitions.length,i);
      assertNotNull(parts);
      assertEquals(1,parts.size());
      assertTrue(contains(partitions,parts.get(0).partition()));
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail(e.getMessage());
  }
}
