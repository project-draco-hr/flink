{
  List<Integer> inputs=new ArrayList<Integer>();
  for (Integer i=1; i <= 10; i++) {
    inputs.add(i);
  }
  List<String> inputs2=new ArrayList<String>();
  inputs2.add("a");
  inputs2.add("b");
  inputs2.add("c");
  inputs2.add("d");
  inputs2.add("e");
  inputs2.add("f");
  inputs2.add("g");
  inputs2.add("h");
  inputs2.add("i");
  CoBatchReduceInvokable<Integer,String,String> invokable=new CoBatchReduceInvokable<Integer,String,String>(new MyCoReduceFunction(),4L,3L,2L,2L);
  List<String> expected=new ArrayList<String>();
  expected.add("10");
  expected.add("18");
  expected.add("26");
  expected.add("34");
  expected.add("abc");
  expected.add("cde");
  expected.add("efg");
  expected.add("ghi");
  List<String> result=MockCoContext.createAndExecute(invokable,inputs,inputs2);
  Collections.sort(result);
  Collections.sort(expected);
  assertEquals(expected,result);
}
