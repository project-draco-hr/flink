{
  List<Integer> inputs=new ArrayList<Integer>();
  for (Integer i=1; i <= 10; i++) {
    inputs.add(i);
  }
  List<Integer> inputs2=new ArrayList<Integer>();
  inputs2.add(1);
  inputs2.add(2);
  inputs2.add(-1);
  inputs2.add(-3);
  inputs2.add(-4);
  CoBatchReduceInvokable<Integer,Integer,String> invokable=new CoBatchReduceInvokable<Integer,Integer,String>(new MyCoReduceFunction(),3L,3L,2L,2L);
  List<String> expected=new ArrayList<String>();
  expected.add("6");
  expected.add("12");
  expected.add("18");
  expected.add("24");
  expected.add("19");
  expected.add("2");
  expected.add("-8");
  expected.add("-4");
  List<String> result=MockCoInvokable.createAndExecute(invokable,inputs,inputs2);
  Collections.sort(result);
  Collections.sort(expected);
  assertEquals(expected,result);
}
