{
  workerIndex=in.readInt();
  boolean hasAggregate=in.readBoolean();
  if (hasAggregate) {
    String classname=in.readUTF();
    try {
      aggregate=Class.forName(classname).asSubclass(Value.class).newInstance();
    }
 catch (    Exception e) {
      throw new IOException(e);
    }
    aggregate.read(in);
  }
 else {
    aggregate=null;
  }
}
