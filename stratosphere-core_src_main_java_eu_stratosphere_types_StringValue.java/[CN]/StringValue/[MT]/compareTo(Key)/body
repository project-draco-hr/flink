{
  if (o instanceof StringValue) {
    StringValue other=(StringValue)o;
    int len1=this.len;
    int len2=other.len;
    int n=Math.min(len1,len2);
    char[] v1=value;
    char[] v2=other.value;
    for (int k=0; k < n; k++) {
      char c1=v1[k];
      char c2=v2[k];
      if (c1 != c2) {
        return c1 - c2;
      }
    }
    return len1 - len2;
  }
 else {
    throw new ClassCastException("Cannot compare StringValue to " + o.getClass().getName());
  }
}
