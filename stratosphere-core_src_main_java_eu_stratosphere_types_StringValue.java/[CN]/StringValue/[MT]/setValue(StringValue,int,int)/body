{
  if (value == null)   throw new NullPointerException();
  if (offset < 0 || len < 0 || offset > value.len - len)   throw new IndexOutOfBoundsException("offset: " + offset + " len: "+ len+ " value.len: "+ value.len);
  ensureSize(len);
  this.len=len;
  System.arraycopy(value.value,offset,this.value,0,len);
  this.hashCode=0;
}
