{
  TypeSerializerWrapper<OUT> outTypeWrapper=new FunctionTypeWrapper<OUT>(function,SourceFunction.class,0);
  DataStreamSource<OUT> returnStream=new DataStreamSource<OUT>(this,"source",outTypeWrapper);
  try {
    jobGraphBuilder.addStreamVertex(returnStream.getId(),new SourceInvokable<OUT>(function),null,outTypeWrapper,"source",SerializationUtils.serialize(function),parallelism);
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize SourceFunction");
  }
  return returnStream;
}
