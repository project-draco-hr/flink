{
  TypeInformation<OUT> outTypeInfo=TypeExtractor.createTypeInfo(SourceFunction.class,function.getClass(),0,null,null);
  DataStreamSource<OUT> returnStream=new DataStreamSource<OUT>(this,"source",outTypeInfo);
  try {
    jobGraphBuilder.addSourceVertex(returnStream.getId(),function,null,outTypeInfo,"source",SerializationUtils.serialize(function),getDegreeOfParallelism());
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize SourceFunction");
  }
  return returnStream;
}
