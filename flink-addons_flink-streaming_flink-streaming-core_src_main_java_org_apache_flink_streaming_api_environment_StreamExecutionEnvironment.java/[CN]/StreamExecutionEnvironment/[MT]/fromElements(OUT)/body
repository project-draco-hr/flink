{
  DataStreamSource<OUT> returnStream=new DataStreamSource<OUT>(this,"elements");
  try {
    SourceFunction<OUT> function=new FromElementsFunction<OUT>(data);
    jobGraphBuilder.addSource(returnStream.getId(),new SourceInvokable<OUT>(function),new ObjectTypeWrapper<OUT,Tuple,OUT>(data[0],null,data[0]),"source",SerializationUtils.serialize(function),1);
  }
 catch (  SerializationException e) {
    throw new RuntimeException("Cannot serialize elements");
  }
  return returnStream;
}
