{
  if (!repeatKey) {
    key.setValue(keyCnt++);
    value.setValue(valCnt);
    if (keyCnt == numKeys) {
      keyCnt=0;
      valCnt++;
    }
  }
 else {
    key.setValue(keyCnt);
    value.setValue(valCnt++);
    if (valCnt == numVals) {
      valCnt=0;
      keyCnt++;
    }
  }
  this.record.setField(0,this.key);
  this.record.setField(1,this.value);
  return this.record;
}
