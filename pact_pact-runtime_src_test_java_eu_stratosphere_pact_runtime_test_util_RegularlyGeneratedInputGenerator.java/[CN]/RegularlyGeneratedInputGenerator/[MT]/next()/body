{
  if (!repeatKey) {
    PactInteger key=new PactInteger(keyCnt++);
    PactInteger val=new PactInteger(valCnt);
    if (keyCnt == numKeys) {
      keyCnt=0;
      valCnt++;
    }
    return new KeyValuePair<PactInteger,PactInteger>(key,val);
  }
 else {
    PactInteger key=new PactInteger(keyCnt);
    PactInteger val=new PactInteger(valCnt++);
    if (valCnt == numVals) {
      valCnt=0;
      keyCnt++;
    }
    return new KeyValuePair<PactInteger,PactInteger>(key,val);
  }
}
