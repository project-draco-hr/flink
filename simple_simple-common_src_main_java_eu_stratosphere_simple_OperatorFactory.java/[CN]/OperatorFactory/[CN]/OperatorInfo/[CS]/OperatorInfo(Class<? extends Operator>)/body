{
  this.operatorClass=operatorClass;
  try {
    PropertyDescriptor[] propertyDescriptors=Introspector.getBeanInfo(operatorClass).getPropertyDescriptors();
    for (    PropertyDescriptor propertyDescriptor : propertyDescriptors)     if (propertyDescriptor.getWriteMethod() != null)     options.put(propertyDescriptor.getName(),propertyDescriptor);
  }
 catch (  IntrospectionException e) {
    e.printStackTrace();
  }
}
