{
  String name;
  Name nameAnnotation=ReflectUtil.getAnnotation(operatorClass,Name.class);
  if (nameAnnotation != null)   name=operatorNameChooser.choose(nameAnnotation.noun(),nameAnnotation.verb(),nameAnnotation.adjective(),nameAnnotation.preposition());
 else   name=operatorClass.getSimpleName().toLowerCase();
  operators.put(name,new OperatorInfo(operatorClass));
}
