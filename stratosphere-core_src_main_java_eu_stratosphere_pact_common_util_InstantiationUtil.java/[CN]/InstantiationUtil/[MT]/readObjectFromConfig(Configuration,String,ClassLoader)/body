{
  byte[] bytes=config.getBytes(key,null);
  if (bytes == null) {
    return null;
  }
  ObjectInputStream oois=null;
  try {
    oois=new ClassLoaderObjectInputStream(new ByteArrayInputStream(bytes),cl);
    return oois.readObject();
  }
  finally {
    if (oois != null) {
      oois.close();
    }
  }
}
