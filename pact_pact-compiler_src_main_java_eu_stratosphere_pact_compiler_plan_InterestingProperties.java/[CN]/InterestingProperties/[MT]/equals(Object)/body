{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  InterestingProperties other=(InterestingProperties)obj;
  if (globalProps == null) {
    if (other.globalProps != null)     return false;
  }
 else   if (!globalProps.equals(other.globalProps))   return false;
  if (localProps == null) {
    if (other.localProps != null)     return false;
  }
 else   if (!localProps.equals(other.localProps))   return false;
  if (maximalCosts == null) {
    if (other.maximalCosts != null)     return false;
  }
 else   if (!maximalCosts.equals(other.maximalCosts))   return false;
  return true;
}
