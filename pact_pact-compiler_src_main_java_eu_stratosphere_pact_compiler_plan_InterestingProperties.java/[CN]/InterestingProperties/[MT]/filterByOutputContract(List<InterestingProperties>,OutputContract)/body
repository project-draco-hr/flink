{
  if (contract == OutputContract.None) {
    return new ArrayList<InterestingProperties>();
  }
 else {
    List<InterestingProperties> preserved=new ArrayList<InterestingProperties>();
    for (    InterestingProperties p : props) {
      boolean nonTrivial=p.getGlobalProperties().getPreservedAfterContract(contract);
      nonTrivial|=p.getLocalProperties().getPreservedAfterContract(contract);
      if (nonTrivial) {
        preserved.add(p);
      }
    }
    return preserved;
  }
}
