{
  Path local=new Path(getLocalCheckpointPath() + Path.SEPARATOR + METADATA_PREFIX+ "_"+ vertexID+ "_0");
  final FileSystem localFs=local.getFileSystem();
  if (localFs.exists(local)) {
    return true;
  }
  local=new Path(getLocalCheckpointPath() + Path.SEPARATOR + METADATA_PREFIX+ "_"+ vertexID+ "_part");
  return localFs.exists(local);
}
