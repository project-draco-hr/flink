{
  Path p=pathPrefix.suffix(COMPLETED_CHECKPOINT_SUFFIX);
  FileSystem fs=p.getFileSystem();
  if (fs.exists(p)) {
    fs.delete(p,false);
    return;
  }
  p=pathPrefix.suffix("_0");
  if (fs.exists(p)) {
    fs.delete(p,false);
  }
  p=pathPrefix.suffix("_part");
  if (fs.exists(p)) {
    fs.delete(p,false);
  }
}
