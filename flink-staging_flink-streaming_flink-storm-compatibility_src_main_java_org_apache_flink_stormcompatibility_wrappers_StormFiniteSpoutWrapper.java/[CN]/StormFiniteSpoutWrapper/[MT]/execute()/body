{
  if (this.numberOfInvocations >= 0) {
    while ((--this.numberOfInvocations >= 0) && super.isRunning) {
      super.spout.nextTuple();
    }
  }
 else {
    do {
      super.collector.tupleEmitted=false;
      super.spout.nextTuple();
    }
 while (super.collector.tupleEmitted && super.isRunning);
  }
}
