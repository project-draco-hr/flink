{
  if (fileOutput) {
    return env.readCsvFile(vertexInputPath).fieldDelimiter(" ").lineDelimiter("\n").types(Long.class,Long.class).map(new MapFunction<Tuple2<Long,Long>,Vertex<Long,Long>>(){
      @Override public Vertex<Long,Long> map(      Tuple2<Long,Long> value) throws Exception {
        return new Vertex<Long,Long>(value.f0,value.f1);
      }
    }
);
  }
  return env.generateSequence(1,numVertices).map(new MapFunction<Long,Vertex<Long,Long>>(){
    public Vertex<Long,Long> map(    Long l) throws Exception {
      return new Vertex<Long,Long>(l,l);
    }
  }
);
}
