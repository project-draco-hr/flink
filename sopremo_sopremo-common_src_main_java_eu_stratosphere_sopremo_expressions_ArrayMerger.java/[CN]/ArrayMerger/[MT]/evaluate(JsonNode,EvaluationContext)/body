{
  final Iterator<JsonNode> arrays=((ArrayNode)node).iterator();
  final ArrayNode mergedArray=new ArrayNode();
  JsonNode nextNode;
  while (arrays.hasNext())   if ((nextNode=arrays.next()) != NullNode.getInstance()) {
    final ArrayNode array=(ArrayNode)nextNode;
    for (int index=0; index < array.size(); index++)     if (mergedArray.size() <= index)     mergedArray.add(array.get(index));
 else     if (this.isNull(mergedArray.get(index)) && !this.isNull(array.get(index)))     mergedArray.set(index,array.get(index));
  }
  return mergedArray;
}
