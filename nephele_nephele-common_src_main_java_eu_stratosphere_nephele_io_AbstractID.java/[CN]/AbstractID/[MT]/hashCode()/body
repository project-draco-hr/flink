{
  int hashCode=0;
  if (this.bytes == null) {
    return 0;
  }
  int tmp=0;
  for (int i=0; i < this.bytes.length; i++) {
    final int shift=(Integer.SIZE - 1 - (i % Integer.SIZE)) * 8;
    tmp+=(this.bytes[i] & 0x000000FF) << shift;
    if ((i % Integer.SIZE) == 0) {
      hashCode=hashCode ^ tmp;
      tmp=0;
    }
  }
  return hashCode;
}
