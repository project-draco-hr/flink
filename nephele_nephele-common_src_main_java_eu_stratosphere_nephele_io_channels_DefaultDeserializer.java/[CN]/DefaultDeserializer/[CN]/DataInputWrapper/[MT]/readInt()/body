{
  if (this.position < this.limit - 3) {
    int num=((this.source[this.position + 0] & 0xff) << 24) | ((this.source[this.position + 1] & 0xff) << 16) | ((this.source[this.position + 2] & 0xff) << 8)| ((this.source[this.position + 3] & 0xff));
    this.position+=2;
    return num;
  }
 else {
    throw new EOFException();
  }
}
