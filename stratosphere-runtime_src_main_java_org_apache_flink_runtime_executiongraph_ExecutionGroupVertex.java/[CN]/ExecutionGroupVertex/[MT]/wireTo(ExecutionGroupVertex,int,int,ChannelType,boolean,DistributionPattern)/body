{
  try {
    final ExecutionGroupEdge previousEdge=this.forwardLinks.get(indexOfOutputGate);
    if (previousEdge != null) {
      throw new GraphConversionException("Output gate " + indexOfOutputGate + " of"+ getName()+ " already has an outgoing edge");
    }
  }
 catch (  ArrayIndexOutOfBoundsException e) {
  }
  final ExecutionGroupEdge edge=new ExecutionGroupEdge(this,indexOfOutputGate,groupVertex,indexOfInputGate,channelType,userDefinedChannelType,distributionPattern);
  this.forwardLinks.add(edge);
  groupVertex.wireBackLink(edge);
  return edge;
}
