{
  final EvaluationExpression[][] projections={new EvaluationExpression[2],new EvaluationExpression[]{getAggregativeProjection1(),getAggregativeProjection2()}};
  final boolean[] useIds={true,false};
  final ArrayList<Object[]> parameters=new ArrayList<Object[]>();
  for (  final EvaluationExpression[] projection : projections)   for (  final boolean useId : useIds)   parameters.add(new Object[]{projection,useId});
  return parameters;
}
