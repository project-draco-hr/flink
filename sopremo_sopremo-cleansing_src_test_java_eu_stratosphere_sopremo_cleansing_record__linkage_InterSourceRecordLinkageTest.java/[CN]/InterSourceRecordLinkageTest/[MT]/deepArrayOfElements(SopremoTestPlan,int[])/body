{
  Object[][] array=new JsonNode[ids.length][];
  for (int sourceIndex=0; sourceIndex < array.length; sourceIndex++) {
    array[sourceIndex]=new JsonNode[ids[sourceIndex].length];
    EvaluationExpression resultProjection=this.resultProjections[sourceIndex];
    if (resultProjection == null)     resultProjection=EvaluationExpression.VALUE;
    for (int tupleIndex=0; tupleIndex < array[sourceIndex].length; tupleIndex++)     array[sourceIndex][tupleIndex]=resultProjection.evaluate(this.findTuple(testPlan,sourceIndex,ids[sourceIndex][tupleIndex]),testPlan.getEvaluationContext());
  }
  return createArrayNode((Object[])array);
}
