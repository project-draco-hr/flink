{
  Object[] array=new JsonNode[ids.length];
  for (int index=0; index < array.length; index++) {
    EvaluationExpression resultProjection=this.resultProjections[index];
    if (resultProjection == null)     resultProjection=EvaluationExpression.VALUE;
    array[index]=resultProjection.evaluate(this.findTuple(testPlan,index,ids[index]),testPlan.getEvaluationContext());
  }
  Arrays.sort(array);
  return createArrayNode(array);
}
