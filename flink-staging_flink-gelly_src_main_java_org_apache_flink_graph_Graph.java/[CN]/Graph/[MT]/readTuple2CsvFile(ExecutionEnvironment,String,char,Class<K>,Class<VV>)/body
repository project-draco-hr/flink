{
  CsvReader reader=new CsvReader(filePath,env);
  DataSet<Tuple2<K,VV>> vertices=reader.fieldDelimiter(delimiter).types(Tuple2IdClass,Tuple2ValueClass).map(new MapFunction<Tuple2<K,VV>,Tuple2<K,VV>>(){
    public Tuple2<K,VV> map(    Tuple2<K,VV> value) throws Exception {
      return (Tuple2<K,VV>)value;
    }
  }
);
  return vertices;
}
