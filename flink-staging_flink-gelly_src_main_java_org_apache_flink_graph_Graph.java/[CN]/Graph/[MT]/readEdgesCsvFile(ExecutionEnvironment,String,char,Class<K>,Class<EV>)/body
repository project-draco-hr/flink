{
  CsvReader reader=new CsvReader(filePath,env);
  DataSet<Tuple3<K,K,EV>> edges=reader.fieldDelimiter(delimiter).types(Tuple2IdClass,Tuple2IdClass,edgeValueClass).map(new MapFunction<Tuple3<K,K,EV>,Tuple3<K,K,EV>>(){
    public Tuple3<K,K,EV> map(    Tuple3<K,K,EV> value) throws Exception {
      return (Tuple3<K,K,EV>)value;
    }
  }
);
  return edges;
}
