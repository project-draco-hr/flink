{
  PactString s=line;
  List<PactString> cols=new ArrayList<PactString>();
  int start=0;
  for (int i=0; i < s.length(); i++) {
    if (s.charAt(i) == delimiter) {
      if (start == i)       cols.add(new PactString());
 else       cols.add(s.substring(start,i));
      start=i + 1;
    }
  }
  if (start != s.length() - 1)   cols.add(s.substring(start));
  return cols;
}
