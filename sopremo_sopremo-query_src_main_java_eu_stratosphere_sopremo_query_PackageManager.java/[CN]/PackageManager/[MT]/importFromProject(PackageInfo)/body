{
  Queue<File> directories=new LinkedList<File>();
  directories.add(info.getPackagePath());
  while (!directories.isEmpty())   for (  File file : directories.poll().listFiles())   if (file.isDirectory())   directories.add(file);
 else   if (file.getName().endsWith(".class") && !file.getName().contains("$"))   this.importFromFile(info,file);
}
