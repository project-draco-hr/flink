{
  return new NamedChildIterator("groupingExpression","second"){
    @Override protected void set(    int index,    EvaluationExpression childExpression){
      if (index == 0)       GroupingExpression.this.groupingExpression=childExpression;
 else       GroupingExpression.this.resultExpression=childExpression;
    }
    @Override protected EvaluationExpression get(    int index){
      if (index == 0)       return GroupingExpression.this.groupingExpression;
      return GroupingExpression.this.resultExpression;
    }
  }
;
}
