{
  final List<ArrayNode> nodes=new ArrayList<ArrayNode>();
  for (  final IJsonNode jsonNode : (IArrayNode)node)   nodes.add(JsonUtil.asArray(this.groupingExpression.evaluate(jsonNode,null,context),jsonNode));
  Collections.sort(nodes,new Comparator<ArrayNode>(){
    @Override public int compare(    final ArrayNode o1,    final ArrayNode o2){
      return o1.get(0).compareTo(o2.get(0));
    }
  }
);
  return nodes;
}
