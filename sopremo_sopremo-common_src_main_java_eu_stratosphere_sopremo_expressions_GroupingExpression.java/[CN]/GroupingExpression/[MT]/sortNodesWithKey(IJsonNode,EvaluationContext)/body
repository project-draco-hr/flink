{
  final List<IArrayNode> nodes=new ArrayList<IArrayNode>();
  for (  final IJsonNode jsonNode : (IArrayNode)node)   nodes.add(JsonUtil.asArray(this.groupingExpression.evaluate(jsonNode,null,context),jsonNode));
  Collections.sort(nodes,new Comparator<IArrayNode>(){
    @Override public int compare(    final IArrayNode o1,    final IArrayNode o2){
      return o1.get(0).compareTo(o2.get(0));
    }
  }
);
  return nodes;
}
