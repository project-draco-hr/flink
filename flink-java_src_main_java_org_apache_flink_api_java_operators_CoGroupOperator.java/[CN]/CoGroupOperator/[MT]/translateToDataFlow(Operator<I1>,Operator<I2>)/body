    po.setDegreeOfParallelism(this.getParallelism());
    return po;
  }
 else   if (keys1 instanceof Keys.SelectorFunctionKeys) {
    @SuppressWarnings("unchecked") Keys.SelectorFunctionKeys<I1,?> selectorKeys1=(Keys.SelectorFunctionKeys<I1,?>)keys1;
