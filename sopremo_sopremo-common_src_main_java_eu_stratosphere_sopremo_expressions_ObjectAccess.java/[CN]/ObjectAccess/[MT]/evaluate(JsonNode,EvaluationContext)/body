{
  if (!node.isObject()) {
    if (node.isNull() && this.safeDereference)     return node;
    throw new EvaluationException(String.format("Cannot access field %s of non-object %s",this.field,node.getClass().getSimpleName()));
  }
  final JsonNode value=((ObjectNode)node).get(this.field);
  return value == null ? NullNode.getInstance() : value;
}
