{
  if (e1 instanceof NumericNode && e2 instanceof NumericNode)   return this.evaluateComparable(((NumericNode)e1).getDecimalValue(),((NumericNode)e2).getDecimalValue());
  if (e1 instanceof TextNode && e2 instanceof TextNode)   return this.evaluateComparable(e1.getTextValue(),e2.getTextValue());
  throw new EvaluationException(String.format("Cannot compare %s %s %s",e1,this,e2));
}
