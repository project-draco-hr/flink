{
  Execution execution=currentExecutions.get(executionId);
  if (execution == null) {
    fail(new IllegalStateException("Cannot find execution for execution ID " + executionId));
  }
 else   if (execution.getVertex() == null) {
    fail(new IllegalStateException("Execution with execution ID " + executionId + " has no vertex assigned."));
  }
 else {
    execution.getVertex().scheduleOrUpdateConsumers(partitionIndex);
  }
}
