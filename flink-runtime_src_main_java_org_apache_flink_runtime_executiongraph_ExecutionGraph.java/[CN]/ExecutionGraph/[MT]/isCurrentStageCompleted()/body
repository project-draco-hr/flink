{
  if (this.indexToCurrentExecutionStage >= this.stages.size()) {
    return true;
  }
  final ExecutionGraphIterator it=new ExecutionGraphIterator(this,this.indexToCurrentExecutionStage,true,true);
  while (it.hasNext()) {
    final ExecutionVertex vertex=it.next();
    if (vertex.getExecutionState() != ExecutionState.FINISHED) {
      return false;
    }
  }
  return true;
}
