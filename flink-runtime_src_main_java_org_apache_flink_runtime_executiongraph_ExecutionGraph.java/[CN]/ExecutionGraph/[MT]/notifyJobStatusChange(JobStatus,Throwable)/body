{
  if (jobStatusListenerActors.size() > 0) {
    ExecutionGraphMessages.JobStatusChanged message=new ExecutionGraphMessages.JobStatusChanged(jobID,newState,System.currentTimeMillis(),error);
    for (    ActorRef listener : jobStatusListenerActors) {
      listener.tell(message,ActorRef.noSender());
    }
  }
}
