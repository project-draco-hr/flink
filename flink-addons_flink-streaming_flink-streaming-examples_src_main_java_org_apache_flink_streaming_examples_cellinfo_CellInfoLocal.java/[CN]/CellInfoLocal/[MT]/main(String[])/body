{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment(PARALLELISM).setBufferTimeout(100);
  DataStream<Tuple3<Integer,Long,Integer>> querySource=env.addSource(new QuerySource(),SOURCE_PARALLELISM).partitionBy(0);
  DataStream<String> stream=env.addSource(new InfoSource(),SOURCE_PARALLELISM).partitionBy(0).connect(querySource).map(new CellTask());
  stream.print();
  env.execute();
}
