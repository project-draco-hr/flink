{
  super(name,new Source[numberOfInputs],new Sink[numberOfOutputs],OperatorNavigator.INSTANCE);
  for (int index=0; index < this.outputNodes.length; index++)   this.outputNodes[index]=new Sink(PersistenceType.HDFS,String.format("%s %d",name,index));
  for (int index=0; index < this.inputNodes.length; index++)   this.inputNodes[index]=new Source(PersistenceType.HDFS,String.format("%s %d",name,index));
}
