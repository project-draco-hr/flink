{
  super(new Source[numberOfInputs],new Sink[numberOfOutputs],OperatorNavigator.INSTANCE);
  for (int index=0; index < this.outputNodes.length; index++)   this.outputNodes[index]=new Sink(DataType.HDFS,String.valueOf(index),null);
  for (int index=0; index < this.inputNodes.length; index++)   this.inputNodes[index]=new Source(DataType.HDFS,String.valueOf(index));
}
