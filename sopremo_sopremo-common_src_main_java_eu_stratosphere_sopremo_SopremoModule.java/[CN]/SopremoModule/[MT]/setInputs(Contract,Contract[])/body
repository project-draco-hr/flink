{
  if (contract instanceof SingleInputContract)   ((SingleInputContract<?,?,?,?>)contract).setInput(inputs[0]);
 else   if (contract instanceof DualInputContract) {
    ((DualInputContract<?,?,?,?,?,?>)contract).setFirstInput(inputs[0]);
    ((DualInputContract<?,?,?,?,?,?>)contract).setSecondInput(inputs[1]);
  }
 else   if (contract instanceof DataSinkContract)   ((DataSinkContract<?,?>)contract).setInput(inputs[0]);
}
