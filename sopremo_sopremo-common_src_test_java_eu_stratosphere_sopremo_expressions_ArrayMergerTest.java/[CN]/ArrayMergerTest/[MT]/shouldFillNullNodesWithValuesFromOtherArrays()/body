{
  final JsonNode result=new ArrayMerger().evaluate(createArrayNode(createArrayNode(JsonUtil.NODE_FACTORY.nullNode(),createObjectNode("fieldName",2),JsonUtil.NODE_FACTORY.nullNode()),createArrayNode(createObjectNode("fieldName",1)),createArrayNode(null,null,createObjectNode("fieldName",3))),this.context);
  Assert.assertEquals(createArrayNode(createObjectNode("fieldName",1),createObjectNode("fieldName",2),createObjectNode("fieldName",3)),result);
}
