{
  final IJsonNode result=new ArrayMerger().evaluate(createArrayNode(createArrayNode(NullNode.getInstance(),createObjectNode("fieldName",2),NullNode.getInstance()),createArrayNode(createObjectNode("fieldName",1)),createArrayNode(null,null,createObjectNode("fieldName",3))),null,this.context);
  Assert.assertEquals(createArrayNode(createObjectNode("fieldName",1),createObjectNode("fieldName",2),createObjectNode("fieldName",3)),result);
}
