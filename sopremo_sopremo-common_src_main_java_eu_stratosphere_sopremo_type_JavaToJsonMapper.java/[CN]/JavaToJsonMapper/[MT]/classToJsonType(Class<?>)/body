{
  if (IJsonNode.class.isAssignableFrom(javaClass))   return (Class<? extends IJsonNode>)javaClass;
  if (javaClass == Void.TYPE)   return NullNode.class;
  if (CharSequence.class.isAssignableFrom(javaClass))   return TextNode.class;
  if (javaClass.isArray() || Collection.class.isAssignableFrom(javaClass))   return ArrayNode.class;
  if (Map.class.isAssignableFrom(javaClass))   return ObjectNode.class;
  if (ReflectUtil.isSameTypeOrPrimitive(javaClass,Boolean.class))   return BooleanNode.class;
  return this.typeDict.get(ReflectUtil.getClassForPrimtive(javaClass)).getDeclaringClass();
}
