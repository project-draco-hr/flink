{
  if (obj instanceof TaskState) {
    TaskState other=(TaskState)obj;
    return jobVertexID.equals(other.jobVertexID) && parallelism == other.parallelism && subtaskStates.equals(other.subtaskStates) && keyGroupsStateHandles.equals(other.keyGroupsStateHandles);
  }
 else {
    return false;
  }
}
