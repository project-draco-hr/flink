{
switch (driverStrategy) {
case FULL_OUTER_MERGE:
    int numPages=memoryManager.computeNumberOfPages(driverMemFraction);
  return new NonReusingMergeOuterJoinIterator<>(OuterJoinType.FULL,in1,in2,serializer1,comparator1,serializer2,comparator2,pairComparatorFactory.createComparator12(comparator1,comparator2),memoryManager,ioManager,numPages,super.taskContext.getOwningNepheleTask());
default :
throw new Exception("Unsupported driver strategy for full outer join driver: " + driverStrategy.name());
}
}
