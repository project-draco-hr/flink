{
  boolean nonTrivial=false;
  if (keyOrder != Order.NONE) {
    if (contract == OutputContract.SameKey || contract == OutputContract.SameKeyFirst || contract == OutputContract.SameKeySecond) {
      nonTrivial=true;
    }
 else {
      keyOrder=Order.NONE;
    }
  }
  if (keysGrouped) {
    if (contract == OutputContract.SameKey || contract == OutputContract.SameKeyFirst || contract == OutputContract.SameKeySecond) {
      nonTrivial=true;
    }
 else {
      keysGrouped=false;
    }
  }
  nonTrivial|=(keyUnique=(contract == OutputContract.UniqueKey));
  return nonTrivial;
}
