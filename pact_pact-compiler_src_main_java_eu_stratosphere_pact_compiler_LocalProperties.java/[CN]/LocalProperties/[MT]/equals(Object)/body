{
  if (this == obj) {
    return true;
  }
 else   if (obj == null) {
    return false;
  }
 else   if (getClass() != obj.getClass()) {
    return false;
  }
  LocalProperties other=(LocalProperties)obj;
  if (this.keyOrder == other.keyOrder && this.keyUnique == other.keyUnique && this.keysGrouped == other.keysGrouped) {
    return true;
  }
 else {
    return false;
  }
}
