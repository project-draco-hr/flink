{
  if (this == obj) {
    return true;
  }
 else   if (obj == null) {
    return false;
  }
 else   if (getClass() != obj.getClass()) {
    return false;
  }
  LocalProperties other=(LocalProperties)obj;
  if (this.ordering == other.ordering && this.grouped == other.grouped && (this.groupedFields == other.groupedFields || (this.groupedFields != null && this.groupedFields.equals(other.groupedFields)))) {
    return true;
  }
 else {
    return false;
  }
}
