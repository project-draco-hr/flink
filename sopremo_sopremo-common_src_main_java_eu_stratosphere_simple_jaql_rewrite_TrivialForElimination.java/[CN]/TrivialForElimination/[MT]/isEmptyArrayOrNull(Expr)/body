{
  if (expr instanceof ConstExpr) {
    JsonValue v=((ConstExpr)expr).value;
    if (v == null) {
      return true;
    }
 else     if (v instanceof JsonArray) {
      return ((JsonArray)v).isEmpty();
    }
  }
 else   if (expr instanceof ArrayExpr) {
    return ((ArrayExpr)expr).numChildren() == 0;
  }
  return false;
}
