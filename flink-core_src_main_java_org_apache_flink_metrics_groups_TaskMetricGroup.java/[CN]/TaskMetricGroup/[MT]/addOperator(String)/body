{
  OperatorMetricGroup operator=new OperatorMetricGroup(this.registry,this,name,this.subtaskIndex);
synchronized (this) {
    OperatorMetricGroup previous=operators.put(name,operator);
    if (previous == null) {
      return operator;
    }
 else {
      operators.put(name,previous);
      return previous;
    }
  }
}
