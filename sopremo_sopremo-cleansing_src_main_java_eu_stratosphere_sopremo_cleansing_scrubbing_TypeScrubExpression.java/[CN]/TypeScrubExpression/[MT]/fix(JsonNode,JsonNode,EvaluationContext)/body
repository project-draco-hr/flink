{
  try {
    if (possibleResult.isTextual())     return LenientParser.INSTANCE.parse(possibleResult.getTextValue(),type,LenientParser.ELIMINATE_NOISE);
    return TypeCoercer.INSTANCE.coerce(sourceNode,type);
  }
 catch (  Exception e) {
    return super.fix(sourceNode,possibleResult,context);
  }
}
