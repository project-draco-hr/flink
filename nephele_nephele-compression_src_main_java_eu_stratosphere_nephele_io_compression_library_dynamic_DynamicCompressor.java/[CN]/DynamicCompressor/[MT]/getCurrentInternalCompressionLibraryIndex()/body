{
  int oldVal=this.selectedCompressor;
  final long timestamp=System.currentTimeMillis();
  if (this.lastTimestamp > 0) {
    this.selectedCompressor=this.decisionModel.getCompressionLevelForNextBuffer(this.sizeOfLastUncompressedBuffer,(int)(timestamp - this.lastTimestamp));
  }
  this.lastTimestamp=timestamp;
  return oldVal;
}
