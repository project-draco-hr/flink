{
  try {
    if (shouldWait) {
      return dataChannel.offer(record,iterationWaitTime,TimeUnit.MILLISECONDS);
    }
 else {
      dataChannel.put(record);
      return true;
    }
  }
 catch (  InterruptedException e) {
    if (LOG.isErrorEnabled()) {
      LOG.error("Pushing back record at iteration %s failed due to: {}",iterationId,StringUtils.stringifyException(e));
      throw e;
    }
    return false;
  }
}
