{
  isRunning=true;
  if (LOG.isDebugEnabled()) {
    LOG.debug("Iteration sink {} invoked",getName());
  }
  try {
    forwardRecords();
    if (LOG.isDebugEnabled()) {
      LOG.debug("Iteration sink {} invoke finished",getName());
    }
  }
 catch (  Exception e) {
    LOG.error("Iteration tail " + getEnvironment().getTaskNameWithSubtasks() + " failed",e);
    throw e;
  }
 finally {
    isRunning=false;
    clearBuffers();
  }
}
