{
  if (simulate) {
    this.inputs[EARMARK][LEGAL_ENTITY]=new Source(getInternalInputFormat(),String.format("%s/EarmarkLegalEntities.json",this.outputDir));
    return;
  }
  if (this.scrubbed[EARMARK] == null)   this.scrubEarmark(true);
  UnionAll legalEntities=new UnionAll(this.mapEarmarkReceiver(),this.mapEarmarkStates());
  this.inputs[EARMARK][LEGAL_ENTITY]=legalEntities;
  this.sinks.add(new Sink(getInternalOutputFormat(),String.format("%s/EarmarkLegalEntities.json",this.outputDir),legalEntities));
}
