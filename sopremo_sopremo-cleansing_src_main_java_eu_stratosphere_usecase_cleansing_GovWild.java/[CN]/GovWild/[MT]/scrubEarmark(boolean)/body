{
  if (simulate) {
    this.scrubbedEarmarks=new Source(PersistenceType.HDFS,String.format("%s/ScrubbedEarmarks.json",this.outputDir));
    return;
  }
  Source earmarks=new Source(PersistenceType.HDFS,String.format("%s/OriginalUsEarmark2008.json",this.inputDir));
  Operator scrubbed=new Validation(earmarks);
  this.scrubbedEarmarks=scrubbed;
  this.sinks.add(new Sink(PersistenceType.HDFS,String.format("%s/ScrubbedEarmarks.json",this.outputDir),scrubbed));
}
