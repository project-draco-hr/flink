{
  if (simulate) {
    this.scrubbed[EARMARK]=new Source(getInternalInputFormat(),String.format("%s/ScrubbedEarmarks.json",this.outputDir));
    return;
  }
  Source earmarks=new Source(PersistenceType.HDFS,String.format("%s/OriginalUsEarmark2008.json",this.inputDir));
  Operator scrubbed=new Validation(earmarks);
  this.scrubbed[EARMARK]=scrubbed;
  this.sinks.add(new Sink(getInternalOutputFormat(),String.format("%s/ScrubbedEarmarks.json",this.outputDir),scrubbed));
}
