{
  Operator<?> persons=this.fused[EARMARK][PERSON];
  if (persons == null)   persons=new Source(String.format("%s/Persons.json",this.outputDir));
  Operator<?> selection=new Selection().withCondition(new ComparativeExpression(new FunctionCall("count",new ArrayAccess(0)),BinaryOperator.EQUAL,new ConstantExpression(0))).withInputs(persons);
  this.sinks.add(new Sink(this.getInternalOutputFormat(),String.format("%s/Result.json",this.outputDir)).withInputs(selection));
}
