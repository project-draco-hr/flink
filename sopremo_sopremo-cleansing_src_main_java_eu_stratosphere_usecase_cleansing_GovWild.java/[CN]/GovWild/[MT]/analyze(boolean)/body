{
  Operator persons=fused[EARMARK][PERSON];
  if (persons == null)   persons=new Source(String.format("%s/Persons.json",outputDir));
  Operator selection=new Selection().withCondition(new ComparativeExpression(new FunctionCall("count",new ArrayAccess(0)),BinaryOperator.EQUAL,new ConstantExpression(0))).withInputs(persons);
  sinks.add((Sink)new Sink(getInternalOutputFormat(),String.format("%s/Result.json",outputDir)).withInputs(selection));
}
