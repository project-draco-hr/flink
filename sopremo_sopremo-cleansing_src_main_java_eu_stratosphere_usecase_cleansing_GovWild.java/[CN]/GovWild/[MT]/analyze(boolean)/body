{
  Operator persons=fused[EARMARK][PERSON];
  if (persons == null)   persons=new Source(String.format("%s/Persons.json",outputDir));
  Operator selection=new Selection(new ComparativeExpression(new FunctionCall("count",new ArrayAccess(0)),BinaryOperator.EQUAL,new ConstantExpression(0)),persons);
  sinks.add(new Sink(getInternalOutputFormat(),String.format("%s/Result.json",outputDir),selection));
}
