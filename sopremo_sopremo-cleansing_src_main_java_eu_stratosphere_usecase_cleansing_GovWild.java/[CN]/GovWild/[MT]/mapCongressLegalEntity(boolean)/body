{
  if (simulate) {
    this.inputs[CONGRESS][LEGAL_ENTITY]=new Source(this.getInternalInputFormat(),String.format("%s/CongressLegalEntities.json",this.outputDir));
    return;
  }
  if (this.scrubbed[CONGRESS] == null)   this.scrubCongress(true);
  UnionAll legalEntities=new UnionAll().withInputs(this.mapCongressParties(),this.mapCongressStates());
  this.inputs[CONGRESS][LEGAL_ENTITY]=legalEntities;
  this.sinks.add(new Sink(this.getInternalOutputFormat(),String.format("%s/CongressLegalEntities.json",this.outputDir)).withInputs(legalEntities));
}
