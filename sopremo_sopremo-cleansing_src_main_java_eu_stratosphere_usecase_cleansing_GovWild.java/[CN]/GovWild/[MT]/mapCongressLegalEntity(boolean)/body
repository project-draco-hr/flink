{
  if (simulate) {
    this.legalEntities.add(new Source(PersistenceType.HDFS,String.format("%s/CongressLegalEntities.json",this.outputDir)));
    return;
  }
  if (this.scrubbedCongress == null)   this.scrubCongress(true);
  UnionAll legalEntities=new UnionAll(this.mapCongressParties(),this.mapCongressStates());
  this.legalEntities.add(legalEntities);
  this.sinks.add(new Sink(PersistenceType.HDFS,String.format("%s/CongressLegalEntities.json",this.outputDir),legalEntities));
}
