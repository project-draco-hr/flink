{
  if (simulate) {
    this.inputs[CONGRESS][LEGAL_ENTITY]=new Source(getInternalInputFormat(),String.format("%s/CongressLegalEntities.json",this.outputDir));
    return;
  }
  if (this.scrubbed[CONGRESS] == null)   this.scrubCongress(true);
  UnionAll legalEntities=new UnionAll(this.mapCongressParties(),this.mapCongressStates());
  this.inputs[CONGRESS][LEGAL_ENTITY]=legalEntities;
  this.sinks.add(new Sink(getInternalOutputFormat(),String.format("%s/CongressLegalEntities.json",this.outputDir),legalEntities));
}
