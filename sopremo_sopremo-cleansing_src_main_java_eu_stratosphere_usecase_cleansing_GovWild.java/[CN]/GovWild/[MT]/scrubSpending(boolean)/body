{
  if (simulate) {
    this.scrubbedSpending=new Source(PersistenceType.HDFS,String.format("%s/ScrubbedSpending.json",this.outputDir));
    return;
  }
  Source spending=new Source(PersistenceType.HDFS,String.format("%s/OriginalUsSpending.json",this.inputDir));
  this.scrubbedSpending=spending;
  this.sinks.add(new Sink(PersistenceType.HDFS,String.format("%s/ScrubbedSpending.json",this.outputDir),spending));
}
