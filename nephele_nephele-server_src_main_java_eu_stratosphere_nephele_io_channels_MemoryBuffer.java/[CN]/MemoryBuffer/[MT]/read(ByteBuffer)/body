{
  if (!this.hasRemaining()) {
    return -1;
  }
  int numBytes=dst.remaining();
  final int remaining=this.remaining();
  if (numBytes == 0) {
    return 0;
  }
  if (numBytes > remaining) {
    numBytes=remaining;
  }
  internalMemorySegment.get(position(),dst,numBytes);
  index+=numBytes;
  return numBytes;
}
