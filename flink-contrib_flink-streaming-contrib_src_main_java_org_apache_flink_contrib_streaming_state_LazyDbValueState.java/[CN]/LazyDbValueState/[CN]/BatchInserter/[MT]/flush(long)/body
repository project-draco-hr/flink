{
  for (int i=0; i < inserts.length; i++) {
    List<Tuple2<byte[],byte[]>> insertPartition=inserts[i];
    if (!insertPartition.isEmpty()) {
      dbAdapter.insertBatch(kvStateId,conf,connections.getForIndex(i),insertStatements.getForIndex(i),timestamp,insertPartition);
      insertPartition.clear();
    }
  }
}
