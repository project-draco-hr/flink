{
  List<TwoSourceJoin> joins=new ArrayList<TwoSourceJoin>();
  for (  BooleanExpression expression : condition.getExpressions())   if (expression instanceof ComparativeExpression)   joins.add(new ComparisonJoin(this.getInputForExpression(((ComparativeExpression)expression).getExpr1()),this.getInputForExpression(((ComparativeExpression)expression).getExpr2()),(ComparativeExpression)expression));
 else   if (expression instanceof ElementInSetExpression)   joins.add(new ElementInSetJoin(this.getInputForExpression(((ElementInSetExpression)expression).getElementExpr()),this.getInputForExpression(((ElementInSetExpression)expression).getSetExpr()),(ElementInSetExpression)expression));
 else   throw new UnsupportedOperationException();
  return joins;
}
