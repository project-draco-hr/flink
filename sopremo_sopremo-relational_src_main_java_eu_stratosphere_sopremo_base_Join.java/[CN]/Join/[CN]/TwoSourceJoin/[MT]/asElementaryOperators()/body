{
  SopremoModule sopremoModule=new SopremoModule(this.toString(),2,1);
  Projection leftProjection=new Projection(this.leftJoinKey,EvaluableExpression.SAME_VALUE,sopremoModule.getInput(0));
  Projection rightProjection=new Projection(this.rightJoinKey,EvaluableExpression.SAME_VALUE,sopremoModule.getInput(1));
  Operator joinAlgorithm=this.createJoinContract(leftProjection,rightProjection);
  sopremoModule.getOutput(0).setInputs(new Projection(new ArrayMerger(),joinAlgorithm));
  return sopremoModule;
}
