{
  if (values1.isEmpty()) {
    if (this.rightOuter)     for (    final JsonNode value : values2)     out.collect(key,JsonUtil.asArray(NullNode.getInstance(),value));
    return;
  }
  if (values2.isEmpty()) {
    if (this.leftOuter)     for (    final JsonNode value : values1)     out.collect(key,JsonUtil.asArray(value,NullNode.getInstance()));
    return;
  }
  final ArrayList<JsonNode> firstSourceNodes=new ArrayList<JsonNode>();
  for (  final JsonNode value : values1)   firstSourceNodes.add(value);
  for (  final JsonNode secondSourceNode : values2)   for (  final JsonNode firstSourceNode : firstSourceNodes)   out.collect(key,JsonUtil.asArray(firstSourceNode,secondSourceNode));
}
