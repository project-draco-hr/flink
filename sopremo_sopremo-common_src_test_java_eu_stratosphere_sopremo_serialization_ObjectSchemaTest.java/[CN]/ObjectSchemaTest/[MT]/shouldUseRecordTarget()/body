{
  this.schema.setMappings("firstname","lastname");
  ObjectNode object=new ObjectNode().put("firstname",TextNode.valueOf("testfn")).put("lastname",TextNode.valueOf("testln"));
  PactRecord target=new PactRecord();
  target.setField(2,new JsonNodeWrapper(new ObjectNode()));
  PactRecord result=this.schema.jsonToRecord(object,target);
  Assert.assertSame(target,result);
}
