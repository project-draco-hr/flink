{
  PactRecord record=new PactRecord();
  ObjectNode object=new ObjectNode().put("firstName",TextNode.valueOf("Hans")).put("lastName",TextNode.valueOf("Maier")).put("age",IntNode.valueOf(23));
  record.setField(0,SopremoUtil.wrap(object));
  IJsonNode result=this.schema.recordToJson(record,null);
  Assert.assertEquals(object,result);
}
