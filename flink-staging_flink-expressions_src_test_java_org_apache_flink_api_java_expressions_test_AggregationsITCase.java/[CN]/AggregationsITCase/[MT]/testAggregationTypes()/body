{
  ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  ExpressionOperation<JavaBatchTranslator> expressionOperation=ExpressionUtil.from(CollectionDataSets.get3TupleDataSet(env));
  ExpressionOperation<JavaBatchTranslator> result=expressionOperation.select("f0.sum, f0.min, f0.max, f0.count, f0.avg");
  DataSet<Row> ds=ExpressionUtil.toSet(result,Row.class);
  ds.writeAsText(resultPath,FileSystem.WriteMode.OVERWRITE);
  env.execute();
  expected="231,1,21,21,11";
}
