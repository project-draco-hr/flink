{
  StreamWindow<T> currentWindow=new StreamWindow<T>();
  T finalAggregate=getFinalAggregate();
  if (finalAggregate != null) {
    currentWindow.add(finalAggregate);
    collector.collect(currentWindow);
    updateIndexAtEmit();
    return true;
  }
 else {
    updateIndexAtEmit();
    return false;
  }
}
