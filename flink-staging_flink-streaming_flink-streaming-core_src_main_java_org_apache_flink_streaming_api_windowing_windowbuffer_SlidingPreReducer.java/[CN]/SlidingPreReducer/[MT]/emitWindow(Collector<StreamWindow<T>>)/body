{
  StreamWindow<T> currentWindow=new StreamWindow<T>();
  try {
    if (addFinalAggregate(currentWindow)) {
      collector.collect(currentWindow);
      afterEmit();
      return true;
    }
 else {
      afterEmit();
      return false;
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
