{
  LeaderElectionService leaderElectionService=LeaderElectionUtils.createLeaderElectionService(configuration);
  return Props.create(TestingJobManager.class,configuration,TestingUtils.defaultExecutionContext(),new InstanceManager(),new Scheduler(TestingUtils.defaultExecutionContext()),new BlobLibraryCacheManager(new BlobServer(configuration),10l),ActorRef.noSender(),1,1L,AkkaUtils.getDefaultTimeout(),StreamingMode.BATCH_ONLY,leaderElectionService);
}
