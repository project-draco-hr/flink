{
  if (nextElement != null) {
    return false;
  }
  nextElement=serializer.createInstance();
  nextElement=format.nextRecord(nextElement);
  if (nextElement == null && splitIterator.hasNext()) {
    format.open(splitIterator.next());
    return reachedEnd();
  }
 else   if (nextElement == null) {
    return true;
  }
  return false;
}
