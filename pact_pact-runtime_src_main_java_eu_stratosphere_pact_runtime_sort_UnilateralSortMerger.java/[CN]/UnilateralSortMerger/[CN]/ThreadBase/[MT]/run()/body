{
  try {
    if (this.parentTask != null) {
      this.parentTask.userThreadStarted(this);
    }
    go();
  }
 catch (  Throwable t) {
    internalHandleException(new IOException("Thread '" + getName() + "' terminated due to an exception: "+ t.getMessage(),t));
  }
 finally {
    if (this.parentTask != null) {
      this.parentTask.userThreadFinished(this);
    }
  }
}
