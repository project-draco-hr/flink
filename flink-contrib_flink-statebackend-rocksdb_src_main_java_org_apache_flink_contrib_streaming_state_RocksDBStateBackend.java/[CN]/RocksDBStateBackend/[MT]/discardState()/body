{
  super.discardState();
  nonPartitionedStateBackend.discardState();
synchronized (dbCleanupLock) {
    if (db != null) {
      if (this.dbOptions != null) {
        this.dbOptions.dispose();
        this.dbOptions=null;
      }
      for (      Tuple2<ColumnFamilyHandle,StateDescriptor> column : kvStateInformation.values()) {
        column.f0.dispose();
      }
      db.dispose();
      db=null;
    }
  }
}
