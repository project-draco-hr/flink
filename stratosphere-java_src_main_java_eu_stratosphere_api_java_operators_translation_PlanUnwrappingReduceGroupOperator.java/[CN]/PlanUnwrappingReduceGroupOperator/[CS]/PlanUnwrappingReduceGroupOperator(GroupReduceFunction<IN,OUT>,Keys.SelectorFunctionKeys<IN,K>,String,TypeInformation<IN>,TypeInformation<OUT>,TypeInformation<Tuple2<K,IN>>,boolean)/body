{
  super(combinable ? new TupleUnwrappingCombinableGroupReducer<IN,OUT,K>(udf) : new TupleUnwrappingNonCombinableGroupReducer<IN,OUT,K>(udf),key.computeLogicalKeyPositions(),name);
  this.outputType=outType;
  this.typeInfoWithKey=typeInfoWithKey;
  super.setCombinable(udf.getClass().getAnnotation(Combinable.class) != null);
}
