{
  orderStatus=record.getField(2,PactString.class);
  if (!orderStatus.getValue().equals("F"))   return;
  orderPrio=record.getField(4,PactString.class);
  if (!orderPrio.getValue().startsWith(this.prioFilter))   return;
  orderDate=record.getField(3,PactString.class);
  if (!(Integer.parseInt(orderDate.getValue().substring(0,4)) > this.yearFilter))   return;
  record.setNumFields(2);
  out.collect(record);
}
