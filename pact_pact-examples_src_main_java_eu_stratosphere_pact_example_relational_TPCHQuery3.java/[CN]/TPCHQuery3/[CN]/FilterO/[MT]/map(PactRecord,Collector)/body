{
  final Tuple t=record.getField(0,Tuple.class);
  try {
    if (Integer.parseInt(t.getStringValueAt(4).substring(0,4)) > this.yearFilter && t.getStringValueAt(2).equals("F") && t.getStringValueAt(5).startsWith(this.prioFilter)) {
      this.orderKey.setValue(t.getLongValueAt(0));
      this.shipPriority.setValue(t.getStringValueAt(7));
      record.setField(0,this.orderKey);
      record.setField(1,this.shipPriority);
      out.collect(record);
    }
  }
 catch (  final StringIndexOutOfBoundsException e) {
    LOGGER.error(e);
  }
catch (  final Exception ex) {
    LOGGER.error(ex);
  }
}
