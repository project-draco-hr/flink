{
  record.getFieldInto(2,orderStatus);
  record.getFieldInto(3,orderDate);
  record.getFieldInto(4,orderPrio);
  if (Integer.parseInt(orderDate.getValue().substring(0,4)) > this.yearFilter && orderStatus.getValue().equals("F") && orderPrio.getValue().startsWith(this.prioFilter)) {
    record.setNull(2);
    record.setNull(3);
    record.setNull(4);
    out.collect(record);
  }
}
