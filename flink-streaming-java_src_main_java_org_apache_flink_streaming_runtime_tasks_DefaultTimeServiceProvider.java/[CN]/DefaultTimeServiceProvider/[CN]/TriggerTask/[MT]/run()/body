{
synchronized (lock) {
    try {
      target.trigger(timestamp);
    }
 catch (    Throwable t) {
      if (task != null) {
        TimerException asyncException=new TimerException(t);
        task.registerAsyncException("Caught exception while processing timer.",asyncException);
      }
 else {
        t.printStackTrace();
      }
    }
  }
}
