{
  for (  final Entry<String,InputPropertyInfo> property : info.getInputProperties().entrySet())   for (int index=0; index < op.getInputs().size(); index++) {
    final InputPropertyInfo propertyInfo=property.getValue();
    final Object actualValue=propertyInfo.getValue(op,index);
    final Object defaultValue=propertyInfo.getValue(defaultInstance,index);
    if (!actualValue.equals(defaultValue)) {
      renderInfo.builder.append(renderInfo.getVariableName(op)).append(".set").append(StringUtil.upperFirstChar(propertyInfo.getDescriptor().getName())).append("(").append(index).append(", ");
      this.appendExpression(actualValue,renderInfo);
      renderInfo.builder.append(");\n");
    }
  }
}
