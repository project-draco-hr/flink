{
  String topic=UUID.randomUUID().toString();
  Kafka09JsonTableSource tableSource=new Kafka09JsonTableSource(topic,standardProps,new String[]{"long","string","boolean","double","missing-field"},new TypeInformation<?>[]{BasicTypeInfo.LONG_TYPE_INFO,BasicTypeInfo.STRING_TYPE_INFO,BasicTypeInfo.BOOLEAN_TYPE_INFO,BasicTypeInfo.DOUBLE_TYPE_INFO,BasicTypeInfo.LONG_TYPE_INFO});
  tableSource.setFailOnMissingField(false);
  runJsonTableSource(topic,tableSource);
}
