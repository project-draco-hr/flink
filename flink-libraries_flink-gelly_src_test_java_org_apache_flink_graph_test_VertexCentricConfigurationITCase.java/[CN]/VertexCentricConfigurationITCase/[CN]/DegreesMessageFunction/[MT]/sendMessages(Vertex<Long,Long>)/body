{
  if (vertex.getId().equals(1)) {
    Assert.assertEquals(2,getOutDegree());
    Assert.assertEquals(1,getInDegree());
  }
 else   if (vertex.getId().equals(3)) {
    Assert.assertEquals(2,getOutDegree());
    Assert.assertEquals(2,getInDegree());
  }
  sendMessageToAllNeighbors(vertex.getValue());
}
