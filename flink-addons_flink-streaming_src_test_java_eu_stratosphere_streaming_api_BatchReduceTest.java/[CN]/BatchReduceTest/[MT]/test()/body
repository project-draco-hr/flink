{
  StreamExecutionEnvironment context=new StreamExecutionEnvironment();
  DataStream<Tuple1<Double>> dataStream0=context.addSource(new MySource(),1).batchReduce(new MyBatchReduce(),BATCH_SIZE,PARALELISM).addSink(new MySink());
