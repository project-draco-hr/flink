{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment();
  DataStream<Tuple1<Double>> dataStream0=env.addSource(new MySource(),1).batchReduce(new MyBatchReduce(),BATCH_SIZE,PARALELISM).addSink(new MySink());
