{
  try {
    return Class.forName(className,true,cl).asSubclass(Program.class);
  }
 catch (  ClassNotFoundException e) {
    throw new ProgramInvocationException("The program class '" + className + "' was not found in the jar file '"+ jarFile.getPath()+ "'.",e);
  }
catch (  ClassCastException e) {
    throw new ProgramInvocationException("The program class '" + className + "' cannot be cast to Program.",e);
  }
catch (  Throwable t) {
    throw new ProgramInvocationException("An unknown problem ocurred during the instantiation of the " + "program: " + t,t);
  }
}
