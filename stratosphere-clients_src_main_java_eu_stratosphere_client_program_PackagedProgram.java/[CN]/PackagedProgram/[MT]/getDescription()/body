{
  if (this.planAssembler instanceof ProgramDescription) {
    try {
      return ((ProgramDescription)this.planAssembler).getDescription();
    }
 catch (    Throwable t) {
      throw new ProgramInvocationException("Error while getting the program description" + (t.getMessage() == null ? "." : ": " + t.getMessage()),t);
    }
  }
 else {
    return null;
  }
}
