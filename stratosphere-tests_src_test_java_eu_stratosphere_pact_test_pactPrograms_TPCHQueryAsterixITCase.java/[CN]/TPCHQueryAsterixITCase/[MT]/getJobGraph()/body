{
  TPCHQueryAsterix tpchBench=new TPCHQueryAsterix();
  Job plan=tpchBench.createJob(config.getString("TPCHQueryAsterix#NoSubtasks","1"),getFilesystemProvider().getURIPrefix() + ordersPath,getFilesystemProvider().getURIPrefix() + custPath,getFilesystemProvider().getURIPrefix() + resultPath);
  PactCompiler pc=new PactCompiler(new DataStatistics());
  OptimizedPlan op=pc.compile(plan);
  NepheleJobGraphGenerator jgg=new NepheleJobGraphGenerator();
  return jgg.compileJobGraph(op);
}
