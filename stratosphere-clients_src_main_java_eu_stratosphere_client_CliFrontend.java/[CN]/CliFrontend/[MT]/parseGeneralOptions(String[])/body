{
  CommandLine line=null;
  try {
    line=parser.parse(this.options.get(GENERAL_OPTS),args,true);
  }
 catch (  Exception e) {
    handleError(e);
  }
  if (line.hasOption(HELP_OPTION.getOpt())) {
    printHelp();
    System.exit(0);
  }
  this.verbose=line.hasOption(VERBOSE_OPTION.getOpt());
  return line.getArgs();
}
