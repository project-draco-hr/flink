{
  timestamp=0;
  for (int i=0; i < 100; ++i) {
    AtomRecord mottoRecord=new AtomRecord(2);
    mottoRecord.setField(0,new StringValue(motto));
    mottoRecord.setField(1,new LongValue(timestamp));
    mottoRecords.addRecord(mottoRecord);
    ++timestamp;
    if (timestamp % 10 == 0) {
      emit(mottoRecords);
    }
  }
}
