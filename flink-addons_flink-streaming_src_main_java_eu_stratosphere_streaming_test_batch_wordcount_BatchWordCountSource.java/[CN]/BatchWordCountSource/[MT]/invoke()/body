{
  line=br.readLine().replaceAll("[\\-\\+\\.\\^:,]","");
  timestamp=0;
  while (line != null) {
    if (line != "") {
      AtomRecord hamletRecord=new AtomRecord(2);
      hamletRecord.setField(0,new StringValue(line));
      hamletRecord.setField(1,new LongValue(timestamp));
      hamletRecords.addRecord(hamletRecord);
      ++timestamp;
      if (timestamp % 10 == 0) {
        emit(hamletRecords);
      }
      line=br.readLine();
    }
  }
}
