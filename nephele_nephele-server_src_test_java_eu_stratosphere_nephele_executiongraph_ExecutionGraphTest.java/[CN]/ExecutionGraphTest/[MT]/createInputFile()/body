{
  File inputFile=new File(PATHNAME + INPUTFILENAME);
  if (inputFile.exists()) {
    inputFile.delete();
  }
  try {
    inputFile.createNewFile();
    FileWriter fw=new FileWriter(inputFile);
    fw.write(INPUTCONTENT);
    fw.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
