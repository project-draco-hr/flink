{
  final IJsonNode[] params=new IJsonNode[this.paramExprs.size()];
  for (int index=0; index < params.length; index++)   params[index]=this.paramExprs.get(index).evaluate(node,context);
  try {
    return this.function.call(JsonUtil.asArray(params),target,context);
  }
 catch (  Exception e) {
    throw new EvaluationException(e);
  }
}
