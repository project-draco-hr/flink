{
  JsonNode[] params=new JsonNode[paramExprs.length];
  for (int index=0; index < params.length; index++)   params[index]=paramExprs[index].evaluate(node);
  if (name.equals("count"))   return BuiltinFunctions.count(JsonUtils.asArray(params));
  if (name.equals("sum"))   return BuiltinFunctions.sum(JsonUtils.asArray(params));
  throw new EvaluationException("undefined function " + this.name);
}
