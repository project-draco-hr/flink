{
  DataDistribution<PactRecord> dd=new PactRecordDataDistribution(new int[]{0},new Key[][]{{new PactInteger(1)},{new PactInteger(2)},{new PactInteger(3)}});
  Assert.assertTrue(dd != null);
  try {
    dd=new PactRecordDataDistribution(new int[]{0},new Key[][]{{new PactInteger(1)},{new PactString("ABC")},{new PactInteger(3)}});
    Assert.fail("Data distribution accepts inconsistent key types");
  }
 catch (  IllegalArgumentException iae) {
  }
  try {
    dd=new PactRecordDataDistribution(new int[]{0},new Key[][]{{new PactInteger(1)},{new PactInteger(2),new PactInteger(2)},{new PactInteger(3)}});
    Assert.fail("Data distribution accepts inconsistent many keys");
  }
 catch (  IllegalArgumentException iae) {
  }
}
