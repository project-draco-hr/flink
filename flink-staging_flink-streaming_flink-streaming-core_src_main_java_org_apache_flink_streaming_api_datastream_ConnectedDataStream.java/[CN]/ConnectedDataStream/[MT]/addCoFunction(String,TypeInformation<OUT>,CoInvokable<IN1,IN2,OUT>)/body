{
  @SuppressWarnings({"unchecked","rawtypes"}) SingleOutputStreamOperator<OUT,?> returnStream=new SingleOutputStreamOperator(environment,functionName,outTypeInfo,functionInvokable);
  dataStream1.streamGraph.addCoTask(returnStream.getId(),functionInvokable,getInputType1(),getInputType2(),outTypeInfo,functionName,environment.getDegreeOfParallelism());
  dataStream1.connectGraph(dataStream1,returnStream.getId(),1);
  dataStream1.connectGraph(dataStream2,returnStream.getId(),2);
  return returnStream;
}
