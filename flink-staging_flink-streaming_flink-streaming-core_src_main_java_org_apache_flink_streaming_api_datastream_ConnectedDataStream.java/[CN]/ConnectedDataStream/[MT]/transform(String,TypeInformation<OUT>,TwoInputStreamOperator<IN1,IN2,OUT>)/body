{
  dataStream1.getType();
  dataStream2.getType();
  TwoInputTransformation<IN1,IN2,OUT> transform=new TwoInputTransformation<IN1,IN2,OUT>(dataStream1.getTransformation(),dataStream2.getTransformation(),functionName,operator,outTypeInfo,environment.getParallelism());
  @SuppressWarnings({"unchecked","rawtypes"}) SingleOutputStreamOperator<OUT,?> returnStream=new SingleOutputStreamOperator(environment,transform);
  getExecutionEnvironment().addOperator(transform);
  return returnStream;
}
