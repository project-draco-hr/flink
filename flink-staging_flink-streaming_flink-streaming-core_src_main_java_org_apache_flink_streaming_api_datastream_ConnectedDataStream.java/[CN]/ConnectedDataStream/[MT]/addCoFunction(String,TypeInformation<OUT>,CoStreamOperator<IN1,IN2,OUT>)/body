{
  @SuppressWarnings({"unchecked","rawtypes"}) SingleOutputStreamOperator<OUT,?> returnStream=new SingleOutputStreamOperator(environment,functionName,outTypeInfo,operator);
  dataStream1.streamGraph.addCoOperator(returnStream.getId(),operator,getInputType1(),getInputType2(),outTypeInfo,functionName);
  dataStream1.connectGraph(dataStream1,returnStream.getId(),1);
  dataStream1.connectGraph(dataStream2,returnStream.getId(),2);
  return returnStream;
}
