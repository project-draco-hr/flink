{
  EnumTrianglesRdfFoaf enumTriangles=new EnumTrianglesRdfFoaf();
  Job plan=enumTriangles.createJob(config.getString("EnumTrianglesTest#NoSubtasks","4"),getFilesystemProvider().getURIPrefix() + edgesPath,getFilesystemProvider().getURIPrefix() + resultPath);
  PactCompiler pc=new PactCompiler(new DataStatistics());
  OptimizedPlan op=pc.compile(plan);
  NepheleJobGraphGenerator jgg=new NepheleJobGraphGenerator();
  return jgg.compileJobGraph(op);
}
