{
  final Set<EvaluationExpression> keyExpressions=new HashSet<EvaluationExpression>();
  for (  ElementaryOperator<?> operator : this.getReachableNodes())   for (  List<? extends EvaluationExpression> expressions : operator.getAllKeyExpressions())   keyExpressions.addAll(expressions);
  this.schema=schemaFactory.create(keyExpressions);
}
