{
  if (fileOutput) {
    return Graph.fromCsvReader(edgesInputPath,new InitVertices(srcVertexId),env).fieldDelimiterEdges("\t").lineDelimiterEdges("\n").typesEdges(Long.class,Double.class).typesVertices(Long.class,Double.class);
  }
 else {
    return Graph.fromDataSet(SingleSourceShortestPathsData.getDefaultEdgeDataSet(env),new InitVertices(srcVertexId),env);
  }
}
