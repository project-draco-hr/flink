{
  final SopremoTestPlan sopremoPlan=new SopremoTestPlan(3,1);
  final Difference difference=new Difference();
  difference.setInputs(sopremoPlan.getInputOperators(0,3));
  sopremoPlan.getOutputOperator(0).setInputs(difference);
  sopremoPlan.getInput(0).addValue(1).addValue(2).addValue(3);
  sopremoPlan.getInput(1).addValue(1).addValue(2).addValue(4);
  sopremoPlan.getInput(2).addValue(2).addValue(3).addValue(5);
  sopremoPlan.getExpectedOutput(0).setEmpty();
  sopremoPlan.run();
}
