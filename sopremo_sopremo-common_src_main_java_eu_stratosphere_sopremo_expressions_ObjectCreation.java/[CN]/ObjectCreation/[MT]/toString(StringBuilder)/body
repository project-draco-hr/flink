{
  builder.append("{");
  final Iterator<Mapping<?>> mappingIterator=this.mappings.iterator();
  while (mappingIterator.hasNext()) {
    final Mapping<?> entry=mappingIterator.next();
    entry.toString(builder);
    if (mappingIterator.hasNext())     builder.append(", ");
  }
  builder.append("}");
}
