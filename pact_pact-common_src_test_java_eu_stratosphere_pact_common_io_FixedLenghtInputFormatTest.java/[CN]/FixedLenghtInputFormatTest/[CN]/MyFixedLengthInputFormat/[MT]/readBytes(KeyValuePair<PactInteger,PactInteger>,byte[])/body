{
  int key=0;
  key=(key | record[0]) << 8;
  key=(key | record[1]) << 8;
  key=(key | record[2]) << 8;
  key=(key | record[3]);
  int value=0;
  value=(value | record[4]) << 8;
  value=(value | record[5]) << 8;
  value=(value | record[6]) << 8;
  value=(value | record[7]);
  pair.getKey().setValue(key);
  pair.getValue().setValue(value);
  return true;
}
