{
  String filename=queryParams.get("file");
  if (filename != null) {
    File f=new File(jarDir,filename);
    if (f.exists()) {
      if (f.getName().endsWith(".jar")) {
        return "{}";
      }
 else {
        f.delete();
        return "{\"error\": \"Only Jar files are allowed.\"}";
      }
    }
  }
  return "{\"error\": \"Failed to upload the file.\"}";
}
