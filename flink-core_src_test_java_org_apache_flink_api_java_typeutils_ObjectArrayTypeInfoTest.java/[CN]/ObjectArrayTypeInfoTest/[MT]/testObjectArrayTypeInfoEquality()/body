{
  ObjectArrayTypeInfo<TestClass[],TestClass> tpeInfo1=ObjectArrayTypeInfo.getInfoFor(TestClass[].class,new GenericTypeInfo<TestClass>(TestClass.class));
  ObjectArrayTypeInfo<TestClass[],TestClass> tpeInfo2=ObjectArrayTypeInfo.getInfoFor(TestClass[].class,new GenericTypeInfo<TestClass>(TestClass.class));
  assertEquals(tpeInfo1,tpeInfo2);
  assertEquals(tpeInfo1.hashCode(),tpeInfo2.hashCode());
}
