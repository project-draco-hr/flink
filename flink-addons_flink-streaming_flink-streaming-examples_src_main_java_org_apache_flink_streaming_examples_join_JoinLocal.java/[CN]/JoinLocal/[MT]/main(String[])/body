{
  LogUtils.initializeDefaultConsoleLogger(Level.DEBUG,Level.INFO);
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment(PARALLELISM).setBufferTimeout(100);
  DataStream<Tuple3<String,String,Integer>> source1=env.addSource(new JoinSourceOne(),SOURCE_PARALLELISM);
  @SuppressWarnings("unused") DataStream<Tuple3<String,Integer,Integer>> source2=env.addSource(new JoinSourceTwo(),SOURCE_PARALLELISM).merge(source1).partitionBy(1).flatMap(new JoinTask()).addSink(new JoinSink());
  env.execute();
}
