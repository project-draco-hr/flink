{
  if (serializer.getClass() == PactRecordSerializer.class) {
    @SuppressWarnings("unchecked") MutableReader<PactRecord> reader=(MutableReader<PactRecord>)inputReader;
    return new PactRecordNepheleReaderIterator(reader);
  }
 else {
    @SuppressWarnings("unchecked") MutableReader<DeserializationDelegate<?>> reader=(MutableReader<DeserializationDelegate<?>>)inputReader;
    @SuppressWarnings({"unchecked","rawtypes"}) final MutableObjectIterator<?> iter=new NepheleReaderIterator(reader,serializer);
    return iter;
  }
}
