{
  GroupByExpr g=(GroupByExpr)expr;
  if (g.numInputs() != 1) {
    return false;
  }
  Expr aggExpr=AggregateFullExpr.makeIfAggregating(engine.env,g.getAsVar(0),null,g.collectExpr(),engine.exprList,engine.aggList,false);
  if (aggExpr == null) {
    return false;
  }
  g.setCollectExpr(aggExpr);
  return true;
}
