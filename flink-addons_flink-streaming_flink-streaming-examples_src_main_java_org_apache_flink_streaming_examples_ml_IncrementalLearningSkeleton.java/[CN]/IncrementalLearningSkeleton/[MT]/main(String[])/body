{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment(PARALLELISM).setBufferTimeout(1000);
  DataStream<Double[]> model=env.addSource(new TrainingDataSource(),SOURCE_PARALLELISM).windowReduce(new PartialModelBuilder(),5000);
  DataStream<Integer> prediction=env.addSource(new NewDataSource(),SOURCE_PARALLELISM).connect(model).map(new Predictor());
  prediction.print();
  env.execute();
}
