{
  if (getFileSystem().exists(new Path(hdfsFile))) {
    return false;
  }
  FileReader fr=new FileReader(new File(localFile));
  OutputStream os=this.getOutputStream(hdfsFile);
  Writer wr=new OutputStreamWriter(os);
  while (fr.ready()) {
    wr.write(fr.read());
  }
  wr.close();
  fr.close();
  return true;
}
