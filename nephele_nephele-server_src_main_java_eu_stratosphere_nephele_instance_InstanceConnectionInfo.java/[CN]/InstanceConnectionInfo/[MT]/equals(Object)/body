{
  if (obj instanceof InstanceConnectionInfo) {
    InstanceConnectionInfo ici=(InstanceConnectionInfo)obj;
    if (!this.inetAddress.equals(ici.getAddress())) {
      return false;
    }
    if (this.ipcPort != ici.getIPCPort()) {
      return false;
    }
    if (this.dataPort != ici.getDataPort()) {
      return false;
    }
    return true;
  }
  return false;
}
