{
  try {
    ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
    env.setDegreeOfParallelism(100);
    DataSet<Long> input=env.generateSequence(1,10);
    DataSet<Long> bc_input=env.generateSequence(1,10);
    input.map(new IdentityMapper<Long>()).withBroadcastSet(bc_input,"name1").map(new IdentityMapper<Long>()).withBroadcastSet(bc_input,"name2").print();
    Plan plan=env.createProgramPlan();
    compileNoStats(plan);
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail(e.getMessage());
  }
}
