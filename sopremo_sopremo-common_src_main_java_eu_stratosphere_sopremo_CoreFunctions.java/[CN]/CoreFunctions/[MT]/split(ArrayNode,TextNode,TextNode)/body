{
  final String[] split=input.getTextValue().split(splitString.getTextValue());
  for (int index=result.size() - 1; index > split.length; index--)   result.remove(index);
  int index=0;
  for (; index < result.size(); index++)   ((TextNode)result.get(index)).setValue(split[index]);
  for (; index < split.length; index++)   result.add(TextNode.valueOf(split[index]));
}
