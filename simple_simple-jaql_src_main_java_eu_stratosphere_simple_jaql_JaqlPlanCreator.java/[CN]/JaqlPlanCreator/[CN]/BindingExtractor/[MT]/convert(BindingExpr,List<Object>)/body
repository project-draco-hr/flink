{
  if (childOperators.isEmpty() && expr.numChildren() == 0)   return null;
  Expr valueExpr;
switch (expr.type) {
case IN:
    valueExpr=expr.inExpr();
  break;
case EQ:
valueExpr=expr.eqExpr();
break;
default :
System.out.println("unhandled binding");
return null;
}
Object transformedExpr=null;
if (!childOperators.isEmpty()) transformedExpr=childOperators.get(0);
 else if (valueExpr instanceof VarExpr) transformedExpr=JaqlPlanCreator.this.bindings.get(valueExpr.toString());
 else {
transformedExpr=JaqlPlanCreator.this.parseTransformation(valueExpr);
if (transformedExpr == null) transformedExpr=JaqlPlanCreator.this.parsePath(valueExpr);
}
JaqlPlanCreator.this.bindings.put(expr.var.taggedName(),new Binding(valueExpr,transformedExpr));
return null;
}
