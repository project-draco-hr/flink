{
  final Iterator<IJsonNode> iterator=((ArrayNode)node).iterator();
  if (!iterator.hasNext())   return ZERO;
  NumericNode sum=(NumericNode)iterator.next();
  for (; iterator.hasNext(); )   sum=ArithmeticExpression.ArithmeticOperator.ADDITION.evaluate(sum,(NumericNode)iterator.next());
  return sum;
}
