{
  final SopremoTestPlan sopremoPlan=new SopremoTestPlan(1,1);
  final UnionAll union=new UnionAll();
  union.setInputs(sopremoPlan.getInputOperator(0));
  sopremoPlan.getOutputOperator(0).setInputs(union);
  sopremoPlan.getInput(0).add(createValueNode((Object)1)).add(createValueNode((Object)2));
  sopremoPlan.getExpectedOutput(0).add(createValueNode((Object)1)).add(createValueNode((Object)2));
  sopremoPlan.run();
}
