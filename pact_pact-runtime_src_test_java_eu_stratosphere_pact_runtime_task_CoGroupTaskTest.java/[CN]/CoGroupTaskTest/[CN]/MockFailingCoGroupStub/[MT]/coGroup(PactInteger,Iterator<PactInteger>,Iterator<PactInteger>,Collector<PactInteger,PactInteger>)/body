{
  int val1Cnt=0;
  while (values1.hasNext()) {
    val1Cnt++;
    values1.next();
  }
  while (values2.hasNext()) {
    PactInteger val2=values2.next();
    if (val1Cnt == 0) {
      if (++cnt >= 10) {
        throw new RuntimeException("Expected Test Exception");
      }
      out.collect(key,val2);
    }
 else {
      for (int i=0; i < val1Cnt; i++) {
        if (++cnt >= 10) {
          throw new RuntimeException("Expected Test Exception");
        }
        out.collect(key,val2);
      }
    }
  }
}
