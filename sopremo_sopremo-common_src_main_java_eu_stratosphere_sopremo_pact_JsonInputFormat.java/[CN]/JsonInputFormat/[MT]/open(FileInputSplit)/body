{
  super.open(split);
  this.end=false;
  if (this.encoding != null)   this.parser=new JsonParser(new InputStreamReader(this.stream,this.encoding));
 else   this.parser=new JsonParser(this.stream);
  this.parser.setCodec(JsonUtil.OBJECT_MAPPER);
  if (this.array=this.parser.nextToken() == JsonToken.START_ARRAY)   this.parser.clearCurrentToken();
  this.checkEnd();
}
