{
  super.open(split);
  this.end=false;
  if (encoding != null)   this.parser=JsonUtil.FACTORY.createJsonParser(new InputStreamReader(stream,encoding));
 else   this.parser=JsonUtil.FACTORY.createJsonParser(stream);
  this.parser.setCodec(JsonUtil.OBJECT_MAPPER);
  this.parser.enable(JsonParser.Feature.ALLOW_UNQUOTED_CONTROL_CHARS);
  this.parser.enable(JsonParser.Feature.INTERN_FIELD_NAMES);
  if (this.array=this.parser.nextToken() == JsonToken.START_ARRAY)   this.parser.clearCurrentToken();
  this.checkEnd();
}
