{
  JsonNode outKey=key, outValue=value;
  if (this.keyTransformation != EvaluationExpression.KEY)   outKey=this.keyTransformation.evaluate(value,this.getContext());
  if (this.valueTransformation == EvaluationExpression.KEY)   outValue=key;
 else   if (this.valueTransformation != EvaluationExpression.VALUE)   outValue=this.valueTransformation.evaluate(value,this.getContext());
  out.collect(outKey,outValue);
}
