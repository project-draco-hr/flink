{
  if (LOG.isDebugEnabled())   LOG.debug(String.format("Projecting %s/%s",key,value));
  if (this.keyTransformation != EvaluableExpression.SAME_KEY)   key=this.keyTransformation.evaluate(value,this.getContext());
  if (this.keyTransformation != EvaluableExpression.SAME_VALUE)   value=this.valueTransformation.evaluate(value,this.getContext());
  if (LOG.isDebugEnabled())   LOG.debug(String.format(" to %s/%s",key,value));
  out.collect(key,value);
}
