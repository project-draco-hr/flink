{
  if (transformation == null || inputs == null)   throw new NullPointerException();
  this.inputs=new ArrayList<Operator.Output>();
  for (  Operator operator : inputs)   this.inputs.add(operator == null ? null : operator.getOutput(0));
  this.name=name == null ? this.getClass().getSimpleName() : name;
  this.transformation=transformation;
  this.outputs=new Output[]{new Output(0)};
}
