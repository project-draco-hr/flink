{
  if (inputs == null)   throw new NullPointerException();
  if (numberOfOutputs < 0)   throw new IllegalArgumentException("numberOfOutputs < 0");
  for (  final JsonStream input : inputs)   this.inputs.add(input == null ? null : input.getSource());
  this.name=this.getClass().getSimpleName();
  this.setNumberOfOutputs(numberOfOutputs);
}
