{
  if (inputs == null)   throw new NullPointerException("inputs must not be null");
  if (this.minInputs > inputs.length || inputs.length > this.maxInputs)   throw new IndexOutOfBoundsException();
  this.inputs.clear();
  for (  final JsonStream input : inputs)   this.inputs.add(input == null ? null : input.getSource());
}
