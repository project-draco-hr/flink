{
  SopremoModule module=new SopremoModule(getName(),getInputs().size(),getOutputs().size());
  Operator clone=clone();
  for (int index=0; index < getInputs().size(); index++)   clone.setInput(index,module.getInput(index));
  for (int index=0; index < getOutputs().size(); index++)   module.getOutput(index).setInput(index,clone.getOutput(index));
  return module;
}
