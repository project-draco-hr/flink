{
  for (  String fileToClean : this.filesToCleanup)   try {
    if (!getFilesystemProvider().delete(fileToClean,false))     getFilesystemProvider().delete(fileToClean,true);
  }
 catch (  IOException e) {
  }
  try {
    this.cluster.stopCluster();
    ClusterProviderPool.removeInstance(this.configName);
  }
 catch (  Exception e) {
  }
  this.server.close();
  if (this.executor != this.server)   this.rpcService.shutDown();
  FileSystem.closeAll();
}
