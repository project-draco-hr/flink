{
  if (this.lineDelimiterIsLinebreak == true && bytes[offset + numBytes - 1] == '\r') {
    numBytes--;
  }
  if (parseRecord(parsedValues,bytes,offset,numBytes)) {
    for (int i=0; i < parsedValues.length; i++) {
      reuse.setField(targetPositions[i],parsedValues[i]);
    }
    return reuse;
  }
 else {
    return null;
  }
}
