{
  final TransferEnvelope duplicatedTransferEnvelope=new TransferEnvelope(this.sequenceNumber,this.jobID,this.source);
  duplicatedTransferEnvelope.eventList=this.eventList;
  if (this.buffer != null) {
    duplicatedTransferEnvelope.buffer=this.buffer.duplicate();
  }
 else {
    duplicatedTransferEnvelope.buffer=null;
  }
  return duplicatedTransferEnvelope;
}
