{
  if (reachedEnd()) {
    return null;
  }
  if (reuseAvroValue) {
    return dataFileReader.next(reuseValue);
  }
 else {
    if (GenericRecord.class == avroValueType) {
      return dataFileReader.next();
    }
 else {
      return dataFileReader.next(InstantiationUtil.instantiate(avroValueType,Object.class));
    }
  }
}
