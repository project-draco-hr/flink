{
synchronized (this) {
    if (this.closed) {
      throw new IllegalStateException("Writer is already closing or has been closed.");
    }
    this.closed=true;
  }
  this.returnBuffers.add(currentBuffer);
  this.currentBuffer=null;
  List<MemorySegment> segments=super.close();
  fileChannel.close();
  return segments;
}
