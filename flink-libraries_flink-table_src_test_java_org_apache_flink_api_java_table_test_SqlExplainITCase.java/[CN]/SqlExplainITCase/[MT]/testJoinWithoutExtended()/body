{
  ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  TableEnvironment tableEnv=new TableEnvironment();
  DataSet<WC> input1=env.fromElements(new WC(1,"d"),new WC(1,"d"),new WC(1,"d"));
  Table table1=tableEnv.fromDataSet(input1,"count as a, word as b");
  DataSet<WC> input2=env.fromElements(new WC(1,"d"),new WC(1,"d"),new WC(1,"d"));
  Table table2=tableEnv.fromDataSet(input2,"count as c, word as d");
  String result=table1.join(table2).where("b = d").select("a, c").explain();
  String source=new Scanner(new File(testFilePath + "../../src/test/scala/resources/testJoin0.out")).useDelimiter("\\A").next();
  assertEquals(source,result);
}
