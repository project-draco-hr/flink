{
  ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  TableEnvironment tableEnv=new TableEnvironment();
  DataSet<WC> input=env.fromElements(new WC(1,"d"),new WC(2,"d"),new WC(3,"d"));
  Table table=tableEnv.fromDataSet(input,"count as a, word as b");
  String result=table.filter("a % 2 = 0").explain(true);
  String source=new Scanner(new File(testFilePath + "../../src/test/scala/resources/testFilter1.out")).useDelimiter("\\A").next();
  assertEquals(source,result);
}
