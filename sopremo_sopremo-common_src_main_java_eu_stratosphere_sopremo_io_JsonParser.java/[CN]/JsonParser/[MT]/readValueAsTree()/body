{
  this.state.push(this.root);
  StringBuilder sb=new StringBuilder();
  int nextChar=this.reader.read();
  if (this.reachedEnd || nextChar == -1)   throw new NoSuchElementException("Reached end of json document!");
  while ((this.state.top() != this.root || nextChar != ',') && nextChar != -1) {
    char character=(char)nextChar;
    if (this.insideString && character != '\"')     sb.append(character);
 else     this.handler.get(character).handleCharacter(sb,character);
    nextChar=this.reader.read();
  }
  ;
  if (sb.length() != 0) {
    this.root.addValue(JsonParser.parsePrimitive(sb.toString()));
    if (!this.isArray)     this.reachedEnd=true;
    sb.setLength(0);
  }
  return this.root.remove(0);
}
