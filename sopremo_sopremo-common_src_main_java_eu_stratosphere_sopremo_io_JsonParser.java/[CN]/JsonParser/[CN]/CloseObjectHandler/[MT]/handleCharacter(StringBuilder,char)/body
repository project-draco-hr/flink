{
  ContainerNode node=(ContainerNode)state.pop();
  if (sb.length() != 0) {
    if (!wasString) {
      node.addValue(JsonParser.parsePrimitive(sb.toString()));
    }
 else {
      node.addValue(TextNode.valueOf(sb.toString()));
      wasString=false;
    }
    sb.setLength(0);
  }
  ((ContainerNode)state.top()).addValue(node.build());
}
