{
  JsonNode node;
  if (this.keys.size() == 0) {
    node=new ArrayNode();
    for (    JsonNode value : this.values) {
      ((ArrayNode)node).add(value);
    }
  }
 else {
    if (this.keys.size() != this.values.size()) {
      throw new JsonParseException();
    }
    node=new ObjectNode();
    for (int i=0; i < this.keys.size(); i++) {
      ((ObjectNode)node).put(this.keys.get(i),this.values.get(i));
    }
  }
  return node;
}
