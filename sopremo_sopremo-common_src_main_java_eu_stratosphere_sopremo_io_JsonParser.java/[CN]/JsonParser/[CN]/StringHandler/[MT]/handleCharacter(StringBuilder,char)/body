{
  if (sb.length() == 0) {
    insideString=true;
  }
 else {
    if (!sb.toString().endsWith("\\")) {
      insideString=false;
      ArrayNode node=(ArrayNode)state.top();
      node.add(TextNode.valueOf(sb.toString()));
      sb.setLength(0);
    }
 else {
      sb.append(character);
    }
  }
}
