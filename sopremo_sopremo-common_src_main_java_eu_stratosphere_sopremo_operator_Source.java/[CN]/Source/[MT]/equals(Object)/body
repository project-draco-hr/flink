{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (getClass() != obj.getClass())   return false;
  Source other=(Source)obj;
  if (adhocValue == null) {
    if (other.adhocValue != null)     return false;
  }
 else   if (!adhocValue.equals(other.adhocValue))   return false;
  if (inputName == null) {
    if (other.inputName != null)     return false;
  }
 else   if (!inputName.equals(other.inputName))   return false;
  if (type != other.type)   return false;
  return true;
}
