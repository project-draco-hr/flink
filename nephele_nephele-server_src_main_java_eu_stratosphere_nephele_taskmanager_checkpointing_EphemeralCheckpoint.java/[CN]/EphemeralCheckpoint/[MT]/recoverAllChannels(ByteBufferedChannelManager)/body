{
  Iterator<ChannelID> channelIDIterator=this.channelCheckpoints.keySet().iterator();
  while (channelIDIterator.hasNext()) {
    ChannelID channelID=channelIDIterator.next();
    final CheckpointRecoveryThread thread=new CheckpointRecoveryThread(byteBufferedChannelManager,this.channelCheckpoints.get(channelID),channelID,false);
    thread.start();
  }
}
