{
  Assert.assertTrue(isOpened);
  Assert.assertFalse(isClosed);
  String f0=null;
  int sumF1=0;
  for (  Tuple2<String,Integer> input : first) {
    f0=(f0 == null) ? input.f0 : f0;
    sumF1+=input.f1;
  }
  for (  Tuple2<String,Integer> input : second) {
    f0=(f0 == null) ? input.f0 : f0;
    sumF1+=input.f1;
  }
  out.collect(new Tuple2<String,Integer>(f0,sumF1));
}
