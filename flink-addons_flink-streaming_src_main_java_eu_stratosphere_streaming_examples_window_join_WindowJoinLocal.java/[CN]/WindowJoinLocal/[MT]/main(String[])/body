{
  LogUtils.initializeDefaultConsoleLogger(Level.DEBUG,Level.INFO);
  StreamExecutionEnvironment context=new StreamExecutionEnvironment();
  DataStream<Tuple4<String,String,Integer,Long>> source1=context.addSource(new WindowJoinSourceOne());
  @SuppressWarnings("unused") DataStream<Tuple3<String,Integer,Integer>> source2=context.addSource(new WindowJoinSourceTwo()).connectWith(source1).partitionBy(1).flatMap(new WindowJoinTask()).addSink(new JoinSink());
  context.execute();
}
