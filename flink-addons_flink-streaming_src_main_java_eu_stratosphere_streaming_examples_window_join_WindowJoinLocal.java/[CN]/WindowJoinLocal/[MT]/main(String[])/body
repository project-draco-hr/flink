{
  LogUtils.initializeDefaultConsoleLogger(Level.DEBUG,Level.INFO);
  StreamExecutionEnvironment env=new StreamExecutionEnvironment();
  DataStream<Tuple4<String,String,Integer,Long>> source1=env.addSource(new WindowJoinSourceOne(),SOURCE_PARALELISM);
  @SuppressWarnings("unused") DataStream<Tuple3<String,Integer,Integer>> source2=env.addSource(new WindowJoinSourceTwo(),SOURCE_PARALELISM).connectWith(source1).partitionBy(1).flatMap(new WindowJoinTask(),PARALELISM).addSink(new JoinSink());
  env.execute();
}
