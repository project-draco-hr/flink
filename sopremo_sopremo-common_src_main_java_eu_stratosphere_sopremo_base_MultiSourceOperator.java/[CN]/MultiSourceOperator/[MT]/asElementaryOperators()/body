{
  int numInputs=this.getInputOperators().size();
  SopremoModule module=new SopremoModule(this.getName(),numInputs,1);
  List<Operator> inputs=new ArrayList<Operator>();
  for (int index=0; index < numInputs; index++)   inputs.add(new Projection(this.getKeyProjection(index),this.getValueProjection(index),module.getInput(index)));
  Operator lastOperator=this.createElementaryOperations(inputs);
  module.getOutput(0).setInput(0,new Projection(EvaluableExpression.NULL,EvaluableExpression.SAME_VALUE,lastOperator));
  return module;
}
