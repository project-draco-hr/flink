{
  StreamInvokable<?,?> headInvokable=streamGraph.getInvokable(vertexName);
  StreamInvokable<?,?> outInvokable=streamGraph.getInvokable(outName);
  return streamGraph.getInEdges(outName).size() == 1 && outInvokable != null && outInvokable.getChainingStrategy() == ChainingStrategy.ALWAYS && (headInvokable.getChainingStrategy() == ChainingStrategy.HEAD || headInvokable.getChainingStrategy() == ChainingStrategy.ALWAYS) && streamGraph.getOutPartitioner(vertexName,outName).getStrategy() == PartitioningStrategy.FORWARD && streamGraph.getParallelism(vertexName) == streamGraph.getParallelism(outName) && streamGraph.chaining;
}
