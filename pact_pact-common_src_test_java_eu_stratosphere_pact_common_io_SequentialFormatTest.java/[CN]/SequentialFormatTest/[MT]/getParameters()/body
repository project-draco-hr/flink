{
  ArrayList<Object[]> params=new ArrayList<Object[]>();
  for (int dop=1; dop <= 2; dop++)   for (int numberOfTuples=100; numberOfTuples < 10000; numberOfTuples*=10) {
    for (long blockSize=1000; blockSize < 1 << 20; blockSize*=10)     params.add(new Object[]{numberOfTuples,blockSize,dop});
    params.add(new Object[]{numberOfTuples,SequentialOutputFormat.NATIVE_BLOCK_SIZE,dop});
  }
  return params;
}
