{
  if (value instanceof ObjectNode)   for (  Entry<String,JsonNode> entry : ((ObjectNode)value).getEntries()) {
    path.push(new ObjectAccess(entry.getKey()));
    this.discoverEntries(entry.getValue(),path);
    path.pop();
  }
 else   if (value instanceof ArrayNode)   for (int index=0; index < ((ArrayNode)value).size(); index++) {
    path.push(new ArrayAccess(index));
    this.discoverEntries(((ArrayNode)value).get(index),path);
    path.pop();
  }
 else   this.extractionExpressions.add(PathExpression.wrapIfNecessary(path));
}
