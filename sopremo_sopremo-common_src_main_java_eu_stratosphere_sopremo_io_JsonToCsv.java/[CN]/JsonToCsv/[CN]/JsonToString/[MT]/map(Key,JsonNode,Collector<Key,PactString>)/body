{
  StringBuilder string=new StringBuilder();
  value=((JsonNodeWrapper)value).getValue();
  if (this.extractionExpressions.isEmpty())   discoverEntries(value,new LinkedList<EvaluationExpression>());
  for (  EvaluationExpression expr : this.extractionExpressions)   string.append(expr.evaluate(value,this.context)).append(this.separator);
  string.setLength(string.length() - 1);
  out.collect(key,new PactString(string.toString()));
}
