{
  if (value instanceof ObjectNode)   for (  final Entry<String,IJsonNode> entry : ((IObjectNode)value).getEntries()) {
    path.push(new ObjectAccess(entry.getKey()));
    this.discoverEntries(entry.getValue(),path);
    path.pop();
  }
 else   if (value instanceof ArrayNode)   for (int index=0; index < ((IArrayNode)value).size(); index++) {
    path.push(new ArrayAccess(index));
    this.discoverEntries(((IArrayNode)value).get(index),path);
    path.pop();
  }
 else   this.extractionExpressions.add(PathExpression.wrapIfNecessary(path));
}
