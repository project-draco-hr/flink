{
switch (versionId) {
case 0:
    return new JoinGroupResponse(Errors.forException(e).code(),JoinGroupResponse.UNKNOWN_GENERATION_ID,JoinGroupResponse.UNKNOWN_CONSUMER_ID,Collections.<TopicPartition>emptyList());
default :
  throw new IllegalArgumentException(String.format("Version %d is not valid. Valid versions for %s are 0 to %d",versionId,this.getClass().getSimpleName(),ProtoUtils.latestVersion(ApiKeys.JOIN_GROUP.id)));
}
}
