{
  StreamExecutionEnvironment env=StreamExecutionEnvironment.createLocalEnvironment();
  DataStream<Tuple1<Integer>> model=env.addSource(new TrainingDataSource(),SOURCE_PARALELISM).map(new PartialModelBuilder(),PARALELISM).broadcast();
  DataStream<Tuple1<Integer>> prediction=env.addSource(new NewDataSource(),SOURCE_PARALELISM).connectWith(model).map(new Predictor(),PARALELISM);
  prediction.print();
  env.execute();
}
