{
  final TransferEnvelope transferEnvelope=new TransferEnvelope(SENDER_HINT_SEQUENCE_NUMBER,originalEnvelope.getJobID(),originalEnvelope.getSource());
  final SenderHintEvent senderEvent=new SenderHintEvent(source,remoteReceiver);
  transferEnvelope.addEvent(senderEvent);
  return transferEnvelope;
}
