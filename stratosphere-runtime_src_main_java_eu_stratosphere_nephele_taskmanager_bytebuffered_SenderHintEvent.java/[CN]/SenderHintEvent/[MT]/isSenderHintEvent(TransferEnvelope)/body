{
  if (transferEnvelope.getSequenceNumber() != SENDER_HINT_SEQUENCE_NUMBER) {
    return false;
  }
  if (transferEnvelope.getBuffer() != null) {
    return false;
  }
  final EventList eventList=transferEnvelope.getEventList();
  if (eventList == null) {
    return false;
  }
  if (eventList.size() != 1) {
    return false;
  }
  if (!(eventList.get(0) instanceof SenderHintEvent)) {
    return false;
  }
  return true;
}
