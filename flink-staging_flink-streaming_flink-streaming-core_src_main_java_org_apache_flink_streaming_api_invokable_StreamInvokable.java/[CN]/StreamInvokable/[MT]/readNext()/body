{
  this.nextRecord=inSerializer.createInstance();
  try {
    nextRecord=recordIterator.next(nextRecord);
    try {
      nextObject=nextRecord.getObject();
    }
 catch (    NullPointerException e) {
    }
    return nextRecord;
  }
 catch (  IOException e) {
    throw new RuntimeException("Could not read next record.");
  }
}
