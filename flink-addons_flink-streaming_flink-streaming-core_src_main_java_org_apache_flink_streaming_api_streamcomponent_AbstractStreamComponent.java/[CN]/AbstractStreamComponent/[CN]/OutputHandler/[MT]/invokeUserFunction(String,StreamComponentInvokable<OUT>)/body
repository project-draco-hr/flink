{
  if (LOG.isDebugEnabled()) {
    LOG.debug(componentTypeName + " " + name+ " invoked with instance id "+ instanceID);
  }
  initializeOutputSerializers();
  try {
    userInvokable.open(getTaskConfiguration());
    userInvokable.invoke();
    userInvokable.close();
  }
 catch (  Exception e) {
    flushOutputs();
    throw new RuntimeException(e);
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug(componentTypeName + " " + name+ " invoke finished with instance id "+ instanceID);
  }
  flushOutputs();
}
