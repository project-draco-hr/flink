{
  if (holder1.readBinary(this.keyFields,this.keyHolders1,sources1,firstSegment1,offset1) & holder2.readBinary(this.keyFields,this.keyHolders2,sources2,firstSegment2,offset2)) {
    for (int i=0; i < this.keyHolders1.length; i++) {
      final int val=this.keyHolders1[i].compareTo(this.keyHolders2[i]);
      if (val != 0)       return val;
    }
    return 0;
  }
 else   throw new NullKeyFieldException();
}
