{
  for (  OutputNode output : this.getAllOutputs())   for (  Node node : this.navigator.getConnectedNodes(output))   if (node == null)   throw new IllegalStateException(String.format("Output %s is not fully connected",output));
  final Iterable<? extends Node> reachableNodes=this.getReachableNodes();
  List<InputNode> inputList=new LinkedList<InputNode>(Arrays.asList(this.inputNodes));
  for (  Node node : reachableNodes)   inputList.remove(node);
  if (!inputList.isEmpty())   throw new IllegalStateException(String.format("Inputs %s are not fully connected",inputList));
}
