{
  if (contract instanceof SingleInputContract)   return new Contract[]{((SingleInputContract<?,?,?,?>)contract).getInput()};
  if (contract instanceof DualInputContract)   return new Contract[]{((DualInputContract<?,?,?,?,?,?>)contract).getFirstInput(),((DualInputContract<?,?,?,?,?,?>)contract).getSecondInput()};
  if (contract instanceof DataSinkContract<?,?>)   return new Contract[]{((DataSinkContract<?,?>)contract).getInput()};
  return new Contract[0];
}
