{
  final ExecutionEnvironment env=ExecutionEnvironment.getExecutionEnvironment();
  DataSet<Tuple5<Integer,Long,Integer,String,Long>> ds=CollectionDataSets.getSmall5TupleDataSet(env);
  DataSet<Tuple5<Integer,Long,Integer,String,Long>> ds2=CollectionDataSets.getSmall5TupleDataSet(env);
  DataSet<Tuple2<Integer,String>> crossDs=ds.crossWithTiny(ds2).with(new Tuple5Cross());
  crossDs.writeAsCsv(resultPath);
  env.execute();
  expected="0,HalloHallo\n" + "1,HalloHallo Welt\n" + "2,HalloHallo Welt wie\n"+ "1,Hallo WeltHallo\n"+ "2,Hallo WeltHallo Welt\n"+ "3,Hallo WeltHallo Welt wie\n"+ "2,Hallo Welt wieHallo\n"+ "3,Hallo Welt wieHallo Welt\n"+ "4,Hallo Welt wieHallo Welt wie\n";
}
