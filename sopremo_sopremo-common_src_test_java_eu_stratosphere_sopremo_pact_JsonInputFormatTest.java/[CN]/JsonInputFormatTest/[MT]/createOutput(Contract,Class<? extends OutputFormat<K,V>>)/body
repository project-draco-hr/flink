{
  try {
    final DataSinkContract<K,V> out=new DataSinkContract<K,V>(outputFormatClass,File.createTempFile("output",null).toURI().toString(),"Output");
    out.setInput(input);
    return out;
  }
 catch (  final IOException e) {
    Assert.fail("cannot create temporary output file" + e);
    return null;
  }
}
