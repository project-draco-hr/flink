{
  Random rnd=new Random();
  while (true) {
    if (counter > 9999) {
      System.out.println("Source pauses now!");
      Thread.sleep(5000);
      System.out.println("Source continouse with emitting now!");
      counter=0;
    }
    collector.collect(rnd.nextInt(9) + 1);
    Thread.sleep(1);
    counter++;
  }
}
