{
  for (  final Method method : javaFunctions.getDeclaredMethods())   if ((method.getModifiers() & Modifier.STATIC) != 0 && JsonNode.class.isAssignableFrom(method.getReturnType())) {
    final Class<?>[] parameterTypes=method.getParameterTypes();
    if (!this.isCompatibleSignature(method,parameterTypes))     continue;
    Function javaFunction=this.registeredFunctions.get(method.getName());
    if (javaFunction == null)     this.registeredFunctions.put(method.getName(),javaFunction=new JavaFunction(method.getName()));
 else     if (!(javaFunction instanceof JavaFunction))     throw new IllegalArgumentException(String.format("a function with the name %s is already registered and not a java function: %s",method.getName(),javaFunction));
    ((JavaFunction)javaFunction).addSignature(method);
  }
}
