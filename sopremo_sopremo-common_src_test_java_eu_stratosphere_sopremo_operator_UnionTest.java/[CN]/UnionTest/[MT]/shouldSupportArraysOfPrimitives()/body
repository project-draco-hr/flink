{
  SopremoTestPlan sopremoPlan=new SopremoTestPlan(2,1);
  Union union=new Union(sopremoPlan.getInputOperators(0,2));
  sopremoPlan.getOutputOperator(0).setInputs(union);
  sopremoPlan.getInput(0).add(createJsonArray(1,2)).add(createJsonArray(3,4)).add(createJsonArray(5,6));
  sopremoPlan.getInput(1).add(createJsonArray(1,2)).add(createJsonArray(3,4)).add(createJsonArray(7,8));
  sopremoPlan.getExpectedOutput(0).add(createJsonArray(1,2)).add(createJsonArray(3,4)).add(createJsonArray(5,6)).add(createJsonArray(7,8));
  sopremoPlan.run();
}
