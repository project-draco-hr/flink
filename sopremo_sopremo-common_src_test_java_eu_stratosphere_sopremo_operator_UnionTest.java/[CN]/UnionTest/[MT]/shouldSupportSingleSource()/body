{
  SopremoTestPlan sopremoPlan=new SopremoTestPlan(1,1);
  Union union=new Union(sopremoPlan.getInputOperator(0));
  sopremoPlan.getOutputOperator(0).setInputs(union);
  sopremoPlan.getInput(0).add(createPactJsonValue(1)).add(createPactJsonValue(2)).add(createPactJsonValue(3));
  sopremoPlan.getExpectedOutput(0).add(createPactJsonValue(1)).add(createPactJsonValue(2)).add(createPactJsonValue(3));
  sopremoPlan.run();
}
