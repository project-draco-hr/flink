{
  SopremoTestPlan sopremoPlan=new SopremoTestPlan(2,1);
  Union union=new Union(sopremoPlan.getInputOperators(0,2));
  sopremoPlan.getOutputOperator(0).setInputs(union);
  sopremoPlan.getInput(0).add(createJsonValue(1)).add(createJsonValue(2)).add(createJsonValue(3));
  sopremoPlan.getInput(1).add(createJsonValue(1)).add(createJsonValue(2)).add(createJsonValue(4));
  sopremoPlan.getExpectedOutput(0).add(createJsonValue(1)).add(createJsonValue(2)).add(createJsonValue(3)).add(createJsonValue(4));
  sopremoPlan.run();
}
