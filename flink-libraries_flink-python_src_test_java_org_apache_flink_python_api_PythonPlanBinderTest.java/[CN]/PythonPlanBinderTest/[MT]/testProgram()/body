{
  if (isPython2Supported()) {
    for (    String file : findTestFiles()) {
      PythonPlanBinder.main(new String[]{ARGUMENT_PYTHON_2,file});
    }
  }
  if (isPython3Supported()) {
    for (    String file : findTestFiles()) {
      PythonPlanBinder.main(new String[]{ARGUMENT_PYTHON_3,file});
    }
  }
}
