{
  if (currentInput == null) {
    return null;
  }
 else {
    try {
      Serializable key=keySelector.getKey(currentInput);
      if (stateStore.containsKey(key)) {
        return stateStore.getStateForKey(key);
      }
 else {
        return defaultState;
      }
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
}
