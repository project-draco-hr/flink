{
  if (!isCombineable()) {
    return false;
  }
  if (this.input.get(0).getShipStrategy() == ShipStrategy.PARTITION_HASH) {
    for (    PactConnection c : this.input) {
      if (c.getShipStrategy() != ShipStrategy.PARTITION_HASH)       return false;
    }
    return true;
  }
  if (this.input.get(0).getShipStrategy() == ShipStrategy.PARTITION_RANGE) {
    for (    PactConnection c : this.input) {
      if (c.getShipStrategy() != ShipStrategy.PARTITION_RANGE)       return false;
    }
    return true;
  }
  return false;
}
