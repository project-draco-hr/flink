{
  if (series[0] == -1) {
    series[0]=compressionTime;
    series[3]=compressionTime;
    series[2]=0;
  }
 else {
    if (series[3] != -1) {
      series[0]=series[3];
    }
 else {
      series[0]=series[1];
    }
    series[2]=0;
    series[1]=compressionTime;
    series[3]=(long)((EXP_SMOOTHING_ALPHA * (double)(series[1])) + ((1 - EXP_SMOOTHING_ALPHA) * (double)(series[0])));
  }
}
