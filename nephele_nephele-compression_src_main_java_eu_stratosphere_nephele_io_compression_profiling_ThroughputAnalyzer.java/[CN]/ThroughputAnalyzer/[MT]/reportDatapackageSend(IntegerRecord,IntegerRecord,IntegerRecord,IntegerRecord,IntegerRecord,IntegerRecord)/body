{
  long sendTime=System.nanoTime();
synchronized (livingDataPackages) {
    livingDataPackages.put(dataID.getValue(),new DataInformation(compressorID.getValue(),dataID.getValue(),sendTime,bytes.getValue(),uncompressedBytes.getValue(),uncompressedBufferSize.getValue(),compressionLevel.getValue()));
  }
  LOG.debug("ThroughputAnalyzer: new package on the way! " + dataID.getValue());
}
