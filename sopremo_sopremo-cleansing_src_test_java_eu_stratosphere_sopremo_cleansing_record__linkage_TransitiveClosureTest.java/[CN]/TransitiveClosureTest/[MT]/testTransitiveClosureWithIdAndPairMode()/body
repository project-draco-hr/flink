{
  final TransitiveClosure transitiveClosure=new TransitiveClosure();
  transitiveClosure.setClosureMode(ClosureMode.LINKS);
  final SopremoTestPlan sopremoTestPlan=new SopremoTestPlan(transitiveClosure);
  sopremoTestPlan.getInput(0).addArray(createObjectNode("id",11,"name","a"),createObjectNode("id",22,"name","b")).addArray(createObjectNode("id",11,"name","a"),createObjectNode("id",23,"name","c")).addArray(createObjectNode("id",14,"name","e"),createObjectNode("id",25,"name","d")).addArray(createObjectNode("id",16,"name","a"),createObjectNode("id",25,"name","d"));
  sopremoTestPlan.getExpectedOutput(0).addArray(createObjectNode("id",11,"name","a"),createObjectNode("id",22,"name","b")).addArray(createObjectNode("id",11,"name","a"),createObjectNode("id",23,"name","c")).addArray(createObjectNode("id",22,"name","b"),createObjectNode("id",23,"name","c")).addArray(createObjectNode("id",14,"name","e"),createObjectNode("id",25,"name","d")).addArray(createObjectNode("id",16,"name","a"),createObjectNode("id",25,"name","d")).addArray(createObjectNode("id",14,"name","e"),createObjectNode("id",16,"name","a"));
  sopremoTestPlan.run();
}
