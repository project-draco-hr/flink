  Collections.shuffle(data);
  TupleTypeInfo<Tuple3<Integer,Long,String>> type=new TupleTypeInfo<Tuple3<Integer,Long,String>>(BasicTypeInfo.INT_TYPE_INFO,BasicTypeInfo.LONG_TYPE_INFO,BasicTypeInfo.STRING_TYPE_INFO);
