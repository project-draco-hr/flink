{
synchronized (this.floatingInstances) {
    Iterator<Map.Entry<InstanceConnectionInfo,FloatingInstance>> it=this.floatingInstances.entrySet().iterator();
    while (it.hasNext()) {
      final Map.Entry<InstanceConnectionInfo,FloatingInstance> entry=it.next();
      if (entry.getValue().checkIfLifeCycleEnded()) {
        it.remove();
        LOG.info("Floating Instance " + entry.getValue().getInstanceID() + " ended its lifecycle. Terminated");
      }
    }
  }
}
