{
  super(registry,registry.getScopeFormats().getTaskFormat().formatScope(checkNotNull(parent),vertexId,checkNotNull(executionId),taskName,subtaskIndex,attemptNumber));
  this.parent=parent;
  this.executionId=executionId;
  this.vertexId=vertexId;
  this.taskName=taskName;
  this.subtaskIndex=subtaskIndex;
  this.attemptNumber=attemptNumber;
  this.ioMetrics=new IOMetricGroup(this);
}
