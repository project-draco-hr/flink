{
  if (this.type == PersistenceType.ADHOC)   throw new UnsupportedOperationException();
  final PactModule pactModule=new PactModule(this.toString(),0,1);
  final FileDataSourceContract<PactJsonObject.Key,PactJsonObject> contract=new FileDataSourceContract<PactJsonObject.Key,PactJsonObject>(JsonInputFormat.class,this.inputName,this.inputName);
  pactModule.getOutput(0).setInput(contract);
  return pactModule;
}
