{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (this.getClass() != obj.getClass())   return false;
  final Source other=(Source)obj;
  return (this.inputName == null ? other.inputFormat == null : this.inputName.equals(other.inputName)) && (this.adhocExpression == null ? this.adhocExpression == null : this.adhocExpression.equals(other.adhocExpression));
}
