{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (this.getClass() != obj.getClass())   return false;
  final Source other=(Source)obj;
  return (this.inputPath == null ? other.inputPath == null : this.inputPath.equals(other.inputPath)) && (this.adhocExpression == null ? this.adhocExpression == null : this.adhocExpression.equals(other.adhocExpression));
}
