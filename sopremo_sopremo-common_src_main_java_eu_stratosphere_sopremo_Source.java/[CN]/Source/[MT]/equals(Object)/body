{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (this.getClass() != obj.getClass())   return false;
  final Source other=(Source)obj;
  return this.type == other.type && (this.inputName == other.inputName || this.inputName.equals(other.inputName)) && (this.adhocValue == other.adhocValue || this.adhocValue.equals(other.adhocValue));
}
