{
  if (isForceKryoEnabled()) {
    List<Class<?>> result=new ArrayList<Class<?>>();
    result.addAll(registeredKryoTypes);
    for (    Class<?> t : registeredPojoTypes) {
      if (!result.contains(t)) {
        result.add(t);
      }
    }
    return result;
  }
 else {
    return registeredKryoTypes;
  }
}
