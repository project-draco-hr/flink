{
  this.buildSideRecordCounter++;
  if (isInMemory()) {
    final long pointer=this.buildSideWriteBuffer.getCurrentPointer();
    this.buildSideAccessors.serialize(record,this.buildSideWriteBuffer);
    return isInMemory() ? pointer : -1;
  }
 else {
    this.buildSideAccessors.serialize(record,this.buildSideWriteBuffer);
    return -1;
  }
}
