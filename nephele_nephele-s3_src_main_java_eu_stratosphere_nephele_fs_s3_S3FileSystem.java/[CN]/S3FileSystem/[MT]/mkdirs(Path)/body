{
  final URI uri=f.toUri();
  HttpURLConnection conn=null;
  try {
    conn=openHttpURLConnection("PUT",uri.getPath());
    if (conn.getResponseCode() != HTTP_SUCCESS_CODE) {
      throw new IOException(conn.getResponseMessage());
    }
  }
  finally {
    if (conn != null) {
      conn.disconnect();
    }
  }
  return true;
}
