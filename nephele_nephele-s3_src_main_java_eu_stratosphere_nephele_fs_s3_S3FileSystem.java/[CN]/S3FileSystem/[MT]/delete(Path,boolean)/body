{
  final FileStatus fileStatus=getFileStatus(f);
  if (fileStatus.isDir()) {
    final FileStatus[] dirContent=listStatus(f);
    if (dirContent.length > 0) {
      if (!recursive) {
        throw new IOException("Found non-empty directory " + f + " while performing non-recursive delete");
      }
    }
