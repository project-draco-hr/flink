{
  Channel server=this.serverChannel;
  ServerBootstrap bootstrap=this.bootstrap;
  if (server != null) {
    server.close().awaitUninterruptibly();
    this.serverChannel=null;
  }
  if (bootstrap != null) {
    if (bootstrap.group() != null) {
      bootstrap.group().shutdownGracefully();
    }
    this.bootstrap=null;
  }
}
