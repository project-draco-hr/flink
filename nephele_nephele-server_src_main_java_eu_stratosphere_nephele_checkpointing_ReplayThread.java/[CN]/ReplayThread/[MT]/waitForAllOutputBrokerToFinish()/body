{
  while (!this.executionObserver.isCanceled()) {
    boolean finished=true;
    final Iterator<ReplayOutputChannelBroker> it=this.outputBrokerMap.values().iterator();
    while (it.hasNext()) {
      if (!it.next().hasFinished()) {
        finished=false;
      }
    }
    if (finished) {
      break;
    }
    Thread.sleep(SLEEPINTERVAL);
  }
}
