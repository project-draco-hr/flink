{
  if (this.restartRequested.compareAndSet(true,false)) {
    if (!this.interruptCalled.compareAndSet(true,false)) {
      try {
        Thread.sleep(10L);
      }
 catch (      InterruptedException e) {
      }
      this.interruptCalled.set(false);
    }
    Thread.interrupted();
    return true;
  }
  return false;
}
