{
  SopremoTestPlan sopremoPlan=new SopremoTestPlan(1,1);
  Intersection intersection=new Intersection(sopremoPlan.getInputOperator(0));
  sopremoPlan.getOutputOperator(0).setInputs(intersection);
  sopremoPlan.getInput(0).add(createJsonValue(1)).add(createJsonValue(2)).add(createJsonValue(3));
  sopremoPlan.getExpectedOutput(0).add(createJsonValue(1)).add(createJsonValue(2)).add(createJsonValue(3));
  sopremoPlan.run();
}
