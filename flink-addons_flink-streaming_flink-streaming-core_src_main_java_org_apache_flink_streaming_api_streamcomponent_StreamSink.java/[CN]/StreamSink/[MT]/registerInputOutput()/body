{
  initialize();
  try {
    setSerializers();
    setSinkSerializer();
    inputs=getConfigInputs();
    inputIter=createInputIterator(inputs,inTupleSerializer);
  }
 catch (  Exception e) {
    throw new StreamComponentException("Cannot register inputs for " + getClass().getSimpleName(),e);
  }
  setInvokable();
}
