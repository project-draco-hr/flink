{
  Class<?>[] parameterTypes=method.getParameterTypes();
  Signature signature;
  if (parameterTypes.length == 1 && parameterTypes[0].isArray() && JsonNode.class.isAssignableFrom(parameterTypes[0].getComponentType()))   signature=new ArraySignature(parameterTypes);
 else   if (method.isVarArgs())   signature=new VarArgSignature(parameterTypes);
 else   signature=new Signature(parameterTypes);
  this.originalSignatures.put(signature,method);
  this.cachedSignatures.clear();
  this.cachedSignatures.putAll(this.originalSignatures);
}
