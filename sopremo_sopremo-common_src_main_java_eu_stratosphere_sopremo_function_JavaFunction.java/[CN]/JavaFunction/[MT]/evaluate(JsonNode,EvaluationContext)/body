{
  JsonNode[] params=getParams(node);
  Class<?>[] paramTypes=getParamTypes(params);
  Method method=findBestMethod(new Signature(paramTypes));
  if (method == null)   throw new EvaluationException(String.format("No method %s found for parameter types %s",getName(),Arrays.toString(paramTypes)));
  return invoke(method,params);
}
