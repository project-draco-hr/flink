{
synchronized (this) {
    if (executingThread == null) {
      String name=owner.getTaskNameWithSubtasks();
      if (LOG.isDebugEnabled()) {
        name=name + " (" + owner.getExecutionId()+ ")";
      }
      executingThread=new Thread(TASK_THREADS,this,name);
    }
    return executingThread;
  }
}
