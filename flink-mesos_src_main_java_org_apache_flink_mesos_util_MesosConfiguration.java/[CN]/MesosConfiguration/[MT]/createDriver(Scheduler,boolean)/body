{
  MesosSchedulerDriver schedulerDriver;
  if (this.credential().isDefined()) {
    schedulerDriver=new MesosSchedulerDriver(scheduler,frameworkInfo.build(),this.masterUrl(),false,this.credential().get().build());
  }
 else {
    schedulerDriver=new MesosSchedulerDriver(scheduler,frameworkInfo.build(),this.masterUrl(),false);
  }
  return schedulerDriver;
}
