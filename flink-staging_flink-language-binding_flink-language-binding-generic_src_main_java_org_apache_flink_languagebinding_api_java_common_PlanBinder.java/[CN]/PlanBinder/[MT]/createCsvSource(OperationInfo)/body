{
  if (!(info.types instanceof CompositeType)) {
    throw new RuntimeException("The output type of a csv source has to be a tuple or a " + "pojo type. The derived type is " + info);
  }
  sets.put(info.setID,env.createInput(new CsvInputFormat(new Path(info.path),info.lineDelimiter,info.fieldDelimiter,(CompositeType)info.types),info.types).name("CsvSource"));
}
