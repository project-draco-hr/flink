{
  BlockingQueue<Object> queue=null;
  int count=0;
  while (queue == null) {
    queue=safeRetrieval(iterOpenMap,getIdentifier(jobID,subTaskId));
    if (queue == null) {
      Thread.sleep(100);
      count++;
      if (count == 100) {
        throw new RuntimeException("Internal Error");
      }
    }
  }
  return queue.take();
}
