{
  @SuppressWarnings("resource") BufferedReader br=new BufferedReader(new FileReader("src/test/resources/testdata/ASTopology.data"));
  while (true) {
    String line=br.readLine();
    if (line == null) {
      break;
    }
    if (!line.equals("")) {
      String[] link=line.split(":");
      outRecord.f0=Integer.valueOf(link[0]);
      outRecord.f1=Integer.valueOf(link[1]);
      outRecord.f2=timestamp;
      collector.collect(outRecord);
      timestamp+=1;
    }
  }
}
