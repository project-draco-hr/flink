def collect(self, value):
    if (not isinstance(value, (list, tuple))):
        self._send_field(value)
    else:
        self._connection.write(Types.TYPE_TUPLE)
        meta = pack('>I', len(value))
        self._connection.write((bytes([meta[3]]) if PY3 else meta[3]))
        for field in value:
            self.collect(field)
