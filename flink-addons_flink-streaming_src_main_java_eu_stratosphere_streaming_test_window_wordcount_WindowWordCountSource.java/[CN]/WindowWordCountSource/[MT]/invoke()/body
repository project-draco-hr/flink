{
  line=br.readLine().replaceAll("[\\-\\+\\.\\^:,]","");
  timestamp=0;
  for (int i=0; i < 2; ++i) {
    if (line != "") {
      hamletRecord=new StreamRecord(2);
      hamletRecord.setField(0,new StringValue(line));
      hamletRecord.setField(1,new LongValue(timestamp));
      System.out.println("========line number: " + timestamp + ", "+ line+ "==========");
      emit(hamletRecord);
      line=br.readLine();
      ++timestamp;
    }
  }
}
