{
  line=br.readLine();
  line=line.replaceAll("[\\-\\+\\.\\^:,]","");
  timestamp=0;
  for (int i=0; i < 1000; ++i) {
    if (line == "") {
      continue;
    }
    hamletRecord=new Record(new StringValue("You do not understand yourself so clearly"));
    System.out.println("========line number: " + timestamp + ", "+ line+ "==========");
    emit(hamletRecord);
    line=br.readLine();
    ++timestamp;
  }
}
