{
  int numTuple=record.getNumOfTuples();
  for (int i=0; i < numTuple; ++i) {
    word=record.getString(i,0);
    count=record.getInteger(i,1);
    timestamp=record.getLong(i,2);
    System.out.println("============================================");
    System.out.println(word + " " + count+ " "+ timestamp);
    System.out.println("============================================");
  }
}
