def _run(self):
    collector = self._collector
    function = self.reduce
    iterator = self._group_iterator
    iterator._init()
    while iterator.has_group():
        iterator.next_group()
        if iterator.has_next():
            base = iterator.next()
            for value in iterator:
                base = function(base, value)
        collector.collect(base)
    collector._close()
