{
  if (len == 1) {
    int highByte=this.value & 0xff;
    highByte-=Byte.MIN_VALUE;
    target[offset]=(byte)highByte;
  }
 else   if (len <= 0) {
  }
 else {
    int highByte=this.value & 0xff;
    highByte-=Byte.MIN_VALUE;
    target[offset]=(byte)highByte;
    for (int i=1; i < len; i++) {
      target[offset + i]=0;
    }
  }
}
