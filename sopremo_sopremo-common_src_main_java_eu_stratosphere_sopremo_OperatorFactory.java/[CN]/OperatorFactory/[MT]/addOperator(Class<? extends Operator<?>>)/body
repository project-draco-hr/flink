{
  String name;
  Name nameAnnotation=ReflectUtil.getAnnotation(operatorClass,Name.class);
  if (nameAnnotation != null)   name=this.operatorNameChooser.choose(nameAnnotation.noun(),nameAnnotation.verb(),nameAnnotation.adjective(),nameAnnotation.preposition());
 else   name=operatorClass.getSimpleName().toLowerCase();
  this.operators.put(name,new OperatorInfo(operatorClass,name));
}
