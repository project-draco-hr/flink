{
  long val=0;
  boolean neg=false;
  if (bytes[startPos] == '-') {
    neg=true;
    startPos++;
  }
  for (int i=startPos; i < limit; i++) {
    if (bytes[i] == delim) {
      return valueSet(field,val,neg,i + 1);
    }
    if (bytes[i] < 48 || bytes[i] > 57) {
      return -1;
    }
    val*=10;
    val+=bytes[i] - 48;
  }
  return valueSet(field,val,neg,limit);
}
