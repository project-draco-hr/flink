def addSpeed(counters):
    for (tname, number, df) in counters:
        speed = [0]
        values = list(df.ix[:, 0])
        for i in range(1, len(values)):
            speed.append((float((values[i] - values[(i - 1)])) / float(((df.index[i] - df.index[(i - 1)]) + 0.01))))
        df['speed'] = speed
    return counters
