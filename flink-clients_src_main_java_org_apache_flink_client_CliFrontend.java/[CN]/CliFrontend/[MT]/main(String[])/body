{
  EnvironmentInformation.logEnvironmentInfo(LOG,"Command Line Client",args);
  try {
    CliFrontend cli=new CliFrontend();
    int retCode=cli.parseParameters(args);
    System.exit(retCode);
  }
 catch (  Throwable t) {
    LOG.error("Fatal error while running command line interface.",t);
    t.printStackTrace();
    System.exit(31);
  }
}
