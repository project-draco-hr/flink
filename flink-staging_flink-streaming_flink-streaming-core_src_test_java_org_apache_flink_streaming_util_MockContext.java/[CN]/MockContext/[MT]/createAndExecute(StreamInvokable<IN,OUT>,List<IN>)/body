{
  MockContext<IN,OUT> mockContext=new MockContext<IN,OUT>(inputs);
  invokable.setup(mockContext);
  try {
    invokable.open(null);
    invokable.invoke();
    invokable.close();
  }
 catch (  Exception e) {
    throw new RuntimeException("Cannot invoke invokable.",e);
  }
  return mockContext.getOutputs();
}
