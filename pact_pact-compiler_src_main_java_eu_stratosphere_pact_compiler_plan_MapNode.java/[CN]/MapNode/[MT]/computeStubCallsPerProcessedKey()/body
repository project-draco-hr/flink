{
  OptimizerNode pred=input == null ? null : input.getSourcePact();
  if (pred != null) {
    if (pred.getEstimatedNumRecords() != -1 && pred.getEstimatedKeyCardinality() != -1) {
      return (pred.getEstimatedNumRecords() / pred.getEstimatedKeyCardinality());
    }
 else {
      return -1;
    }
  }
 else {
    return -1;
  }
}
