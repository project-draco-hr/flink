{
  final SopremoModule sopremoModule=new SopremoModule(this.toString(),2,1);
  final Projection leftProjection=new Projection(sopremoModule.getInput(0)).withKeyTransformation(this.leftJoinKey);
  final Projection rightProjection=new Projection(sopremoModule.getInput(1)).withKeyTransformation(this.rightJoinKey);
  final Operator joinAlgorithm=this.createJoinContract(leftProjection,rightProjection);
  sopremoModule.getOutput(0).setInputs(new Projection(joinAlgorithm).withValueTransformation(new ArrayMerger()));
  return sopremoModule;
}
