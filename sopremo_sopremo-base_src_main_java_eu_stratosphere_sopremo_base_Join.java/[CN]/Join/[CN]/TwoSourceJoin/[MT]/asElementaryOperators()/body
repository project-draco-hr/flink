{
  final SopremoModule sopremoModule=new SopremoModule(this.toString(),2,1);
  final Operator leftProjection=new Projection().withKeyTransformation(this.leftJoinKey).withInputs(sopremoModule.getInput(0));
  final Operator rightProjection=new Projection().withKeyTransformation(this.rightJoinKey).withInputs(sopremoModule.getInput(1));
  final Operator joinAlgorithm=this.createJoinContract(leftProjection,rightProjection);
  sopremoModule.getOutput(0).setInputs(new Projection().withValueTransformation(new ArrayMerger()).withInputs(joinAlgorithm));
  return sopremoModule;
}
