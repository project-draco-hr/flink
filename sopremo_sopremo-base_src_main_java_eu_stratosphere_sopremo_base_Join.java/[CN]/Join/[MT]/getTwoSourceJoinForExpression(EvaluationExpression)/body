{
  if (condition instanceof ComparativeExpression)   return new ComparisonJoin(this.getInputForExpression(((ComparativeExpression)condition).getExpr1()),this.getInputForExpression(((ComparativeExpression)condition).getExpr2()),(ComparativeExpression)condition);
  if (condition instanceof ElementInSetExpression)   return new ElementInSetJoin(this.getInputForExpression(((ElementInSetExpression)condition).getElementExpr()),this.getInputForExpression(((ElementInSetExpression)condition).getSetExpr()),(ElementInSetExpression)condition);
  throw new UnsupportedOperationException("condition " + condition + " not supported");
}
