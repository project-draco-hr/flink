{
  if (condition instanceof ComparativeExpression)   return new ComparisonJoin(module.getInput(getInputIndex(((ComparativeExpression)condition).getExpr1())),module.getInput(getInputIndex(((ComparativeExpression)condition).getExpr2())),(ComparativeExpression)condition);
  if (condition instanceof ElementInSetExpression)   return new ElementInSetJoin(module.getInput(getInputIndex(((ElementInSetExpression)condition).getElementExpr())),module.getInput(getInputIndex(((ElementInSetExpression)condition).getSetExpr())),(ElementInSetExpression)condition);
  throw new UnsupportedOperationException("condition " + condition + " not supported");
}
