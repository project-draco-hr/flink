{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (this.getClass() != obj.getClass())   return false;
  return super.equals(obj) && this.joinCondition.equals(((Join)obj).joinCondition) && this.outerJoinSources.equals(((Join)obj).outerJoinSources)&& this.resultProjection.equals(((Join)obj).resultProjection);
}
