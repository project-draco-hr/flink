{
  Assert.assertTrue((cross.getDriverStrategy() == DriverStrategy.NESTEDLOOP_BLOCKED_OUTER_FIRST && outerFirstOkay) || (cross.getDriverStrategy() == DriverStrategy.NESTEDLOOP_BLOCKED_OUTER_SECOND && outerSecondOkay));
  Assert.assertNull(combiner);
  Assert.assertEquals(ShipStrategyType.FORWARD,reducer.getInput().getShipStrategy());
}
