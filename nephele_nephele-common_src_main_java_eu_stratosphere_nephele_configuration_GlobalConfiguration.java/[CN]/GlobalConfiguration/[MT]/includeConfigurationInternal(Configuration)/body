{
  if (conf == null) {
    LOG.error("Given configuration object is null, ignoring it...");
    return;
  }
synchronized (this.confData) {
    final Iterator<String> it=conf.keySet().iterator();
    while (it.hasNext()) {
      final String key=it.next();
      this.confData.put(key,conf.getString(key,""));
    }
  }
}
