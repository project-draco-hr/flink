{
  DataSet op1=(DataSet)sets.get(info.parentID);
  DataSet op2=(DataSet)sets.get(info.otherID);
  if (info.usesUDF) {
    sets.put(info.setID,createDefaultJoin(op1,op2,info.keys1,info.keys2,mode).mapPartition(new PythonMapPartition(info.setID,info.types)).name(info.name));
  }
 else {
    sets.put(info.setID,createDefaultJoin(op1,op2,info.keys1,info.keys2,mode));
  }
}
