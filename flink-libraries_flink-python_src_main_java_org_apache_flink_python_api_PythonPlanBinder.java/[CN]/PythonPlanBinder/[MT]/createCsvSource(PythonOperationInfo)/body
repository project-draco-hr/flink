{
  if (!(info.types instanceof TupleTypeInfo)) {
    throw new RuntimeException("The output type of a csv source has to be a tuple. The derived type is " + info);
  }
  sets.put(info.setID,env.createInput(new TupleCsvInputFormat(new Path(info.path),info.lineDelimiter,info.fieldDelimiter,(TupleTypeInfo)info.types),info.types).name("CsvSource").map(new SerializerMap()).name("CsvSourcePostStep"));
}
