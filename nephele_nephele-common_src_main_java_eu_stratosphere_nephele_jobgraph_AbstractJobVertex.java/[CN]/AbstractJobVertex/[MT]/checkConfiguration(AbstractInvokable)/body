{
  if (invokable == null) {
    throw new IllegalArgumentException("Argument invokable is null");
  }
  try {
    invokable.checkConfiguration();
  }
 catch (  IllegalConfigurationException icex) {
    throw icex;
  }
catch (  Throwable t) {
    throw new IllegalConfigurationException("Checking the invokable's configuration caused an error: " + StringUtils.stringifyException(t));
  }
}
