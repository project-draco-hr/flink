{
  if (LOG.isDebugEnabled())   LOG.debug(getLogString("Start registering input and output"));
  ClassLoader cl;
  try {
    cl=LibraryCacheManager.getClassLoader(getEnvironment().getJobID());
  }
 catch (  IOException ioe) {
    throw new RuntimeException("Usercode ClassLoader could not be obtained for job: " + getEnvironment().getJobID(),ioe);
  }
  initInputFormat(cl);
  try {
    initOutputs(cl);
  }
 catch (  Exception ex) {
    throw new RuntimeException("The initialization of the DataSource's outputs caused an error: " + ex.getMessage(),ex);
  }
  if (LOG.isDebugEnabled())   LOG.debug(getLogString("Finished registering input and output"));
}
