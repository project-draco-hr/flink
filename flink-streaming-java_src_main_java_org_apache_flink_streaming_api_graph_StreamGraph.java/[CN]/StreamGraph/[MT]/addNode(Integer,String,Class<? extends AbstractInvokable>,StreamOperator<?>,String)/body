{
  if (streamNodes.containsKey(vertexID)) {
    throw new RuntimeException("Duplicate vertexID " + vertexID);
  }
  StreamNode vertex=new StreamNode(environemnt,vertexID,slotSharingGroup,operatorObject,operatorName,new ArrayList<OutputSelector<?>>(),vertexClass);
  streamNodes.put(vertexID,vertex);
  return vertex;
}
