{
  timestamp=0;
  line=br.readLine().replaceAll("[\\-\\+\\.\\^:,]","");
  while (line != null) {
    if (line != "") {
      lineValue.setValue(line);
      timestampValue.setValue(timestamp);
      emit(new StreamRecord(lineValue,timestampValue));
    }
    line=br.readLine();
    timestamp++;
  }
}
