{
  Class<? extends Key>[] keyClasses1=this.getKeyClasses(globalSchema,keyIndices1);
  Class<? extends Key>[] keyClasses2=this.getKeyClasses(globalSchema,keyIndices2);
  if (!Arrays.equals(keyClasses1,keyClasses2))   throw new IllegalStateException(String.format("The key classes are not compatible (schema %s; indices %s %s; key classes: %s %s)",globalSchema,keyIndices1,keyIndices2,keyClasses1,keyClasses2));
  return keyClasses1;
}
